   <script>
    function toggleMiniMenu(setClass) {
                $('.' + setClass + ".toggle-icon").toggleClass("icon-plus-sign").toggleClass("icon-minus-sign");
                $('.' + setClass + ":not(.toggle-icon)").toggle();
                return false;
            }

            $(document).ready(function () {
                //$('#main-nav').load("quicklinks.html");

                tryLoadJQueryElements();

                // only do this once, and do it after the objects have been collapsed
                var uncollapsers = $('.page_collapsible_show');
                if (uncollapsers) {
                    uncollapsers.collapsible('open');
                }

                // Add the jQuery center() method...
                jQuery.fn.center = function () {
                    this.css("position", "absolute");
                    // consoleLog("vCenter ($(window).height()[" + $(window).height() + "] - this.height()[" + this.height() + "]) / 2 + $(window).scrollTop()[" + $(window).scrollTop() + "] -> " + (($(window).height() - this.height()) / 2 + $(window).scrollTop()));
                    // consoleLog("hCenter ($(window).width()[" + $(window).width() + "] - this.width()[" + this.width() + "]) / 2 + $(window).scrollLeft()[" + $(window).scrollLeft() + "] -> " + (($(window).width() - this.width()) / 2 + $(window).scrollLeft()));
                    var offLeft = 0;
                    var offTop = 0;



                    var main = $("#main-content");
                    if ($(this).closest(main).length > 0) {
                        var contentOffset = main.offset();
                        var offLeft = contentOffset.left;
                        var offTop = contentOffset.top;
                    }
                    this.css("top", ($(window).height() - this.height()) / 2 + $(window).scrollTop() - offTop + "px");
                    this.css("left", ($(window).width() - this.width()) / 2 + $(window).scrollLeft() - offLeft + "px");
                    return this;
                };

                $("body").prepend("<div id='PleaseWait' style='display:none;'><img border='0' src='/iwe/img/bg-spinner.gif' alt='loading' title='loading' /><br />Please wait...</div>");

            });
   </script>
