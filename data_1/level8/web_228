   <script type="text/javascript">
    var selectedTabIndex = 0;
    var showWaitSpinner = 0;
    var collapsedObjects;

    function tryLoadJQueryElements() {
        try {
            var dt;

            if (dt = $('.dropdown-toggle')) { dt.dropdown(); }

            if (dt = $('.uit-popover')) { dt.popover({ html: true }).css({ cursor: "pointer" }); }

            if (dt = $('.uit-popover-bottom')) { dt.popover({ placement: 'bottom', html: true }).css({ cursor: "pointer" }); }

            if (dt = $('.uit-popover-left')) { dt.popover({ placement: 'left', html: true }).css({ cursor: "pointer" }); }

            if (dt = $('.uit-clickover')) { dt.clickover({ html: true }).css({ cursor: "pointer" }); }

            if (dt = $('.uit-clickover-bottom')) { dt.clickover({ placement: 'bottom', html: true }).css({ cursor: "pointer" }); }

            if (dt = $('.uit-clickover-left')) { dt.clickover({ placement: 'left', html: true }).css({ cursor: "pointer" }); }

            if (dt = $('#tabs')) { dt.tabs({ active: selectedTabIndex }); }

            if (dt = $('.page_collapsible')) {
                dt.collapsible();
            }

        } catch (e) {
            if (/popover/i.test(e.message)) {
                alert("There was an error while loading the page.  This is sometimes caused by certain browser plugins.  Please disable AdBlock or other similar browser plugins and reload the page.");
            } else {
                alert("tryLoadJQueryElements Error: " + e.message);
            }
        }
    }
   </script>
   <script type="text/javascript">
    function EndRequestHandler_mp(sender, args) {
        try {
            // hide spinnerbox if visible, cancel timer if not
            $('#PleaseWait').hide();
            $("#loginWidget").show();

            $('body').removeClass('ajax-spinner');
            showWaitSpinner = 0;

            if (args.get_error() != undefined) {
                var errorMessage = args.get_error().message;
                args.set_errorHandled(true);
                alert("Ajax Error: " + errorMessage);
            }

            tryLoadJQueryElements();

            if (collapsedObjects) {
                for (var i = 0; i < collapsedObjects.length; i++) {
                    if (dt = $('#' + collapsedObjects[i])) {
                        dt.collapsible('open');
                    }
                }
            }

            if (eval('typeof(EndRequestHandler_client) == "function"')) {
                EndRequestHandler_client(sender, args)
            }

            //alert("EndRequestHandler_mp success");
        } catch (e) {
            alert("EndRequestHandler_mp Error: " + e.message);
        }
    }

    function BeginRequestHandler_mp(sender, args) {
        try {
            if (eval('typeof(BeginRequestHandler_client) == "function"')) {
                BeginRequestHandler_client(sender, args)
            }


            if (dt = $('#tabs')) { selectedTabIndex = dt.tabs('option', 'active'); }

            // set timer to show spinnerbox after 0.5 seconds
            showWaitSpinner = 1;
            setTimeout(function () {
                if (showWaitSpinner == 1) {
                    $('#PleaseWait').center().show(); // have to recenter it every time because the user has probably scrolled
                    $('body').addClass('ajax-spinner');
                }
            }, 100);


            collapsedObjects = new Array();
            if (dt = $('.collapse-open')) {
                dt.each(function (i) { collapsedObjects.push(this.id); });
            }

        } catch (e) {
            alert("BeginRequestHandler_mp Error: " + e.message);
        }
    }
   </script>
  </form>
