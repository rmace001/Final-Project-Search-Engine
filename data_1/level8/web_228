         <script>
          $(document).ready(function () {
                                        // if we're this far, javascript is running; check the browser version
                                        // (these will have to be updated at some point)
                                        var showWarning = true;
                                        var isIE11 = !!window.MSStream;
                                        switch (BrowserDetect.browser) {
                                            case "Chrome":
                                                if (BrowserDetect.version >= 30) showWarning = false;
                                                break;
                                            case "Explorer":
                                                if (BrowserDetect.version >= 11) showWarning = false;
                                                break;
                                            case "Firefox":
                                                if (BrowserDetect.version >= 24) showWarning = false;
                                                break;
                                            case "Safari":
                                                if (BrowserDetect.version > 530) showWarning = false;
                                                break;
                                            case "Other":
                                                showWarning = !isIE11;
                                                break;
                                        }
                                        if (showWarning) $("div.browserversioninfo").show();
                                        $("#loginWidget").show();
                                    });
         </script>
