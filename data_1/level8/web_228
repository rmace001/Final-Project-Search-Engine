  <script type="text/javascript">
   $(document).ready(function () {
		        $(".show-mega-menu").on("click", showMegaMenu);
		    });

		    function showMegaMenu() {
		        $("#main-nav").toggleClass("hide-medium-small")
		        return false; //don't follow the link
		    }

		    //when clicking on a smallmenu, open the menu directly below it, or close all menus below it
		    $(document).on("click", "[id^='smallmenu_']", function () {
		        var thisID = $(this).attr('id');
		        $('.' + thisID).toggle();
		        $(this).find('.smallmenu_icon').toggleClass('icon-plus-sign').toggleClass('icon-minus-sign');
		        $("[class^='" + thisID + "_']").hide();
		        $("." + thisID + " .smallmenu_icon").removeClass('icon-minus-sign').addClass('icon-plus-sign');
		        return false; //don't follow the link
		    });

		    $(document).ready(function () {

		        // bind a click event to the 'skip' link
		        $("#skip-nav").on("click", function (event) {

		            // strip the leading hash and declare
		            // the content we're skipping to
		            var skipTo = "#" + this.href.split('#')[1];

		            // Setting 'tabindex' to -1 takes an element out of normal 
		            // tab flow but allows it to be focused via javascript
		            $(skipTo).attr('tabindex', -1).on('blur focusout', function () {

		                // when focus leaves this element, 
		                // remove the tabindex attribute
		                $(this).removeAttr('tabindex');

		            }).focus(); // focus on the content container
		        });

		    });
  </script>
