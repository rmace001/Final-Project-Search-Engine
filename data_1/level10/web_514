   <script type="text/javascript">
    jQuery(document).ready(function($) {
    	jQuery.fn.cleardefault = function() {

    	return this.focus(function() {
        	if( this.value == this.defaultValue ) {
            	this.value = "";
        	}

    	}).blur(function() {
        	if( !this.value.length ) {
            	this.value = this.defaultValue;
        	}
    	});
	};
	jQuery("input.clearit, textarea.clearit, .mailllist_wrapper input, .clearthis .large").cleardefault();
});


		jQuery(document).ready(function($) {
		var responsiveContainer = $( ".responsivenav" );
		var responsiveNav = $( ".responsivenav .menu-topnav-container" );
        var responsiveNavLI = $( ".responsivenav .menu-topnav-container ul li").not(".responsivenav .menu-topnav-container ul li ul li").not(".responsivenav .menu-topnav-container ul li ul li ul li");
        var responsiveSubNavLI = $( ".responsivenav .menu-topnav-container ul li ul li").not(".responsivenav .menu-topnav-container ul li ul li ul li");
		var responsiveSearch = $( ".responsivenav #searchContainer" );

		responsiveContainer.find("#menuBtn").click(
			function(){
				responsiveNav.toggle();
				if (responsiveNav.is(":visible")) {
					$(this).parent().addClass("activated");
				}
				else {
					$(this).parent().removeClass("activated");
				}
				if (responsiveSearch.is(":visible")) {
					responsiveSearch.hide();
				}
			}
		);

		responsiveContainer.find("#searchBtn").click(
			function(){
				responsiveSearch.toggle();
				if (responsiveNav.is(":visible")) {
					responsiveNav.hide();
					responsiveContainer.find("#menuBtn").parent().removeClass("activated");
				}
			}
		);

		responsiveSubNavLI.has( "ul" ).prepend('<i class="fa fa-chevron-right" aria-hidden="true"></i>');
        responsiveSubNavLI.click(
            function(evt){
                evt.stopPropagation();
                $(this).find("> ul.sub-menu").toggle();

                if ($(this).find(" ul.sub-menu").is(":visible")) {
                    $(this).find("i.fa").removeClass("fa-chevron-right");
                    $(this).find("i.fa").addClass("fa-chevron-down");
                }
                else {
                    $(this).find("i.fa").removeClass(" ul.sub-menu fa-chevron-down");
                    $(this).find("i.fa").addClass("> ul.sub-menu fa-chevron-right");
                }
            }
        );
        responsiveNavLI.has( "ul" ).prepend('<i class="fa fa-chevron-right" aria-hidden="true"></i>');
		responsiveNavLI.click(
			function(){
                $(this).find("> ul.sub-menu").toggle();
				if ($(this).find("> ul.sub-menu").is(":visible")) {
					$(this).find("> i.fa").removeClass("fa-chevron-right");
					$(this).find("> i.fa").addClass("fa-chevron-down");
				}
				else {
					$(this).find("> i.fa").removeClass("fa-chevron-down");
					$(this).find("> i.fa").addClass("fa-chevron-right");
				}
			}
		)
    });
   </script>
