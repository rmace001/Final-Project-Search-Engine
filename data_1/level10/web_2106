    CALENDAR_RESOURCES['month.long.dec'] = 'December';
    CALENDAR_RESOURCES['day.short.mon'] = 'Mon';
    CALENDAR_RESOURCES['month.long.jun'] = 'June';
    CALENDAR_RESOURCES['month.long.feb'] = 'February';
    CALENDAR_RESOURCES['month.short.dec'] = 'Dec';
    CALENDAR_RESOURCES['month.short.feb'] = 'Feb';
    CALENDAR_RESOURCES['month.short.jun'] = 'Jun';
    CALENDAR_RESOURCES['month.short.jul'] = 'Jul';
    CALENDAR_RESOURCES['day.long.wed'] = 'Wednesday';
    CALENDAR_RESOURCES['month.long.nov'] = 'November';
    CALENDAR_RESOURCES['day.long.sun'] = 'Sunday';
    CALENDAR_RESOURCES['picker.height'] = '220';
    CALENDAR_RESOURCES['month.long.mar'] = 'March';
    CALENDAR_RESOURCES['month.long.sep'] = 'September';
