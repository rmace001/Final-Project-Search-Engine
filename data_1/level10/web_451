				showArrows,
				showDotsGet = that.settings.get("hide_dots"),
			 	showArrowsGet = that.settings.get("hide_arrows");
			if (that.settings.get("transition_styles") == "slide") {
				var slideTrans = false;
			} else {
				var slideTrans = that.settings.get("transition_styles");
			}
