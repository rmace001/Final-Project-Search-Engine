              (function($) {
		function addFixedLayout(element) {
			var elementsCss = $(element).css('display');
			if(elementsCss == "table"){
				$(element).css('table-layout','fixed');
			}			
		}
		
		var elementsList = ['.splash-page .main-wrap > .container', '#banner', '.wsite-background'];
		
		for (var i = 0; i < elementsList.length; i++) {
			var theElement = elementsList[i];
			addFixedLayout(theElement);
		}

	}(jQuery));
