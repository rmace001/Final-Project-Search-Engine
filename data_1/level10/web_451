			});
			function addBgImage() {
				$('.bg_holder img').each(function() {
					var $parent = $(this).closest('.use_image_bg'),
						slidebg = $(this).attr('src');
					$parent.css('background-image','url('+slidebg+')');
				});
			}
			if ($('.full_slide_ind').hasClass('use_image_bg')) {addBgImage();}			
			that.$('.upload_bg').on("click", function() {
				addBgImage();
				var uploadImg = $(this).next('.slidebg_img');
				if (uploadImg.is(':visible')) {
					$(this).text('click to upload image').css('background','#4B5155');
					uploadImg.slideUp();
				}else{
					$(this).text('save image').css('background','#00B26F');
					uploadImg.slideDown();
				}
			});	

		}
	});
