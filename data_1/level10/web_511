var radioHandler = function() {
                                if (this.checked == true) {
                                        this.className = "active";
                                        $("#" + this.id + "-label").each(function() {
                                                this.className = "active";
                                        });
                                } else {
                                        this.className = "inactive";
                                        $("#" + this.id + "-label").each(function() {
                                                this.className = "inactive";
                                        });
                                }
        }

        $(document).ready(function() {
                $("#tdr_search2_content input[type=radio]").click(function() {
                        $("input[type=radio]", this.form).each(radioHandler);
                });
                $("#tdr_search2_content input[type=radio]").each(radioHandler);

                $('#sitesearch').on('submit', function() { if ($('#search-scope2').is(':checked')) { var q = $('#q').val() + ' more:cascade-cwp'; var loc = 'https://act.ucsd.edu/cwp/tools/search?search-scope=default_collection&q='+q+'&site=blink2'; window.location.href = loc; return false; } });

        });
