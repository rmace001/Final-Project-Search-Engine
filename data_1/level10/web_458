 $(document).ready(function() {
  // determine if a modal window was specified
  if(window.location.href.indexOf('#') != -1) {
    // get rest of #
    var anchor = document.location.hash;
    // determine if modal window
    if($(anchor)[0] && $(anchor).hasClass('modal')) {
        $(anchor).modal('show');
    } // end - if($(anchor)[0])

    // determine if tab
    if($(anchor)[0] && $(anchor).hasClass('tab-pane')) {
        $('.nav-tabs a[href='+anchor+']').tab('show');
        // Change anchor for page-reload
        $('.nav-tabs a').on('shown.bs.tab', function (e) {
            window.location.hash = e.target.hash.replace("#", "#");
        });
    } // end - if($(anchor)[0] && $(anchor).hasClass('tab-pane'))
  } // end - if(window.location.href.indexOf('#') != -1)
}); // end - $(document).ready(function()
</script>
