    <div id="navSec">
    </div>
    <script type="text/javascript">
     $(function () {
        var timeoutTimer = null;
        var warningTimer = null;

        var UPDATE_INTERVAL = 1000;   // update every sec

        var warningCountDownTimer = null;
        var warningCountDownTimeSpan;

        window.onload = function () {
            $("#renew").bind("click", function () { timeoutPostBack(); renewSession(); });
            renewSession();
        }

        function renewSession() {
            warningCountDownTimeSpan = TIMEOUT_TIME - WARNING_TIME;
            if (warningCountDownTimeSpan <= 0)
                return;
            $("#warningTime").html(formatTime(WARNING_TIME));
            $("#warningMessage").css("display", "none");
            $("#timeoutMessage").css("display", "none");

            clearInterval(warningCountDownTimer);
            clearTimeout(warningTimer);
            clearTimeout(timeoutTimer);

            warningTimer = setTimeout(onWarning, WARNING_TIME);
        }

        function onWarning() {
            warningCountDownTimer = setInterval(updateCountDown, UPDATE_INTERVAL);
            timeoutTimer = setTimeout(onTimeout, warningCountDownTimeSpan + 2 * UPDATE_INTERVAL);
            $("#warningMessage").css("display", "");
        }

        function onTimeout() {
            clearTimeout(warningCountDownTimer);
            $("#warningMessage").css("display", "none");
            $("#timeoutMessage").css("display", "");
        }

        function updateCountDown() {
            $("#countDown").html(formatTime(warningCountDownTimeSpan));
            warningCountDownTimeSpan -= UPDATE_INTERVAL;
        }

        function formatTime(ms) {
            var x = ms / 1000;
            var sec = x % 60;
            var min = Math.floor(x / 60);
            return (min + ":" + ((sec < 10) ? "0" : "") + sec);
        }
    });
    </script>
    <div id="warningMessage" style="display: none; background-color: #f7d600; padding: 10px 15px 10px 15px; text-align: center;">
     We haven't noticed any activity for
     <span id="warningTime">
     </span>
     minutes.
     <br/>
     <a href="#" id="renew" style="color: #000000; text-decoration: underline;">
      Would you like to refresh your session?
     </a>
     <br/>
     <strong>
      Time remaining:
      <span id="countDown">
      </span>
     </strong>
    </div>
    <div id="timeoutMessage" style="display: none; background-color: #f03942; font-weight: bold; padding: 10px 15px 10px 15px; text-align: center;">
     Your session has timed out.
     <br/>
     <a href="/" id="ctl00_SessionTimeoutWarning_timeoutLink" style="color: #000000; text-decoration: underline;">
      Would you like to restart your session?
     </a>
    </div>
