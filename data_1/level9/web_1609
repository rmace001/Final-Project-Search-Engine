#front-columns-box #front-columns li.widget-container.tribe-events-list-widget {
    padding-left: 5px;
    font-size:14px;
    width: 32%;
    min-width: 250px;
    margin-right: 0%;
}

#front-columns-box #front-columns a.cat-post-title {
    font-size: 14px !important;
}

#front-columns-box #front-columns h4.tribe-event-title a {
    font-size: 14px;
}

#front-columns-box #front-columns div.textwidget p a {
    font-size: 14px;
} 

@media (max-width: 800px) {
    #front-columns-box #front-columns li.widget-container.widget_text,
    #front-columns-box #front-columns li.widget-container.cat-post-widget,
   #front-columns-box #front-columns li.widget-container.tribe-events-list-widget  {
        width:100%;
        margin:0px;
    }
} 

