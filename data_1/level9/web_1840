      $(function() {


  /** Mobile Menu **/
  $('.responsive-link').click(function() {
  $('.navbar-vertical').toggleClass('active');
  $(this).text(function(i, v){
  return v === 'Menu' ? 'Close' : 'Menu'
  })
  });

  $('li.drop i').click(function () {

  $(this).parent('li').toggleClass('drop-active');
  var $menuItem = $(this).siblings('.drop-down');
  $('li.drop .drop-down').not($menuItem).slideUp().parent().removeClass('drop-active');
  });

  });
     </script>
