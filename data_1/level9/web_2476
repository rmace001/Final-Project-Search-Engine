       <div class="widget-container widget_uc_featured_video_popup_widget widget-container-inline" id="uc_featured_video_popup_widget-2">
        <div class="widget-wrapper">
         <h3 class="widget-title">
          <i aria-hidden="true" class="icon-video">
          </i>
          Featured Videos
         </h3>
         <script type="text/javascript">
          (function($){
                var widget_id = '#uc_featured_video_popup_widget-2';
                var play_mode = 'inline';
                $(document).ready(function() {
                    $('.featured-video-thumb-wrapper').click(function() {
                        uc_featured_video_active(this);
                        return false;
                    });
                });
                window.uc_featured_video_active = function(video)
                {
                    prev_id = $(widget_id + ' .video-thumb-wrapper.active').attr('data-playerid');
                    try{uc.players[prev_id].pauseVideo();} catch(ex){}
                    id = $(video).attr('data-video-id');
                    $('.featured-video-thumb-wrapper').removeClass('active');
                    $(widget_id + ' .video-thumb-wrapper').removeClass('active');
                    $(video).addClass('active');
                    $(widget_id + ' .video-thumb-wrapper[data-playerid=' + id + ']').addClass('active');
                    $(widget_id + ' .widget-subtitle').html($(video).find('.video-title').html());
                    $(widget_id + ' .textwidget').html($(video).find('.video-text').html());

                }
            })(jQuery)
         </script>
         <div class="active video-thumb-wrapper default-player youtube inline" data-playerid="video-Cia6Oa0c1bE" data-video="https://www.youtube.com/embed/Cia6Oa0c1bE?playsinline=1&amp;enablejsapi=1&amp;rel=0&amp;origin=https://careers.uclaextension.edu">
