.special-slider-link .slick-track .field-item.even.slick-slide:nth-child(2) .field-name-field-pg-slider-link a { display: inline-block; font-family: "Trebuchet MS", Helvetica, sans-serif; color:#fff !important; font-size:16px !important; line-height:30px; font-weight:normal; padding:0 !important; border:0 !important;  }
.special-slider-link .field-name-field-pg-slider-link a:hover { background-color:transparent; border:0; padding:0; }

@media all and (max-width: 455px) {
.special-slider-link .slick-track .field-item.even.slick-slide:nth-child(2) .field-name-field-pg-slider-link::before { opacity:0; }
}

@media all and (max-width: 359px) {
x.special-slider-link .slick-track .field-item.even.slick-slide:nth-child(2) .field-name-field-pg-slider-link {  top:336px; height:64px; padding-top:10px; }
x.special-slider-link .slick-track .field-item.even.slick-slide:nth-child(2) .field-name-field-pg-slider-link a { line-height: 22px; padding-top: 8px; height:64px; }
}

/*--><!]]>*/
         </style>
        </div>
       </div>
      </div>
     </div>
    </div>
