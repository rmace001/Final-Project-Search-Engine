         <div class="region region-sidebar">
          <div class="block block-block block-menu-block callout-block" id="block-block-116">
           <h3>
            MRC Spotlight
           </h3>
           <div class="content">
            <p class="spotlight-item no-indent" onmouseover="stopAutoSwitchEvent()" style="text-align:center; margin-top: 20px !important;">
             <a href="/sites/mrc.ucsf.edu/files/Hidden_Figures-4.pdf" target="_blank" title="Get the flyer.">
              <img alt="Hidden Figures screening: Wed, Feb. 20th 5:30 PM, Genentech Auditorium; Thurs, Feb. 21st 5:30 PM, Cole Hall, Food served." height="730" src="/sites/mrc.ucsf.edu/files/Hidden_Figures-spotlight.jpg" width="730"/>
             </a>
            </p>
            <p class="spotlight-item no-indent" onmouseover="stopAutoSwitchEvent()" style="text-align:center; margin-top: 20px !important;">
             <a href="/sites/mrc.ucsf.edu/files/Free_Speech_and_Diversity_Discussion_flyer.pdf" target="_blank" title="Get the flyer.">
              <img alt="Free Speech and Diversity: Notes from and Ongoing Debate." height="730" src="/sites/mrc.ucsf.edu/files/fsd-spotlight.png" width="730"/>
             </a>
            </p>
            <p class="spotlight-item no-indent" onmouseover="stopAutoSwitchEvent()" style="text-align:center; margin-top: 20px !important;">
             <a href="/sites/mrc.ucsf.edu/files/Catalyst_Project_Flyer.pdf" target="_blank" title="Get the flyer.">
              <img alt="Be a Catalyst for Change: Discussion group (Tues, Feb. 26th 12-1PM S-157) and training (Sat, Mar. 9 10-5 PM Millberry Union Conference Center)." height="730" src="/sites/mrc.ucsf.edu/files/BaCfC.png" width="730"/>
             </a>
            </p>
            <p class="spotlight-item no-indent" onmouseover="stopAutoSwitchEvent()" style="text-align:center; margin-top: 20px !important;">
             <a href="https://tiny.ucsf.edu/bhmdance" target="_blank" title="Get the flyer.">
              <img alt="Black Heritage Month Cultural Celebration: Thurs, Feb. 28th 5:30 PM Millberry Union Conference Center" height="730" src="/sites/mrc.ucsf.edu/files/BHMCC-spotlight.png" width="730"/>
             </a>
            </p>
            <script>
             <!--//--><![CDATA[// ><!--

//var randno = Math.floor(Math.random() * (4 - 0 + 0)) + 0; // max is how many you want. min is starting index.
var randno = Math.floor(Math.random() * (4 - 0 + 0)) + 0; // max is how many you want. min is starting index.
var spotlightItems = document.getElementsByClassName("spotlight-item");
spotlightItems[randno].classList.add("spotted");
var thePager = document.createElement("p");
thePager.setAttribute("class","geoPager no-indent");
thePager.innerHTML = '<a href="javascript:void(3);" onclick="switchEvent(-1)">&lang;&nbsp;&nbsp;&nbsp;&nbsp;';
spotlightItems[0].parentNode.appendChild(thePager);
for(i=0; i<spotlightItems.length; i++) {
	var pagerLabel=i+1;
	thePager.innerHTML=thePager.innerHTML + '<a class="geoPagerLink" href="javascript:void(0);" onclick="switchEvent(' + i + ')"><span>' + pagerLabel + '';
}
thePager.innerHTML += '&nbsp;&nbsp;&nbsp;&nbsp;<a href="javascript:void(0);" onclick="switchEvent(1000)">&rang;';
var geoPagerLinks = document.getElementsByClassName("geoPagerLink");
geoPagerLinks[randno].classList.add("spotting");
function switchEvent(theEvent) {
	stopAutoSwitchEvent();
	spotlightItems[randno].classList.remove("spotted");
	geoPagerLinks[randno].classList.remove("spotting");
	if(theEvent==-1) {
		theEvent=randno-1;
		if(theEvent==-1) {
			theEvent=spotlightItems.length-1;
		}
	}
	if(theEvent==1000) {
		theEvent=randno+1;
		if(theEvent==spotlightItems.length) {
			theEvent=0;
		}
	}
	randno=theEvent;
	spotlightItems[theEvent].classList.add("spotted");
	geoPagerLinks[theEvent].classList.add("spotting");
}
var theEventTimer = setInterval(autoSwitchEvent, 20000);
function autoSwitchEvent() {
	spotlightItems[randno].classList.remove("spotted");
	geoPagerLinks[randno].classList.remove("spotting");
	if(randno == spotlightItems.length-1) {
		randno=-1;
	}
	spotlightItems[randno+1].classList.add("spotted");
	geoPagerLinks[randno+1].classList.add("spotting");
	randno=randno+1;
}
function stopAutoSwitchEvent() {
    clearInterval(theEventTimer);
}

//--><!]]>
            </script>
