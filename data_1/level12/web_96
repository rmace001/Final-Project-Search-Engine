		//$(".slider-secondary-no-image h1#page-title").after('<div class="title-wrapper">').append($("h1#page-title"));
		$(".title-wrapper").append($("h1#page-title"));
		$(".content-top").after($(".title-wrapper"));	
	}
	if($("div.sidebar").length > 0 ) {
		$("body").addClass("two-column");
		$("#content").before('<div id="sidebar-2" class="column">');
		$("#sidebar-2").append($("#sidebar .sidebar"));
		if($("div#block-menu-block-2").length > 0) {
			$("body").removeClass("two-column");
			$("body").addClass("three-column");
		}
		if($("div.sidebar_top").length > 0) {
			$("body").removeClass("two-column");
			$("body").addClass("three-column");
		}
