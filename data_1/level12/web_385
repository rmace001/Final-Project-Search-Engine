        // var max_height = $(this).children('div:nth-child(2)').prop("scrollHeight") + 20;
        var max_height = $(this).children('div:nth-child(2)').outerHeight();
        
        $(this).attr('data-max-height', max_height);
        if (!$(this).hasClass('open')) {
          $(this).addClass('collapsed-initial');
        }
        else {
          $(this).addClass('expanded');
        }
        
      });
      var accordion_triggers = $('.accordion-trigger');
      accordion_triggers.click(function() {
        var accordion_pane = $(this).parent();

        if (accordion_pane.hasClass('expanded')) {

          var content = accordion_pane.children('.accordion-content');
          var scroll_height = content.prop("scrollHeight");
          var max_height = content.outerHeight();
          accordion_pane.attr('data-max-height', max_height);
          accordion_pane.removeClass('expanded');
          accordion_pane.addClass('collapsed');
          content.animate({
            height: 0
          }, 300, function() {
            // Animation complete.
            
          });
        }
        else {
          var max_height = accordion_pane.attr('data-max-height');
          accordion_pane.addClass('expanded').removeClass('collapsed');
          var content = accordion_pane.children('.accordion-content');
          var scroll_height = content.prop("scrollHeight");
          content.animate({
            height: scroll_height
          }, 300, function() {
            // Animation complete.
            content.css('height', 'auto');
          });
        }
        // check if this accordion list is inside an accordion tab
        // if yes, then we need to change the height of the accordion tab
        if ($(this).parents('.accordion-tab').length > 0) {
          var accordion_tabs_parents = $(this).parents('.accordion-tab');
          accordion_tabs_parents.each(function() {
            var accordion_tab_content = $(this).children('.accordion-content');
            accordion_tab_content.css('height','auto');
            var accordion_tab_height = accordion_tab_content.height();
          })
        }
      }); // accordion_list.click()
      
      var accordion_group_markup = empty_markup.clone().addClass('accordion-group');
      $(':not(.accordion-tab) + .accordion-tab, * > .accordion-tab:first-of-type').each(function() {
        $(this).nextUntil(':not(.accordion-tab)').addBack().wrapAll(accordion_group_markup);
      });
  
      var accordion_tabs = $('.accordion-tab').addClass('accordion-pane');
      //accordion_tabs.wrapAll(accordion_group_markup);
      $.each(accordion_tabs, function() {
        var max_height = $(this).children('div').prop("scrollHeight");
        $(this).attr('data-max-height', max_height);
        $(this).addClass('collapsed-initial');
        var content = $(this).children('div').addClass('accordion-content');
      });
      var accordion_tabs_title = $('.accordion-tab > h3');
      accordion_tabs_title.click(function() {
        var parent_tab = $(this).parent('.accordion-tab');
        var accordion_pane = $(this).parent();
        var accordion_group = accordion_pane.parent('.accordion-group');
        var expanded_accordion_panes = accordion_group.children('.accordion-pane.expanded');

        if (parent_tab.hasClass('expanded')) {
          var content = parent_tab.children('div');
          var max_height = content.prop("scrollHeight");
          parent_tab.attr('data-max-height', max_height);
          parent_tab.removeClass('expanded');
          content.animate({
            height: 0
          }, 300, function() {
            // Animation complete.
            content.hide();
          });
        }
        else {
          var max_height = parent_tab.attr('data-max-height');
          
          var content = parent_tab.children('div');
          content.show();
          expanded_accordion_panes.children('.accordion-content').animate({
            height: 0
          }, 300, function() {  
            // Animation complete.
          });
          expanded_accordion_panes.removeClass('expanded');

          // Animation complete.
          content.animate({
            height: max_height
          }, 300, function() {
            // Animation complete.
            parent_tab.addClass('expanded');
          });
        }
        

      }); // accordion_tabs  
      
      // popup
      var popup_triggers = $('.popup-trigger');
      var i = 1;
      $.each(popup_triggers, function() {
        var popup_trigger = $(this);
        var popup_target = popup_trigger.next();
        popup_trigger.attr('data-popup-target', '#popup-target-' + i);
        var popup_target_inner_html = empty_markup.clone().addClass('popup-target-inner');
        $(popup_target).wrapInner(popup_target_inner_html);
        var popup_close_button = empty_markup.clone().addClass('popup-close-button'); 
        popup_target.prepend(popup_close_button);
        popup_target.attr('id', 'popup-target-' + i).addClass('popup-target-cloned');
        popup_target.appendTo('html');
        i = i + 1;
      });
      popup_triggers.click(function() {
        $('html').addClass('popup-visible');
        var popup_trigger = $(this);
        var target_id = popup_trigger.attr('data-popup-target');
        var popup_target = $(target_id);
        popup_target.addClass('popup-target-show');
      });    
      $('.popup-target').on('click', '.popup-close-button', function(){
        var popup_close_button = $(this);
        $('html').removeClass('popup-visible');
        popup_close_button.parent('.popup-target').removeClass('popup-target-show');
      });
      
      var popup_overlay = empty_markup.clone().addClass('popup-overlay');
      $('html').prepend(popup_overlay);
      // end of popup
      
      // start of "steps"
      var steps_connecting_line = empty_markup.clone().addClass('steps-connecting-line');
      $('.step').prepend(steps_connecting_line);
      // end of steps
