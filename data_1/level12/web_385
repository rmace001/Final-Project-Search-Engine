      $.each(button_bubble, function() {
        if ($(this).css('float') == 'right') {
          $(this).addClass('right');
          $(this).removeAttr('style');
        }
        if ($(this).css('float') == 'left') {
          $(this).addClass('left');
          $(this).removeAttr('style');
        }        
      });
      
      var accordion_list = $('.accordion-list > li');
      
      $.each(accordion_list, function() {
        var accordion_pane = $(this);
        accordion_pane.addClass('accordion-pane');
        var empty_title = empty_markup.clone().addClass('accordion-trigger');
        var title_text = $(this).clone().children().remove().end().text();
        
        var title_html = empty_title.append(title_text);
        
        var content_html = $(this).children('div:nth-child(2)').addClass('accordion-content').html();
        var new_content_html = empty_markup.clone().append(content_html).html();
        //console.log('new_content_html', new_content_html);
        // .wrap(empty_markup.clone().addClass('accordion-content'));
