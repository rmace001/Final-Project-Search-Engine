 $(document).ready(function(){
		var alerturl = "https://police.stanford.edu/alertsu.json"; 
		$.ajaxSetup({cache:false});
	    $.getJSON(alerturl, function(result){
		
			var d = new Date();
			var pubdate = new Date(result.PubDate);
			pubdate.setDate(pubdate.getDate()+3);
			var headerdate = new Date(result.PubDate);
			
			alertid = result.AlertID;
			active = result.Active;
			
			if (d > pubdate || active != '1'){
				$('#feed').append('<li style="text-align:center;"><div style="font-size:125%;color:#6A6A6A;">There are currently no Alerts.</div>');
				alertexpired = "yes";
			}else{
				description = result.Description;
				description = description.replace(/<a[^>]*>|<\/a>/g, "");
				description = description.replace(/<(.|\n)*?>/g, '');
				desclen = 400; //number of characters to display on the alert
				if (description.length > desclen) {
					description = description.substring(0, desclen) + " ... <P><a href=\"alertsu.html?alertid=" + result.AlertID + "#top\"> Visit Page</a>";
				} else {
					description = description.substr(0, desclen) + " <P><a href=\"alertsu.html?alertid=" + result.AlertID + "#top\"> Visit Page</a>";
				}

				alertbody = '<div style="text-align:center;font-weight:400;font-size:150%;"><a href="alertsu.html?alertid=' + result.AlertID + '#top">' + result.Title + '</a></div><div style="text-align:center;">Posted: ' + headerdate.toLocaleString() + '</div><div style="padding-top:30px;">' + description + '</div>';
				alertbody.replace(("\n", "\t", "\r"), '');
				
				$('#feed').html(alertbody);
				alertexpired = "no";
			}	
			
			// Display Alert and set localstorage so alert only displays once
			alertpage = 'no';
			if (localStorage.getItem("AlertID") != alertid && alertexpired == "no" && alertpage == "no") {
				$(window).load(function(){
					$('#AlertModal').modal('show');
					localStorage.setItem("AlertID",alertid);
				});
			}
			
			
        });
	
	
		$('#notification').append('<span style="font-size:14pt;">There are no active notifications.</span><P>');
		
	    $("#prev-btn").click(function(){
			$("#previous").toggle();
		});
	});
	
	
	// Display Notification and set localstorage so note only displays once
	if (localStorage.getItem("NoteID") != 0 && "yes" == "no") {
		$(window).load(function(){
			$('#NotificationModal').modal('show');
			localStorage.setItem("NoteID","0");
		});
	}
</script>
<!-- include page level javascript here -->

