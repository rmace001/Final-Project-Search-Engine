        <script src="/~item.management/_js/jquery.cookie.js">
        </script>
        <script>
         $(document).ready(function(){
		var administrationExpandLinkAndIcon = $("#expandAdministrationLink, #adminExpandIcon");
		
		var administrationCollapseLinkAndIcon = $("#collapseAdministrationLink, #adminCollapseIcon");
		var administrationCollapseSection = $("#administrationSectionCollapse");
		
		
		if($.cookie('Expand_Administration_Section') == 1){
			administrationExpandLinkAndIcon.hide();
			administrationCollapseSection.show();
		}
		else if ($.cookie('Expand_Administration_Section') == 1){
			administrationCollapseSection.hide();
			administrationExpandLinkAndIcon.show();
		} 
		
		
		administrationExpandLinkAndIcon.on("click", function(){
			administrationExpandLinkAndIcon.hide();
			administrationCollapseSection.show();
			
			$.cookie('Expand_Administration_Section', 1, {expires: 365});
		});
		
		administrationCollapseLinkAndIcon.on("click", function(){
			administrationCollapseSection.hide();
			administrationExpandLinkAndIcon.show();
			
			$.cookie('Expand_Administration_Section', 0, {expires: 365});
		});
	});
        </script>
