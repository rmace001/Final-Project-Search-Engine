  <script>
   $( document ).ready(function() {
		if($("#search-form").length) {
			$(".csudhBannerImage").replaceWith($("#search-form"));
			$("#social-container").insertAfter($("#search-form"));
		}
	});

	function searchPrimo() {
		$("#primoJournalsQuery").attr("value", "any," + $("#search-query").attr("value").replace(/[,]/g, " "));
		$("#primoQuery").attr("value", "any,contains," + $("#search-query").attr("value").replace(/[,]/g, " "));
	}

	function primoScopeSwitch(scope) {
		$("#search-form").attr("action", "https://csudh-primo.hosted.exlibrisgroup.com/primo-explore/search");
		switch($(scope).attr("value")) {
			case "jsearch_slot":
				$("#search-form").attr("action", "https://csudh-primo.hosted.exlibrisgroup.com/primo-explore/jsearch");
				$("#primoScope").attr("value", "EVERYTHING");
				$("#search-query").attr("placeholder", "Search Journals and Magazines A-Z");
				break;

			case "articles":
				$("#primoScope").attr("value", "articles");
				$("#search-query").attr("placeholder", "Search databases and peer reviewed journals");
				break;

			case "books_local":
				$("#primoScope").attr("value", "01CALS_UDH");
				$("#search-query").attr("placeholder", "Search items found here in the library");
				break;

			case "books_csu":
				$("#primoScope").attr("value", "01CALS");
				$("#search-query").attr("placeholder", "Find items from all CSU libraries");
				break;

			case "course_reserves":
				$("#primoScope").attr("value", "01CALS_UDH_CR");
				$("#search-query").attr("placeholder", "Search Course Reserves found here in the library");
				break;
			case "everything":
			default:
				$("#primoScope").attr("value", "EVERYTHING");
				$("#search-query").attr("placeholder", "Discover books, articles, videos, and more");
		}
		$("#primoTab").attr("value", $(scope).attr("value"));
	}
  </script>
