	function changeSelect(schoolCode, formIndex) {

		schoolCode = schoolCode.value;
		var i = 0;
		// Changed so I pass in the index value to make it easier to call this function when the page loads (there is no selected index when the page loads) -- JSP
		// var selectedValue = document.forms[f].elements[0].selectedIndex;
		var optionArray = getArray(schoolCode);

		var len = optionArray.length;

		document.forms[formIndex].second.length = len;

		for(i=0; i < len; ++i) {
			document.forms[formIndex].second[i].value = optionArray[i][0];
			document.forms[formIndex].second[i].text = optionArray[i][1];
		}
	    return true;
	}


	function getSchools() {
		return [['RCI','Research Centers and Institutes']
 ,['N/A','School Not Applicable']
 ,['ARTS','Claire Trevor School of the Arts']
 ,['ICS','Donald Bren School of Information and Computer Sciences']
 ,['ENGR','The Henry Samueli School of Engineering']
 ,['GSM','Paul Merage School of Business']
 ,['BIOSCI','School of Biological Sciences']
 ,['DEPTED','School of Education']
 ,['HUM','School of Humanities']
 ,['LAW','School of Law']
 ,['COM','School of Medicine']
 ,['HS','College of Health Sciences']
 ,['PUBHLTH','Public Health']
 ,['PHRMSCI','Pharmaceutical Sciences']
 ,['NURSCI','Sue & Bill Gross School of Nursing']
 ,['PHYSSCI','School of Physical Sciences']
 ,['SOCECO','School of Social Ecology']
 ,['SOCSCI','School of Social Sciences']

		];
	}

	function recreateFirst(formIndex) {
		var schools = getSchools();
		var form = document.forms.first[formIndex];

		form.length = schools.length;

		for (var i=0; i<form.first.length; ++i) {
			form.first[i].value = schools[i][0];
			form.first[i].text = schools[i][1];
		}
	}

