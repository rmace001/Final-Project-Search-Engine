   <script>
    function is_tab() {
            var WIn_w = $(window).width()
            if (parseInt(WIn_w) < parseInt(767)) {
                return true;
            } else {
                return false;
            }
        }
        function is_ipad() {
            var WIn_w = $(window).width()
            if (parseInt(WIn_w) < parseInt(1000)) {
                return true;
            } else {
                return false;
            }
        }

        $(window).resize(function () {
            $('.map_listings').css({
                'overflow-Y': 'scroll',
                'overflow-X': 'hidden'
            });
            if (is_mobile() === true) {
                //alert(1);
                $('ul.campus-new-list').css('width', '100%');

                var h = ($('body').height() - ($('.topsearchbar2').height() + $('.navbar').height() + $('.topsearchbar').height()))
                h = (h - 30);

                //var h = ($(window).height() - ($('#topsearchbar').height() + $('.navbar').height()))
                $('#map_listings_over_wrapper').height(h);
                $('#map_listings_list, .map_listings_wrapper,.map_listings').height(h)
                $('.viewport').height(h)
                // } else if(is_tab() === true){
            } else if (is_ipad() === true) {
                //alert(2);
                $('ul.campus-new-list').css('width', '99%');
                var h = ($(window).height() - ($('.topsearchbar2').height() + $('.navbar').height() + $('.topsearchbar').height())) + 5;
                h = (h - 30);

                //var h = ($(window).height() - ($('#topsearchbar').height() + $('.navbar').height()))
                $('#map_listings_over_wrapper').height(h);
                $('#map_listings_list, .map_listings_wrapper,.map_listings').height(h)
                $('.viewport').height(h)
            } else {
                $('ul.campus-new-list').css('width', '99%');
                var h = ($(window).height() - ($('#topsearchbar').height() + $('.navbar').height())) + 5;
                $('#map_listings_over_wrapper').height(h);
                $('#map_listings_list, .map_listings_wrapper,.map_listings').height(h)
                $('.viewport').height(h)
            }
        });
        $(document).ready(function () {


            $('.open-popup').click(function () {
                var obj = $(this);
                $('.new-school-popup').hide();
                $(obj).parents('.pull-left').find('.new-school-popup').show();
            });
            $('.glyphicon-remove-circle').click(function () {

                $('.new-school-popup').hide();
            });


            $('#map_listings_over_wrapper').css('top', '54px');
            if (is_mobile() === true) {
                $('ul.campus-new-list > li').live('click', function () {
                    if (is_mobile() === true) {
                        var link = $(this).attr('data-link');
                        window.location = link;
                    }
                });
                var WIn_w = $(window).width()
                if (WIn_w >= 480) {
                    $('body').height($(window).height() + 250);
                }
            }
            $('.map_listings').css({
                'overflow-Y': 'scroll',
                'overflow-X': 'hidden'
            });
            $(window).resize();

            $('li[data-link]').live('click', function () {
                var link = $(this).attr('data-link');
                window.location = link;
            });
        });
        $(window).load(function () {

            var duplicateChk = {};
            $('ul.amazingcarousel-list > li').each(function () {
                if (duplicateChk.hasOwnProperty(this.id)) {
                    $(this).remove();
                } else {
                    duplicateChk[this.id] = 'true';
                }
            });


            $(window).resize();
            $('.map_listings').css({
                'overflow-Y': 'auto',
                'overflow-X': 'hidden'
            });

        });
   </script>
