   <script type="text/javascript">
    function setListings() {

            var locations = [
                
                ['S_0', 34.414001, -119.848900, 0]
            ];

            return locations;

        }

        function getMapCenter() {
            center = new google.maps.LatLng(34.414001, -119.848900)

            return center;
        }
   </script>
