$(window).scroll(function(){
  if($(window).scrollTop() > 80){
      $('header').addClass('fixed');
  } else if($(window).scrollTop() < 80){
      $('header').removeClass('fixed');
  };
  
  var scrollTop = $(window).scrollTop();
  var divOffset2 = $('.description-text2').offset().top;
  var divOffset3 = $('.description-text3').offset().top;
  var divOffset4 = $('.description-text4').offset().top;
  //var divOffset5 = $('.description-text5').offset().top;
  //var divOffset6 = $('.description-text6').offset().top;
  //var divOffsetF = $('.footer-top').offset().top;
  var dist2 = (divOffset2 - scrollTop);
  var dist3 = (divOffset3 - scrollTop);
  var dist4 = (divOffset4 - scrollTop);
  //var dist5 = (divOffset5 - scrollTop);
  //var dist6 = (divOffset6 - scrollTop);
  //var distFooter = (divOffsetF - scrollTop);

  
  if (dist4 < 100) {
    $('.description-text4').addClass('colour-bg');
  } else if (dist3 < 100) {
    $('.description-text3').addClass('colour-bg');
  } else if (dist2 < 100) {
    $('.description-text2').addClass('colour-bg');
  }
});
