  <!-- Custom JavaScript-->
  <script>
   var ucrComponentContainerEl;
var ucrContainers = [];

//This is where the magic happens
$(document).ready(function() {
    ucrComponentContainerEl = $('#statusio_components');
    ucrParseComponentInfo(ucrContainers, ucrComponentContainerEl);
    ucrSeparateIntoContainerEls(ucrContainers, ucrComponentContainerEl);
    ucrCreateStatusGrid(ucrContainers);
    ucrAdjustStatsWidgetMobile();
    ucrCreateStatsWidgetLinks();
});

function ucrCreateStatsWidgetLinks() {
    var numbers = $('.status-grid-outer').next().find('.data-block').next();
    var text = numbers.next();

    var activeIncidentEl = $('#section_incident_active');
    var activeMaintenanceEl = $('#section_maintenance_active');
    var upcomingMaintenanceEl = $('#section_maintenance_scheduled');

    for (var i = 0; i < numbers.length; i++) {
        var number = $(numbers[i]);
        var numberValue = number.text();
        var textEl = $(text[i]);
        var textValue = textEl.text();

        //if it's has a value other than 0, then turn into link to scroll to appropriate section
        if (numberValue != '0') {
            number.html('<a href="javascript:void(0);">' + numberValue + '</a>').addClass('stats-link');
            textEl.html('<a href="javascript:void(0);">' + textValue + '</a>').addClass('stats-link');

            if (i === 0) {
                number.on('click', function() {ucrScrollToComponent(activeIncidentEl)});
                textEl.on('click', function() {ucrScrollToComponent(activeIncidentEl)});
            }
            else if (i === 1) {
                number.on('click', function() {ucrScrollToComponent(activeMaintenanceEl)});
                textEl.on('click', function() {ucrScrollToComponent(activeMaintenanceEl)});
            }
            else if (i === 2) {
                number.on('click', function() {ucrScrollToComponent(upcomingMaintenanceEl)});
                textEl.on('click', function() {ucrScrollToComponent(upcomingMaintenanceEl)});
            }
        }
    }
}

function ucrAdjustStatsWidgetMobile() {
    $('#statusio_stats_widget').find('.panel-body .col-xs-12').addClass('col-xs-4').removeClass('col-xs-12');
}

function ucrParseComponentInfo(containers, componentContainerEl) {
    componentContainerEl.find('div.component').each(function(i, el) {
        var componentDiv = $(el);
        var componentName = componentDiv.find('.component_name').text();
        var containerName = componentDiv.find('.container_name').text();
        var componentStatus = componentDiv.find('.component-status').text();
        ucrAddComponent(containers, containerName, componentName, componentStatus, componentDiv);
    });
}

function ucrAddComponent(containers, containerName, componentName, componentStatus, componentEl) {
    var container = ucrFindOrAddContainer(containers, containerName);
    container.components.push({
        name: componentName,
        lowerName: componentName.toLowerCase(),
        status: componentStatus,
        el: componentEl.removeClass('component_last').removeClass('component_first'),
        container: container,
        statusPriority: ucrGetStatusPriority(componentStatus)
    });
}

function ucrFindOrAddContainer(containers, containerName) {
    var container;
    for (var i = 0; i < containers.length; i++) {
        if (containers[i].name == containerName) {
            container = containers[i];
            break;
        }
    }

    if (!container) {
        container = {
            name: containerName,
            components: [],
            el: null
        };
        containers.push(container);
    }

    return container;
}

function ucrSeparateIntoContainerEls(containers, componentContainerEl) {
    var allIssueComponenets = [];
    var allOperationalComponenets = [];
    _.forEach(containers, function(container) {
        _.forEach(container.components, function(component) {
            if (component.status !== 'Operational') {
                allIssueComponenets.push(component);
            }
            else {
                allOperationalComponenets.push(component);
            }
        });
    });

    var sortedOperationalComponents = _.sortBy(allOperationalComponenets, [function(component){
        return component.lowerName;
    }]);

    var operationalContainerEl = ucrGetNewContainerEl(componentContainerEl);

    _.forEach(sortedOperationalComponents, function(component) {
        if (sortedOperationalComponents[0] === component) {
            component.el.addClass('component_first');
        }
        if (sortedOperationalComponents[sortedOperationalComponents.length - 1] === component) {
            component.el.addClass('component_last');
        }

        component.el.appendTo(operationalContainerEl);
    });

    if (sortedOperationalComponents.length > 0) {
        $('<h3>Operational Services</h3>').addClass('extra-contrast').prependTo(operationalContainerEl);
    }

    var sortedIssueComponents = _.sortBy(allIssueComponenets, [function(component){
        return ucrGetStatusPriority(component.status);
    }]).reverse();

    var issueContainerEl = ucrGetNewContainerEl(componentContainerEl);
    _.forEach(sortedIssueComponents, function(component) {
        if (sortedIssueComponents[0] === component) {
            component.el.addClass('component_first');
        }
        if (sortedIssueComponents[sortedIssueComponents.length - 1] === component) {
            component.el.addClass('component_last');
        }

        component.el.appendTo(issueContainerEl);
    });

    if (sortedIssueComponents.length > 0) {
        $('<h3>Current Disruptions/Maintenance</h3>').addClass('extra-contrast').prependTo(issueContainerEl);
    }

    // $('#statusio_external_services h3').addClass('extra-contrast');
    // $('#section_maintenance_scheduled h3').addClass('extra-contrast');
    $('.page_section h3').addClass('extra-contrast');
    $('.timelineMajorMarker').addClass('extra-contrast');
}

function ucrGetStatusPriority(status) {
    if (status === 'Security Issue') {
        return 6;
    }
    else if (status === 'Service Disruption') {
        return 5;
    }
    else if (status === 'Partial Service Disruption') {
        return 4;
    }
    else if (status === 'Partial Service Disruption') {
        return 3;
    }
    else if (status === 'Degraded Performance') {
        return 2;
    }
    else if (status === 'Planned Maintenance') {
        return 1;
    }
    else if (status === 'Operational') {
        return 0;
    }
}

function ucrGetNewContainerEl(componentContainerEl) {
    var containerEl = $("<div/>").addClass('service-container');
    componentContainerEl.prepend(containerEl);
    return containerEl;
}

function ucrCreateStatusGrid(containers) {
    var panel = $('<div class="status-grid-outer"></div>');
    var row = $('<div class="row"></div>');
    row.html('<div class="col-xs-12 status-grid-container"></div>');
    var gridContainerEl = $('#statusio_stats_widget');

    if (!gridContainerEl.length) {
        return;
    }

    panel.append(row);
    gridContainerEl.prepend(panel);

    var statusGridEl = $('<div/>').addClass('status-grid').addClass('text-center');
    row.find('.status-grid-container').append(statusGridEl);

    var components = [];
    _.forEach(containers, function(container) {
        _.forEach(container.components, function(component) {
            components.push(component);
        });
    });

    var sortedComponents = _.sortBy(components, ['statusPriority', 'lowerName']);
    var modalStatusGridEl = $('#mobileStatusGridModal').find('.modal-body');

    _.forEach(sortedComponents, function(component) {
        ucrAddStatusBox(modalStatusGridEl, component, 'box-sm');
        ucrAddStatusBox(statusGridEl, component, 'box');
    });

    var statusGridHeaderRowEl = $('<div class="row"></div>');
    var statusGridHeaderColEl = $('<div class="col-md-8 col-sm-6 col-xs-6 text-left"></div>').append($('<h3></h3>').text(ucrGetStatusGridTitle(containers)));
    var statusGridLastUpdatedColEl = $('<div class="col-md-4 col-sm-6 col-xs-6"><p class="pull-right white"></p></div>');

    statusGridHeaderRowEl.append(statusGridHeaderColEl).append(statusGridLastUpdatedColEl);
    statusGridEl.prepend(statusGridHeaderRowEl);

    var updatedEl = statusGridLastUpdatedColEl.find('p');
    ucrUpdatePageLoadTime(updatedEl);

    setInterval(function() {
        ucrUpdatePageLoadTime(updatedEl);
    }, 10 * 1000);

    ucrSetupStickyStatusGrid(gridContainerEl);

    $('<div class="ucr-sticky-spacer"></div>').insertAfter(gridContainerEl);
}

var ucrGridContainerTop = null;
function ucrSetupStickyStatusGrid(gridContainerEl) {
    var mobileBreakpoint = 768;

    $(window).on('scroll', function() {
        var top = jQuery(window).scrollTop();
        var gridContainerTop = ucrGridContainerTop || gridContainerEl.offset().top;// + gridContainerEl.outerHeight();
        ucrGridContainerTop = gridContainerTop;
        var adjustmentOffset = 83; //adjustment on mobile since header is fixed
        if (window.innerWidth >= mobileBreakpoint) {
            adjustmentOffset = 0;
        }

        if ((gridContainerTop - adjustmentOffset) > top) {
            gridContainerEl.removeClass('sticky').removeClass('container');
        }
        else {
            gridContainerEl.addClass('sticky').addClass('container');
        }
    });
}

var ucrPageLoadTime = new Date();
function ucrUpdatePageLoadTime(lastUpdatedEl) {
    lastUpdatedEl.text('Updated ' + moment().to(ucrPageLoadTime));
}

function ucrGetStatusGridTitle(containers) {
    var allComponents = _.flatMap(containers, function(container) {return container.components});

    var hasIssues = _.some(allComponents, function(component) {return ucrGetStatusPriority(component.status) >= 2});
    if (hasIssues) {
        return 'Active Issues';
    }

    var hasMaintenance = _.some(allComponents, function(component) {return ucrGetStatusPriority(component.status) === 1});
    if (hasMaintenance) {
        return 'Planned Maintenance In Progress';
    }

    return 'All Systems Operational';
}

function ucrAddStatusBox(statusGridEl, component, type) {
    var statusBox = $('<div/>').addClass('status-box').addClass(_.kebabCase(component.status)).attr('id', component.el.attr('id') + '-' + type);

    if (component.status === 'Operational') {
        statusGridEl.append(statusBox);
    }
    else {
        statusGridEl.prepend(statusBox);
    }

    ucrAddTooltipTemplateEl(statusGridEl, component);
    ucrAddTooltip(component, type);
}

function ucrAddTooltipTemplateEl(statusGridEl, component) {
    var parentEl = $('<div/>').attr('id', component.el.attr('id') + '-template');
    var titleEl = $('<h5 class="ucr-tooltip-title"><a href="javascript:void(0);"><span></span> <i class="fa fa-arrow-circle-o-down"></i></a></h5>');
    titleEl.find('span').text(component.name);
    var statusEl = $('<span class="ucr-tooltip-status"></span>');
    statusEl.addClass(_.kebabCase(component.status));
    statusEl.text(component.status);
    var containerOuterEl = $('<div class="ucr-tooltip-container-outer">');
    var containerEl = $('<label class="ucr-tooltip-container label"></label>');
    containerEl.text(component.container.name);
    containerEl.addClass(_.kebabCase(component.status));
    containerOuterEl.append(containerEl);

    parentEl.append(titleEl).append(statusEl).append(containerOuterEl);
    statusGridEl.append(parentEl);

    titleEl.find('a').on('click', function() {ucrScrollToComponent(component.el)}); //.attr('href', '#' + component.el.attr('id'));
}

function ucrScrollToComponent(el) {
    Velocity(el, "scroll", { duration: 750, easing: 'ease-out', offset: -$('#statusio_stats_widget').outerHeight()});
}

function ucrAddTooltip(component, type) {
    tippy('#' + component.el.attr('id') + '-' + type, {
        html: $('#' + component.el.attr('id') + '-template')[0],
        arrow: true,
        theme: 'light',
        size: 'big',
        interactive: true
    });
}
  </script>
