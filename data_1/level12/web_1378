         )

       $('#s9 img').hover(
         function(){ $(this).addClass('sFade') },
         function out(){ $(this).removeClass('sFade') }
         )

       $('img').bind('dragstart', function(event) { event.preventDefault(); });

    });

     $(document).ready(function () {
       $("#breadcrumb").remove();
       var mySwiper = new Swiper ('.sponsors', {
         autoplay: 4000,
         loop: true,
      })
    });

     $(document).ready(function () {
       var mySwiper = new Swiper ('.mainswiper', {
         autoplay: 5000,
         speed: 500,
         loop: true,
         effect: 'fade',
         pagination: '.swiper-pagination',
         paginationClickable: true,
         keyboardControl: true
      })
    });

     $(document).ready(function() {
       var trihi = $('#s1').height();
       if (greaterThanOrEqual($(window).width(), 800) ) {
         $("#s3").css('height', trihi-10);
      } else {
         $("#s3").css('height', (trihi*2)-10);
      }
   });

     $(window).resize(function() {
       var trihi = $('#s1').height();
       if (greaterThanOrEqual($(window).width(), 800) ) {
         $("#s3").css('height', trihi-10);
      } else {
         $("#s3").css('height', (trihi*2)-10);
      }
   });

     $(window).scroll(function() {
       var trihi = $('#s1').height();
       if (greaterThanOrEqual($(window).width(), 800) ) {
         $("#s3").css('height', trihi-10);
      } else {
         $("#s3").css('height', (trihi*2)-10);
      }
   });

     $(window).bind("load", function() {
       var trihi = $('#s1').height();
       if (greaterThanOrEqual($(window).width(), 800) ) {
         $("#s3").css('height', trihi-10);
      } else {
         $("#s3").css('height', (trihi*2)-10);
      }
   });


//--><!]]>
          </script>
         </div>
        </div>
       </div>
