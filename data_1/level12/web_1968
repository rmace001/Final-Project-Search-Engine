      <script type="text/javascript">
       $(document).ready(function() {
		var str = $('div[id="loginErrorMessage"]').text();
		if (str.indexOf("Login unsuccessful") >= 0) {
			//DO THIS
			$("#loginErrorMessageNew").html("<span><img src='./en-US/images/warning.png' style='display:inline; vertical-align:middle;' /> " + str + "</span>");
		}
		else if (str.indexOf("You have been logged out") >= 0) {
			//DO THIS
			$("#loginErrorMessageNew").html("<span><img src='./en-US/images/warning.png' style='display:inline; vertical-align:middle;' /> Thanks for using MyChart.<br />You have been logged out.</span>");
		}
		else if (str.indexOf("Please enter a") >= 0) {
			//DO THIS
			$("#loginErrorMessageNew").html("<span><img src='./en-US/images/warning.png' style='display:inline; vertical-align:middle;' /> " + str + "</span>");
		}
		else
		{
			//DO THAT
			$("#loginErrorMessageNew").html("");
		}
	});
	function myErrorCheck() {
	var strtwo = $('div[id="loginErrorMessage"]').text();
		if (strtwo.indexOf("Please enter a") >= 0) {
			//DO THIS
			return $("#loginErrorMessageNew").html("<span><img src='./en-US/images/warning.png' style='display:inline; vertical-align:middle;' /> " + strtwo + "</span>");
		}
		else
		{
			//DO THAT
			$("#loginErrorMessageNew").html("");
		}
	};
      </script>
