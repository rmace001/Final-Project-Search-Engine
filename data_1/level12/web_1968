      <script type="text/javascript">
       function copyErrorToLivePage()
{
	var errorList = document.getElementsByClassName("loginAlerts");
	var rowCount = errorList.length;
	if (rowCount == 2) //Make sure there are 2 instances
	{
		var replacementValue = errorList[1].innerHTML;
		errorList[0].innerHTML = replacementValue;
	}
	var javaItem = document.getElementById("nojavascript");
	javaItem.className += " hidden";
	var logoutCheck = document.getElementById("loginErrorMessage");
	var logoutCheckText = logoutCheck.innerText;
	if (logoutCheckText.indexOf('logged out') >= 0)
	{
		logoutCheck.className += " erroraddspace";
	}
}
copyErrorToLivePage();
function backToFolderRoot()
{
	window.location.replace(document.URL.split('/').slice(0, -1).join('/'));
}
var pageName = window.location.pathname;
var stringSplit = window.location.pathname.split("/");
var parseCount = stringSplit.length - 1;
switch (stringSplit[parseCount])
{
	case "default.asp": //For pages with "default.asp" as the locator, get the page title name and decide what to do
		var searchParams = new URLSearchParams(window.location.search);
		if (searchParams.get("action") == 'logout')
		{
			setTimeout(function(){backToFolderRoot();}, 10000);
		}
}
      </script>
