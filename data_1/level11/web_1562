  <script type="text/javascript">
   $(document).ready(function() {
    //Add mobile menu functionality
     if ($(window).width() < 768) {
         $('li.dropdown').click(function(event) {
             event.preventDefault();
             $(this).toggleClass('open');
             $('li.dropdown').not(this).removeClass('open');
         });
         $('button.navbar-toggle').click(function(event) {
             event.preventDefault();
             $('li.dropdown').removeClass('open');
         });
     }
     $('ul.dropdown-menu li a').on('click', function() {
         window.location = ($(this).attr('href'));
     });
     if ($("#news-header").length) {
	 $('li#newsNav.dropdown').addClass('active');
     }
     if ($("#navHomeactive").length) {
	 $('li#navHome').addClass('active');
     }
     if ($("#support-header").length) {
	 $('li#navSupport').addClass('active');
     }
     if ($("#services-header").length) {
	 $('li#navServices').addClass('active');
     }
     if ($("#about-header").length) {
	 $('li#navAbout').addClass('active');
     }
     if ($("#merger-header").length) {
         $('li#navMerger').addClass('active');
     }

});

// device detection
if (navigator.userAgent.match(/(iPod|iPhone|iPad)/)) {
    $(document).scroll(function() {
        $('.paralax-bg').css('background-position', 'center ' + $(document).scrollTop() + 'px').css('background-size',' 1000px 1000px');
    });
}
  </script>
