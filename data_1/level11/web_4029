      <script>
       // example: <li data-expire="April 17, 2017"></li>
	(function() {
		var $elems = document.querySelectorAll('[data-expire]'),
			today = new Date();

		for(
			var el = 0, 
			len = $elems.length,
			expire = null;
			el < len; 
			el++) {

			expire = new Date($elems[el].dataset.expire);

			if (today.getTime()>expire.getTime()) {
				$elems[el].style.display='none';
			}
		}
	})();
      </script>
