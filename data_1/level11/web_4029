     <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.8/angular.min.js" type="text/javascript">
     </script>
     <script src="https://code.angularjs.org/1.5.8/angular-sanitize.min.js" type="text/javascript">
     </script>
     <script>
      (function () {
	var gardenNewsComponent, GardensAppNextEvent, $nextEventApp;
	gardenNewsComponent = {
        bindings: {
            posts: '=',
			limit: '@',
            reverse: '='
        },
        template: [
            '<ul id="event-news" class="tight reveal">',
				'<li ng-repeat="post in $ctrl.data | orderBy:\'date\':$ctrl.reverse | filter: upcoming(\'date\', $ctrl.today) | filter: important(\'showOnHomePage\') | limitTo:$ctrl.limit">',
					'<div>',
						'<time>{{post.date | date: \'EEEE, MMMM d\'}}</time>',
						'<a ng-if="post.link" href="{{post.link}}">{{post.title}}</a>',
						'<span ng-if="!post.link">{{post.title}}</span>',
						'<span ng-if="post.details">, {{post.details}}</span>',

						'<div ng-if="post.post" class="accordion" title="Read More">',
							'<div class="accordion-section">',
								'<input id="read-more" type="checkbox" name="accordion-group" />',
								'<label onclick="" for="read-more"><h4>Read More</h4></label>',
								'<section>',
									'<span ng-bind-html="post.post"></span>',
								'</section>',
							'</div>',
						'</div>',

						'<span ng-if="post.update"> - Check Back Soon for Details</span>',
					'</div>',
				'</li>',
            '</ul>',
			'<p class="no-event" ng-show="($ctrl.data | filter: upcoming(\'date\', $ctrl.today)).length == 0">Check back soon for new events</p>'
        ].join(''),
        controller: function ($http, $scope) {
            var posts, tomorrow, tzoffset;
            posts = this;
            tzoffset = new Date().getTimezoneOffset() * 60000;
            tomorrow = new Date(Date.now() - tzoffset);
            tomorrow.setDate(tomorrow.getDate() - 1);
			$scope.upcoming = function(prop, val){
				return function(item){
					if (item[prop] >= val) return true;
				}
			}
			$scope.important = function(prop){
				return function(item){
					if (item[prop]) return true;
				}
			}
            $http.get('/script/events.json').success(function (data) {
                posts.data = data;
                posts.today = tomorrow.toISOString().slice(0, -1);
            });
        }
    };
	
    GardensAppNextEvent = angular.module('GardensAppNextEvent', [
        'ngSanitize'
    ]).component('gardensNews', gardenNewsComponent);
	
	$nextEventApp = document.getElementById('next-event-app');

	angular.bootstrap(angular.element($nextEventApp), ['GardensAppNextEvent']);
}.call(this));
     </script>
