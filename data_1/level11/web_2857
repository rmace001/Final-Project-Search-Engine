     <h1>
      Student Health &amp; Counseling Services
     </h1>
     <!-- node.tpl.php -->
     <article about="/student-health-counseling-services" class="node node-page " role="article" typeof="sioc:Item foaf:Document">
      <span class="rdf-meta element-hidden" content="Student Health &amp; Counseling Services" property="dc:title">
      </span>
      <span class="rdf-meta element-hidden" content="0" datatype="xsd:integer" property="sioc:num_replies">
