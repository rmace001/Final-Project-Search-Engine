       <script type="text/javascript">
        var submit;
				function doSearch(form)
				{
					var searchURL;
					switch(submit.value) {
						case "people":
							searchURL="http://directory.ucdavis.edu/search/directory_results.shtml?filter="+form.search.value;
							break;
						case "departments":
							searchURL="http://directory.ucdavis.edu/DeptListingSearchResults.htm?filter="+form.search.value;
							break;
						default:
							searchURL="https://www.ucdavis.edu/search/google/"+form.search.value;
					}
					window.location.href=searchURL;
					return false;
				}

				function doClicked(button)
				{
  					submit=button;
				}
       </script>
       <form action="#" class="search-directory" onsubmit="return doSearch(this);">
        <input autofocus="" class="search-directory__input" name="search" placeholder="search" type="text"/>
        <input class="search-directory__submit" onclick="doClicked(this);" type="submit" value="pages"/>
        <input class="search-directory__submit" onclick="doClicked(this);" type="submit" value="people"/>
        <input class="search-directory__submit" onclick="doClicked(this);" type="submit" value="departments"/>
       </form>
      </div>
