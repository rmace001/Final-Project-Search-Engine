            <a class="hideOnFallback" style=" display: block; position: absolute; left: -100%; height: 0; width: 0; overflow: hidden;">
             <path d="M24.5 12.5a12 12 0 1 1-12-12 12 12 0 0 1 12 12m-12-10.15A10.15 10.15 0 1 0 22.66 12.5 10.15 10.15 0 0 0 12.5 2.35">
             </path>
             <path d="M16.51 13.91l-3-1.49a1.66 1.66 0 0 0-.29-.86l2.5-5.1a.79.79 0 1 0-1.42-.7l-2.5 5.1a1.64 1.64 0 1 0 1 3l3 1.48a.79.79 0 1 0 .69-1.42">
             </path>
            </a>
            <image alt="Clock Icon" class="CSUNUSULockup" src="https://usu.csun.edu/webone/src_hours/includes/clock_icon.png" style="width:15px; height:15px;" xlink:href=""/>
