  <script type="text/javascript">
   $('#departments_with_major_prep').change(function() {
        var val = $(this).val();
        if (val == "all") {
            $('div.dept_majors_group').slideDown();
        } else{
            $('div.dept_majors_group:not(#dept_majors_' + val + ')').slideUp();
            $('#dept_majors_' + val).slideDown();
        }
    });
  </script>
