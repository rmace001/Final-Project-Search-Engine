    <div class="bucket-wrap cf video-edit">
     <div class="bucket large-6 medium-portrait-12 force-width">
      <div class="videoWrapper">
       <iframe allowfullscreen="" frameborder="0" height="360" src="https://player.vimeo.com/video/280780257" width="640">
       </iframe>
