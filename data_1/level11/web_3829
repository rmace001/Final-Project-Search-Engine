   $(".major").click(function(){

		$('.active').removeClass( "active" );
		$(this).addClass( "active" );
		$('.major').addClass('inactive');
		$('.inactive.active').removeClass('inactive');
	 });

	$(".need").click(function(){
		$('#need').css('display','block');
	});
