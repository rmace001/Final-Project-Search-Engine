  <script type="text/javascript">
   //resets the select2 dropdowns to their default values based on recieved option
    function reset(option) {
        /*
        SELECT2 ELEMENTS

        $("#interested-in").val('firstdefault');
        $("#interested-in").trigger('change.select2');

        $("#need-study").val('seconddefault');
        $("#need-study").trigger('change.select2');

        $("#browse-undergrad").val('thirddefault');
        $("#browse-undergrad").trigger('change.select2');

        LOGGING

        console.log($("#interested-in").select2('val'));
        console.log($("#need-study").select2('val'));
        console.log($("#browse-undergrad").select2('val'));

        var data1 = $("#interested-in").select2("data");
        delete data1.element;
        console.log("Selected data is: "+JSON.stringify(data1));

        var data2 = $("#need-study").select2("data");
        delete data2.element;
        console.log("Selected data is: "+JSON.stringify(data2));

        var data3 = $("#browse-undergrad").select2("data");
        delete data3.element;
        console.log("Selected data is: "+JSON.stringify(data3));



        */
        switch(option){
            case 1:
                //console.log('got 1, resetting 2 and 3...');

                $("#interested-in").val('firstdefault');
                $("#interested-in").trigger('change.select2');

                $("#need-study").val('seconddefault');
                $("#need-study").trigger('change.select2');

                $("#browse-undergrad").val('thirddefault');
                $("#browse-undergrad").trigger('change.select2');
                break;
            case 2:
                //console.log('got 2, resetting 1 and 3...');

                $("#interested-in").val('firstdefault');
                $("#interested-in").trigger('change.select2');

                $("#need-study").val('seconddefault');
                $("#need-study").trigger('change.select2');

                $("#browse-undergrad").val('thirddefault');
                $("#browse-undergrad").trigger('change.select2');
                break;
            case 3:
                //console.log('got 3, resetting 1 and 2...');

                $("#interested-in").val('firstdefault');
                $("#interested-in").trigger('change.select2');

                $("#need-study").val('seconddefault');
                $("#need-study").trigger('change.select2');

                $("#browse-undergrad").val('thirddefault');
                $("#browse-undergrad").trigger('change.select2');
                break;
        } 
    }
	$(document).ready(function() {
        $('select[id=browse-undergrad] option:eq(1)').text('Explore All Majors');
        //$('select[id=browse-undergrad] option:eq(2)').text('Explore All Majors');
        $('select[id=browse-undergrad] option:eq(2)').text('View Majors By College :');
        $('select[id=browse-undergrad] option:eq(2)').attr('disabled', 'disabled');
        /*$('#major-ajax-option>h5').change(function(e){
            //console.log($('#major-ajax-option>h5').text());
        })*/
        $("#interested-in").select2().on("select2:open", function(e) {
            $('*[aria-labelledby="select2-interested-in-container"]').css('background-color','#035642');
            $('*[aria-labelledby="select2-need-study-container"]').css('background-color','#6e9e92');
            $('*[aria-labelledby="select2-browse-undergrad-container"]').css('background-color','#6e9e92');
        });
        $("#need-study").select2().on("select2:open", function(e) {
            $('*[aria-labelledby="select2-interested-in-container"]').css('background-color','#6e9e92');
            $('*[aria-labelledby="select2-need-study-container"]').css('background-color','#035642');
            $('*[aria-labelledby="select2-browse-undergrad-container"]').css('background-color','#6e9e92');
        });
        $("#browse-undergrad").select2().on("select2:open", function(e) {
            $('*[aria-labelledby="select2-interested-in-container"]').css('background-color','#6e9e92');
            $('*[aria-labelledby="select2-need-study-container"]').css('background-color','#6e9e92');
            $('*[aria-labelledby="select2-browse-undergrad-container"]').css('background-color','#035642');
        });

        $("#interested-in").select2().on("select2:select", function(e) {reset(1)});
        $("#need-study").select2().on("select2:select", function(e) {reset(2)});
        $("#browse-undergrad").select2().on("select2:select", function(e) {reset(3)});
	});
  </script>
