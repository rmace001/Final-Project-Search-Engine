  <script>
   function placeholder(){
		jQuery("#page .nf-form-cont .nf-form-layout .nf-form-content .nf-field .datepicker").removeAttr('placeholder');
		jQuery("#page .nf-form-cont .nf-form-layout .nf-form-content .nf-field .datepicker").attr("placeholder", "Birthdate");
	}
jQuery(window).load(function(){
    //placeholder();
		
		setTimeout(placeholder, 1000);
        //jQuery("<p>Test</p>").insertBefore("#select2-browse-undergrad-results li:nth-child(1)");
        //jQuery("#select2-browse-undergrad-results li:nth-child(2)").before('test');
        //jQuery("#select2-browse-undergrad-results").color("red");
		    
   /* $("#page .nf-form-cont .nf-form-layout .nf-form-content .submit-wrap #nf-field-16").click(function(){
			//$("html, body").animate({ scrollTop: 0 }, 100);
			//placeholder();
			//alert('test');
 		});*/  
});
jQuery( document ).ajaxComplete(function() {
    //console.log('triggered ajaxComplete');
	$('html, body').animate({scrollTop: $('#page').offset().top}, 'slow');
    //placeholder();
    setTimeout(placeholder, 1000);
});
