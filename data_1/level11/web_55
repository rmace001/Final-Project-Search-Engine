  <script type="text/javascript">
   (function ($) {
    
    if($('ul.three-column-masonry').length > 0) {
        var $container = $('ul.three-column-masonry');
        // initialize Masonry after all images have loaded  
        $container.imagesLoaded( function() {
            $container.masonry({
              itemSelector: 'li',
              isAnimated: false,
            });
        });
    }
    
    if($('.flexslider').length > 0 ){
    $( ".slides li .views-field-field-header-image" ).each(function( index ) {
        var slide_image = $(this).find("img").attr("src");
        $(this).css('background','url('+ slide_image +')').css('background-size', 'cover').css('background-position', '50% 25%').css('background-repeat', 'no-repeat');
    });
    }
    
    $( "#menu-hamburger" ).click(function() {
    $( ".menu-wrapper" ).slideToggle("fast");
    }); 
    
    $( "#mobile-search" ).click(function() {
    $( ".mobile-form-search" ).slideToggle("fast");
    }); 
    
    $(window).resize(function() {
    if ($(window).width() > 767) {
      $('.menu-wrapper').css('display','');
      $('.mobile-form-search').css('display', '');
    }
    });
  
    /* 2015-09-22 -jh, based on TK code on megasite
       Insert expand/collapse all button above Bootstrap Accordian elements
    */
    $.each($('.panel-group'), function(idx, elem){
      $('<button class="btn btn-small pull-right">+ Expand All</button>')
        .addClass('expand-all')
        .on('click', function(){
          var expanded = $(this).data('expanded');
          $('.collapse', elem).collapse(expanded ? 'hide' : 'show');
          $(this).html(expanded ? '+ Expand All' : '- Collapse All');
          $(this).data('expanded', !expanded);
        })
        .insertBefore(elem)
        .after('<br><br>');
      $('.collapse:first', elem).collapse('show');
    });
    
    
}(jQuery));
  </script>
