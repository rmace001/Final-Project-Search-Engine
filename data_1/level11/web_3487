  <script type="text/javascript">
   var spotItemWidth = 470;
var numSpotItems = 5;
var minSpotOffset = -695;
var maxSpotOffset = 1185;
$(document).ready(function (e) {
var spotItems = $("#spotlightItems").children();
$("#rightArrow")
.mouseenter(function () {
$(this).children(".rollover").css("right", "-10px");
})
.mouseleave(function () {
$(this).children(".rollover").css("right", "-60px");
})
.click(function () {
$("#spotlightItems").children().animate({
left: "-=470"
}, 500, "easeInOutQuart", function () {
var str = $(this).css("left");
if (Number(str.substring(0, str.length - 2)) < minSpotOffset) {
$(this).css("left", (1185).toString() + "px");
}
});
});
$("#leftArrow")
.mouseenter(function () {
$(this).children(".rollover").css("left", "-10px");
})
.mouseleave(function () {
$(this).children(".rollover").css("left", "-60px");
})
.click(function () {
$("#spotlightItems").children().animate({
left: "+=470"
}, 500, "easeInOutQuart", function () {
var str = $(this).css("left");
if (Number(str.substring(0, str.length - 2)) > maxSpotOffset) {
$(this).css("left", (-695).toString() + "px");
}
});
});
$("a.homeItem").mouseenter(function () {
$(this).find("div.homeItemRollover").css("bottom", "-10px");
$(this).find("div.gradientOverlay").css("opacity", "0.3");
})
.mouseleave(function () {
$(this).find("div.homeItemRollover").css("bottom", "-78px");
$(this).find("div.gradientOverlay").css("opacity", "1");
});
});
  </script>
