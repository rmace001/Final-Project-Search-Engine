       <script>
        function Submit_Hits(){
          var loc='';
          var $loc = $('#Campus_campus_id :selected').text();
           var $Search_term = document.getElementById('keywords').value;
           if($Search_term ==""){
             $Search_term ="Blank";
           }
           var $res_selection =   $loc;
           _gaq.push(['_trackEvent', 'Search event for: '+ ' '+ $Search_term , $res_selection, 'Search from homepage']);
        }
       </script>
       <button class="ani noselect" name="search" onclick="Submit_Hits()" type="submit">
        Search
       </button>
