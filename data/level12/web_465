  <script>
   var popup_displayed = false;

// If 7 seconds pass, trigger pop up display
setTimeout(function(){ reveal_pop_up(); }, 7000);

// If scrolled 300 pixels from the top, trigger pop up display
jQuery(window).on('scroll', function() {
    var y_scroll_pos = window.pageYOffset;
    var scroll_pos_test = 300;

    if(y_scroll_pos > scroll_pos_test) {
        reveal_pop_up();
    }
});

function reveal_pop_up() {
	if (popup_displayed == false) {
		jQuery(".pop-up-wrapper").addClass("pop-up-active");
		setTimeout(function(){ 
			jQuery(".pop-up-wrapper").css("opacity", "1.0");
		} , 1000);
		popup_displayed = true;
	}
}


jQuery( document ).ready(function() {
    jQuery( ".pop-up-close" ).click(function() {
    	jQuery(".pop-up-wrapper").css("display", "none");
        document.cookie = "newsletter_completed=true";
    });
});
  </script>
