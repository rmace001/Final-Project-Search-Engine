  <script>
   function showlocation() {
   // One-shot position request.
   navigator.geolocation.getCurrentPosition(callback);
}
function callback(position) {
   window.open('/cgi-bin/droman/my_local_weather_station.cgi?latcen='+position.coords.latitude+'&loncen='+position.coords.longitude,"_self");
}
  </script>
