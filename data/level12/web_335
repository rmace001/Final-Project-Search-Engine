      <script type="text/javascript">
       function changeGoogleStyles() {
    if($('.goog-te-menu-frame').contents().find('.goog-te-menu2').length) {
        $('.goog-te-menu-frame').contents().find('.goog-te-menu2').css(
            {
                'box-sizing':'border-box',
                'height':'auto'
            }
        )
       $('.goog-te-menu-frame').append("Google Translate");
    } else {
        setTimeout(changeGoogleStyles, 50);
    }
}
changeGoogleStyles();

			function searchPrimo() {
				// Submits the search form
				document.getElementById("primoQuery").value = "any,contains," + document.getElementById("query").value.replace(/[,]/g, " ");
				document.forms["searchForm"].submit();
			}

			function primoScopeSwitch(scope) {
				// Changes form variables depending on the selected scope
				switch(scope.value) {
					// Double check that the scope values are correct
					case "articles":
					document.getElementById("primoScope").value = "articles";
					document.getElementById("query").placeholder = "Search databases and peer reviewed journals";
					break;

					case "books_local":
					document.getElementById("primoScope").value = "01CALS_USB";
					document.getElementById("query").placeholder = "Search items found here in the library";
					break;

					case "books_csu":
					document.getElementById("primoScope").value = "01CALS";
					document.getElementById("query").placeholder = "Find items from all CSU libraries";
					break;

					case "course_reserves":
					document.getElementById("primoScope").value = "01CALS_USB_CR";
					document.getElementById("query").placeholder = "Search Course Reserves found here in the library";
					break;

					case "everything":
					default:
					document.getElementById("primoScope").value = "EVERYTHING";
					document.getElementById("query").placeholder = "Discover books, articles, media, and more";
				}
				document.getElementById("primoTab").value = scope.value; // Set the primo tab value from the selected option
			}
      </script>
