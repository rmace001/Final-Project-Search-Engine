                            hash.w.fadeOut('1000', function() { 
                                hash.o.fadeOut('1000');
                                ResumeSlideshow4808();
                            });
                        };
             </script>
            </div>
           </div>
          </div>
         </div>
         <div class="pager_container" id="dpgMyPage_ctl14_pnlPager">
          <ul class="items_pager" id="story_pager_4808">
          </ul>
         </div>
         <script type="text/javascript">
          $("#dpgMyPage_ctl14_pnlItems ul li").css('display', 'none');
        $("#dpgMyPage_ctl14_pnlItems ul li:first").css('display', 'block');
        var img_slide_width_4808;

        $(document).ready(function() {
        
            img_slide_width_4808 = $('#story_cycler_4808').width();
            
            $('#story_cycler_4808').cycle({
                fx: 'fade',
                speed: 500,
                timeout: 10000,
                pager: '#story_pager_4808',
                pagerAnchorBuilder: function(index, slide) {
                    return "<li><a href='#'><img src='" + $(slide).children(0).attr('thumbnail') + "' alt='Jump to slide " + index + ".' class='slide_thumb_" + index + "' /></a></li>";
                },
                pause: 1,
                before: onBeforeCycle
            });

            $('#item_description_4808').css('width', img_slide_width_4808 * 8);
            // Set the width for the slider text.
            $('#item_description_4808 .description_content').css('width', img_slide_width_4808);
            
            $('#item_description_4808').mouseover(function() {
                PauseSlideshow4808();
            });
            $('#item_description_4808').mouseout(function() {
                ResumeSlideshow4808();
            });
            
            $('body').append($('.jqmWindow'));
        });
        
        function onBeforeCycle() {
            $('#item_description_4808').animate({
                left: '-' + (img_slide_width_4808 * $('#story_cycler_4808').children().index(this))
            }, 'slow', 'swing');
        }
        
        var modalOpen4808 = false;
        
        function PauseSlideshow4808() {
            $('#story_cycler_4808').cycle('pause');
        }
        
        function ResumeSlideshow4808() {
            if(modalOpen4808 == false) {
                $('#story_cycler_4808').cycle('resume');
            }
        }
         </script>
        </div>
       </div>
       <div class="top_section_left_bottom" id="dpgMyPage_pnlTopLeftBottom">
       </div>
      </div>
