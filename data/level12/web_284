             <script>
              $(document).ready(function() {
                            $('#video_modal_4808_5772').jqm({
                                modal: false,
                                trigger: '#video_trigger_4808_5772',
                                onShow: open_sic_5772,
                                onHide: close_sic_5772
                            });
                            $('#video_trigger_4808_5772').click(PauseSlideshow4808);
                        });
                        var video_content_5772 = "";
                        
                        var open_sic_5772 = function(hash) {
                            // Add the video content to the modal popup.  The reason that we don't
                            // just stick in the HTML straight in is because we have to remove the 
                            // HTML when we close the modal otherwise IE will continue to play the
                            // video (you can heare the sound) in the background.
                            $('#video_modal_4808_5772 .video_content').html(video_content_5772);
                            modalOpen4808 = true;
                            PauseSlideshow4808();
                            hash.w.css('opacity', 1).show();
                        };
                        
                        var close_sic_5772 = function(hash) {
                            // Remove the video HTML so that we cannot hear it playing in the
                            // background in IE.
                            $('#video_modal_4808_5772 .video_content').html("");
                            modalOpen4808 = false
                            hash.w.fadeOut('1000', function() { 
                                hash.o.fadeOut('1000');
                                ResumeSlideshow4808();
                            });
                        };
             </script>
            </div>
           </div>
           <div class="description_content">
            <div class="description description_left">
