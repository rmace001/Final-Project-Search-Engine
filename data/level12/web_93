        $(document).ready(function(){
//add guide name writ large
  var subjName = $('#s-lg-guide-name').text();
  $('#subjName').text(subjName);
  
  $('#s-lg-guide-search').attr('placeholder', "Find a guide"); //Search box on main page to search for guides
  $('#s-lg-guide-search-terms').attr('placeholder', "Search this guide"); //Search box on guide pages to search w/in each guide
  $('#s-lg-srch-nav-col #s-lg-guide-search').attr('placeholder', "New search");
  $('#s-lg-sb-cols #s-lg-guide-search').attr('placeholder', "Search in this subject");
  $('.s-lib-profile-container').closest('.s-lib-box').addClass("profileBox");
  $('.s-lib-featured-profile-container').closest('.margin-bottom-xlg').addClass("profileBox");
  $('.s-lib-profile-container a.label').html('<span class="fa fa-envelope"><span class="sr-only">Email</span></span>');
  $('.profileBox').each(function(){
    $(this).find('.s-lib-profile-image, .s-lib-profile-name, .s-lib-profile-email').wrapAll('<div class="profileLeft"></div>');
    $(this).find('.s-lib-profile-contact, .s-lib-profile-social, .s-lib-profile-subjects').wrapAll('<div class="profileRight"></div>');
    $(this).find('.profileRight').after('<div class="clear"></div>');
  });
  $( ".profileBox:eq(1)" ).find('h2.s-lib-box-title').addClass( "noDisplay" );
  var profileCt = $('.profileBox').length;
  $('.s-lib-profile-contact strong').html('<span class="ion-ios-calendar-outline calendar"></span>');

//Calendar for DML
  $("tr.treg").find("td>a").text("Register now!");
  
  $('#s-lg-hp-nav-bottom').find('form').find('span').html('');
  $('#s-lg-hp-nav-bottom').find('form').find('.btn-default').html('<span class="fa fa-search icon-md"><span class="hidden">Search</span></span>');
  $('form#s-lg-srch-form').find('.btn-default').html('<span class="fa fa-search icon-md"><span class="hidden">Search</span></span>');
  $('form#s-lg-guide-search-form').find('.btn-default').html('<span class="fa fa-search icon-md"><span class="hidden">Search</span></span>');
  $('#s-lg-sb-cols').find('form').find('.btn-info').html('<span class="fa fa-search icon-md"><span class="hidden">Search</span></span>');
  $('#s-lg-az-search-bar').find('form').find('.btn-default').html('<span class="fa fa-search icon-md"><span class="hidden">Search</span></span>');
