//Run these fxns once the entire page is loaded (LG scripts aren't affected by JS unless they are wrapped in this load fxn)
function collapseToggle(){
  $('#s-lib-panel-container-collapse3 .panel-collapse').collapse('toggle');
}
$(window).load(function ()
{
    var i = setInterval(function ()
    {
        if ($('#s-lib-panel-container-collapse3').length){ 
          clearInterval(i);
          collapseToggle();
        } 
        if ($('#s-lg-srch-nav-col #s-lg-srch-local').length) { 
          clearInterval(i);
          //Toggle doesn't work for search page filter options in mobile
          var srchForm = $('#s-lg-srch-nav-col #s-lg-srch-local .navbar').find('#s-lg-srch-nav-1').html();
          $('#s-lg-srch-nav-col #s-lg-srch-local').find('.navbar').remove();
          $('#s-lg-srch-nav-col #s-lg-srch-local').after(srchForm);
          $('#s-lg-srch-input-q').attr('placeholder','New search');
          //$('#col0 #s-lg-srch-local').find('button').remove(); //get rid of search page toggle fxn 
        } 
        if ($('#s-lg-index-list').length) {
          clearInterval(i);
          var observer = new MutationObserver(function(mutations) {
            // For the sake of...observation...let's output the mutation to console to see how this all works
            mutations.forEach(function(mutation) {
              console.log(mutation.type);
            });    
          });
           
          // Notify me of everything!
          var observerConfig = {
            attributes: true, 
            childList: true, 
            characterData: true 
          };
           
          // Node, config
          // In this case we'll listen to all changes to body and child nodes
          var targetNode = document.body;
          observer.observe(targetNode, observerConfig);
        //fix functionality for tags in (i) popups - homepage
          $('.s-lg-gtitle').find('i').click(function(){
            var tagArea = $('.popover-content').find('span:contains("Tags:")').nextAll('a');
          //console.log('Tag area length: '+tagArea.length);
            $(tagArea).each(function( index ) {
              var tagName = $(this).text();
            //console.log('tag name: '+tagName);
              var tagNameURI = encodeURI(tagName);
            //console.log('tag name: '+tagName);
              $(this).attr('href',"http://humboldt.beta.libguides.com/srch.php?q="+tagNameURI);
            }); //end of tags fix
            var subjArea = $('.popover-content').find('span:contains("Subjects:")').nextAll('a');
          //console.log('Subject area length: '+subjArea.length);
            $(subjArea).each(function( index ) {
              var subjName = $(this).text();
            //console.log('subj name: '+subjName);
              var subjNameURI = encodeURI(subjName);
            //console.log('URI subj name: '+subjNameURI);
              $(this).attr('href',"http://humboldt.beta.libguides.com/srch.php?subject="+subjNameURI);
            }); //end of subject fix
          }); //end of click function
        }
    }, 100);
}); //snippet from: http://stackoverflow.com/questions/5494773/running-jquery-after-all-other-js-has-executed

//Collapse icon change - down arrow to up arrow (use with Bootstrap collapse - http://www.w3schools.com/bootstrap/bootstrap_ref_js_collapse.asp)
$('.offOn').click(function(){  
 if ($(this).find('.hideIcon').hasClass('ion-arrow-down-b')) {
   $(this).find('.hideIcon').removeClass('ion-arrow-down-b');   
   $(this).find('.hideIcon').addClass('ion-arrow-up-b');
 } else if ($(this).find('.hideIcon').hasClass('ion-arrow-up-b')) {
   $(this).find('.hideIcon').removeClass('ion-arrow-up-b');   
   $(this).find('.hideIcon').addClass('ion-arrow-down-b');
 }
});

//CHECK AVAILABILITY BUTTON AUTOPOPULATE
  /* Add this html to autopopulate any box: <div class="dmlAvailability"><a class="btn btn-outline-success" href="https://humboldt-primo.hosted.exlibrisgroup.com/primo-explore/search?vid=01CALS_HUL&lang=en_US&sortby=rank" role="button">Check availability</a></div> */

$(".dmlAvailability").each(function( index ) {
  var term = $(this).closest(".s-lib-box").find("h2").text();
  var itemName = term;
  $(this).find('a').text('Check '+itemName+' availability');
  term = 'digital media equipment '+term;
  term = term.trim();
  term = term.replace(/ /g,'%20');
  term = 'https://humboldt-primo.hosted.exlibrisgroup.com/primo-explore/search?query=any,contains,'+term+'&tab=books_csu&search_scope=01CALS&vid=01CALS_HUL&lang=en_US&offset=0';
//console.log('term: '+term);
  $(this).find('a').attr('href',term).attr('target','_blank');
});


  //BADGE NUMBERS FOR DML MENUS
  $( ".listGroupBadgePanel" ).each(function( index ) {
    var badgeLength = $(this).find('.listGroupBadgeCount').find('a').length;
    console.log(badgeLength);
    $(this).find('.badge').text(badgeLength);
  });

