function shareLinks(){	
  var curURL, msg, guideName, pgTitle, blurb;
  curURL = window.location.href;
  curURL = curURL.trim();
  curURL = encodeURIComponent(curURL);
  msg = $('#s-lg-guide-description').text();
  guideName = $('#s-lg-guide-name').text();
  pgTitle = guideName;
  if (!guideName){
    guideName = "HSU Library Research Guide";
  } else {
    guideName = $('#s-lg-guide-name').text();
  }
  console.log('page title: '+guideName);
  blurb = '';
  if (!msg) {
    blurb = guideName;    
  } else {
    blurb = guideName+': '+msg;
  }
  blurb = blurb.trim();
  blurb = blurb.replace(' ','%20');
  var twitterURL = 'https://twitter.com/intent/tweet?text=Just%20found%20the%20'+blurb+'%20from%20%23HSULibrary%26url='+curURL;
  var gplusURL = 'https://plus.google.com/share?url='+curURL;
  var facebookURL = 'https://www.facebook.com/sharer/sharer.php?u='+curURL+'%2f'+blurb;
  var emailURL = 'mailto:?subject='+guideName+'%26body=Check%20out%20this%20research%20guide%20from%20the%20Humboldt%20State%20University%20Library%20-%20%3Ca%20href="'+curURL+'"%20title="'+pgTitle+'%3E'+curURL+'"%3Ca%3E'+blurb;
//Twitter
  $('#social a.twitter').attr('href',twitterURL);
//Facebook
  $('#social a.facebook').attr('href',facebookURL);
//Google+
  $('#social a.googleplus').attr('href',gplusURL);
//Email
  $('#social a.email').attr('href',emailURL);      
}
shareLinks();

var atozBox = $('.s-lib-public-body').find('.s-lib-public-side-header').find('div:contains("A-Z Database List")').closest('.margin-bottom-xlg');
$(atozBox).find('.s-lib-public-side-header').hide();
$(atozBox).find('.pad-top-sm').text('');
$(atozBox).addClass('noBackground');
$(atozBox).find('.s-lg-hp-btn-section').text('A-Z Database List').addClass('atoz');

