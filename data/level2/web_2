  var loginSource = "WCS";
  var lgSrcCookieVal = $.cookie(lgSrcCookieName);
  if (typeof cookieVal != 'undefined' && cookieVal != null){
  userType="registered-user";
  }else if(typeof userDetailscookieVal !='undefined' && userDetailscookieVal !=null && userDetailscookieVal !="null" && userDetailscookieVal!=""){
  userType="registered-user";
  }
  if (typeof lgSrcCookieVal != 'undefined' && lgSrcCookieVal != null){
  loginSource = lgSrcCookieVal.trim();
  }else if(typeof loginSourceCookieVal !='undefined' && loginSourceCookieVal !=null && loginSourceCookieVal !="null" && loginSourceCookieVal!=""){
  loginSource = loginSourceCookieVal.trim();
