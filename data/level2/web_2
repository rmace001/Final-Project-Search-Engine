        <script type="text/javascript">
         function callPageElementTagGM(elementCat) {
		var partNumber = '';
		var partNumPrefix = partNumber.split("_");
		var containerPageId = $('#CONTAINER_PAGE_ID').val();
		var expAttrArray = new Array(50);
		var elemPageId = 'Homepage';
		expAttrArray[0] = 'STR_8106';
		expAttrArray[1]=partNumPrefix[0];
		expAttrArray[2] = 'Home';
		if (elemPageId == 'Quickview') {
			expAttrArray[8] = 'QuickView_'+containerPageId;
		} else {
			expAttrArray[8] = containerPageId;
		}	
		expAttrArray[15] = 'NA';
		expAttrArray[16] = 'NA';
		expAttrArray[17] = 'NA';
		expAttrArray[18] = 'NA';
		expAttrArray[19] = 'NA';
		cmCreatePageElementTag(elemPageId, elementCat, 'Homepage', 'Home', null, expAttrArray);
	}

	function callPageElementTag(elementName , elementCat){
		var containerPageId = $('#CONTAINER_PAGE_ID').val();
		var expAttrArray = new Array(50);
		expAttrArray[0] = 'STR_8106';
		expAttrArray[2] = 'Home';
		expAttrArray[8] = containerPageId;
		expAttrArray[15] = 'NA';
		expAttrArray[16] = 'NA';
		expAttrArray[17] = 'NA';
		expAttrArray[18] = 'NA';
		expAttrArray[19] = 'NA';
	cmCreatePageElementTag(elementName, elementCat, 'Homepage', 'Home', null, expAttrArray);
	}
	
	function callPageElementTagMiniShop(elementName , elementCat){
		var containerPageId = $('#CONTAINER_PAGE_ID').val();
		var expAttrArray = new Array(50);
		expAttrArray[0] = 'STR_8106';
		expAttrArray[2] = 'Home';
		expAttrArray[8] = containerPageId;
		expAttrArray[15] = 'NA';
		expAttrArray[16] = 'NA';
		expAttrArray[17] = 'NA';
		expAttrArray[18] = 'NA';
		expAttrArray[19] = 'NA';
	cmCreatePageElementTag(elementName, elementCat, 'Homepage', 'Home', null, expAttrArray);
	}
        </script>
