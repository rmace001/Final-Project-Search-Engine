<script type="text/javascript">
 $( document ).ready(function() {
	var temp = '0';
	if ('false'=='true'){
	temp = "";
	}
	var cookieValueFromJspObject = $('#userTypeCookieVal').val();
	var ckVal = $.cookie("BNCB_userType");
	
	if(typeof ckVal != 'undefined' && ckVal != null){
		ckVal=ckVal;
	}else if(typeof cookieValueFromJspObject !='undefined' && cookieValueFromJspObject !=null && cookieValueFromJspObject !="null" && cookieValueFromJspObject!=""){
		ckVal = cookieValueFromJspObject;
	}
	if (temp == '1' && ckVal=='1'){
		$('.fbCustomerTypeOverlayContainer').fadeIn();
		$('.screenFBCustomerType').show();
