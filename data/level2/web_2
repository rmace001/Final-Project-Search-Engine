    if (typeof cmDeptNum == 'undefined') {
        cmDeptNum='';
    }
   

  function popupPageViewCallQuickView(popupPageId,popupCategoryId,partNumber){
    var partNumberPrefix = partNumber.split("_");
    var departmentNumber = partNumberPrefix[0];
    var categoryName = $('#CM_PV_CATEGORY').val();
    var collectionName = $('#CM_PV_COLLECTIONS').val();
    var quickViewParentPageId = $('#QUICK_VIEW_PAGE_ID_SUFFIX').val();
    if (typeof quickViewParentPageId == 'undefined') {
        quickViewParentPageId = '';
    }
    var exploreAttributesArray = new Array(50);
    exploreAttributesArray[0] = 'STR_8106';
    exploreAttributesArray[1] = departmentNumber;
    exploreAttributesArray[2] = popupCategoryId;
    if (popupPageId == 'TBQuickview') {
        exploreAttributesArray[9] = 'TB';
    } else {
    exploreAttributesArray[9] = 'GM';
    }
    exploreAttributesArray[15] = commonSchYear;
    exploreAttributesArray[16] = commonSchSports;
    exploreAttributesArray[17] = commonSchSpeciality;
    exploreAttributesArray[18] = commonSchPvtState;
    exploreAttributesArray[19] = commonSchSuperStr;
    if(categoryName != null && typeof categoryName !== 'undefined' ){
        exploreAttributesArray[20] = categoryName;
        exploreAttributesArray[21] = 'Category';
    }else if(collectionName != null && typeof collectionName !== 'undefined'){
        exploreAttributesArray[20] = collectionName;
        exploreAttributesArray[21] = 'Collection';
    }
    cmCreatePageviewTag(popupPageId+'_'+quickViewParentPageId, 'STR_8106', '', '', '8106',exploreAttributesArray);
  }

