     <script type="text/javascript">
      var publisherBookExists='';
if(publisherBookExists!="" && publisherBookExists=='true')
{
$('.showGuest').hide();
}
var okToSubmit=true;
function checkDoubleSubmit() {
	var currentOkToSubmit = okToSubmit;
	if (currentOkToSubmit) okToSubmit=false;
	return currentOkToSubmit;
}
function submitRegisterForm(){

	document.ResgiterForm.submit();
}
function fnrememberme(invalidUser)
{
	var cookieName = "logonId";
	var cookieValue = document.getElementById("logonIdSignIn").value;
	if(document.getElementById("rememberAccountSignIn").checked && invalidUser != "true")
	{
		var cookieString=cookieName+"="+escape(cookieValue)+";"+"EXPIRES="+getexpirydate(100);
		document.cookie=cookieString;
		document.cookie="firstTime=NO;"+"EXPIRES="+getexpirydate(100);
	}
	else
	{
		var cookieString=cookieName+"="+escape(cookieValue)+";"+"EXPIRES="+getexpirydate(-1);
		document.cookie=cookieString;
		document.cookie="firstTime=YES;"+"EXPIRES="+getexpirydate(100);
	}
}
function getexpirydate( nodays){
var UTCstring;
Today = new Date();
nomilli=Date.parse(Today);
Today.setTime(nomilli+nodays*24*60*60*1000);
UTCstring = Today.toUTCString();
return UTCstring;
}
function fngetLogonId()
{
	var cookieValue = null;
	var cookieName = "logonId";
	var cookies = document.cookie;
	var prefix = cookieName + "=";
	var begin = cookies.indexOf("; " + prefix);
	if (begin == -1) {
	  begin = cookies.indexOf(prefix);
	  if (begin != 0) 
	    cookieValue = null;
	} else {
	  begin += 2;
	}
	var end = document.cookie.indexOf(";", begin);
	if (end == -1) {
	  end = cookies.length;
	}
	if (begin!=-1)
	{
		cookieValue = unescape(cookies.substring(begin + prefix.length, end));
	}
	var tempcookieValue = null;
	cookieName = "firstTime";
	prefix = cookieName + "=";
	begin = cookies.indexOf("; " + prefix);
	if (begin == -1) {
	  begin = cookies.indexOf(prefix);
	  if (begin != 0) 
	    tempcookieValue = null;
	} else {
	  begin += 2;
	}
	var end = document.cookie.indexOf(";", begin);
	if (end == -1) {
	  end = cookies.length;
	}
	tempcookieValue = unescape(cookies.substring(begin + prefix.length, end));
	if(tempcookieValue == "NO" && cookieValue != null)
	{
		document.getElementById("logonIdSignIn").value = cookieValue;
		document.getElementById("rememberAccountSignIn").checked = true;
	} 
}
     </script>
