   <div class="container">
    <div class="container">
     <div class="row">
      <div class="col-md-6">
       <h3>
        Newsroom
       </h3>
       <p>
        <style type="text/css">
         .rss-item {
    color: silver;
    margin-bottom: 20px;
    line-height: 20px;
}
.rss-item.rss-event {
    color: #666;
}
.rss-event .event-description {
    margin-top: 1em;
}
        </style>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.15.1/moment.min.js">
        </script>
        <script>
         $(document).ready(function() {
    //get a random number so that in the event of multiple rss widgets being on the same page the ul container bound to is unique
    var randomNumber;
    
    var itemCount = 1;
    var maxItems = 5;

    // URL of feed to be parsed
    var feed = "https://news.uwgb.edu/tag/marinette/feed/";

    // feed type (blog/calendar)
    var feedType = "blog";

    switch(feedType) {
        // CALENDAR FEED TYPE
        case "calendar":
            randomNumber = Math.floor((Math.random() * 1000) + 1);
            $('#rss-container').attr('id','rss-container-' + randomNumber);
            
            // this is the string format of the pubDate field retrieved from calendar.uwgb.edu - moment.js requires this
            // var calDateTimeFormat = "ddd, D MMM YYYY HH:mm:ss z";

            $.ajax(feed, {
                async: true,
                accepts: {
                    xml: "application/rss+xml"
                },
                dataType: "xml",
                success: function(data) {
                    $(data).find("item").each(function() {
                        if (itemCount <= maxItems) {
                            var el = $(this);
                            
                            // get event information from feed (only if needed)
                            var eventTitle, eventLink, eventDate, eventStartTime, eventLocation, eventDescription;
                            eventTitle =  el.find("title").text();
                            eventLink = el.find("link").text();
                            eventDate = el.find("mc\\:EventDate, EventDate").text();
                            if ("False" === "True") {
                                eventStartTime = el.find("mc\\:StartTime, StartTime").text();
                                if (eventStartTime.length > 0) {
                                    eventDate += ", " + eventStartTime;
                                }
                            }
                            if ("False" === "True") {
                                eventLocation = el.find("mc\\:Location, Location").text();
                            }
                            if ("False" === "True") {
                                eventDescription = el.find("description").text();
                                if (eventDescription.length > 400) {
                                    eventDescription = eventDescription.substring(0, eventDescription.substring(0, 400).lastIndexOf(" "));
                                    eventDescription += "... <a href='" + eventLink + "' target='_blank' style='color:silver;'>Read more &raquo;</a>";
                                }
                            }
                            // var formattedDateTime = moment(el.find("pubDate").text(), calDateTimeFormat).subtract(6, "hours").format('MMMM D, YYYY');

                            // add event to the page
                            $('#rss-container-' + randomNumber).append("<div class='rss-item rss-event'>" +
                                "<div class='event-title'>" +
                                    "<a href='" + eventLink + "' target='_blank'>" + eventTitle + "</a>" +
                                "</div>" +
                                "<div class='event-date'>" +
                                    eventDate +
                                "</div>" +
                                (eventLocation !=null ? "<div class='event-location'>" + eventLocation + "</div>" : "") +
                                (eventDescription !=null ? "<div class='event-description'>" + eventDescription + "</div>" : "") +
                                "</div>");
                            // $('#rss-container-' + randomNumber).append("<li class='rss-item'><a href='" + el.find("link").text() + "'>" + el.find("title").text() + "</a><br>" + formattedDateTime + "</li>");
                        }
                        itemCount++;
                    });
                }
            });
            break;

        // BLOG FEED TYPE
        case "blog":
        
            randomNumber = Math.floor((Math.random() * 1000) + 1);
            $('#rss-container').attr('id','rss-container-' + randomNumber);

            //this is the string format of the pubDate field retrieved from blog.uwgb.edu - moment.js requires this
            var calDateTimeFormat = "ddd, D MMM YYYY HH:mm:ss z";

            var jqxhr = $.ajax("https://news.uwgb.edu/tag/marinette/feed/")
                .done(function(data) {
                    $(data).find("item").each(function() {
                        if (itemCount <= maxItems) {
                            var el = $(this);
                            //console.log("------------------------");
                            //console.log("title      : " + el.find("title").text());
                            //console.log("link       : " + el.find("link").text());
                            //console.log("description: " + el.find("description").text());

                            var date;
                            
                            if (el.find("eventdate").text() != "") {
                                date = el.find("eventdate").text();
                            } else {
                                date = moment(el.find("pubDate").text(), calDateTimeFormat).format('MMMM D, YYYY');
                                //date = el.find("pubDate").text();
                            }
                            
                            $('#rss-container-' + randomNumber).append("<div class='rss-item'><a href='" + el.find("link").text() + "'>" + el.find("title").text() + "</a><br>" + date + "</div>");
                        }
                        itemCount++;
                    });
                })
                .fail(function() {
                    console.log("RSS Error.");
                })
                .always(function(data) {
                    //console.log("Always.");
                });
                
            break;
        }
    });
        </script>
        <h4>
        </h4>
        <div class="rss-feed-container" id="rss-container">
        </div>
        <p class="find-more pull-right">
         <a href="https://news.uwgb.edu/tag/marinette/" target="_blank">
          View More 
         </a>
        </p>
       </p>
      </div>
      <div class="col-md-6">
