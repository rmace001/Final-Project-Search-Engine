            var jqxhr = $.ajax("https://calendar.uwgb.edu/RSSFeeds.aspx?data=rIJFxb6N2pl7IeEiHpyPfAVOK2keoLx%2bW5m8ujaCRNmhku3bwCniBsndC91sj5zo")
                .done(function(data) {
                    $(data).find("item").each(function() {
                        if (itemCount <= maxItems) {
                            var el = $(this);
                            //console.log("------------------------");
