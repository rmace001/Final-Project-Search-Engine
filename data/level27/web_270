   function showSpinner() {
                "use strict";
                var ts = document.getElementById('top_spin');
                if (ts) ts.style.display = 'inline-block';

                var tss = document.getElementById('top_spin_small');
                if (tss) tss.style.display = 'inline-block';
            }

            function onSubmitHandler() {
                "use strict";
                showSpinner();
                var js = document.getElementById('JSEnabled');
                if (js) js.value = 'Y';
            }

            var _gaq = _gaq || [];
            //this is for eCampus
            _gaq.push(['_setDomainName', '.wisconsin.edu']);
            _gaq.push(['_setAccount', 'UA-332214-21']);
            _gaq.push(['_trackPageview']);
            //this is for eApp
            _gaq.push(['b._setAccount', 'UA-332214-28']);
            _gaq.push(['b._trackPageview']);

            (function () {
                "use strict";
                var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
                ga.src = ('https:' === document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
            })();
  </script>
