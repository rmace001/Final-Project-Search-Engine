   <script type="text/javascript">
    function validateCredentials() {
            // Password and login id must not be empty or null
            // Must not contain  < followed by a letter a-z, A-Z

            var strLogin = document.getElementById('txtLoginID').value;
            var strPassWord = document.getElementById('txtPassword').value;
            var valLoginID = true;
            var valPassword = true;
            var valChar = true;
            var valCharPW = true;
            var valCharID = true;
            var invalidChar = "<";

            // clear any error messages
            $('#pageModalError-body').html("");
            $('#pageModalErrorPopup').hide();

            //show the spinner
            $('#imgLoginSpinner').show();

            if (!strPassWord || /^\s*$/.test(strPassWord))
                valPassword = false;
            else if (strPassWord.indexOf(invalidChar) > -1) {
                valCharPW = valString(strPassWord, "<");
                valChar = valCharPW;
            }

            if (!strLogin || /^\s*$/.test(strLogin))
                valLoginID = false;
            else if (strLogin.indexOf(invalidChar) > -1) {
                valCharID = valString(strLogin, "<");

                if (!valCharID)
                    valChar = false;
            }

            if ((valLoginID) && (valPassword) && (valChar))
                return true;

            if ((!valLoginID) && (!valPassword)) {
                $('#pageModalError-body').html("Please enter your Login ID and Password.");
                $('#pageModalError').modal('show');
                $('#pageModalErrorPopup').show();
                $('#imgLoginSpinner').hide();
                return false;
            }

            if (!valLoginID) {
                $('#pageModalError-body').html("Please enter your Login ID.");
                $('#pageModalError').modal('show');
                $('#pageModalErrorPopup').show();
                $('#imgLoginSpinner').hide();
                return false;
            }

            if (!valPassword) {
                $('#pageModalError-body').html("Please enter your password.");
                $('#pageModalError').modal('show');
                $('#pageModalErrorPopup').show();
                $('#imgLoginSpinner').hide();
                return false;
            }
            if (!valChar) {
                if (!valCharID && !valCharPW) {
                    $('#pageModalError-body').html("Login ID and Password contain invalid characters (<).");
                    $('#pageModalError').modal('show');
                    $('#pageModalErrorPopup').show();
                }

                if (valCharPW && !valCharID) {
                    $('#pageModalError-body').html("Login ID contains invalid characters (<).");
                    $('#pageModalError').modal('show');
                    $('#pageModalErrorPopup').show();
                }

                if (!valCharPW && valCharID){ 
                    $('#pageModalError-body').html("Password contains invalid characters (<).");
                    $('#pageModalError').modal('show');
                    $('#pageModalErrorPopup').show();
                }
                $('#imgLoginSpinner').hide();
                return false;
            }
            alert("Error: You have gone where no human should.");  // we should never get here, but if we do something is wrong
            $('#imgLoginSpinner').hide();
            return false;
        }

        function valString(str2Check, checkChar) {
            var pos = 0;
            var idxNext = -1;
            var intLen = str2Check.length;
            var alphaBet = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz";

            while (pos != -1) {
                // alert("Top of the While Loop str2Check = " + str2Check + " looking for " + checkChar);
                pos = str2Check.indexOf(checkChar, idxNext + 1);   //start looking at 0
                if (pos != intLen - 1) {
                    // check to see if the next character is a-zA-Z if it is, return false
                    if (alphaBet.indexOf(str2Check.charAt(pos + 1)) > -1) {
                        return false;
                    }
                    else {
                        idxNext = pos;
                        pos = str2Check.indexOf(checkChar, idxNext + 1);
                    }
                }
                else
                    pos = -1;        // invalid character is at the end of the string, that is OK, we are at the end so exit loop
            }
            return true;
        }
   </script>
   <script type="text/javascript">
    $(document).ready(function () {
                    if ($('#JSEnabled').length) $('#JSEnabled').html('Y');

                    resizeHandler();

                    // check for an error message and show a page's error message modal
                    // (doesn't work to add the message to the body in vb then check,
                    //  since there will be a bootstrap function)
                    if (($('#ErrorMsg').length > 0) && ($('#pageModalError-body').length > 0)) {
                        if ($('#ErrorMsg').val().length > 0) {
                            $('#pageModalError-body').html($('#ErrorMsg').val());
                            $('#pageModalError').modal('show');
                            $('#pageModalErrorPopup').show();
                        } else {
                            $('#pageModalErrorPopup').hide();
                        }
                    }
                });

                function resizeHandler() {
	                var w = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
	                var h = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;
	                $('#lblViewportTop').html(w + 'x' + h);
	                $('#lblViewport').html(w + 'x' + h);
	            }
   </script>
   <script type="text/javascript">
    // prevent multiple form submit and same-tab navigation clicks, as well as 
	            // menu clicks for the current page (but in that case, still allow other clicks)
	            var clicked = false;
	            $('form').submit(function () {
	                if (clicked) {
	                    return false;
	                } else {
	                    clicked = true;
	                }
	            });
	            $('a[target != "_blank"][href *= ".aspx"]').click(function (e) {
	                if (clicked) {
	                    e.preventDefault();
                    } else {
	                    if ($(this).attr('aria-current') === 'page') {
	                        e.preventDefault();
                        } else {
	                        clicked = true;
	                    }
	                }
	            });
   </script>
  </form>
