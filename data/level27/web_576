<script type="text/javascript">
 $('.footer-block').click(function () {
            $(this).children().css('display', 'block');
        });

        var content = document.getElementById('feed');
        if (content) {
            var xhr = new XMLHttpRequest();

            xhr.onreadystatechange = function () {
                if (xhr.readyState == 4 && xhr.status == 200) {
                    var data = JSON.parse(xhr.responseText);
                    if (data.status == 'ok') {

                        var output = '</p><p></p><p style="font-size:14px;"><strong>' + data.feed.title + '</strong></p><p></p><p>';

                        for (var i = 0; i < data.items.length; ++i) {
                            var item = data.items[i];
                            output += '</p><p></p><p style="font-size:14px;">' + item.title + '</p><p></p><p><a href="' + item.link + '" >Read article on our blog»</a>';
                        }


                        feed.innerHTML = output;

                    }
                }
            };
            xhr.open('GET', 'https://api.rss2json.com/v1/api.json?rss_url=https%3A%2F%2Fnews.continuingstudies.wisc.edu%2Ffeed%2F&api_key=ibz9eppvof8mssmhrimqpzqlju7ktza478nwkuep&order_by=pubDate&order_dir=desc&count=3', true);
            xhr.send();
        }
</script>
