        function checkText() {
            var widthStr = $('#spotlight_img').css('width');
            var width = widthStr.replace('px', '');
            width = Number(width);
            if (width <= 599) {
                $('#spotlight-content > a:last-child').html("<div class='spot-learn'>LEARN MORE</div>");
                if ($('#spotlight-content').html().indexOf('SPOTLIGHT') < 0) {
                    $('#spotlight-content').prepend("<h2 class='spot-title'>SPOTLIGHT</h2><hr class='hr-spot'>");
                    $('#conf-link').prepend("<a href='conference'><div class='view-conf'>VIEW ALL CONFERENCES</div></a>");
                }
            }
        }
</script>
