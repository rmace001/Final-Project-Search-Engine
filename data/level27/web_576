      $('#course_search_form').submit(function(){
   if ($('#childSeg').val() != '?' && $('#childSeg').val() != '') {
     $(this).attr('action', $(this).attr('action')+ '#' + $('#childSeg').val());
     $(this).submit();
   } else if ($('#parentSeg').val() != '?' && $('#parentSeg').val() != '') {
     $(this).attr('action', $(this).attr('action')+ '#' + $('#parentSeg').val());
     $(this).submit();
   }
});
