<script async="" type="text/javascript">
 function showNPMenu(){
    	//console.log('show');
    //$('ul', this).slideDown(200);

    	$('ul', this).removeClass('osu-semantic')
         .slideDown('fast');


    } 
    function hideNPMenu(){
					var fu =  $('ul', this);

					//http://css-tricks.com/places-its-tempting-to-use-display-none-but-dont/
    	//console.log('hide');
    //	$('ul', this).slideUp(200);
  fu.slideUp('fast',function(){
    fu.addClass('osu-semantic')
         .slideDown(0);
  });
							//console.log('hide');
    };
	//var localURL = window.location.href;
	var localURL = document.location.hostname;
	popService = {
		//navLi: $(".navPop li"),
		run : function(text,head,navItem){
			//$(navItem).addClass("active");
			head.fadeOut("slow", function(){
				$(this).html('Popular: '+text);
				$(this).fadeIn("fast");
			});			

			$.ajax({
			 	 url: "assetsDB/site/php/popularity/cloud.php",
			  	type: "GET",
			  	data: {pop : text},
			  	dataType: "html",
				success: function(data,navItem){
					$('#link_cloud').fadeOut("slow", function(){
						$(this).replaceWith(data);
						$(this).fadeIn("slow");
						$(this).attr('role', 'alert');
					});
				}
			});	
							
		}
	};
	
	mediaMagnet = {
	    run : function() {
            var template = document.getElementById('item-template').innerHTML;
            $('#mr-news-feed').hide();
            
            /* removed &tags=promoted and &per_page=3 from mm query adding promoted logic in the processing of items */
            
            var promotedItems = 1;
            
            // $.getJSON('https://mediamagnet.osu.edu/api/v1/items.json?entity_id=16&channel_id=114&channel_type=rss', function(data) {
            $.getJSON('https://mediamagnet.osu.edu/api/v1/items.json?entity_id=16&channel_id=599&channel_type=rss', function(data) {  
              $.each(data.items, function(idx, item) {
                  // if( $.inArray('promoted', item['tags'] ) > -1 && promotedItems <= 3 ) {
                  if (promotedItems <= 2) {
                    item['formatted_title'] = trimNewsText(item['title']);
                    item['formatted_description'] = trimNewsText(item['description']);
                    item['index'] = promotedItems;
                    promotedItems++;
                    
        			var html = Mustache.to_html(template, item);
        			$('#mr-news-feed').append(html);
                  }
              });
              }).done(function() {
                  $('#mr-news-feed').fadeIn();
              }).fail(function() {
              		$('#mr-news-feed').append('<h2>News will return momentarily.</h2>');
            });
            
            var twrTemplate = document.getElementById('twitter-template').innerHTML;
            $('#twitter-feed').hide();
            $.getJSON('https://mediamagnet.osu.edu/api/v1/items.json?entity_id=1&channel_id=1&per_page=2', function(data) {
              $.each(data.items, function(idx, item) {
                  item['formatted_published_at'] = moment(item['published_at'], 'YYYY-MM-DD HH:mm:ss Z').fromNow();
    			  var html = Mustache.to_html(twrTemplate, item);
    			  $('#twitter-feed').append(html);
              });
            }).done(function() {
                $('#twitter-feed').fadeIn("fast");
            }).fail(function() {
            	$('#news-feed').append('<h2>Tweets will return momentarily.</h2>');
            });
	    }
	}


    function trimNewsText(text, length) {
        text = $.trim(text);
        length = typeof a !== 'undefined' ? length : 13;

        var words = text.split(" ");
        if (words.length > length) {
            return words.splice(0, length).join(" ").concat('...');
        } else {
            return text;
        }
    }
	

    $(document).ready(function () { 
		//popService.hoverInt();
		//twitterService.run('#newsevent_feed_ul');
		popService.run('Current Students',$('#navPopHead'));
		// popService.run($('tag-cloud_title'), $('tag-cloud_filter'));
		setTimeout(mediaMagnet.run, 500);
		/**/
		$(".navPop li").hoverIntent({
			over: showNPMenu,
			timeout: 200, 
			out: hideNPMenu
		});
		$(".navPop li a").click(function(e) {
			e.preventDefault(); 
		});
		$(".navPop ul li a").click(function(e) {
			e.preventDefault(); 
			$('.tag-cloud h2').text("Popular: " + $(this).text());
			console.log($(this).text());
			popService.run(this.text,$('#navPopHead'),this);
			//$(this).addClass("active");
		});
		
	});
</script>
<script async="" type="text/javascript">
 function tagFilter(selected) {
        if (selected != null) {
            var title = $('.tag-cloud_title');
            $(title).fadeOut("slow", function(){
                $(this).html('Popular for ' + selected);
                $(this).fadeIn("fast");
            });
            $.ajax({
                url: "assetsDB/site/php/popularity/cloud.php",
                type: "GET",
                data: {pop : selected},
                dataType: "html",
                success: function(data) {
                    $('#link_cloud').fadeOut("slow", function(){
                        $(this).replaceWith(data);
                        $(this).fadeIn("slow");
                        $(this).attr('role', 'alert');
                    });
                }
            });
        }
    };
</script>
