<script type="text/javascript">
 $(function() {

  var lift_in_play = false;
  var $personal_hero = $('.personal-hero__wrapper.lift-data');
  var $personal_hero_default = $('.personal-hero__wrapper.default');
  var $personal_hero_mask = $('.personal-hero__mask');

  window.addEventListener('acquiaLiftContentAvailable', function(e) {
    $('div[data-lift-slot] > *').each(function() {
      $(this).show();
    });
    lift_in_play = true;
    $personal_hero.removeClass('liquid');
    $personal_hero_default.remove();
    $personal_hero_mask.fadeOut();
  });

  setTimeout(function(){
    if (!lift_in_play) {
      $personal_hero.remove();
      $personal_hero_mask.fadeOut();
    }
  }, 1500);

});
</script>
<noscript>
 <style>
  .personal-hero__mask {
      display: none;
    }
 </style>
</noscript>

