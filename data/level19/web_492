         $(document).ready(function() {
                $( "#tabs472746" ).tabs({
                    create: function(event, ui) { /* Fix for WebKit draw*/  
                        $(this).find('.ui-tabs-panel').hide().show();
                    },
                    activate: function( event, ui ) {
                           //#
                           console.log(event);console.log(ui);
                           if(ui.newPanel){
                              var __trubmaIframe = ui.newPanel.find('.trumba-spud iframe');
                              __trubmaIframe.attr('height', '1000');
                              var tableHeight = __trubmaIframe.contents().find('table').height();
                              if(tableHeight && tableHeight > 0){
                                 __trubmaIframe.attr('height', tableHeight);
                              } 
                           }
                           //
                       },
                    show: function(event, ui){
                        //#
                        var __trubmaIframe = $(ui.panel).find('.trumba-spud iframe');
                        console.log(ui);
                        if(__trubmaIframe.length == 1){
                            __trubmaIframe.attr('height', '1000');
                            var tableHeight = __trubmaIframe.contents().find('table').height();
                            if(tableHeight && tableHeight > 0){
                                __trubmaIframe.attr('height', tableHeight);
                            } 
                        }
                        //
                    }
                });
                            });
        </script>
