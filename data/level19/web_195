      <div class="block block-views plain first odd" id="block-views-location-location-tabs">
       <div class="content">
        <div class="view view-location view-id-location view-display-id-location_tabs view-dom-id-bb33ce565b21f5319998757f8fb1b204">
         <div class="view-content">
          <div class="views-row views-row-1 views-row-odd views-row-first">
           <div class="ds-2col-stacked node node-location view-mode-full clearfix">
