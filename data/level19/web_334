   <script type="text/javascript">
    var mainImgsArray = [document.getElementById('rbiMainImage1'), document.getElementById('rbiMainImage2'),
									 document.getElementById('rbiMainImage3'), document.getElementById('rbiMainImage4'),
								 document.getElementById('rbiMainImage5')];

			var myHandle;
			var currentImage = 1;
			setFocus(1);


			function initTimer() {
				myHandle = setTimeout(function () {
					currentImage++;
					if (currentImage > mainImgsArray.length)
						currentImage = 1;
					setFocus(currentImage);
				},
				6000);
			}

			function setFocus(imageNum) {
				// get selected images
				var varSelectedMainImage = mainImgsArray[imageNum - 1];

				// set visibility on all main images back to invisible
				for (i = 0; i <= 4; i++)
					mainImgsArray[i].className = "invisible";

				// set visibility on selected tile image
				varSelectedMainImage.className = "";
				currentImage = imageNum;
				clearTimeout(myHandle);
				initTimer();
				// return false to prevent postback
				return false;
			}
   </script>
