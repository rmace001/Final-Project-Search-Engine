  <script type="text/javascript">
   function up(obj)
{
	obj.className = 'clsButtonUp';
};

function down(obj)
{
	obj.className = 'clsButtonDown';
};

function off(obj)
{
	obj.className = 'clsButton';
};

function button_up(obj, classname) {
	obj.className = classname + 'Up';
}

function button_down(obj, classname) {
	obj.className = classname + 'Down';
}

function button_off(obj, classname) {
	obj.className = classname;
}
  </script>
