        <script>
         function searchPrimoMobile() {
    document.getElementById("primoQuery").value = "any,contains," + document.getElementById("primoQueryMobile").value;
    document.forms["primoSearchFormMobile"].submit();
}

function searchPrimo() {
    document.getElementsByClassName("primoSecond")[0].value = "any,contains," + document.getElementById("primoQueryMain").value;
    document.forms["primoSearchForm"].submit();
}
        </script>
