               <span content="UGA Online Learning Overview Video" itemprop="name">
               </span>
               <span content="About UGA Online Learning. Extraordinary educators, exceptional degree." itemprop="description">
               </span>
               <span content="http://cfvod.kaltura.com/p/1727411/sp/172741100/thumbnail/entry_id/1_ak1arest/version/100021" itemprop="thumbnail">
               </span>
              </div>
              <script>
               kWidget.thumbEmbed({ "targetId": "kaltura_player_1_ak1arest", "wid": "_1727411", "uiconf_id": 30345561, "flashvars": { "streamerType": "auto" }, "cache_st": kaltura_player_1_ak1arest, "entry_id": "1_ak1arest", "captureClickEventForiOS": true});
