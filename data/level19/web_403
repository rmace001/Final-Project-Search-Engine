      <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.8/jquery.min.js" type="text/javascript">
      </script>
      <script src="https://www.statuscake.com/App/Chart/highcharts.js?CacheBuster=1346008176" type="text/javascript">
      </script>
      <script src="https://www.statuscake.com/App/Templates/highstock_.js" type="text/javascript">
      </script>
      <script src="https://www.statuscake.com/App/Templates/generatemap.js" type="text/javascript">
      </script>
      <script src="https://www.statuscake.com/App/Templates/jquery-jvectormap-1.1.1.min.js" type="text/javascript">
      </script>
      <script src="https://www.statuscake.com/App/Templates/jquery-jvectormap-world-mill-en.js" type="text/javascript">
      </script>
      <script src="statusPage_files/blur.js" type="text/javascript">
      </script>
      <script type="text/javascript">
       //	var PublicID = 'rLKAM5fCYP'; 
							var PublicID = 'pSreN3ZDz7';
		 	 


				 		  function UptimeToIcons() {
				    $( ".Uptime" ).each(function( index ) {
				      var Icon = 'Green';
				      var Uptime = parseFloat($(this).attr('title')); 
				      if (Uptime == 100) {
				        Icon = 'Green';
				      } else if (Uptime < 100 && Uptime > 98) { 
				        Icon = 'Yellow';
				      } else if (Uptime <= 98) { 
				        Icon = 'Red';
				      }
				      $(this).html('<div style="text-align:center;" title="' + Uptime + '% Uptime"><img style="width:20px;" src="https://www.statuscake.com/App/Templates/' + Icon + '.png" /></div>');
				    }); 

				  }

					function LoadTime(TestID,Render) {

					 $.getJSON('https://www.statuscake.com/App/Workfloor/LoadTime_.php?VID=' + TestID + '&limit=200&callback=?', function(data) {
							// Create the chart
							window.chart = new Highcharts.StockChart({
								chart : {
									renderTo : 'LoadTime',
									backgroundColor: null
								},
						
							yAxis: {        
								labels: {
									formatter: function() {
										return this.value + " ms";
									}
								},
								min: -0
							},

										navigator : {
									adaptToUpdatedData: false,
									series : {
										data : data.Data
									}
								},


								xAxis : {
									events : {
										afterSetExtremes : afterSetExtremes
									},
									minRange: 3600 // one hour
								},


					          credits: {
					              enabled: false
					          },

								title : {
									text : ''

								},
								plotOptions: {
					                spline: {
					                    lineWidth: 4,
					                    states: {
					                        hover: {
					                            lineWidth: 5
					                        }
					                    },
					                    marker: {
					                        enabled: false,
					                        states: {
					                            hover: {
					                                enabled: true,
					                                symbol: 'circle',
					                                radius: 5,
					                                lineWidth: 1
					                            }
					                        }
					                    },
					                    pointInterval: 3600000, // one hour
					                    pointStart: Date.UTC(2009, 9, 6, 0, 0, 0)
					                }
					            },
						
								series : [{
									name : 'Avg Load Time (in ms)',
									data : data.Data,
									threshold : null,
										type: 'spline',
									color: '#00a2ff',
									shadow : true,
					
									tooltip: {
										valueDecimals: 0
									}
								},
				
								{
									name : 'Min Load Time (in ms)',
									data : data.Min,
									threshold : null,
									type: 'spline',
									color: '#8aff00',
							
									tooltip: {
										valueDecimals: 0
									}
								}]

							});
						});
					}; 

					function Period(TestID) { 
						$.getJSON('https://www.statuscake.com/App/Workfloor/GroupData.php?callback=?&PublicID=' + PublicID + '&VID=' + TestID, function(data) {
							 $.each(data.Periods, function(key, val) {

							 	var Status = ''; 

							 	if (val.Status == 'Up') {
							 		Status = '<div class="alert success" style="padding:0px; margin:0px;">Up</div>'; 
							 	} else { 
							 		Status = '<div class="alert error" style="padding:0px; margin:0px;">Down</div>'; 
							 	}
								
								
								//create the dates for EST for the uptime history portion of the page (dates corrected for EDT)
								var SN = moment.utc(val.Start).local().add('h', 0).format('MM/DD/YYYY h:mm A [EDT]');
								var EN = moment.utc(val.End).local().add('h', 0).format('MM/DD/YYYY h:mm A [EDT]');

							 	$('#HistoryTable tbody').prepend("<tr><td>" + Status + "</td><td>" + SN  + "</td><td>" + EN + "</td><td>" + val.Period + "</td></tr>");

							 	if (val.Anno != '') {
							 			$('#HistoryTable tbody').prepend("<tr><td colspan='4' style='color: #727272; font-weight: bold;'>&#x25B2; " + val.Anno + "</td></tr>");
							 	}

							 }); 
						}); 
					}

					function ViewSingle(TestID) { 
						$("#HistoryTable > tbody").html("");
						$('#map').empty(); 

						$('#AllData').css("display","none");
						$('.showonhome').css("display","none");
						$('.showonsingle').css("display","block");		
						$('#SingleData').css("display","block");
						LoadTime(TestID);
						GenerateMap(TestID);
						Period(TestID);
						$.getJSON('https://www.statuscake.com/App/Workfloor/PublicReportHandler.php?PublicID=' + PublicID + '&callback=?&TestID=' + TestID, function(data) {
							$('#CurrentStatus').text(data.Status);
							$('#TestName').text(data.SiteName);
							document.title = Title + " - " + data.SiteName; 

							$('#TestType').text(data.TestType);
						}); 
					}

					 if(window.location.hash) {
					      var hash = window.location.hash.substring(1); //Puts hash in variable, and removes the # character
					      ViewSingle(hash);
					      // hash found
					  } else {
					      // No hash found
					  }

					function ViewHome() { 
						$('#AllData').css("display","block");
						$('.showonhome').css("display","block");
						$('.showonsingle').css("display","none");		
						$('#SingleData').css("display","none");
					}

					function afterSetExtremes(e) {
						var url,
							currentExtremes = this.getExtremes(),
							range = e.max - e.min;
				
						chart.showLoading('Loading data from server...');
						$.getJSON('https://www.statuscake.com/App/Workfloor/LoadTime_.php?VID=' + PublicID + '&start='+ Math.round(e.min) +
								'&end='+ Math.round(e.max) , function(data) {
					
							chart.series[0].setData(data.Data);
									chart.series[1].setData(data.Min);

							chart.hideLoading();
						});
				
					}

					$.getJSON('https://www.statuscake.com/App/Workfloor/PublicReportHandler.php?PublicID=' + PublicID + '&callback=?', function(data) {

						// Set the custom background image
						if (data.BG) {
							$('body').css('background-color',data.BG);
						}

						// Set Google Analytics Code
						if (data.GA) {

						}


						if (data.Skin) {	
							$("#Skin").attr("href","css/" + data.Skin + ".css");
						}

						$('.MainTitle').html(data.Title);
						Title = data.Title
						 document.title = Title; 

						if (data.LogoImage) {
							$('.MainTitle').html("<img src='" + data.LogoImage + "' />");
							$('#header').css("padding","12px");
						}

						if (data.ShowAd === true) {
							$('#FreeAccount').css("display","inline");
						}

						// Setup the date headers
						$.each(data.Dates, function(key, val) {
							$('#MainData thead tr').append('<th class="AlignCentre">' + val + '</th>');
						 }); 

						

						$.each(data.TestData, function(key, val) {
						   // alert(val.Uptime);
						    // Each Test Date
						    var Status = ''; 

							if (val.Status == 'Up') {
							 		Status = '<div class="alert success" style="padding:0px; margin:0px;">Up</div>'; 
							 	} else { 
							 		Status = '<div class="alert error" style="padding:0px; margin:0px;">Down</div>'; 
							 }

						   var Row = '<tr onclick="ViewSingle(' + val.TestID + ')"><td>' + Status + '</td><td>' + val.Name + '</td></td>';
						    $.each(val.Uptime, function(key, val) {
						    	 Row += '<td class="AlignCentre"><span class=" Uptime" title="' + val + '">' + val + '%</span></td>';
						    }); 
				    
						    $('#MainData tbody').append(Row + '</tr>');

							if (data.Orbs == 1) {
								UptimeToIcons(); 
							}
						  });

					});
      </script>
     </div>
    </div>
   </div>
  </link>
 </body>
</html>
