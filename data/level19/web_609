  <script type="text/javascript">
   var xhttp = new XMLHttpRequest();
		var events = [];
		
		var xhttponreadystatechange = function() {
			
			if (this.readyState == 4 && this.status == 200) {
				var data = JSON.parse(this.responseText);
				events = events.concat(data.items);
				if (data.pageNumber < data.totalPages) {
					document.getElementById('loader').innerHTML = 'Loading (' + (data.pageNumber/data.totalPages*100) + '%)';
					
					xhttp = new XMLHttpRequest();
					xhttp.onreadystatechange = xhttponreadystatechange;
					xhttp.open("GET", "https://cap.uncg.edu/wp-admin/admin-ajax.php?action=getSpartanConnectEvents&page=" + (data.pageNumber+1), true);
					xhttp.send();
				}
				else {
					events.forEach(function(e) {
						var tr = document.createElement('tr');
						
						var eventName = document.createElement('td');
						var eventLink = document.createElement('a');
						eventLink.href = e.eventUrl;
						eventLink.innerHTML = e.eventName;
						eventName.appendChild(eventLink);
						
						var eventDate = document.createElement('td');
						var date = new Date(parseInt(e.startDateTime));
						eventDate.innerHTML = date.getDate() + "/" + date.getMonth() + "/" + date.getFullYear() + " " + (date.getHours()%12+1) + ":" + date.getMinutes() + " " + (date.getHours() > 11 ? "pm" : "am");
						
						var organizer = document.createElement('td');
						organizer.innerHTML = e.organizationName;
						
						var location = document.createElement('td');
						location.innerHTML = e.otherLocation == "" ? "(none given)" : e.otherLocation;
						
						tr.appendChild(eventName);
						tr.appendChild(eventDate);
						tr.appendChild(organizer);
						tr.appendChild(location);
						
						document.getElementById('_spartanConnectTable').getElementsByTagName('tbody')[0].appendChild(tr);
					});
					
					document.getElementById('_spartanConnectTable').sortable().style.display = 'block';
					document.getElementById('loader').style.display = 'none';
				}
			}
		};
		
		xhttp.onreadystatechange = xhttponreadystatechange;
		xhttp.open("GET", "https://cap.uncg.edu/wp-admin/admin-ajax.php?action=getSpartanConnectEvents&page=1", true);
		xhttp.send();
  </script>
