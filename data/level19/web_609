     <div class="infoBox">
      <h1>
       Upcoming Events:
      </h1>
     </div>
     <script type="text/javascript">
      var eventDays;
var l = setInterval(function() {document.getElementById('loader').innerHTML = (document.getElementById('loader').innerHTML.length > 2 ? '' : document.getElementById('loader').innerHTML + '.');}, 500);
jQuery.post('https://cap.uncg.edu/wp-admin/admin-ajax.php', {action: "get_upcoming_events"}, function(response) {
	clearInterval(l);
	console.log(response);
	data = JSON.parse(response);
	
	if (Object.keys(data).length == 0) {
		var p = document.createElement('p');
		p.innerHTML = 'There are no events scheduled for the next five days.';
		document.getElementById('eventList').appendChild(p);
	}
	else {
		eventDays = Object.keys(data).map(function(i) {
				var o = {date: new Date(data[i]['start']*1000), timestamp: data[i]['start']*1000};
				for (var j in data[i]) {
					o[j] = data[i][j];
				}
				return o;
			}).sort(function(a,b) {
				return a.timestamp - b.timestamp;
			}).reduce(function(carry, cur, i, arr) {
				var date = new Date(cur.timestamp);
				var key = Date.parse((date.getMonth()+1) + "/" + date.getDate() + "/" + date.getFullYear());
				if (!carry[key]) carry[key] = [];
				carry[key].push(cur);
				return carry;
			}, {});
		
		Object.keys(eventDays).forEach(function(d) {
			var eNames = (eventDays[d].map(function(e) {return e.name;}));
			
			eventDays[d] = eventDays[d].filter(function(item, pos, self) {
				return eNames.indexOf(item.name) == pos;
			});
		});
		
		for (var i in eventDays) {
			var dt = document.createElement('dt');
			var date = new Date(parseInt(i));
			dt.innerHTML = ['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday'][date.getDay()] + ' ' + ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'][date.getMonth()] + ' ' + date.getDate();
			var dd = document.createElement('dd');
			var ul = document.createElement('ul');
			
			for (var d = 0; d < eventDays[i].length; d++) {
				var li = document.createElement('li');
				var time = (eventDays[i][d].date.getHours() == 0
					? 12
					: eventDays[i][d].date.getHours() > 11 ? eventDays[i][d].date.getHours() - 11 : eventDays[i][d].date.getHours())
				+ ':' + (eventDays[i][d].date.getMinutes().length > 1 ? '' : '0') + eventDays[i][d].date.getMinutes() + ' ' + (eventDays[i][d].date.getHours() > 10 ? "pm" : "am");
				
				var a = document.createElement('a');
				a.innerHTML = eventDays[i][d].name;
				a.setAttribute("href", "https://www.google.com/calendar/event?eid=" + eventDays[i][d].eid + "&ctz=America/New_York");
				li.appendChild(a);
				
				ul.appendChild(li);
			}
			dd.appendChild(ul);
			
			document.getElementById('eventList').appendChild(dt);
			document.getElementById('eventList').appendChild(dd);
		}
	}
	
	document.getElementById('loadingEvents').parentNode.removeChild(document.getElementById('loadingEvents'));
});
     </script>
