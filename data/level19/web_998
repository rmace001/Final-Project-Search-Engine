   <script>
    var pbarValues = [45,8,99];
        function init(){
          for (var i=0; i<pbarValues.length;  i++) {
            var selector = "#pbar" + i;
            $(selector).progressbar({ "value": pbarValues[i] });
            $(selector).bind('progressbarchange', function(event, ui) {
                var selector = "#" + this.id + " > div";
                var value = this.getAttribute( "aria-valuenow" );
                if (value < 10){
                    $(selector).css({ 'background': 'Red' });
                } else if (value < 30){
                    $(selector).css({ 'background': 'Orange' });
                } else if (value < 50){
                    $(selector).css({ 'background': 'Yellow' });
                } else{
                    $(selector).css({ 'background': 'Green' });
                }
            });
          }
            for (var i=0; i<pbarValues.length;  i++) {
              pbarValues[i] += 1;
              var selector = "#pbar" + i;
              $(selector).progressbar({ "value": pbarValues[i] });
        
          }
        }
      $(document).ready(init);
   </script>
