         <script type="text/javascript">
          jQuery(window).load(function() {

    	jQuery('#front_slides ul').addClass('slides');
    	jQuery('#front_slides img').wrap('<div class="rounded nobottom">');
    	jQuery('#front_slides_wrap li:nth-child(2)').addClass('second');
    	jQuery('#front_slides_wrap li:nth-child(3)').addClass('third');
    	jQuery('#front_slides_wrap li:nth-child(4)').addClass('fourth');
    	jQuery('#front_slides_wrap li:nth-child(5)').addClass('fifth');

  		jQuery('.flexslider').flexslider({
    		slideshowSpeed: 20000,
    		// slideshow: false, // prevent auto start slides
    		animationSpeed: 1000,
    		randomize: true,    		pauseOnHover: true
  		});

  		jQuery('#front_slides_wrap').css('display','block');

  		jQuery('#front_slides img').each(function() {
  			var imgTtl = jQuery(this).attr('title');
  			var imgAlt = jQuery(this).attr('alt');
  			jQuery(this).after('<div class="img_cap"><h2>'+imgTtl+'</h2><p>'+imgAlt+'</p></div>');
  			jQuery(this).removeAttr('title');
			jQuery(this).attr('alt', imgTtl + ' - ' + imgAlt);
  		});

  		jQuery('#front_slides img.adaptive-image').hover(
  			function() {
  				jQuery('.img_cap').stop(true, true).fadeIn();
  			},
  			function() {
  				jQuery('.img_cap').stop(true, true).fadeOut();
  			}
  		);

  		jQuery('.front_path_wrap .adaptive-image').each(function(){
			var imgPath = jQuery(this).attr('src');
			jQuery(this).parent().css('background-image','url("'+imgPath+'")');
		});


		var imgCapA = '';

    	jQuery('ul.slides li').each(function(){
    		if (jQuery('.img_cap a', this).length > 0) {
    			imgCapA = jQuery('.img_cap a', this).attr('href');
    			jQuery('img', this).attr('onclick', 'window.location = \'' + imgCapA + '\'').css('cursor', 'pointer');
    		}
    	});

  		
	});
         </script>
         <div class="highlightbox contenttable span6" id="front_mission">
          <h2 class="top10">
           <i class="icon-compass">
           </i>
           <p>
            Explore. Engage.
            <strong>
             Study Abroad
            </strong>
           </p>
          </h2>
          <div class="clear">
          </div>
          <div id="mission_text">
           <p>
            Yale Study Abroad is here to guide you through the process of identifying the international experience that fits your goals and interests. Choose from hundreds of study abroad options, including Yale Summer Session Programs Abroad, Non-Yale Summer Abroad, and Year or Term Abroad.
           </p>
