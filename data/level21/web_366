   ( function( $, document ) {
	if ( $( "body" ).hasClass( "page-id-939" ) ) {
		$( ".trigger" ).on( "click", function() {
			var $heading = $( this ),
				section = document.getElementsByClassName( "details " + this.id );

			$heading.toggleClass( "open" );

			$.each( section, function() {
				$( this ).toggleClass( "open" );
			} );
		} );
        
        $( ".request > header" ).on( "click", function() {
         	$( this ).siblings( ".column" ).toggleClass( "open" );   
        } );
	}
}( jQuery, document ) );

(function($){
	
	if ( $('body.xhome').length !== 0 ) {
	
	function display_posts( data ) {
		var results = data;

		$('main').append('<div id="feed" class="guttered json feed"></div>');

		var i = 0;
		$.each( results, function( index, value ) {
			if(index<1){
				console.log( value ); // Use this to see what object properties are available, like date, content, etc...

	            	var html = '';

	                html += '<article><header>'
	                + '<h2 class="article-title">'
	                + '<a href="'
	                + value.link
	                + '">'
	                + value.title
	                + '</a>'
	                + '</h2>';

					var date = $.datepicker.formatDate( 'MM d, yy', new Date(value.date) );

	                html += '<div class="article-meta">'
                    + '<time>'+date+'</time>'
                    // + '<cite>'+value.author.name+'</cite>'
	                + '</div>'
	                + '</header>';

	                html += '<div class="article-tease">'
	                + value.excerpt
                    + '<a href="'
	                + value.link
	                + '">» More</a>'
	                + '</div>';
	                + '</article>';

	            $('#feed').append(html);

            }

		});

		$('#feed').prepend('<h2>Brand Updates</h2>');
		$('#feed').append('<a href="/updates" class="more-posts">More updates...</a>');

	}
function pull_posts(e) {
    var post_url = 'https://brand.wsu.edu/wp-json/posts/';
    $.getJSON( post_url, display_posts );
}

	pull_posts();
	
	};

	
}(jQuery));
  </script>
