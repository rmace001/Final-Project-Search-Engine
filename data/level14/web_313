         <script type="text/javascript">
          function start_slideshow_7008(frame, start_frame, end_frame, delay) {
	fadeTimer = setTimeout(switch_slides_7008(frame, start_frame, end_frame, delay), delay);
}
function switch_slides_7008(frame, start_frame, end_frame, delay) {
	return (function() {
		Effect.Fade('slide_7008_' + cSlide7008, { duration: 1 });
		if (cSlide7008 == end_frame) { cSlide7008 = start_frame; } else { cSlide7008 = cSlide7008 + 1; }
		clearTimeout(fadeTimer);
		Effect.Appear('slide_7008_' + cSlide7008, { duration: 1 });
		fadeTimer = setTimeout(switch_slides_7008(cSlide7008, start_frame, end_frame, delay), delay);
	})
}
cSlide7008 = 0;
         </script>
         <div class="slideshow_7008" id="main_feature">
          <script type="text/javascript">
           start_slideshow_7008(0, 0, -1, 6500);
          </script>
