              function GoNextSlide(containerID, nextSlideIndex) {
        var navItems = $("#" + containerID + " .nivo-nextNav");
        $(navItems[0]).click();
    }

    function confirmDeleteImage(message) {
        result = window.confirm(message);
        if (result) {
            spinnerAlertModule.show();
            return true;
        }
        else
            return false;
    }
