             <a href="#" onclick="new Ajax.Updater('1718-content','/modules/1718.asp', { method: 'post', parameters: { session_id: '{873C5C29-D8C2-4687-ABCF-BD79DBDE48BF}', tab: 'Undergrads', user: '', portal: 'ece' } }); return false;">
              Undergrads
             </a>
