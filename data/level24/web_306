            <div class="vt-rawhtml ">
             <script type="text/javascript">
              // store the hash , it has to be executed right away before the browser can start scrolling
var target = window.location.hash,
 target = target.replace('#', '');

// delete hash so the page won't scroll to it
window.location.hash = "";

function downloadJSAtOnload() {
 switch (target) {
  case "summon":
   scrollToSearch();
   $('.nav-tabs a[href="#summon-tab"]').tab('show');
   break;
  case "addison":
   scrollToSearch();
   $('.nav-tabs a[href="#addison-tab"]').tab('show');
   break;
  case "databases":
   scrollToSearch();
   $('.nav-tabs a[href="#databases-tab"]').tab('show');
   break;
  case "course-reserves":
   scrollToSearch();
   $('.nav-tabs a[href="#course-reserves-tab"]').tab('show');
   break;
  case "discovery":
   scrollToSearch();
   $('.nav-tabs a[href="#discovery-tab"]').tab('show');
   break;
  default:
   break;
 }
}

function scrollToSearch() {
 $('html, body').animate({
  scrollTop: $("#search-panel").offset().top
 }, 'fast');
}

if (window.addEventListener)
 window.addEventListener("load", downloadJSAtOnload, false);
else if (window.attachEvent)
 window.attachEvent("onload", downloadJSAtOnload);
else window.onload = downloadJSAtOnload;
             </script>
            </div>
           </div>
          </div>
