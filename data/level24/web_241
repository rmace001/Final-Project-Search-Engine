     <script language="javascript">
      <!--// 
//-->
/* ####################### changeFiles ####################### */

function changeFilesDMA(){
aMenu=document.formDoubleMenuDMA.menuSubjectsDMA
aMenu2=document.formDoubleMenuDMA.menuFilesDMA
with  (aMenu){

switch (selectedIndex) {
case 0:
aMenu2.options.length=0;
aMenu2.options[0]=
new Option("Pages appear here","none")
aMenu2.options[0].selected=true;
history.go(0)
break    
case 1:
MySubjectDMA0(aMenu2)
aMenu2.options[0].text="Select entry that best fits the request"
break

case 2:
MySubjectDMA1(aMenu2)
aMenu2.options[0].text="Select entry that best fits the request"
break

case 3:
MySubjectDMA2(aMenu2)
aMenu2.options[0].text="Select entry that best fits the request"
break

case 4:
MySubjectDMA3(aMenu2)
aMenu2.options[0].text="Select entry that best fits the request"
break

case 5:
MySubjectDMA4(aMenu2)
aMenu2.options[0].text="Select entry that best fits the request"
break

case 6:
MySubjectDMA5(aMenu2)
aMenu2.options[0].text="Select entry that best fits the request"
break

case 7:
MySubjectDMA6(aMenu2)
aMenu2.options[0].text="Select entry that best fits the request"
break

case 8:
MySubjectDMA7(aMenu2)
aMenu2.options[0].text="Select entry that best fits the request"
break

case 9:
MySubjectDMA8(aMenu2)
aMenu2.options[0].text="Select entry that best fits the request"
break


}
}

}


/* ####################### end of doublemenu code ####################### */



function MySubjectDMA0(aMenu){
nullOptionsDMA(aMenu)
with (aMenu){
//Rewrites the text and values
options[0]=new Option("Select a Account problem","none")
options[1]=new Option(" Research Data Depot Access and Issues","index.php?cat=Account&type=40");
options[2]=new Option("Computer Lab account","index.php?cat=Account&type=2");
options[3]=new Option("Network drive (smb/t:)","index.php?cat=Account&type=3");
options[4]=new Option("Other Account  problem not listed above","index.php?cat=Account&type=6");
options[5]=new Option("Restricted Research Computer Access","index.php?cat=Account&type=39");
options[0].selected=true
}
}

function MySubjectDMA1(aMenu){
nullOptionsDMA(aMenu)
with (aMenu){
//Rewrites the text and values
options[0]=new Option("Select a Classroom problem","none")
options[1]=new Option("Other Classroom  problem not listed above","index.php?cat=Classroom&type=6");
options[2]=new Option("Webinar","index.php?cat=Classroom&type=25");
options[0].selected=true
}
}

function MySubjectDMA2(aMenu){
nullOptionsDMA(aMenu)
with (aMenu){
//Rewrites the text and values
options[0]=new Option("Select a Computer problem","none")
options[1]=new Option(" Active Directory Transition","index.php?cat=Computer&type=41");
options[2]=new Option(" Changing existing computer's name registration","index.php?cat=Computer&type=21");
options[3]=new Option(" Computer Replacement/NIC Update/New Network Card","index.php?cat=Computer&type=8");
options[4]=new Option(" Moving computer to new location","index.php?cat=Computer&type=20");
options[5]=new Option(" Register a new machine for use on Statler network","index.php?cat=Computer&type=7");
options[6]=new Option(" Software Install","index.php?cat=Computer&type=24");
options[7]=new Option("Computer Purchase Assistance","index.php?cat=Computer&type=27");
options[8]=new Option("Other Computer  problem not listed above","index.php?cat=Computer&type=6");
options[9]=new Option("Research Laptop Reservation Request","index.php?cat=Computer&type=38");
options[0].selected=true
}
}

function MySubjectDMA3(aMenu){
nullOptionsDMA(aMenu)
with (aMenu){
//Rewrites the text and values
options[0]=new Option("Select a Doors problem","none")
options[1]=new Option(" Card Locks","index.php?cat=Doors&type=11");
options[2]=new Option("Key request","index.php?cat=Doors&type=37");
options[3]=new Option("Other Doors  problem not listed above","index.php?cat=Doors&type=6");
options[0].selected=true
}
}

options[0]=new Option("Select a MAE problem","none")
function MySubjectDMA4(aMenu){
nullOptionsDMA(aMenu)
with (aMenu){
//Rewrites the text and values
options[0]=new Option("Select a Network problem","none")
options[1]=new Option(" Computer Replacement/NIC Update/New Network Card","index.php?cat=Network&type=8");
options[2]=new Option(" DNS Request","index.php?cat=Network&type=12");
options[3]=new Option(" Port activation","index.php?cat=Network&type=15");
options[4]=new Option(" Register a new machine for use on Statler network","index.php?cat=Network&type=7");
options[5]=new Option(" Report network problem","index.php?cat=Network&type=22");
options[6]=new Option(" Statler network ACL","index.php?cat=Network&type=19");
options[7]=new Option(" Statler Network Restricted Access Exception Request","index.php?cat=Network&type=9");
options[8]=new Option("Network drive (smb/t:)","index.php?cat=Network&type=3");
options[9]=new Option("Other Network  problem not listed above","index.php?cat=Network&type=6");
options[10]=new Option("Port Modification","index.php?cat=Network&type=26");
options[0].selected=true
}
}

function MySubjectDMA5(aMenu){
nullOptionsDMA(aMenu)
with (aMenu){
//Rewrites the text and values
options[0]=new Option("Select a Phone problem","none")
options[1]=new Option("New Activation","index.php?cat=Phone&type=18");
options[2]=new Option("Phone Reprogrammed ","index.php?cat=Phone&type=17");
options[3]=new Option("Problem with Phone","index.php?cat=Phone&type=16");
options[0].selected=true
}
}

function MySubjectDMA6(aMenu){
nullOptionsDMA(aMenu)
with (aMenu){
//Rewrites the text and values
options[0]=new Option("Select a Printer problem","none")
options[1]=new Option(" Port activation","index.php?cat=Printer&type=15");
options[2]=new Option(" Printer Replacement/NIC Update/New Network Card","index.php?cat=Printer&type=14");
options[3]=new Option(" Register a printer for use on Statler network","index.php?cat=Printer&type=13");
options[4]=new Option("Other Printer  problem not listed above","index.php?cat=Printer&type=6");
options[5]=new Option("Register Managed Printer","index.php?cat=Printer&type=36");
options[0].selected=true
}
}

function MySubjectDMA7(aMenu){
nullOptionsDMA(aMenu)
with (aMenu){
//Rewrites the text and values
options[0]=new Option("Select a Software problem","none")
options[1]=new Option(" Licensing","index.php?cat=Software&type=23");
options[2]=new Option(" Matlab Request/Update","index.php?cat=Software&type=10");
options[3]=new Option(" Software Install","index.php?cat=Software&type=24");
options[4]=new Option("Other Software  problem not listed above","index.php?cat=Software&type=6");
options[0].selected=true
}
}








function nullOptionsDMA(aMenu){

var tot=aMenu.options.length

for (i=0;i<tot;i++)

{

aMenu.options[i]=null

}

aMenu.options.length=0;

}


function setUpDMA(){

with (document.formDoubleMenuDMA) {

if (menuSubjectsDMA.selectedIndex==0){

menuSubjectsDMA.options[0].selected=true

menuFilesDMA.options[0].selected=true

}

if (menuSubjectsDMA.selectedIndex==1)

MySubjectDMA0(menuFilesDMA)

if (menuSubjectsDMA.selectedIndex==2)

MySubjectDMA1(menuFilesDMA)

if (menuSubjectsDMA.selectedIndex==3)

MySubjectDMA2(menuFilesDMA)

if (menuSubjectsDMA.selectedIndex==4)

MySubjectDMA3(menuFilesDMA)

if (menuSubjectsDMA.selectedIndex==5)

MySubjectDMA4(menuFilesDMA)

if (menuSubjectsDMA.selectedIndex==6)

MySubjectDMA5(menuFilesDMA)

if (menuSubjectsDMA.selectedIndex==7)

MySubjectDMA6(menuFilesDMA)

if (menuSubjectsDMA.selectedIndex==8)

MySubjectDMA7(menuFilesDMA)

if (menuSubjectsDMA.selectedIndex==9)

MySubjectDMA8(menuFilesDMA)

}

}


/* ####################### goDM() ####################### */

function goDMA(aMenu){
if (aMenu.options.value!="none")
{
location=aMenu.options[aMenu.selectedIndex].value
} 

}
     </script>
