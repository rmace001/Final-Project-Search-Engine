      <div class="region region-content">
       <section class="block block-views clearfix" id="block-views-client-services-block-2">
        <h2 class="block-title">
         Client Services
        </h2>
        <div class="view view-client-services view-id-client_services view-display-id-block_2 view-dom-id-4b0e13ac7aff2f9fd240fa1bf79f8f73">
         <div class="view-content">
          <div class="views-bootstrap-grid-plugin-style" id="views-bootstrap-grid-1">
           <div class="row">
            <div class="col col-lg-3">
