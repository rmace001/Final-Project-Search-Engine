  <script>
   <!-- handle various "loaded" statuses -->
    var body = document.getElementsByTagName('body')[0];
    var isloaded;
    (function () {
        if (cook.get('loaded') == 'css-loaded') {
            isloaded = true;
        } else {
            isloaded = false;
        }
    })();

    if (isloaded) {
        body.classList.remove('preload');
        body.classList.add('loaded');
    } else {
        setTimeout(function () {
            cook.set('loaded', 'css-loaded', { expires: 7, path: '' });
            //setTimeout(function(){
            body.classList.add('loaded');
            //}, 1000);
        }, 500);
    }
    //cook.remove('loaded');
  </script>
