    <script type="text/javascript">
     <!--hide script from old browsers
       function SetFocus()
               {
               if (document.UTsearchform.mysite[0].checked == true)
                       {
                       //Put the url of your site here, directories are allowed,
                       // for example, www.uthscsa.edu/faculty/
                       document.UTsearchform.hq.value="site:profiles.uthscsa.edu/"
                       }
               else
                       {
                       document.UTsearchform.hq.value="site:uthscsa.edu"
                       }
               if (document.UTsearchform.q.value=="Search site" || document.UTsearchform.q.value=="All university sites" || document.UTsearchform.q.value=="All university sites")
                       {
                       document.UTsearchform.q.value="";
                       }
               document.UTsearchform.q.focus();
               }
       // end hiding from old browsers-->
    </script>
    <form action="http://google2.utsystem.edu/search" method="get" name="UTsearchform">
     <label for="input">
      <input id="input" maxlength="255" name="q" onclick="SetFocus()" size="20" value="Enter Keywords"/>
     </label>
     <input id="inputsubmit2" name="Submit" type="submit" value="Search"/>
