  <script type="text/javascript">
   // Profile Table display
function displayProfileTable( tableName, linkName ){
  // Hide Tables
  tables = document.getElementsByTagName("table");
  for (i=0;i < tables.length; i++){
    if (tables[i].className == 'work')
      tables[i].className = 'hidden';
  }
  // Unselect Links
  
  
  navlinks = document.getElementById("profilenav").getElementsByTagName("a");
  
  for (i=0;i < navlinks.length; i++){
    navlinks[i].className = 'link';
  }
  // Show Talbe/ Select Link
  document.getElementById(tableName).className='work';
  document.getElementById(linkName).className = 'selected';
  document.getElementById(linkName).blur();
}
  </script>
