    jQuery(document).ready(function($) {

      $('.job-internship ul li:gt(5)').hide();
      $('.link-less-job-internship').hide();
      $('.link-more-job-internship').click(function() {
          $('.job-internship ul li:gt(5)').slideToggle();
          $('.link-more-job-internship').toggle();
          $('.link-less-job-internship').toggle();
      });
      $('.link-less-job-internship').click(function() {
          $('.job-internship ul li:gt(5)').slideToggle();
          $('.link-more-job-internship').toggle();
          $('.link-less-job-internship').toggle();
      });

      $('.career-planning ul li:gt(5)').hide();
      $('.link-less-career-planning').hide();
      $('.link-more-career-planning').click(function() {
          $('.career-planning ul li:gt(5)').slideToggle();
          $('.link-more-career-planning').toggle();
          $('.link-less-career-planning').toggle();
      });
      $('.link-less-career-planning').click(function() {
          $('.career-planning ul li:gt(5)').toggle();
          $('.link-more-career-planning').toggle();
          $('.link-less-career-planning').toggle();
      });

      $('.professional-association ul li:gt(5)').hide();
      $('.link-less-professional-association').hide();
      $('.link-more-professional-association').click(function() {
          $('.professional-association ul li:gt(5)').slideToggle();
          $('.link-more-professional-association').toggle();
          $('.link-less-professional-association').toggle();
      });
      $('.link-less-professional-association').click(function() {
          $('.professional-association ul li:gt(5)').slideToggle();
          $('.link-more-professional-association').toggle();
          $('.link-less-professional-association').toggle();
      });

