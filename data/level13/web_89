         <script type="text/javascript">
          <!--//--><![CDATA[// ><!--


jQuery(document).ready(function($){

    $(".sponsors-sidebar .view-content .slider-slide").first().show();

    var index = 0;
    var count = 6;

    function bannerRotator() {
        $('.sponsors-sidebar .view-content .slider-slide').delay(4300).eq(index).fadeOut(function() {
            if (index === count){
                index = -1;
            }

            $('.sponsors-sidebar .view-content .slider-slide').eq(index + 1).fadeIn(function() {
                index++;
                bannerRotator();
            });
        });
    }
    bannerRotator();

});


//--><!]]>
         </script>
