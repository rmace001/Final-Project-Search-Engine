<script>
 $(document).ready(function(){
        $('#carousel').on('slid.bs.carousel', function(){
			var active = $('.carousel-inner .active');
			$('.carousel-caption .caption-title > a').text(active.attr('data-title')).attr('href',active.attr('data-link'));
            if (active.attr('data-desc'))
            {
                $('.carousel-caption .caption-text').text(active.attr('data-desc'));
                $('.carousel-caption .caption-text').show();
            } else {
                $('.carousel-caption .caption-text').hide();
            }
        });
	});
</script>
