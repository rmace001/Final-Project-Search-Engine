             <script>
              <!--//--><![CDATA[// ><!--

//document.body.innerHTML = document.body.innerHTML.replace(/NetMath/g, '<span class="NM_capitalize">NetMath');
//document.body.innerHTML = document.body.innerHTML.replace(/™/gi, '<sup>™');
// JS code to embed heading level TMs in new class
    jQuery.fn.highlight = function (str, className) {    
        var regex = new RegExp(str, "gi");
        return this.each(function () {
            this.innerHTML = this.innerHTML.replace(regex, function(matched) {return "<span class=\"" + className + "\">" + matched + "<\/span>";});
        });
    };

    $("#main-content h1").highlight("\u2122[^ ]*","tinytm");
    $("#main-content h2").highlight("\u2122[^ ]*","tinytm");

