       <script type="text/javascript">
        var searchText = "search";

        $(document).ready(function() {
            var textBox = $("#dpgMyPage_ctl12_txtSearch");

            textBox.attr('value', searchText);
            textBox.click(function() {
                var txt = $("#dpgMyPage_ctl12_txtSearch");

                if (txt.attr("value") == searchText) {
                    txt.attr("value", "");
                }
            });
            textBox.blur(function() {
                var txt = $("#dpgMyPage_ctl12_txtSearch");

                if (txt.attr("value") == "") {
                    txt.attr("value", searchText);
                }
            });
        });
       </script>
       <div class="search" id="dpgMyPage_ctl12_pnlSearch" onkeypress="javascript:return WebForm_FireDefaultButton(event, 'dpgMyPage_ctl12_btnSearch')">
