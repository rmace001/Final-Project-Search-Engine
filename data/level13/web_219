         <script type="text/javascript">
          $("#dpgMyPage_ctl14_pnlItems ul li").css('display', 'none');
        $("#dpgMyPage_ctl14_pnlItems ul li:first").css('display', 'block');
        var img_slide_width_17314;

        $(document).ready(function() {
        
            img_slide_width_17314 = $('#story_cycler_17314').width();
            
            $('#story_cycler_17314').cycle({
                fx: 'fade',
                speed: 500,
                timeout: 10000,
                pager: '#story_pager_17314',
                pagerAnchorBuilder: function(index, slide) {
                    return "<li><a href='#'><img src='" + $(slide).children(0).attr('thumbnail') + "' alt='Jump to slide " + index + ".' class='slide_thumb_" + index + "' /></a></li>";
                },
                pause: 1,
                before: onBeforeCycle
            });

            $('#item_description_17314').css('width', img_slide_width_17314 * 8);
            // Set the width for the slider text.
            $('#item_description_17314 .description_content').css('width', img_slide_width_17314);
            
            $('#item_description_17314').mouseover(function() {
                PauseSlideshow17314();
            });
            $('#item_description_17314').mouseout(function() {
                ResumeSlideshow17314();
            });
            
            $('body').append($('.jqmWindow'));
        });
        
        function onBeforeCycle() {
            $('#item_description_17314').animate({
                left: '-' + (img_slide_width_17314 * $('#story_cycler_17314').children().index(this))
            }, 'slow', 'swing');
        }
        
        var modalOpen17314 = false;
        
        function PauseSlideshow17314() {
            $('#story_cycler_17314').cycle('pause');
        }
        
        function ResumeSlideshow17314() {
            if(modalOpen17314 == false) {
                $('#story_cycler_17314').cycle('resume');
            }
        }
         </script>
