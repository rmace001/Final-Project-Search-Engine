             <script>
              $(document).ready(function() {
                            $('#video_modal_17314_3434').jqm({
                                modal: false,
                                trigger: '#video_trigger_17314_3434',
                                onShow: open_sic_3434,
                                onHide: close_sic_3434
                            });
                            $('#video_trigger_17314_3434').click(PauseSlideshow17314);
                        });
                        var video_content_3434 = "";
                        
                        var open_sic_3434 = function(hash) {
                            // Add the video content to the modal popup.  The reason that we don't
                            // just stick in the HTML straight in is because we have to remove the 
                            // HTML when we close the modal otherwise IE will continue to play the
                            // video (you can heare the sound) in the background.
                            $('#video_modal_17314_3434 .video_content').html(video_content_3434);
                            modalOpen17314 = true;
                            PauseSlideshow17314();
                            hash.w.css('opacity', 1).show();
                        };
                        
                        var close_sic_3434 = function(hash) {
                            // Remove the video HTML so that we cannot hear it playing in the
                            // background in IE.
                            $('#video_modal_17314_3434 .video_content').html("");
                            modalOpen17314 = false
                            hash.w.fadeOut('1000', function() { 
                                hash.o.fadeOut('1000');
                                ResumeSlideshow17314();
                            });
                        };
             </script>
            </div>
           </div>
           <div class="description_content">
            <div class="description description_left">
             <h3>
