                         <script language="JavaScript" type="text/javascript">
                          var contentMinWidthToMobile = 480;
                                    var searchBar=document.getElementById("search-bar");
                                    contentWidthForSearch = document.getElementById("contDiv").clientWidth;
                                    
                                    var currentUrl = "" + window.location;
                                    
                                    if(contentWidthForSearch < contentMinWidthToMobile && currentUrl.indexOf('scale=true') == -1){
                                        if(navigationLinks.length > 0)
                                        {
                                            searchBar.className="open-search additional-nav";
                                        }
                                        else
                                        {
                                            searchBar.className="open-search"
                                        }
                                    }
                                    function searchButtonFocus(){
                                        contentWidthForSearch = document.getElementById("contDiv").clientWidth;
                                        if(contentWidthForSearch < (contentMinWidthToMobile+1))
                                        {
                                            searchSubmit();
                                            return;
                                        }
                                        else
                                        { 
                                            var searchBar=document.getElementById("search-bar");
                                            searchBar.className = "open-search";
                                            document.getElementById('search_input').focus();
                                        }
                                    }
                                    function searchBoxBlur(){
                                        var searchBar=document.getElementById("search-bar");
                                        if(contentWidthForSearch < 401)
                                        {
                                            return;
                                        }else
                                        {
                                            if(navigationLinks.length > 0){
                                                searchBar.className='close-search additional-nav';
                                            }
                                            else
                                            {
                                                searchBar.className = "close-search";
                                            }   
                                        }
                                    }
                                    function searchSubmit()
                                    {
                                        var searchText = document.getElementById('search_input').value;
                                        var url = "/view/7815?ACTION=POST_LIST&displaySearch=" + searchText;
                                        location.href=url;
                                    }
                                    function searchInputExpand()
                                    {
                                        var searchText=document.getElementById("search_input");
                                        var searchBar=document.getElementById("search-bar");
                                        searchText.focus();
                                        contentWidthForSearch = document.getElementById("contDiv").clientWidth;
                                        if(contentWidthForSearch > (contentMinWidthToMobile+1)){
                                            searchBar.className="open-search"
                                        }
                                    }
                                    function searchInputCollapse()
                                    {
                                        var searchBar=document.getElementById("search-bar");
                                        if(navigationLinks.length > 0){
                                            searchBar.className='close-search additional-nav';
                                        }
                                        else
                                        {
                                            searchBar.className = "close-search";
                                        }   
                                        
                                    }
                                    var docbody = document.body;                                
                                    docbody.addEventListener('click', function(e)
                                    {
                                        var searchBar=document.getElementById("search-bar");
                                        var searchText=document.getElementById("search_input");
                                        target = null;
                                        if (e.target)
                                        {  
                                            targ = e.target;
                                        }
                                        else if (e.srcElement)
                                        {
                                            targ = e.srcElement;
                                        }
                                        if(targ != null && targ.id != null)
                                        {
                                            if(targ.id == 'search_input' || (targ.id == 'search_button' && searchBar.className == 'open-search' && searchText.value.trim().length == 0))                                            {
                                                return;
                                            }
                                            if(targ.id == 'search_button' && searchBar.className != "open-search")
                                            {
                                                searchInputExpand();
                                                return;
                                            }
                                            if(targ.id == 'search_button' && searchBar.className == "open-search"  && searchText.value.trim().length > 0)
                                            {
                                                searchSubmit();
                                                return;
                                            }
                                         }
                                         contentWidthForSearch = document.getElementById("contDiv").clientWidth;
                                         if(contentWidthForSearch > (contentMinWidthToMobile+1)){
                                            searchInputCollapse();
                                         }
                                    });
                                    document.getElementById('search_input').onkeydown = function(event)                                    
                                    {
                                        if(event.keyCode == 13)
                                        {
                                            var searchText=document.getElementById("search_input");
                                            if(searchText.value.trim().length > 0)
                                            {
                                                searchSubmit();
                                            }
                                        }
                                    }
                                    var submitForm = 'false';
                                    function submitTheForm()
                                    {
                                        if(submitForm == 'true')
                                        {
                                            return true;
                                        }
                                        else
                                        {
                                            return false;
                                        }
                                    }
                         </script>
                         <li class="nav-menu" id="nav-menu" style="display:none;">
                          <a href="#">
                           <span class="place-off-screen">
                            click to open dropdown menu
                           </span>
                          </a>
                          <i class="fa fa-bars">
                          </i>
                          <div class="arrow-up">
                          </div>
                          <script type="text/javascript">
                           if(navigationLinks.length > 0)
                                {
                                    var ul = document.createElement('ul');
                                    for(var i = 0; i < navigationLinks.length; i++)
                                    {
                                        var li = document.createElement('li');
                                        li.innerHTML = "<a href=\"" + navigationLinks[i][1] + "\" target=\"_blank\">" + navigationLinks[i][0] + "</a>";
                                        ul.appendChild(li);
                                    }
                                    document.getElementById('nav-menu').appendChild(ul);
                                }
                                var navMenu = document.querySelector(".nav-menu");                                
                                var navMenuClass = navMenu.className;
                                var navMenuClicked = false;
                                
                                navMenu.addEventListener('click', function(){   
                                    var mobileMenu = document.getElementById('mobile-nav');
                                    var mobileMenuClass = mobileMenu.className;
                                    var navMenuClass = navMenu.className;
                                    deviceSize = document.body.offsetWidth;
                                    var contDiv= document.getElementById("contDiv");
                                    var contentWidth = contDiv.clientWidth;
                                    var mode = '';
                                    if((deviceSize < contentMinWidthToMobile+1) || (contentWidth < (contentMinWidthToMobile+1)))
                                    {
                                        mode = 'mobile'
                                    }
                                    if(navMenuClicked == false)
                                    {
                                        if(navMenuClass.indexOf('active') > -1)
                                        {                                      
                                            if(mode == 'mobile')
                                            {
                                                document.getElementById('mobile-nav').style.display = 'none';   
                                            }
                                            else
                                            {
                                                navMenu.className = navMenuClass.substring(0, navMenuClass.indexOf('active') - 1);
                                                mobileMenu.className = mobileMenuClass.substring(0, mobileMenuClass.indexOf('active') - 1);
                                            }
                                        }
                                        else
                                        {     
                                            if(mode == 'mobile')
                                            {
                                                if(document.getElementById('mobile-nav').style.display == 'block')
                                                {
                                                    document.getElementById('mobile-nav').style.display = 'none';   
                                                }
                                                else
                                                {
                                                    document.getElementById('mobile-nav').style.display = 'block';   
                                                }
                                            }
                                            else
                                            {
                                                navMenu.className += " active";
                                                mobileMenu.className += " active";
                                            }
                                        }    
                                    }
                                    
                                }, true);
                                
                                var docbody = document.body;                                
                                docbody.addEventListener('click', function(event){     
                                    var mobileMenu = document.getElementById('mobile-nav');
                                    var mobileMenuClass = mobileMenu.className;                      
                                    var navMenuClass = navMenu.className;
                                    var navMenuList = navMenu.querySelector("ul");
                                    if (typeof(navMenuList) != 'undefined' && navMenuList != null)
                                    {
                                       var viewportOffset = navMenuList.getBoundingClientRect();
                                       var topInt = parseInt(viewportOffset.top);
                                       var leftInt = parseInt(viewportOffset.left);
                                       var bottomInt = Number(parseInt(navMenuList.offsetHeight) + topInt);
                                       var rightInt = Number(parseInt(navMenuList.offsetWidth) + leftInt);
                                       var clickY = event.clientY;
                                       var clickX = event.clientX;
                                       if(
                                           (clickY >= topInt && clickY <= bottomInt)
                                           &&
                                           (clickX >= leftInt && clickX <= rightInt)
                                         )
                                       {
                                           navMenuClicked = true;
                                       }
                                       else
                                       {
                                           navMenuClicked = false;
                                           if(navMenuClass.indexOf('active') > -1)
                                           {
                                               navMenu.className = navMenuClass.substring(0, navMenuClass.indexOf('active') - 1);
                                               mobileMenu.className = mobileMenuClass.substring(0, mobileMenuClass.indexOf('active') - 1);   
                                           }
                                       }    
                                    }
                                }, true);
                          </script>
