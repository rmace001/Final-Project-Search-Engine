                          <button class="search_button" id="search_button" onfocus="searchButtonFocus()" type="button">
                           <span>
                            Search
                           </span>
                          </button>
                          <input class="search_input" id="search_input" onblur="searchBoxBlur()" placeholder="Search" type="text"/>
