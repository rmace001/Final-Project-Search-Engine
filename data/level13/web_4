                            function setCaptionWidth(alignment)
            {
                if(alignment == 'left')
                {
                    document.getElementById('img-caption-wrapper_1').style.marginRight='auto';
                }
                if(alignment == 'right')
                {
                    document.getElementById('img-caption-wrapper_1').style.marginLeft='auto';
                }
                if(alignment == 'center')
                {
                    document.getElementById('img-caption-wrapper_1').style.marginLeft='auto';
                    document.getElementById('img-caption-wrapper_1').style.marginRight='auto';
                }
            }
