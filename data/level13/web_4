                    document.getElementById('img-caption-wrapper_4').style.marginRight='auto';
                }
                if(alignment == 'right')
                {
                    document.getElementById('img-caption-wrapper_4').style.marginLeft='auto';
                }
                if(alignment == 'center')
                {
                    document.getElementById('img-caption-wrapper_4').style.marginLeft='auto';
                    document.getElementById('img-caption-wrapper_4').style.marginRight='auto';
                }
            }
