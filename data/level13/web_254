  <script>
   $(function() {
		document.body.addEventListener('click', e => {
			let t = e.target;
			let d = location.protocol + "//" + location.host;
			if (t.href && ! t.href.startsWith(d)) {
				trackOutboundLink(t.href, (t.target && t.target == "_blank"));
			}
		}, false);
	});
  </script>
