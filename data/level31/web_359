    <div class="block block-block first last odd" id="block-block-61">
     <section class="hero contain-1440">
      <!-- note: no section top -->
      <div class="hero-image in-page" style=" height: 420px; background: url('/sites/ccm/files/styles/hero/public/cover1.jpg') no-repeat center / cover">
       <div class="contain-1120">
        <h3>
         Center for Comparative Medicine
        </h3>
        <div class="text">
         <p>
          Welcome
         </p>
        </div>
