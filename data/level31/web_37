  <script type="text/javascript">
   //Mobile nav toggle menu
$(document).ready(function(){

	    $('.toggle-menu').jPushMenu({closeOnClickLink: false});
		$('.toggle-menu-drought').jPushMenu({closeOnClickLink: false});
	    $('.dropdown-toggle').dropdown();

		// 4-H nav toggle menu
		$('.menu-right-fourh').jPushMenu({showRightClass  : 'menu-right-fourh',closeOnClickLink: false});

// Hide cooperative extension mobile menu when 4-H mobile menu is clicked on
		$('.menu-right-fourh').click(function(){
			if ($('#uncenavbar').hasClass('in')){
				$('#uncenavbar').removeClass('in')
				}
			})
	  });

//Sticky uncenav header
var sticky = new Waypoint.Sticky({
	stuckClass: 'sticky',
  element: $('#navbar, #fourhnavbar')[0]
})


//Back to top button (located in footer)
	   $(function() {
      $(window).scroll(function() {
         if($(window).scrollTop() >= 500) {
            $('#top-link-block').removeClass('hidden').affix({
				offset: {top:100}
			});
            $('#top-link-block').fadeIn('fast');
         }else{
            $('#top-link-block').fadeOut('fast');
         }
      });
   });


//Add active class to sidenav
$(function() {
	var loc = window.location.pathname;
    var dir = loc.substring(0, loc.lastIndexOf('/')+1);
     //var pgurl = window.location.href.substr(window.location.href.lastIndexOf("/")+1);
     $(".sidenav a").each(function(){
          if($(this).attr("href") == dir || $(this).attr("href") == '' )
          $(this).addClass("active");
     })


});


// Smooth scroll to page anchor links. Offset by sticky header height
$(function() {
  $("a[href*='/#']").click(function(){
    if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {


 var target = $(this.hash);


 var scrollTop      = $(window).scrollTop();

   target = target.length ? target : $('[name=' + this.hash.slice(1) +']');

   if ((target.length) && (scrollTop < 150) ){
     $('html,body').animate({
      scrollTop: target.offset().top    //-150
        }, 1000);
        return false;
      }
	  else
	        if ((target.length) && (scrollTop > 150) ){
        $('html,body').animate({
          scrollTop: target.offset().top   //-190
        }, 1000);
        return false;


	  }

    }
  });
});
  </script>
