         <script language="javascript" type="text/javascript">
          $(document).ready(function() {
LBv2HomeSlide = {
	Debug: {
		version: 1.0,
		updated: '12/31/2012 4:00 PM',
		isEditView: !!window.location.href.match('pagedesign')
	}
}
    slideReplaceLBv2();
});
function slideReplaceLBv2() {
	// looks for every bbnc/bbis slideshow part
	$('table.SlideshowTable').each(function() {
		if (LBv2HomeSlide.Debug.isEditView) {
			// grabs the id of the picture cell to find the part count number
			pcID = $(this).find('td.SlideshowPictureCell').attr('id');
			// split the id to isolate the part count number
			pcID = pcID.split('_tdImg');
			// flatten the array to one variable with the part count number
			pcID = pcID[0];
		} else {
			// grabs the id of the picture cell to find the part count number
			pcID = $(this).find('td.SlideshowPictureCell').attr('id');
			// split the id to isolate the part count number
			pcID = pcID.split('_tdImg');
			// flatten the array to one variable with the part count number
			pcID = pcID[0];
		};
		// build the original variable storing the slideshow information based on the part count number
		oldVar = 'SS' + pcID + '_imgSlide';
		// set the variable 'slideReplace' to be the same as the original
		slideReplace = window[oldVar];
		// get the number of slides
		slideReplaceSize = slideReplace.images.length;
		// start building your replacement html
		slideReplace.html = "<div id='" + pcID + "_slideReplace' class='carousel slide'>";
		slideReplace.slideHTML = "<div class='carousel-inner'>";
		slideReplace.bulletHTML = "<ol class='carousel-indicators'>";
		// loop through all images
		for (i=0; i<slideReplaceSize; i++) {
			str = slideReplace.captions[i];
			alt = str.split('alt:');
			if (alt.length > 1) {
				alt = alt[1].split(',');
			}
			href = str.split('href:');
			if (href.length > 1) {
				href = href[1].split(',');
				// build replacement html for each slide
				slideReplace.slideHTML = slideReplace.slideHTML + "<div class='item slide-" + i + "'><a href=" + href[0] + "><img src='" + slideReplace.images[i] + "' alt='" + alt[0] + "' /></a></div>";
			} else {
				// build replacement html for each slide
				slideReplace.slideHTML = slideReplace.slideHTML + "<div class='item slide-" + i + "'><img src='" + slideReplace.images[i] + "' alt='" + alt[0] + "' /></div>";
			}
			
			thumb = str.split('thumb:');
			if (thumb.length > 1) {
				thumb = thumb[1].split(',');
				// build replacement html for each slide
				slideReplace.bulletHTML = slideReplace.bulletHTML + "<li class='thumb thumb-" + i + "'><a onclick='$(&quot;#" + pcID + "_slideReplace&quot;).carousel(" + i + ");'><img src='/view.image?id=" + thumb[0] + "' /></a></li>";
			} else {
				// build replacement html for each slide
				slideReplace.bulletHTML = slideReplace.bulletHTML + "<li class='thumb thumb-" + i + "'><a onclick='$(&quot;#" + pcID + "_slideReplace&quot;).carousel(" + i + ");'><img src='/view.image?id=1124' /></a></li>";
			}
		};
		slideReplace.slideHTML = slideReplace.slideHTML +"</div>";
		slideReplace.bulletHTML = slideReplace.bulletHTML + "</ol>";
		// end your replacement html
		slideReplace.html = slideReplace.html + slideReplace.slideHTML + slideReplace.bulletHTML + '</div>';
		// replace the bbnc/bbis part with new html
		$(this).replaceWith(slideReplace.html);
		$('#' + pcID + '_slideReplace .item:first-child').addClass('active');
		$('#' + pcID + '_slideReplace .thumb:first-child').addClass('active');
		$('#' + pcID + '_slideReplace').carousel({
  			interval: 7000
		})
	});
};

