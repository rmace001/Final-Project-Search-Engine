  <script type="text/javascript">
   $(document).ready(function () {

            // Load settings for Web Slides
            LoadOwlCarouselPageSettings();



            // Get links from database and populate lists
            $.ajax({
                url: "/GetLinks/1",
                type: "GET",
                dataType: "json",
                cache: false,
                async: true,
                success: function (data) {
                    createListItems(data);
                }
            });

            // Check if Chat is Online and display appropriate text and link in gray bar
            checkLibChatStatus();

            // Check if Mobile Chat is Online and display appropriate text and link in gray bar
            checkLibChatMobileStatus();

            // Check if Alert should be hidden or shown
            checkAlertStatus();

        });

        $(function () {
            $('#iconDropdown1').click(function () {
                setTimeout(function () { $('#q').focus(); }, 0);
            });
        });

        //prevents the href from loading
        $(function () {
            $('body').on('click', '.lib-mysched', function (e) {
                e.preventDefault();
                e.stopImmediatePropagation();
            });
        });

        //checks online/offline status for each chat hyperlink
        $(function checkChatStatusPerPerson() {
            $('.lib-mychat').each(function () {

                var ctl = $(this).attr("id");
                if (ctl != undefined) {
                    var springSpace = springSpace || {};
                    var ctlar = ctl.split("_");

                    var iid = ctlar[0];
                    var hash = ctlar[1];

                    $.ajax({
                        url: 'https:\/\/cascade2.libchat.com:443/widget_status',
                        dataType: "jsonp",
                        data: { iid: '493', rules: JSON.stringify([{ "u": iid, "d": 0 }]) },
                        cache: false,
                        async: true,
                        success: function (data) {
                            if (data.u || data.d) {
                                online = true;
                                $(ctl).removeClass("not-active");
                                $(ctl).attr("style", "display:block!important");
                            }
                            else {
                                ctl = "#" + ctl;

                                $(ctl).addClass("not-active");
                                $(ctl).attr("style", "display:none!important");
                            }

                        },
                        timeout: 10000
                    }).fail(function () {
                        ctl = "#" + ctl;
                        $(ctl).addClass("not-active");
                        $(ctl).attr("style", "display:none!important");
                    });
                }
            });
        });

        var $loading = $('#loadingWrapper').hide();

        var animationController = function animationController() {
            var timeout = null;

            var delayBy = 1000; //Number of milliseconds to wait before ajax animation starts.

            var pub = {};

            var actualAnimationStart = function actualAnimationStart() {
                $loading.show();
            };

            var actualAnimationStop = function actualAnimationStop() {
                $loading.hide();
            };

            pub.startAnimation = function animationController$startAnimation() {
                timeout = setTimeout(actualAnimationStart, delayBy);
            };

            pub.stopAnimation = function animationController$stopAnimation() {
                //If ajax call finishes before the timeout occurs, we wouldn't have
                //shown any animation.
                clearTimeout(timeout);
                actualAnimationStop();
            }

            return pub;
        }();

        $(document).ajaxStart(animationController.startAnimation);
        $(document).ajaxStop(animationController.stopAnimation);
  </script>
