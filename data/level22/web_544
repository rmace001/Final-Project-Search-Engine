     <section class="block block-system clearfix" id="block-system-main">
      <div class="block-content">
       <div class="node node-panel clearfix" id="node-1">
        <div class="content">
         <div class="container-fluid">
          <div class="panel-display panel-2col-bricks row clearfix">
           <div class="panel-panel panel-col-first col-xs-12 col-md-6">
            <div class="inside">
             <div class="panel-pane pane-node">
              <h2 class="pane-title">
               Meet Our Department!
              </h2>
              <div class="pane-content">
               <div class="node node-asu-vimeo-video clearfix" id="node-30">
                <div class="content">
                 <div class="embed-responsive embed-responsive-16by9">
                  <iframe allowfullscreen="" class="embed-responsive-item" mozallowfullscreen="" src="//player.vimeo.com/video/78376582?title=0&amp;badge=0&amp;byline=0" webkitallowfullscreen="">
                  </iframe>
                 </div>
                </div>
               </div>
