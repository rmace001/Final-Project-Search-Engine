  <script>
   require(['knockout', 'jquery', 'config', 'jquery-ui','slick'], function(ko, $) {
                $(function(){

                    $(".main-header").on("click", ".search-btn", function(e) {
                      e.preventDefault();
                      $('body').toggleClass("search-open");
                      $(this).toggleClass("open icon-search icon-cancel");
                      $(this).parent().find(".search-box").toggleClass("open");
                    });

                    //Redirect browser if the user presses the enter key on the search input
                    $(".main-header").on("keypress", ".search-box input[type='text']", function(e) {
                      if (e.keyCode == 13) {
                        e.preventDefault();
                        window.location = '/searchresults.aspx?cx=016306950182310286156:yg0ucrntaxs&q=' + $(this).val() + '&sa=Search&cof=FORID%3A11#1141';
                        return false;
                      }
                    });

                    //Redirect browser if there is a search button and grab the search query from some corresponding text input
                    $(".main-header").on("click", ".search-box button", function(e) {
                      e.preventDefault();
                      window.location = '/searchresults.aspx?cx=016306950182310286156:yg0ucrntaxs&q=' + $(this).parent().find("input[type='text']").val() + '&sa=Search&cof=FORID%3A11#1141';
                      return false;
                    });

                });

            });
  </script>
