<script>
 $(document).ready(function () {
        changeMargin()

        $(window).resize(function () {
            changeMargin()
        });
    });

    function changeMargin() {
        var height = $('.header_area').height();
        $(".slider_area").css('margin-top', height + 'px');
    }
</script>
