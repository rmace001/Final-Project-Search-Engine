     <script type="mce-text/javascript">
      // Needed on both page layouts for opening and closing the menu
    function toggleMainMenu() {
      document.getElementById("off-canvas").classList.toggle('open');
      document.getElementById("main").classList.toggle('hide');
    }
    function toggleSecondaryMenu() {
      document.getElementById("secondary-nav").classList.toggle('open-on-mobile');
    }
    // Only needed on Homepage
    $(".feature-block div ul, .feature-block div .upcoming-events, .feature-block>a").matchHeight();

    // Needed on all pages with a slider
    function swapImage() {
      window_width = $(window).width();
      $(".slider img").each(function() {

        var large = $(this).attr("data-large");
        var small = $(this).attr("data-small");

        if((window_width >= 700)) {
          $(this).attr("src", $(this).attr("src").replace($(this).attr("src"), large));
        }
        else if ((window_width < 700)){
          $(this).attr("src", $(this).attr("src").replace($(this).attr("src"), small));
        }

      });

    }
    jQuery(function($) {

                                      
      $('.slider').slick({
        autoplay: true,
        autoplaySpeed: 2000,
        dots: true,
        autoplaySpeed: 7000
      });

      //swapImage();
      
      // Originally I had set the slideshow to initialise
      // only when whe change .slider-image to disblay block.
      // However, this meant it would initialise once for each
      // image with the class of .slider-image. So what I've 
      // done is set a variable first_image_loaded which will
      // set itself to true after the first .slider-image is
      // display:block. At this point the slider is initialised.

      // The ideal solution is wait for the first image to be 
      // fully downloaded before initialising the slideshow

      /*first_image_loaded = false;
      $(".slider-image").show(function(){
        if(!first_image_loaded) {
          $(".slider ul").hipsterSlider();
        }
        first_image_loaded = true;
      });*/

      /** slider replaced
      first_image_loaded = false;
      
      $('.slider-image').load(function() {
        $('.slider-image').show(0,function(){

          if (!first_image_loaded) {

            $(".slider ul").hipsterSlider();

          }

          first_image_loaded = true;
          
        });
      });
      
      $(window).resize(function() {
        swapImage();
      });
      */

      //if an inner page does not have any content in the sub menu on the left then make the main content full width
      if ( $('#secondary-nav .section-navigation').is(':empty') && $('#secondary-nav .quicklinks').length == 0 ) {

        $('#secondary-nav').remove();

      }

      secondaryNavHeight    = $('aside.secondary-nav').innerHeight();
      contentSectionHeight  = $('main.content-section').innerHeight();

      if ( secondaryNavHeight > contentSectionHeight ) {

        $('.content-section').css('height',secondaryNavHeight);

      }

      //polyfill for Android 2.3 to get click event working
      Modernizr.load({
       test: Modernizr.classlist,
       nope: 'http://www.morehouse.edu/media/morehouse2015/styleassets/js/classList.min.js'
      });
    });
     </script>
