   <div class="row" id="announcement" style="display: none;">
    <div class="container">
    </div>
   </div>
   <script type="text/javascript">
    //<![CDATA[
  // Announcements
  $(document).ready(function() {
    $.ajax({
      url: '/blacklight_announcements/find',
      method: 'get',
      data: {
        date: moment(new Date()).format('YYYY-MM-DD')
      }
    }).done(function(msg) {

      if (msg.success) {
        var show_message, i = 0, campus, announcement_type, message, class_name;

        $.each(msg.result, function (k, v) {
          show_message = false;
          campus = v['campus'];
          announcement_type = v['announcement_type'];
          message = v['message'];

          if (campus === '' || campus === '') {
            show_message = true;
          }
          else
          if  ('' && campus === 'browse') {
            show_message = true;
          }
          else
          if ('' && campus === 'reserves') {
            show_message = true;
          }
          else
          if ('' && campus === 'music') {
              show_message = true;
          }
            if (show_message === true) {
              class_name = announcement_type.replace(/\s+/g, '-').toLowerCase();
              var div = document.createElement('div');
              $(div).html(message).addClass('alert-warning alert ' + class_name);
              $('#announcement .container').after($(div));
          }
        });
      }
      if (show_message === true) {
        $('#announcement ').fadeIn();
      }
    });
  });
  //]]>
   </script>
