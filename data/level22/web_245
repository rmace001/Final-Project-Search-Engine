   //<![CDATA[
    $(document).ready(function () {
        // Auto submit search when toggling checkboxes
        $("input#all-iucat-2, input#online-only-2").on("click", function(){
            $(this).change();
            $('form#search-form').submit();
        });
    });

    //]]>
  </script>
  <script type="text/javascript">
   $(document).ready(function(){
        $("#btn-search").click(function(){

            var name = $('#q').val().replace(/[-]/gi," ");
            $('#q').val(name);
        });

    });
  </script>
