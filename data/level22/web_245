   // iurps announcement
  $(function(){
    var pathName = window.location.pathname + window.location.search;
    console.log(pathName);
      if (pathName === "/?iurps=true") {
          function doOneTime() {
              if (document.cookie.replace(/(?:(?:^|.*;\s*)iucatRPS\s*\=\s*([^;]*).*$)|^.*$/, "$1") !== "true") {
                  document.cookie = "iucatRPS=true; expires=Fri, 31 Dec 9999 23:59:59 GMT";
                  $("#main-container").css('padding-top', '10px').prepend('<div class="row blue" id="announcement-1"> <div class="container"><div class="alert alert-warning hidden-print"> <p class="lead" style="color:#000000" id="announcement-message">The Bloomington Residential Programs &amp; Services view of IUCAT has been retired. You can still find their collections by searching all IUCAT.</p> </div> </div></div>');
              }
          };
          doOneTime();
      };
  });
  </script>
