      <section aria-labelledby="header-content-feed-1" class="panel panel-count-1 panel-type-content-feed panel-even" role="region">
       <h1 class="sr-only" id="header-content-feed-1">
        Latest HLS News
       </h1>
       <div class="panel-content-feed default">
        <div class="panel-heading">
         <h2 class="panel-title-small">
          <span class="link-follows">
           Latest HLS News
          </span>
          <!-- .link-follows -->
          <span class="panel-see-more see-more-link see-more-inline">
           <a href=" http://today.law.harvard.edu/ " title="View All">
            View All
           </a>
          </span>
         </h2>
        </div>
