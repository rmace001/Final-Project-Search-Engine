      <script type="text/javascript">
       function pageLoad() {
        SetAccordionClickHandlers();
    }

    function pageUnload() {
        RemoveAccordionClickHandlers();
    }

    function RemoveAccordionClickHandlers() {
        try {
            var acc = $find('ctl00_MainContent_mainContent1_MainContentAccordion_AccordionExtender');
            for (paneIdx = 0; paneIdx < acc.get_Count(); paneIdx++) {
                $addHandler(acc.get_Pane(paneIdx).header, "click", acc._headerClickHandler);
            }
        }
        catch (v) {
        }
    }

    function SetAccordionClickHandlers() {
        try {
            var acc = $find('ctl00_MainContent_mainContent1_MainContentAccordion_AccordionExtender');
            var totalPanes = acc.get_Count();
            for (paneIdx = 0; paneIdx < totalPanes; paneIdx++) {
                $removeHandler(acc.get_Pane(paneIdx).header, "click", acc._headerClickHandler);

                var pHeader = acc.get_Pane(paneIdx).header;
                var x = pHeader.children[0].children[0].children[0].children[0].children[1].children[1];

                $addHandler(x, "click", acc._headerClickHandler);
            }
        }
        catch (vv) { }
    }

    function toggleText(sender) {
        if (sender.value == "") {
            sender.value = "";
        }
        else {
            sender.value = "";
        }
    }
      </script>
