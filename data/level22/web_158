      <script type="text/javascript">
       function ShowOnly(arg) 
    {
        var SubjSearch = $get('ctl00_MainContentSearchQuery_searchCriteriaEntry_CourseSubjectCombo');
        var FacSearch = $get('ctl00_MainContentSearchQuery_searchCriteriaEntry_FacultyTitleTxtBox');
        var DeptSearch = $get('ctl00_MainContentSearchQuery_searchCriteriaEntry_AcademicDeptTitleCombo');
        var KeySearch = $get('ctl00_MainContentSearchQuery_searchCriteriaEntry_KeywordTxtBox');

        SubjSearch.style.display = 'none';
        FacSearch.style.display = 'none';
        DeptSearch.style.display = 'none';
        KeySearch.style.display = 'none';

        var searchType = arg.toUpperCase();

        if (searchType == 'SUBJECT') 
        {
            ClearSelectiveEntry(false, true, true, true);
            SubjSearch.style.display = '';
            ResetComboBox(SubjSearch.id);
        }
        else if (searchType == 'FACULTY') 
        {
            ClearSelectiveEntry(true, false, true, true);
            FacSearch.style.display = '';
        }
        else if (searchType == 'DEPT') 
        {
            DeptSearch.style.display = '';
            ResetComboBox(DeptSearch.id);
            ClearSelectiveEntry(true, true, false, true);
        }
        else if (searchType == 'KEYWORD') 
        {
            ClearSelectiveEntry(true, true, true, false);
            KeySearch.style.display = '';
        }
    }

    function ClearEntry() 
    {
        return ClearSelectiveEntry(true, true, true, true);
    }

    function ClearSelectiveEntry(clearSubj, clearFaculty, clearDept, clearKeyword) 
    {
        var SubjSearchTXT = $get('ctl00_MainContentSearchQuery_searchCriteriaEntry_CourseSubjectCombo_TextBox');
        var SubjSearchHID = $get('ctl00_MainContentSearchQuery_searchCriteriaEntry_CourseSubjectCombo_HiddenField');        
        var DeptSearchTXT = $get('ctl00_MainContentSearchQuery_searchCriteriaEntry_AcademicDeptTitleCombo_TextBox');
        var DeptSearchHID = $get('ctl00_MainContentSearchQuery_searchCriteriaEntry_AcademicDeptTitleCombo_HiddenField');
        var FacSearch = $get('ctl00_MainContentSearchQuery_searchCriteriaEntry_FacultyTitleTxtBox');
        var KeySearch = $get('ctl00_MainContentSearchQuery_searchCriteriaEntry_KeywordTxtBox');

        if (clearSubj == true) {
            SubjSearchTXT.value = ""; SubjSearchHID.value = 0;
        }
        if (clearDept == true) {
            DeptSearchTXT.value = ""; DeptSearchHID.value = 0;
        }
        if (clearFaculty == true) {
            FacSearch.value = "";
        }
        if (clearKeyword == true) {
            KeySearch.value = "";
        }
    }

    function ResetComboBox(id) 
    {
        try 
        {
            // Get ComboBox
            var Combo = Sys.Application.findComponent(id);

            // Get Button Style
            var ButtonStyle = Combo.get_buttonControl().style;

            // Reset Button Image
            ButtonStyle.height = Combo.get_textBoxControl().offsetHeight + 'px';
            ButtonStyle.width = ButtonStyle.height;

            // Reset Options List
            var OptionListStyle = Combo.get_optionListControl().style;

            Combo._optionListHeight = null;
            Combo._optionListWidth = null;
            OptionListStyle.display = 'block';
        }
        catch (ex) 
        { 
        }
    }
      </script>
