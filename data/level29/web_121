    <script>
     $(document).ready(function () {
	$(".mobile-nav span").on("click",function () {
		if($(this).attr("state") == "off" || !$(this).attr("state")) {
			$(this).next("ul").show();
			$(this).attr("state","on");
			$(this).children("img").attr("src","/application/themes/msucal/images/up-arrow-white.png");
		}
		else {
			$(this).next("ul").hide();
			$(this).attr("state","off");
			$(this).children("img").attr("src","/application/themes/msucal/images/down-arrow-white.png");
		}
	});
        $(".mobile-nav a").click(function (e) {

            if ($(this).next('span').length) {
                $(this).next('span').click();
                e.preventDefault();
            }
        });

});
    </script>
