      <script type="text/javascript">
       function initialize() {
							var mapOptions = {
							  zoomControl: true,
							  center: new google.maps.LatLng(44.450335,-87.510578),
							  mapTypeId: google.maps.MapTypeId.ROADMAP,
							  streetViewControl: false,
							  disableDefaultUI: true,
							  zoom: 15
	 						};

							var map = new google.maps.Map(document.getElementById('sidebar-map'),
								mapOptions);
					
							var MyPlace = new google.maps.LatLng(44.450335,-87.510578);
					
							var marker = new google.maps.Marker({
								position: MyPlace,
								url: 'https://kewaunee.extension.wisc.edu/office-map-and-directions/',
								title: 'Office Map and Directions',
								map: map
							});
					
							google.maps.event.addListener(marker, 'click', function() {
							  window.location.href = marker.url;
							});

						  }
						  google.maps.event.addDomListener(window, 'load', initialize);
      </script>
