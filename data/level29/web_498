eate-basic-page"},  {"label": "Deadlines", "url": "/content/deadlines"},  {"label": "Dissertations", "url": "/content/dissertations"},  {"label": "Document library", "url": "/content/create-document-library"},  {"label": "Gallery", "url": "/content/create-gallery"},  {"label": "Landing page", "url": "/content/create-landing-page"},  {"label": "News article", "url": "/content/create-news-article"},  {"label": "Post-graduate profile", "url": "/content/post-graduate-profile"},  {"label": "Profiles", "url": "/content/people-profiles"},  {"label": "Publication", "url": "/content/create-publication"},  {"label": "Publishing and unpublishing", "url": "/content/publishing-and-unpublishing-content2"},  {"label": "Research interest", "url": "/content/add-research-interest"},  {"label": "Research project", "url": "/content/research-project"},  {"label": "Services", "url": "/content/create-service"},  {"label": "Video Library", "url": "/content/video-library"}  ]  },  {"label": "Site administrators", "url": "" , "children": [  {"label": "Add administrators", "url": "/content/add-site-administrators-0"},  {"label": "Block Administrators", "url": "/content/block-site-administrators-0"}  ]  },  {"label": "Image Quick Reference", "url": "/content/image-specifications-quick-reference"  },  {"label": "Accessibility", "url": "/content/accessibility"  },  {"label": "Editorial Guidelines", "url": "/content/editorial-guidelines"  },  {"label": "Glossary", "url": "/content/glossary"  },  {"label": "Forms", "url": "" , "children": [  {"label": "Create a form", "url": "/content/create-form"},  {"label": "Adding fields", "url": "/content/adding-fields"},  {"label": "Adding logic", "url": "/content/add-logic-forms"},  {"label": "Multi-page forms", "url": "/content/multi-page-forms"},  {"label": "Embedding forms", "url": "/content/embedding-forms"},  {"label": "View Submissions", "url": "/content/view-submissions"}  ]  }  ]  },  { "label": "Release Notes", "url": "/recent-news"  }  ];
