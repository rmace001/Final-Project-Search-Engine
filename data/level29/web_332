            <div class="region region-content">
             <div class="block block-core block-page-title-block" id="block-columbia-university-page-title">
              <div class="row">
               <div class="columns">
                <h1>
                 <span property="schema:name">
                  Homepage
                 </span>
                </h1>
               </div>
              </div>
             </div>
             <div class="block block-system block-system-main-block" id="block-columbia-university-content">
              <article about="/homepage-0" class="node node--type-page node--view-mode-full" data-history-node-id="2906" role="article" typeof="schema:WebPage">
               <span class="hidden" content="Homepage" property="schema:name">
               </span>
               <div class="node__content">
                <div class="field field--name-field-pg-header field--type-entity-reference-revisions field--label-hidden field__items">
                 <div class="field__item">
                  <script>
                   // iOS does not support background video from youtube
    if (navigator.userAgent.match(/(iPod|iPhone|iPad)/)) {
      (function ($, Drupal, window, document) {
        $('.tv').hide();
      } (jQuery, Drupal, this, this.document));
    } else {
      var
        tag = document.createElement('script');
      tag.src = "https://www.youtube.com/iframe_api";

      var
        firstScriptTag = document.getElementsByTagName('script')[0];
      firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

      var
        tv,
        playerDefaults = {
          autoplay: 1,
          autohide: 0,
          modestbranding: 1,
          rel: 0,
          showinfo: 0,
          controls: 0,
          disablekb: 1,
          enablejsapi: 1,
          iv_load_policy: 3
        },
        videoID = '',
        youtubeURL = 'http://youtu.be/HBM_8QgxR-4',
        rx = /^.*(?:(?:youtu\.be\/|v\/|vi\/|u\/\w\/|embed\/)|(?:(?:watch)?\?v(?:i)?=|\&v(?:i)?=))([^#\&\?]*).*/;

      videoID = youtubeURL.match(rx);

      function onYouTubePlayerAPIReady(){
        tv = new YT.Player('tv', {
          events: {
            'onReady': onPlayerReady,
            'onStateChange': onPlayerStateChange
          },
          playerVars: playerDefaults
        });
      }

      function onPlayerStateChange(e) {
        if (e.data === 1){
          (function ($, Drupal, window, document) {

            var
              w = $(window).width(),
              h = $('.paragraph--hero').height();

            if (w/h > 16/9){
              tv.setSize(w, w/16*9);
              $('.tv .screen').css({'left': '0px'});
              $('.tv').css({'height': h + 'px'});
            } else {
              tv.setSize(h/9*16, h);
              $('.tv .screen').css({'left': -($('.tv .screen').outerWidth()-w)/2});
              $('.tv').css({'height': h + 'px'});
            }

            $('#tv').addClass('active');
          } (jQuery, Drupal, this, this.document));
        }
        if (e.data === YT.PlayerState.ENDED) {
          tv.playVideo();
        }
      }

      function onPlayerReady(){
        tv.loadVideoById(videoID[1]);
        tv.mute();
      }
    }
                  </script>
                  <div class="paragraph paragraph--type--hero paragraph--view-mode--default paragraph--hero overlay--light60" style='background-image: url("/sites/giving/files/styles/hero_image/public/hero/2019-03/CampusAndCityFromNWCB.Night_.EileenBarroso_1.jpg?itok=hnGbZYP8");'>
                   <div class="social-media-channels">
                    <div class="share-buttons">
                     <a class="share-twitter" href="https://twitter.com/columbia" target="_blank">
                      Twitter
                     </a>
                     <a class="share-facebook" href="https://www.facebook.com/columbia" target="_blank">
                      Facebook
                     </a>
                     <a class="share-linkedin" href="https://www.linkedin.com/school/2624" target="_blank">
                      LinkedIn
                     </a>
                    </div>
                   </div>
                   <div class="tv">
                    <div class="screen mute" id="tv">
                    </div>
                   </div>
                   <div class="inner-content">
                    <div class="row">
                     <div class="column">
                      <div class="field field--name-field-body field--type-text-with-summary field--label-hidden field__item">
                       <h1 class="hero-title">
                        Giving
                        <em>
                         through
                        </em>
                        Columbia
                       </h1>
                      </div>
                     </div>
                    </div>
                   </div>
                  </div>
                 </div>
                </div>
                <div class="field field--name-field-pg-content field--type-entity-reference-revisions field--label-hidden field__items">
                 <div class="field__item">
                  <div class="paragraph paragraph--type--cta paragraph--view-mode--default paragraph--cta">
                   <div class="row">
                    <div class="column">
                     <div class="cta-content">
                      <div class="field field--name-field-body field--type-text-with-summary field--label-hidden field__item">
                       <h3>
                        Have an Impact
                       </h3>
                       <p>
                        With your support, Columbia will do more for our students and the world
                       </p>
                       <p>
                        <a class="button button-transparent-blue-border" href="/our-campaign">
                         Our Campaign
