        // menu toggle 
        jQuery("#ces-mm-ceslink").click(function() {
            jQuery('#ces-mm-menu').slideToggle("fast", function() {
                // animation complete
            });
        });

        //move banner to start of body
        jQuery('body').prepend( jQuery('#ces-mm') );    
    });
  </script>
