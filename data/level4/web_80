<div class="flex_column av_one_third flex_column_div av-zero-column-padding avia-builder-el-9 el_after_av_one_third el_before_av_one_full " style="border-radius:0px; ">
 <section class="av_textblock_section " itemscope="itemscope" itemtype="https://schema.org/BlogPosting">
  <div class="avia_textblock " itemprop="text">
   <h3 style="font-weight: 400; font-size: 24px; color: #347bad; padding-bottom: 10px; border-bottom: 1px solid #e4e4e4;">
    Connect with UCLA College
   </h3>
  </div>
 </section>
