            <section class="block block-boxes block-boxes-os_sv_list_box" delta="1529596400" id="block-boxes-1529596400" module="boxes">
             <div class="block-inner clearfix">
              <h2 class="block-title" ng-non-bindable="">
               News and Announcements
              </h2>
              <div class="block-content content" ng-non-bindable="">
               <div class="boxes-box" id="boxes-box-1529596400">
                <div class="boxes-box-content">
                 <div class="os-sv-list os_sv_list_box sv-list-news sv-list-view-sidebar_teaser sv-list-layout-list sv-grid-3" data-delta="1529596400" data-page="0" id="box-1529596400-page">
                  <article class="node node-news article modified-in-os_news_process_node clearfix" id="node-1147545" role="article" target="_top">
                   <div class="node-content" ng-non-bindable="">
                    <h1 class="node-title" ng-non-bindable="" rel="nofollow">
                     <a href="https://osl.fas.harvard.edu/news/winter-break-reminders" target="_top">
                      Winter Break Reminders
                     </a>
                    </h1>
