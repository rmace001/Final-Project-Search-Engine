        <span class="rdf-meta element-hidden" content="Front page slideshow" property="dc:title">
        </span>
        <span class="rdf-meta element-hidden" content="0" datatype="xsd:integer" property="sioc:num_replies">
        </span>
        <div class="content">
         <div class="field field-name-field-view field-type-viewfield field-label-hidden">
          <div class="view view-slideshow view-id-slideshow view-display-id-default view-slideshow-home standard-slideshow view-dom-id-daa844f37827558b05f745234fac6f8c">
           <div class="view-content">
            <div class="flexslider optionset-default" id="flexslider-2">
             <ul class="slides">
              <li>
               <div class="views-field views-field-rendered-entity">
                <span class="field-content">
                 <a href="https://mapazdashboard.arizona.edu/article/map-talk-can-rainwater-quench-our-thirst">
                  <div class="field field-name-field-image field-type-image field-label-hidden">
                   <div class="slide-background-image" style="background-image: url('https://mapazdashboard.arizona.edu/sites/default/files/images/slideshow/map_talk_rainwater.fw__0.png');">
                   </div>
                  </div>
                  <div class="group-content field-group-div" id="field-collection-item-field-slideshow-slides-full-group-content">
                   <div class="field field-name-field-slide-body field-type-text-long field-label-hidden">
                    <h2>
                     <span style="color:#2B2B2C;">
                      MAP Talk: Can Rainwater Quench Our Thirst?
                     </span>
                    </h2>
                    <p>
                     <span style="color:#2B2B2C;">
                      <span style="font-size: 24px;">
                       <strong>
                        Potential Resource for Water Independence
                       </strong>
                      </span>
                     </span>
                    </p>
                    <p>
                    </p>
                    <p>
                    </p>
                    <p>
                     <img alt="" src="https://mapazdashboard.arizona.edu/sites/default/files/images/map_talk_rainwater_published.fw.png" style="width: 349px; height: 125px;"/>
                    </p>
                   </div>
                  </div>
                 </a>
                </span>
               </div>
              </li>
              <li>
               <div class="views-field views-field-rendered-entity">
                <span class="field-content">
                 <a href="https://mapazdashboard.arizona.edu/article/farm-school-programs-southern-arizona-case-study-economics-local-foods">
                  <div class="field field-name-field-image field-type-image field-label-hidden">
                   <div class="slide-background-image" style="background-image: url('https://mapazdashboard.arizona.edu/sites/default/files/images/slideshow/white_paper_farm_to_school.fw_.png');">
                   </div>
                  </div>
                  <div class="group-content field-group-div" id="field-collection-item-field-slideshow-slides-full-group-content--2">
