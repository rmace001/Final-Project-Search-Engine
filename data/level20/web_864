<script src="/bundles/jqueryunobtrusiveajax?v=z-mw_iAgwXaI_lkqPwSokOi_XZZLuC-62QwghACK4eU1">
</script>
<script type="text/javascript">
 $("div#inactive").hide();

        $("div#toggleActive").on("click", "#caturl", function (e) {
            e.preventDefault();
            var _url = $(this).attr("href");
            $.ajax({
                cache: false,
                async: true,
                dataType: "html",
                type: "GET",
                url: _url,
                success: function (data) {
                    $("div.categories").html(data);
                    $("div#toggleActive div").toggle();
                },
            })
        });
</script>

