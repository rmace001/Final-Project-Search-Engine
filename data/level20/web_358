                  <label class="views-label-field-deadline-value">
                   Deadline:
                  </label>
                  <span class="field-content">
                   1/15/18
                  </span>
                 </div>
                 <div class="views-field-field-start-value">
                  <label class="views-label-field-start-value">
                   Start Date:
                  </label>
                  <span class="field-content">
                   Variable (End of May)
                  </span>
                 </div>
                 <div class="views-field-field-end-value">
                  <label class="views-label-field-end-value">
                   End Date:
                  </label>
                  <span class="field-content">
                   Variable (End of July)
                  </span>
                 </div>
                </div>
               </div>
              </div>
             </div>
