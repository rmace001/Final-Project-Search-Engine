<section class="col-xs-12 col-md-12 compose-preview padding8px">
 <div class="containerdiv visible-sm hidden-xs visible-md visible-lg">
  <div class="col-xs-12 col-md-12 col-lg-12 compose-preview-image padding8px">
   <table border="0" cellpadding="0" style="margin-top:50px;" width="100%">
    <tr>
     <th colspan="3">
      <h3>
       <a href="/newsroom/">
        <span style="color:#67823a">
         NEWSROOM
        </span>
       </a>
      </h3>
     </th>
     <th>
      <h3>
       <a href="/content/landing.aspx?id=21194">
        <span style="color:#db8902;padding-left:40px;">
         EVENTS
        </span>
       </a>
      </h3>
     </th>
    </tr>
    <tr>
     <td class="homePageNews">
      <div class="thumbnail compose-profile clearfix">
       <img alt="Oak wilt disease threatens Texas oaks in spring" class="img-responsive pull-left col-xs-6 col-md-3" id="ctl00_ContentPlaceHolder1_NewsroomImage1" src="/uploadedImages/TFSMain/Connect/Article_Folder(2)/Oak wilt.jpg" style="float:left; margin: 0px 10px 0px 0px">
        <span style="color:#67823a">
