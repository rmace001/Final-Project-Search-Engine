renameCart();

function renameCart() {
    console.log("In English");
    MSG_BASKET_EMPTY = _("Temporary List is currently empty");
    MSG_RECORD_IN_BASKET = _("This item is already in Temporary Lists");
    MSG_RECORD_ADDED = _("This item has been added to Temporary Lists");
    MSG_RECORD_REMOVED = _("The item has been removed from Temporary Lists");
    MSG_NRECORDS_ADDED = _(" item(s) added to Temporary Lists");
    MSG_NRECORDS_IN_BASKET = _("already in Temporary Lists");
    MSG_NO_RECORD_ADDED = _("No item was added to Temporary Lists");
    MSG_CONFIRM_DEL_BASKET = _("Are you sure you want to empty Temporary Lists?");
    MSG_ITEM_IN_CART = _("Already in Temporary Lists");
    MSG_IN_YOUR_CART = _("Items in Temporary Lists: ");
    MSG_ITEM_NOT_IN_CART = _("Add to Temporary Lists");
    $("a.addtocart").each(function() {
      var a = $(this).text();
      if (a === "Add to cart" || a === "Add to your cart") {
        $(this).text(MSG_ITEM_NOT_IN_CART)
      } else {
        if (a === "In your cart") {
          $(this).text(MSG_ITEM_IN_CART)
        }
      }
    });
    $("a.incart").each(function() {
      var a = $(this).text();
      if (a === "In your cart") {
        $(this).text(MSG_ITEM_IN_CART)
      }
    });
    $('.container > h1:contains("Your Cart")').html("Temporary List");
    $("span.addto").on("click", function() {
      $('select#addto > option:contains("Cart")').html("Temporary List")
    });
    top_button_convert = function() {
      var a = "";
      if ($("#basketcount").text()) {
        a = $("#basketcount").html()
      }
      $("#cartmenulink").html('<i id="carticon" class="icon-folder-open icon-white"></i><span>Temporary List<span id="basketcount">' + a + "</span></span>");
      if (a === "") {
        a = "0"
      }
      $("#cartmenuitem").html(MSG_IN_YOUR_CART + a)
    };
    top_button_convert();
    $("a.addtocart").on("click", function() {
      top_button_convert()
    })
  }



/*Change drop-down from cart to temporary list*/

$(".dropdown span:contains('Cart')").text("Temporary List");

  $('select option:contains("Cart")').text('Temporary List');

/*search result screen */

/*state 1, chance Add to cart to Add to List */

$('.actions a:contains("Add to cart")').text('Add to Temporary List');

/*state 2, In your cart to On your list */

$('.actions a:contains("In your cart")').text('On your Temporary List');

/*detail page */

/*state 1, add to cart to add to list */


$('#action li a:contains("Add to cart")').text('Add to Temporary list');

/*state 2, in your cart to on your list*/

$('#action li a:contains("In your cart")').text('On your Temporary List');




});


/*on List drop-down replace cart with list*/
MSG_IN_YOUR_CART = _("Items in your list: ");
$("#cartmenuitem").text($("#cartmenuitem").text().replace("cart", "list"));


$(".results_summary.online_resources span").text("Online ");


