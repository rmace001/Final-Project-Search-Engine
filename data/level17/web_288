             <div class="panel-pane pane-views-panes pane-gw-repeating-sidebar-panel-pane-1">
              <h2 class="pane-title">
              </h2>
              <div class="pane-content">
               <div class="view view-gw-repeating-sidebar view-id-gw_repeating_sidebar view-display-id-panel_pane_1 view-dom-id-f41e643a8412122e99113c7aab84387e">
                <div class="view-content">
                 <div class="views-row views-row-1 views-row-odd views-row-first views-row-last">
                  <!-- Start List sidebar -->
                  <div about="/repeating-set" class="all-purpose-sidebar node node-list" id="node-67" typeof="sioc:Item foaf:Document">
                   <div class="image ">
                    <a href="https://my.gwu.edu/mod/onlinegiving/index.cfm?designation=Other&amp;other_designation=Film%20Studies" target="">
                     <div class="image-wrapper" style="width:220px;">
                      <img alt="Support Film Studies" height="80" src="https://filmstudies.columbian.gwu.edu/sites/g/files/zaxdzs1706/f/styles/gw_all_purpose_sidebar_large/public/image/give-today-new.jpg?itok=SCGmsuuF" title="Support Film Studies" typeof="foaf:Image" width="220"/>
                      <span class="play">
                       <span>
                       </span>
                      </span>
                      <div class="clearfix">
                      </div>
                     </div>
                    </a>
                   </div>
                   <h3>
                    <a href="https://my.gwu.edu/mod/onlinegiving/index.cfm?designation=Other&amp;other_designation=Film%20Studies" target="">
                     Give Now
                    </a>
                   </h3>
                   <p>
                   </p>
                  </div>
                  <!-- End Allpurpose sidebar -->
                 </div>
                </div>
               </div>
              </div>
             </div>
            </div>
