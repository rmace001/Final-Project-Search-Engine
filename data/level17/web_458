   <script type="text/javascript">
    try {
			// DST Detect
			var rightNow = new Date();
			var jan1 = new Date(rightNow.getFullYear(), 0, 1, 0, 0, 0, 0);  // jan 1st
			var june1 = new Date(rightNow.getFullYear(), 6, 1, 0, 0, 0, 0); // june 1st
			var hemisphere;
			var temp = jan1.toGMTString();
			var jan2 = new Date(temp.substring(0, temp.lastIndexOf(' ')-1));
			temp = june1.toGMTString();
			var june2 = new Date(temp.substring(0, temp.lastIndexOf(' ')-1));

			var january_offset = (jan1 - jan2) / (1000 * 60 * 60);
			var june_offset = (june1 - june2) / (1000 * 60 * 60);

			var dst;
			var std_time_offset;
			var daylight_time_offset;

			if (january_offset == june_offset) {
				dst = 'N'; // daylight savings time is NOT observed
				std_time_offset = january_offset;
				daylight_time_offset = january_offset;
			} else {
				dst = 'Y'; // daylight savings time is observed

				// positive is southern, negative is northern hemisphere
				hemisphere =  june_offset - january_offset;
				if (hemisphere >= 0) {
					var tmp;
					daylight_time_offset = june_offset;
					std_time_offset = january_offset;
				}
				else {
					daylight_time_offset = january_offset;
					std_time_offset = june_offset;
				}
			}

			
			var time_offset = -rightNow.getTimezoneOffset()/60;

			new Ajax.Request('/system/modules/clientinfo/save.php',
						 	{
								parameters: 
								{
									dst_observed:dst,
									dst_now:(time_offset!=std_time_offset)?'Y':'N',
									standard_time_offset: std_time_offset,
									current_time_offset: time_offset,
									daylight_time_offset: daylight_time_offset,
									january_offset: january_offset,
									june_offset: june_offset,
									userAgent: navigator.userAgent,
									appVersion: navigator.appVersion,
									cookieEnabled: navigator.cookieEnabled?'Y':'N'
								}
						 	});
			} catch(e) {}
   </script>
