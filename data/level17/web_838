  <script>
   // Hide subnavs.
// Create buttons after each .expanded first-level menu item.
// Use button tap/click to toggle visibility of relevant hidden subnav.
// ???
// Profit.

jQuery(document).ready(function($){
	$('#block-menu-menu-utility-menu, #block-system-main-menu')
		.find('.expanded.menu-depth-1>.menu')
			.each(function( index, item ) {
				$(item)
//					.addClass('menu-toggled-off')
					.attr('data-index', index)
					.before('<button class="menu-toggle" data-target="'+index+'">+</button>');
			});
	$('.menu-toggle')
		.click(function(event){
			var thisMenu = $(this).next('.menu');
			var allMenus = $('.menu-depth-1>.menu');
			// if the current submenu is already disabled, then disable all open menus and then enable this one.
			if( thisMenu.hasClass('menu-toggled-on') ) {
				allMenus.removeClass('menu-toggled-on');
			// if the current menu is currently enabled, then we can just close all .menu-toggled-off and we're good to go.
			} else {
				allMenus.removeClass('menu-toggled-on');
				thisMenu.addClass('menu-toggled-on');
			}
		});
			
});
  </script>
