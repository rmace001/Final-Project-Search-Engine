   <!--Start of Tawk.to Script-->
   <script type="text/javascript">
    var Tawk_API=Tawk_API||{}, Tawk_LoadStart=new Date();
    (function(){
    var s1=document.createElement("script"),s0=document.getElementsByTagName("script")[0];
    s1.async=true;
    s1.src='https://embed.tawk.to/576f119a35fc099d3c5239f0/default';
    s1.charset='UTF-8';
    s1.setAttribute('crossorigin','*');
    s0.parentNode.insertBefore(s1,s0);
    })();
    

//-------------------------------//
  //         SEARCH FUNCTION       //
  //-------------------------------//

  function search() {
    var search_type = $('.search-type option:selected').text();
    var search_input = $('.search-field').val();
    search_input = search_input.replace(/[^a-zA-Z 0-9]+/g, '');

    //-------------------------------------//
    //       SEARCH FOR COURSE BOOKS       //
    //-------------------------------------//

    if (search_type == 'Course Books') {
      document.location.href = 'http://shop.uabookstore.arizona.edu/main/CourseMaterials.aspx?search=' + search_input;

      $.ajax({
        url: 'SelectTermDept/Search',
        data: {
          "sh": search_input
        },
        dataType: "json",
        error: function(xhr) {
          $('#errorModal').modal('show');
          $('#errorModal').focus();
        },
        success: function(data, jqXHR) {
          if (data.valid == false) {
            $('#Book_Error').html('');
            $('#Book_Error').append("<div>" + data.message + "</div>");
            return;
          }
          //send user to Course material page
          var url = 'CourseMaterials?ids=' + data.retval + '&search=' + search_input;
          window.location.href = url;
          return;
        }
      });


    }

    //-------------------------------------//
    //       SEARCH FOR MERCHANDISE        //
    //-------------------------------------//
    else if (search_type == 'Merchandise') {
      if (isNaN(search_input) == true) {
        document.location.href ='http://shop.uabookstore.arizona.edu/main/Merchlist?searchtype=Description&txtSearch=' + search_input;
      } else {
        document.location.href = 'http://shop.uabookstore.arizona.edu/main/merchlist?searchtype=ItemNum&txtSearch=' + search_input;
      }
    }

    //-------------------------------------//
    //      SEARCH FOR GENERAL BOOKS       //
    //-------------------------------------//
    else if (search_type == 'General Books') {
      if (isNaN(search_input) == true) {
        document.location.href = 'http://shop.uabookstore.arizona.edu/main/trade?http://site.booksite.com/2441/search/?q=' + search_input + '&searchtype=DESCRIPTION';
      } else {
        document.location.href = 'http://shop.uabookstore.arizona.edu/main/trade?http://site.booksite.com/2441/search/?q=' + search_input + '&searchtype=ItemNum';
      }
    }

    //-------------------------------------//
    //      SEARCH ALL OF ARIZONA.EDU       //
    //-------------------------------------//
    else if (search_type == 'All Arizona.edu') {
      document.location.href = 'https://www.arizona.edu/search/google/' + search_input;
    }

    //----------------------------------------------//
    //      SEARCH FOR COMPUTERS & TECHNOLOGY       //
    //---------------------------------------------//
    else if (search_type == 'Computers & Technology') {
      if (isNaN(search_input) == true) {
        document.location.href = 'http://shop.uabookstore.arizona.edu/main/Merchlist?searchtype=Description&txtSearch=' + search_input + '&searchtype=Description';
      } else {
        document.location.href = 'http://shop.uabookstore.arizona.edu/main/merchlist?searchtype=ItemNum&txtSearch=' + search_input + '&searchtype=ItemNum';
      }
    }

    //------------------------------//
    //      SEARCH  AMAZON       //
    //------------------------------//
    else if (search_type == 'Amazon') {
      if (isNaN(search_input) == true) {
        document.location.href = 'http://www.amazon.com/s/ref=nb_sb_noss?url=search-alias%3Daps&tag=uabsumc-20&field-keywords=' + search_input;
      }
    }
  }

  //------------------------------------//
  //      BEING ABLE TO HIT ENTER       //
  //------------------------------------//

  $(function() {
    $('.search-field').bind('keyup', function(e) {
      var search_input = $(this).val();

      if ($(this).val() !== '' && e.keyCode == 13) {
        search();
        return false;
      }
    });

    $('.search-submit').on('click', function() {
      if ($('.search-field').val() != '') {
        search();
        return false;
      }
    });
  });

  //-------------------------------//
  //      SEARCH PLACEHOLDER       //
  //------------------------------//

  $('.search-type').change(function() {
    $('.search-field').prop('placeholder', $(this).val());
  });
   </script>
   <!--End of Tawk.to Script-->
