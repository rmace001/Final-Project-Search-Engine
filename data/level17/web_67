  <script type="text/javascript">
   $(document).ready(function()
	{
		$('.helpicon').click(function (event) {
			event.preventDefault();
			toggleVisibility($(this).data('target'));
		});

		$(function(){
			$(".test").popover({
			'placement':'top',
			'trigger' : 'hover',
			animation : true,
			delay: { hide: 800 },
			html : true,
			content : function() {
				return $(this).attr('data-popover-content');
			}
			});
		});


		$(function(){
			$('.tip').tooltip({
				'placement': 'top',
				'container': 'body',
				'html': true,
				'trigger': 'hover'
			});
		});

		$('.form_advanced_search').bind('inview', ShowAdvancedSearch);

		$('#last_element').unbind('inview');
		$('#last_element').bind('inview', LoadMoreSearchResults);
	});
  </script>
