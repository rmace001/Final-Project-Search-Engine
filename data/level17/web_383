          <script type="text/javascript">
           $(function() {
			$("#buckeye_alert").buckeyeAlert({
				url: "//www.osu.edu/feeds/emergency-alert.rss", 
				messageClass: "content-wrapper clearfix",
				callback: function () { 
				  // Do something here...
				}
			});

			$(document).on('click', '#buckeye_alert a', function() {
				$(this).attr({'target':'_blank'});
				return true;
			});

			$(document).on('click', 'a', function() {
				var href = $(this).attr('href');
				if(href === "#") return;
				if(href.slice(0, 4) !== 'http') return;
				if(href.indexOf('//yp4h.osu.edu') === -1) {
					ga('yp4h.send', 'event', 'Link', 'External', this.href);
					//console.log("ga('yp4h.send', 'event', 'Link', 'External', " + this.href + ");");
				}
				if(href.indexOf('.pdf') > -1) {
					ga('yp4h.send', 'event', 'Link', 'PDF', this.href);
					//console.log("ga('yp4h.send', 'event', 'Link', 'PDF', " + this.href + ");");
				}
				if(href.indexOf('.doc') > -1 || href.indexOf('.docx') > -1) {
					ga('yp4h.send', 'event', 'Link', 'Word', this.href);
					//console.log("ga('yp4h.send', 'event', 'Link', 'Word', " + this.href + ");");
				}
				if(href.indexOf('.xls') > -1 || href.indexOf('.xlsx') > -1) {
					ga('yp4h.send', 'event', 'Link', 'Excel', this.href);
					//console.log("ga('yp4h.send', 'event', 'Link', 'Excel', " + this.href + ");");
				}
			});
			
			<!-- SmartMenus jQuery init -->
			$('#main-menu').smartmenus({
				subMenusSubOffsetX: 1,
				subMenusSubOffsetY: -8
			});
			
			var $mainMenuState = $('#main-menu-state');
			if ($mainMenuState.length) {
				// animate mobile menu
				$mainMenuState.change(function(e) {
				  var $menu = $('#main-menu');
				  if (this.checked) {
					$menu.hide().slideDown(250, function() { $menu.css('display', ''); });
				  } else {
					$menu.show().slideUp(250, function() { $menu.css('display', ''); });
				  }
				});
				// hide mobile menu beforeunload
				$(window).bind('beforeunload unload', function() {
				  if ($mainMenuState[0].checked) {
					$mainMenuState[0].click();
				  }
				});
			}

		});
          </script>
