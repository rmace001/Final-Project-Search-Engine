  <!-- Include the script for Isotope -->
  <script src="/Assets/Scripts/External/isotope-3.0.6.min.js">
  </script>
  <!-- Start Custom Isotope Code -->
  <script>
   //************************************************
    //            Isotope Settings Code
    //************************************************
    function runIsotopeChecks() {
        //Around Campus Isotope Integration - does not support multiple listings on one page (doesn't need to)
        if ($(".ac-listing-items")) {
            initializeIsotope({
                gridContainerClass: "ac-listing-items",
                itemContainerClass: "ac-info",
                noResultsClass: "no-results",
                searchTitleClass: "name",
                searchBoxId: "ac-search",
                dropdownId: "ac-filter-btn",
            });
        }
        //Profile Listing Isotope Integration - supports multiple listings on one page
        if ($(".profile-listing-items")) {
            $(".profile-listing-items").each(function (i) {
                //set UID var
                var UID = this.getAttribute("data-uid");
                //update gridContainerClass
                this.classList.add("profile-listing-items-" + UID);
                //update itemContainerClass
                var summaryArray = this.querySelectorAll(".profile-summary");
                summaryArray = [].slice.call(summaryArray);
                summaryArray.forEach(function (element) {
                    element.classList.add("profile-summary-" + UID);
                });
                //update searchBoxId
                var searchArray = document.querySelectorAll("#profile-search");
                searchArray = [].slice.call(searchArray);
                searchArray.forEach(function (element) {
                    if (element.getAttribute("data-uid") == UID) {
                        element.id = "profile-search-" + UID;
                    }
                });
                //update dropdownId
                var dropdownArray = document.querySelectorAll("#category-filter");
                dropdownArray = [].slice.call(dropdownArray);
                dropdownArray.forEach(function (element) {
                    if (element.getAttribute("data-uid") == UID) {
                        element.id = "category-filter-" + UID;
                    }
                });
                //update no results banner
                var noResultsArray = document.querySelectorAll(".no-results");
                noResultsArray = [].slice.call(noResultsArray);
                noResultsArray.forEach(function (element) {
                    if (element.getAttribute("data-uid") == UID) {
                        element.classList.add("no-results-" + UID);
                    }
                });
                //initialize the instance of isotope with the updated classes and ID's
                initializeIsotope({
                    gridContainerClass: "profile-listing-items-" + UID,
                    itemContainerClass: "profile-summary-" + UID,
                    noResultsClass: "no-results-" + UID,
                    searchTitleClass: "name",
                    searchBoxId: "profile-search-" + UID,
                    dropdownId: "category-filter-" + UID
                });
            });
        }
    }

    //************************************************
    //            Initialize Isotope Code
    //************************************************
    function initializeIsotope(params) {
        var qsRegex; //regex search string used globally by Isotope
        var ddlFilter; //dropdown list filter used globally by Isotope

        //ISOTOPE GRID SETTINGS & FUNCTIONALITY
        var $grid = $("." + params["gridContainerClass"]).isotope({
            itemSelector: "." + params["itemContainerClass"],
            layoutMode: "fitRows",
            filter: function () {
                var $this = $(this);
                var searchResult = qsRegex ? $this.text().match(qsRegex) : true;
                var buttonResult = ddlFilter ? $this.is(ddlFilter) : true;
                return searchResult && buttonResult;
            },
            percentPosition: true,
            masonry: {
              columnWidth: '.program-result-item'
            },
            getSortData: {
                name: '.' + params["searchTitleClass"]
            }
        });

        $grid.isotope({ sortBy: params["searchTitleClass"], sortAscending: true }); //sorts data on page load

        //SEARCH BOX FUNCTIONALITY
        var $quicksearch = $("#" + params["searchBoxId"]).keyup(debounce(function () {
            // Use value of search field to filter
            qsRegex = new RegExp($quicksearch.val(), "gi");
            $grid.isotope();
            updateLazyLoad();
            checkResults();
        }, 200));

        //DROPDOWN BOX FUNCTIONALITY
        if (params["dropdownId"] == "ac-filter-btn") {
            acBtn();
        } else if (params["dropdownId"].includes("category-filter")) {
            plDropdown();
        }

        //DEBOUNCE FUNCTION: Debounce so filtering doesn't happen every millisecond
        function debounce(fn, threshold) {
            var timeout;
            return function debounced() {
                if (timeout) {
                    clearTimeout(timeout);
                }

                function delayed() {
                    fn();
                    timeout = null;
                }

                timeout = setTimeout(delayed, threshold || 100);
            };
        }

        //CHECK RESULTS FUNCITON: If nothing found during search display "no results" message
        function checkResults() {
            var visibleItemsCount = $grid.data("isotope").filteredItems.length;

            if (visibleItemsCount > 0) {
                $("." + params["noResultsClass"]).hide();
            } else {
                $("." + params["noResultsClass"]).show();
            }
        }

        //DROPDOWN BOX IMPLEMENTATION FUNCTIONS:

        //PROFILE LISTING DROPDOWN FUNCTION:
        function plDropdown() {
            $("#" + params["dropdownId"]).change(function () {
                ddlFilter = $(this).find(':selected').attr('data-filter');
                $grid.isotope();
                checkResults();
            });
        }

        //AROUND CAMPUS FILTER BUTTON FUNCTION:
        function acBtn() {
            $("#" + params["dropdownId"]).click(function () {
                var filterName = this.getAttribute("filter");
                if (filterName == "AZ") {
                    $grid.isotope({ sortBy: params["searchTitleClass"], sortAscending: true });
                    $(this).html("Sort: A to Z");
                    this.setAttribute("filter", "ZA");
                } else if (filterName == "ZA") {
                    $grid.isotope({ sortBy: params["searchTitleClass"], sortAscending: false });
                    $(this).html("Sort: Z to A");
                    this.setAttribute("filter", "AZ");
                }
                updateLazyLoad();
                checkResults();
            });
        }

        //UPDATE LAZY LOADED IMAGES
        function updateLazyLoad() {
            setTimeout(function () {
                $("img.lazy").filter(function () {
                    var rect = this.getBoundingClientRect();
                    return rect.top >= 0 && rect.top <= window.innerHeight;
                }).trigger("lazylazy");
            }, 0750);
        }
    }

    // #region Lazy Load Images
    // On document ready start lazy load and isotope integration
    jQuery(document).ready(function () {
        var $win = $(window),
            $imgs = $("img.lazy"),
            $con = $(".profile-listing-items").isotope();

        // Load the images visible to the browser
        function loadVisible($els, trigger) {
            $els.filter(function () {
                var rect = this.getBoundingClientRect();
                return rect.top >= 0 && rect.top <= window.innerHeight;
            }).trigger(trigger);
        }

        // Once layout is complete load the visible images
        $con.isotope("on", "layoutComplete", function () {
            loadVisible($imgs, "lazylazy");
        });

        // When the page is scrolled keep loading images
        $win.on("scroll", function () {
            loadVisible($imgs, "lazylazy");
        });

        // Lazy load image style
        $imgs.lazyload({
            effect: "fadeIn",
            placeholder: "/Assets/Images/default-place-holder-image.jpg",
            failure_limit: Math.max($imgs.length - 1, 0),
            event: "lazylazy"
        });

        setTimeout(function () {
            runIsotopeChecks();
        }, 0050);
    });
    // #endregion
  </script>
  <!-- End Isotope Code -->
  <!-- Include the newsfeed script for scrollbar tools -->
  <script src="/Assets/Scripts/Custom/newsfeed-btn.js">
  </script>
  <!-- Include the quick look script-->
  <script src="/Assets/Scripts/Custom/quick-look.js">
  </script>
  <!-- End Render Section Scripts-->
