   !function () { "use strict"; Array.prototype.slice.call(document.getElementsByTagName("img")).forEach(function (t) { t.hasAttribute("data-src-fallback") && t.addEventListener("error", function () { this.src = this.getAttribute("data-src-fallback") }) }) }();
  </script>
  <script src="https://cdn-01.wsb.wisc.edu/wsb-core-assets/content/2019/01.10/_js/vendor/require.min.js" type="text/javascript">
  </script>
  <script type="text/javascript">
   require.config({
			waitSeconds: 0,
			paths: {
				'jquery': "https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.4/jquery.min",
				'foundation': "https://cdnjs.cloudflare.com/ajax/libs/foundation/6.3.0/js/foundation.min",
				'slick.slider': "https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.7.1/slick.min",
				'wsb.main': "https://cdn-01.wsb.wisc.edu/wsb-core-assets/content/2019/01.10/_js/wsb-main-require.min",
				'knockout': "https://cdnjs.cloudflare.com/ajax/libs/knockout/3.3.0/knockout-min",
				'knockout.validation': "https://cdnjs.cloudflare.com/ajax/libs/knockout-validation/2.0.3/knockout.validation.min",
				'wsb.off.canvas.forms': "https://cdn-01.wsb.wisc.edu/wsb-core-assets/content/2019/01.10/_js/wsb-off-canvas-forms"
			},
			shim: {
				'foundation': ['jquery'],
				'slick.slider': ['jquery'],
				'wsb.main': ['foundation'],
				'knockout.validation': ['knockout'],
				'wsb.off.canvas.forms': ['knockout', 'knockout.validation']
			}
		});
		require(["jquery", "foundation", 'slick.slider', 'wsb.main', 'wsb.off.canvas.forms', 'knockout', 'knockout.validation'], function ($) {

			//console.log("Good to go!");

			function resizer(strCSSSelector) {
				if ($(strCSSSelector).length) {
					$(strCSSSelector).height('');
					var maxHeight = -1;
					$(strCSSSelector).each(function () {
						if ($(this).height() > maxHeight) {
							maxHeight = $(this).height();
						}
					});
					$(strCSSSelector).height(maxHeight);
				}
			}

			$('#wsb-accordion-tab').bind('DOMNodeInserted DOMNodeRemoved', function (event) {
				if ($(this).hasClass('tabs') && $(this).attr('data-wsb-current-setting') != 'tabs') {
					$(this).attr('data-wsb-counter', parseInt($(this).attr('data-wsb-counter')) + 1);

					// console.log('COUNTER: ' + $(this).attr('data-wsb-counter') + ' | ' + $(this).children().length);

					if ($(this).attr('data-wsb-counter') == $(this).children().length) {
						$(this).attr('data-wsb-counter', '0');
						resizer('div.tabs-panel');
						$(this).attr('data-wsb-current-setting', 'tabs');

						var attrlastactive = $(this).attr('data-wsb-lastactive');
						if (typeof attrlastactive !== typeof undefined && attrlastactive !== false) {
							if (attrlastactive.length > 0) {
								attrlastactive = '#' + attrlastactive;
								// console.log('data-wsb-lastactive: ' + attrlastactive);
							}
						}

					}
				} else if ($(this).hasClass('accordion') && $(this).attr('data-wsb-current-setting') != 'accordion') {
					$(this).attr('data-wsb-current-setting', 'accordion');
				}
			});

			$('#wsb-accordion-tab').on('change.zf.tabs up.zf.accordion', function (event) {
				var eventType = event.type;
				var strLastAcctiveTab = "";

				var $this = $(this);

				if($this.find('.is-active').length > 0){
					strLastAcctiveTab = $this.find('.is-active a')[0].id;
					$(this).attr('data-wsb-lastactive', strLastAcctiveTab);
				}

				var gaDataLayerEvent = {
					'event': 'gaEventTrigger',
					'gaCategory': 'Interactive Content',
					'gaAction': '',
					'gaLabel': $(this).find('.is-active a')[0].innerHTML
				};

				if (eventType === 'change') {
					// Tabs
					gaDataLayerEvent.gaAction = 'Tab';
				} else if (eventType === 'up') {
					// Accordions
					gaDataLayerEvent.gaAction = 'Accordion';
				} else {
					// Errors
					gaDataLayerEvent.gaAction = 'Error';
				}

				dataLayer.push(gaDataLayerEvent);
			});

			$(window).resize(function () {
				resizer('div.tabs-panel');
			});

			var buttonstate = "Play";

			$(document).ready(function () {

				$(window).on('load scroll', function () {
					var scrolled = $(this).scrollTop();
					// Fade the hero heading while scrolling
					$('.wsb-hero-video-text').css({
						'transform': 'translate3d(0, ' + -(scrolled * 0.2) + 'px, 0)',
						'opacity': 1 - scrolled / 500
					});
					// Parallax magic happens here
					$('#hero-video').css('transform', 'translate3d(0, ' + -(scrolled * 0.25) + 'px, 0)');
				});

				// Play and pause controls
				$('#hero-video-play-pause').on('click', function () {
					var video = $('#hero-video').get(0);
					var icons = $('#hero-video-play-pause > span > em');
					$('#wsb-hero-row').toggleClass('wsb-video-pause-overlay');
					if (video.paused) {
						video.play();
						buttonstate = "Play";
						icons.removeClass('fi-play').addClass('fi-pause');
					} else {
						video.pause();
						buttonstate = "Pause";
						icons.removeClass('fi-pause').addClass('fi-play');
					}
				});

			});

			$(function () {
				$(".wsb-hero-video-scroll-down-arrow").click(function (event) {
					event.preventDefault();
					$('html,body').animate({
						scrollTop: $(window).scrollTop() + $(window).height()
					});
				});
			});

			function MaxScrollDepth() {
				var intCount = dataLayer.length - 1;
				var percentScrollDepth = "";

				while (percentScrollDepth === "" && intCount > 0) {
					if (dataLayer[intCount].gaAction === "Percentage") {
						percentScrollDepth = dataLayer[intCount].gaLabel;
					}
					intCount = intCount - 1;
				}

				return percentScrollDepth;
			}

			var last_known_scroll_position = 0;
			var ticking = false;

			window.addEventListener('scroll', function (e) {
				last_known_scroll_position = window.scrollY;
				if (!ticking) {
					window.requestAnimationFrame(function () {

						document.getElementById('back-to-top-button').dataset.galabel = MaxScrollDepth();

						ticking = false;
					});
				}
				ticking = true;
			});

			var visibilityChange = (function (window) {
				var inView = false;
				return function (fn) {
					window.onfocus = window.onblur = window.onpageshow = window.onpagehide = function (e) {
						if ({ focus: 1, pageshow: 1 }[e.type]) {
							if (inView) return;
							fn("visible");
							inView = true;
						} else if (inView) {
							fn("hidden");
							inView = false;
						}
					};
				};
			}(this));

			visibilityChange(function (state) {
				// console.log("Webpage Status: " + state);

				// console.log("Button Status: " + buttonstate);

				var video = $('#hero-video').get(0);

				if (state === 'hidden' && buttonstate === 'Play') {
					video.pause();
				} else {
					if (state === 'visible') {
						if (buttonstate === 'Play') {
							video.play();
						} else {
							video.pause();
						}
					}
				}

			});

			
		});
  </script>
