							var map = new google.maps.Map(document.getElementById('sidebar-map'),
								mapOptions);
					
							var MyPlace = new google.maps.LatLng(43.053917,-91.146559);
					
							var marker = new google.maps.Marker({
								position: MyPlace,
								url: 'https://crawford.extension.wisc.edu/office-map-and-directions/',
								title: 'Office Map and Directions',
								map: map
							});
					
							google.maps.event.addListener(marker, 'click', function() {
							  window.location.href = marker.url;
							});

						  }
						  google.maps.event.addDomListener(window, 'load', initialize);
