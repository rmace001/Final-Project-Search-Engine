   <script language="javascript">
    document.onload = setFocus('frmLogin', 'email');
	//document.onload(setFocus('frmLogin', 'email'));
	
	String.prototype.trim = function(){
		return this.replace(/^\s*/,"").replace(/\s*$/,"");
	}
	
	// Detect if cookies are allowed on client browser.
	if(!isCookieAllowed())
	{
		alert("This site requires that your web browser accepts cookies.\nTo continue using this site, please fix your browser settings to allow cookies from this site.");
	}
	
	
	function isFormValid()
	{
		// email is a required field
		if(isEmpty(document.frmLogin.email.value))
		{
			alert('Please enter your email.');
			document.frmLogin.email.focus();
			return false;
		}
		
		// verify format of email
		var theEmail = new String(document.frmLogin.email.value);
		//if(!isEmail(trim(document.frmLogin.email.value)))
		if (!isEmail(theEmail.trim()))
		{
			alert('The email you entered is not in the proper format.\nMake sure to include the host, for example djane@ucsd.edu');
			document.frmLogin.email.focus();
			return false;
		}
				
		// password is a required field
		if(isEmpty(document.frmLogin.password.value))
		{
			alert('Please enter your password.');
			document.frmLogin.password.focus();
			return false;
		}
		document.getElementById('forgot-password-btn').style.display ="none";
		//document.frmLogin.btnSubmit.value = 'Logging in';		
		document.frmLogin.btnSubmit.disabled = true;
		
		// form is valid
		return true;
		
	}
	function isFormValid2()
	{
		if(isEmpty(document.frmForgotPassword.emailaddress.value))
		{
			alert('The email address is required.');
			document.frmForgotPassword.emailaddress.focus();
			return false;
		}
		
		if(!isEmail(document.frmForgotPassword.emailaddress.value))
		{
			alert('The email address you entered is not in the proper format.\nMake sure to include the host, for example djane@ucsd.edu');
			document.frmForgotPassword.emailaddress.focus();
			return false;
		}
		
		DisableButtons("btnSubmit","btnCancel");
		
		// form is valid
		return true;
		
	}	
	function jsfn_af_toggle_block(object_id) {
		// object_id : the name of the div block
		var js_agnt_usr = navigator.userAgent.toLowerCase();
		var js_dom_yes = document.getElementById?1:0;
		var js_nav_yes = js_agnt_usr.indexOf('mozilla')!=-1&&js_agnt_usr.indexOf('compatible')==-1?1:0;
		var js_exp_yes = js_agnt_usr.indexOf('msie')!=-1?1:0;
		var js_opr = js_agnt_usr.indexOf('opera')!=-1?1:0;
		var js_dom_nav = js_dom_yes && js_nav_yes?1:0;
		var js_dom_exp = js_dom_yes && js_exp_yes?1:0;
		var js_nav4 = js_nav_yes && !js_dom_yes && document.layers?1:0;
		var js_exp4 = js_exp_yes && !js_dom_yes && document.all?1:0;
		var js_hide = js_nav4?'hide':'hidden';
		var js_show = js_nav4?'show':'visible';
		var js_current_style_object = jsfn_af_get_style_object(object_id);
		if(js_current_style_object) {
			if (js_current_style_object.display) {
				if ( (js_current_style_object.display + '') == 'none') {
					js_current_style_object.display = 'block';
					js_current_style_object.visibility = 'visible';
				}
				else {
					js_current_style_object.display = 'none';
					js_current_style_object.visibility = 'hidden';
				}
			}
		}
	}
	
	function jsfn_af_get_style_object(object_id) {
		// cross-browser function to get an object's style object given its id
		//alert(object_id);
		if(document.getElementById && document.getElementById(object_id)) {
			//alert("W3C DOM");
			return document.getElementById(object_id).style;
		} 
		else if (document.all && document.all(object_id)) {
			//alert("MSIE 4 DOM");
			return document.all(objectId).style;
		} 
		else if (document.layers && document.layers[object_id]) {
			//alert("NN 4 DOM.. note: this won't find nested layers");
			return document.layers[object_id];
		} 
		else {
			//alert("not know what kind of browser");
			return false;
		}
	}
	
	// check if a string is conforms to a valid email format (i.e. xxxxxx@xxx.xxx)
	function isEmail(the_string)
	{
		var temp_string = String(the_string), x = 0, number_of_at = 0;
		var found_at = false, found_period = false;
		for(x=0; x < temp_string.length; x++)
		{
			// check for a space or a semicolon or a command which are invalid
			if(temp_string.charAt(x) == ' ' || temp_string.charAt(x) == ';' || temp_string.charAt(x) == ',' || temp_string.charAt(x) == '\'')
				return false;
			
			// check for the at-sign @ and count how many
			if(temp_string.charAt(x) == '@' && temp_string.charAt(x+1) != '')
			{
				number_of_at = number_of_at +1;
				found_at = true;
			}
			
			// check for the period
			if(temp_string.charAt(x) == '.'  && temp_string.charAt(x+1) != '')
				found_period = true;
		}
		
		if(number_of_at != 1)
			return false;
		else if(found_at && found_period)
			return true;
		else
			return false;
	}
   </script>
