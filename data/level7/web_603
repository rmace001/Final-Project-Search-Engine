         <script>
          <!--//--><![CDATA[// ><!--

jQuery(document).ready(function($){
	//toggleCareHeader();
	$(window).resize(function() {
		//toggleCareHeader();
	});
	function toggleCareHeader() {
		if(window.innerWidth < 1000 && window.innerWidth > 768) {
			$("#care-left").append($("#care-boxes"));
			$("#care-right").append($("#care-postcard-header"));
		}
		else {
			$("#care-left").append($("#care-postcard-header"));
			$("#care-right").append($("#care-boxes"));
		}
	}
});

