         <script>
          <!--//--><![CDATA[// ><!--

jQuery(document).ready(function($){
	if($("#block-fieldblock-node-page-default-field-paragraph-sliders").length) {
		$("#block-system-main > .content").before($("h1#page-title"));
		$("body").addClass("slider-has-image")
	}
	else { 	
		$("body").addClass("slider-no-image");
		$(".slider-no-image h1#page-title").after('<div class="title-wrapper">').append($("h1#page-title"));
		//$(".slider-secondary-no-image h1#page-title").after('<div class="title-wrapper">').append($("h1#page-title"));
		$(".title-wrapper").append($("h1#page-title"));
		$(".content-top").after($(".title-wrapper"));	
	}
	if($("div.sidebar").length > 0 ) {
		$("body").addClass("two-column");
		$("#content").before('<div id="sidebar-2" class="column">');
		$("#sidebar-2").append($("#sidebar .sidebar"));
		if($("div#block-menu-block-2").length > 0) {
			$("body").removeClass("two-column");
			$("body").addClass("three-column");
		}
		if($("div.sidebar_top").length > 0) {
			$("body").removeClass("two-column");
			$("body").addClass("three-column");
		}
	}
	$("#block-menu-block-2 h3 a").removeAttr("href");
	$('.menu-block-1 > ul > li > ul').addClass("menu-child");
	$('.menu-block-1 > ul > li > ul').each(function(i) {
		$(this).attr("id", "menu-child-" + i);
	});
	var head = document.head;
	var style = document.createElement('style');
	style.type = 'text/css';
	head.appendChild(style);
	if($(".mondo-img").length > 0) {
		var css = '@media screen and (min-width:769px) {';
		$('.menu-block-1 > ul > li > ul').each(function(i) {
			css += 'ul#menu-child-' + i + '::before {';
			css += 'background-image:url("' + $(".mondo-img")[i].innerHTML + '");}';
			css += 'ul#menu-child-' + i + '::after {';
			css += 'content:"' + $(".mondo-content")[i].innerHTML + '";}';
		});
		css += '}';
		style.appendChild(document.createTextNode(css));
	}
	if($("#mondo-float").length > 0) {
		$("body").addClass("mondo-menu-" + $("#mondo-float").html());
	}
	$(".menu-block-1 > ul > li.dropdown > a").each(function(e) {
		$(this).attr("href", "javascript:void(0)");
	});
	$(".menu-block-1 > ul > li.dropdown > a").click(function(e) {
		e.preventDefault();
	});
	$("#block-menu-block-2 .replace-link-text").each(function() {
		$(this).html($(this).attr("name"));
	})
	if($("body.slider-primary").length==1 && $("body.slider-primary #block-fieldblock-node-page-default-field-paragraph-sliders").length==0) {
		$("body").addClass("slider-primary-no-image");
	}
	if($("body.slider-secondary").length==1 && $("body.slider-secondary #block-fieldblock-node-page-default-field-paragraph-sliders").length==1) {
		$("body").removeClass("slider-secondary-no-image");
	}
	// Fix external slider links
	$(".group-slider-content .field-name-field-pg-slider-link a").each(function(i) {
		var reg = new RegExp($(location).attr('hostname'));
		if (!reg.test($(this).attr("href"))) {
		    $(this).attr('target', '_blank');
		}
	});
	toggleMobileSettings();
	$(window).resize(function() {
		//$("#sizer").html(window.innerWidth + ' x '  + window.innerHeight + ' x '  + window.devicePixelRatio + " resize");
		if(window.innerWidth < 769) {
			// Do something
		}
		else {
			// Do something else
		}
		toggleMobileSettings();
	});
	function toggleMobileSettings() {
		if(window.innerWidth < 769) {
			$("#content").after($("#sidebar-2"));
			$("#main").append($("#sidebar"));
		}
		else {
			$("#content").before($("#sidebar-2"));
			$("#main").append($("#sidebar"));
		}
	}
});

//--><!]]>
         </script>
