        <h1 class="title" id="page-title">
         School of Business and Economics
        </h1>
        <article about="/node/1" class="node-1 node node-landing-page view-mode-full clearfix" typeof="sioc:Item foaf:Document">
         <header>
          <span class="rdf-meta element-hidden" content="School of Business and Economics" property="dc:title">
          </span>
          <span class="rdf-meta element-hidden" content="0" datatype="xsd:integer" property="sioc:num_replies">
          </span>
         </header>
        </article>
        <!-- /.node -->
       </div>
