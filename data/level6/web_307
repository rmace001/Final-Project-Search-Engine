   <script type="text/javascript">
    /*	Mobile Nav
        ----------------------------------------------*/
        $(function () {
            $('#navToggle, #overlay').on('click', function () {
                $('header nav').toggleClass('active');
                $('body').toggleClass('openNav');
            });

            $('header nav a').click(function (event) {
                if ($('.mobileNavHeader').is(':visible')) {
                    if ($(this).siblings().size() && !$(this).hasClass('mobileHome') && !$(this).hasClass('mobileBack')) {
                        event.preventDefault();
                        $('.mobileHome').removeClass('active');
                        $('.mobileBack').addClass('active');
                        if (!$(this).next().children('li:first').hasClass('navLabel')) {
                            $(this).next().prepend('<li class="navLabel">' + $(this).html() + '</li>');
                        }
   

                        $('header nav ul ul').hide();
                        $(this).parents('ul').show();
                        $(this).siblings('ul').show();
                        $('#menu-top-navigation').animate({
                            left: '-=100%'
                        }, 250);
                    } else if ($(this).hasClass('mobileBack')) {
                        $('#menu-top-navigation').animate({
                            left: '+=100%'
                        }, 250, function () {
                            if ($(this).css('left') == '0px') {
                                $('.mobileHome').addClass('active');
                                $('.mobileBack').removeClass('active');
                            }
                        });
                    }
                }
            });

            function resetNav() {
                $('.menu-top-navigation-container *').removeAttr('style');
                $('.mobileHome').addClass('active');
                $('.mobileBack').removeClass('active');
            }

            $(window).resize(function () {
                if ($(this).width() > 767) {
                    resetNav();
                }
            });
        });

        
        /*	Add arrows for sub items 
        ----------------------------------------------*/
        
        $('li:has(> ul)').addClass('subarrow');
        $('li:not(:has(ul))').addClass('nosubarrow');
   </script>
   <script src="https://static.sa.ucsb.edu/js/jquery/bxslider/4_1_1/jquery.bxslider.4.1.1min.js" type="text/javascript">
   </script>
   <script type="text/javascript">
    $(document).ready(function () {
            
  $('.bxslider').bxSlider({
    autoStart: true,
    controls:false,
    auto:true,
    pager: false
    
});
  
 $('input.gsc-search-button[type="submit"]').attr("alt", "Search"); 
  
  $('.gsc-branding-img').attr("alt", "Google"); 
  
  $('.bx-wrapper .bx-controls-direction a').css('top', '90%');

    // Add the "focus" value to class attribute 
  $('ul#menu-top-navigation li').focusin( function() {
    $(this).addClass('focus');
  }
  );

  // Remove the "focus" value to class attribute 
  $('ul#menu-top-navigation li').focusout( function() {
    $(this).removeClass('focus');
  }
  );
});
   </script>
   <script type="text/javascript">
    //Allows both ASK and SEARCH buttons to operate on the same field
$('#ucsb-answers-text').on('keyup change', function() {
       $('.sfsearchTxt').val($(this).val());
});
   </script>
