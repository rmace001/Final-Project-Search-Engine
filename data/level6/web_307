         <input class="sfsearchSubmit" id="SFSearchWidget_TB6F6B6F5002_ctl00_ctl00_searchButton" name="ctl00$SFSearchWidget$TB6F6B6F5002$ctl00$ctl00$searchButton" onclick="return false;" type="submit" value="Search"/>
        </fieldset>
       </div>
