      <a href="#" id="navToggle" tabindex="1">
       Menu
      </a>
      <div id="hybrid-search-answers-widget">
       <div id="ucsb-answers">
        <div alt="Ucsb Answers" id="ucsb-answers-image">
        </div>
        <div id="ucsb-answers-text-div">
         <input id="ucsb-answers-t   ext" placeholder="Ask or Search" type="text"/>
         <input id="ucsb-answers-button" name="ask" type="submit" value="ask"/>
        </div>
       </div>
       <script>
        $(function () {
        $("#ucsb-answers-text").focus(function () {
            //$("#ucsb-answers-text-label").hide();
            showValidation(true);
        });
        $("#ucsb-answers-text").focusout(function () {
            if ($("#ucsb-answers-text").val().trim() == '') {
                //$("#ucsb-answers-text-label").show();
            }
        });
        $("#ucsb-answers-button").click(function (event) {
            event.stopPropagation();
            event.preventDefault();
            askQuestion();
        });
        $("#ucsb-answers-text").on('keydown', function(event) {
            if (event.keyCode == 13 || event.which == 13) {
                askQuestion();
                return false;
            }
            return true;
        });
    });

    function askQuestion() {
        var question = $("#ucsb-answers-text").val();
        if (question.trim() != "") {
            window.open("https://ucsb.intelliresponse.com/index.jsp?interfaceID=1&id=-1&requestType=NormalRequest&source=1&question=" + question);
        } else {
            showValidation(false);
        }
    }

    function showValidation(isValid) {
        if (!isValid) {
            $("#ucsb-answers-text").addClass("text-invalid");
        } else {
            $("#ucsb-answers-text").removeClass("text-invalid");
        }
    }
       </script>
       <div id="SFSearchWidget_T6E1431E5001">
