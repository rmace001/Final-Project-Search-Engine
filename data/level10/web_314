            width: 102,
        });
        var chart2 = $('.chart2').ClassyLoader({
            diameter: 48,
            fontColor: '#FFF',
            fontFamily: 'Source Sans Pro',
            fontSize: '20px',
            fontSize: '35px',
            height: 102,
            lineColor: '#14324d',
            lineWidth: 4,
            percentage: 0,
            remainingLineColor: '#00549e',
            speed: 30,
            start: 'top',
            width: 102,
        });
        var chart2a = $('.chart3').ClassyLoader({
            diameter: 48,
            fontColor: '#FFF',
            fontFamily: 'Source Sans Pro',
            fontSize: '20px',
            fontSize: '35px',
            height: 102,
            lineColor: '#14324d',
            lineWidth: 4,
            percentage: 0,
            remainingLineColor: '#00549e',
            speed: 30,
            start: 'top',
            width: 102,
        });
        var chart3 = $('.chart4').ClassyLoader({
            diameter: 48,
            fontColor: '#FFF',
            fontFamily: 'Source Sans Pro',
            fontSize: '20px',
            fontSize: '35px',
            height: 102,
            lineColor: '#14324d',
            lineWidth: 4,
            percentage: 0,
            remainingLineColor: '#00549e',
            speed: 30,
            start: 'top',
            width: 102,
        });
        unset = true;

        $(window).scroll(function (e) {
            if (($(window).scrollTop() + $(window).innerHeight()) > $('#charts-load-here').offset().top && unset) {
                chart1.draw(57);
                chart2.draw(81);
                chart2a.draw(86);
                chart3.draw(79);
                unset = false;
            }
        });
        }(jQuery);
