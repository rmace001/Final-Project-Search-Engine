     <script language="JavaScript">
      function randomString() {
	var length = 5+Math.floor((Math.random()*6)); //length from 5 to 10
	var r="";
	for(i=0;i<length;i++) {
		r+=Math.floor((Math.random()*10)) + "";
	}
	r+="";
	return r;
}

function submitAndTest(submitTo) {
	
	var name = randomString();

	setCookie(name,"testCookies");
	if(getCookie(name) == "testCookies") {
		deleteCookie(name);
		//trvSubmit("initial");
		trvRelocate("CASLogin.asp");
	} else {
		document.emptyform.action = "nocookies.asp";
		trvRelocate("nocookies.asp");
	}
	
	
}
     </script>
