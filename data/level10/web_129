  <script type="text/javascript">
   $('.fmOptions').on('show.bs.collapse', function () {
        $(this).siblings('.fmOptions').find('#glyphExpand').removeClass("glyphicon-chevron-down").addClass("glyphicon-chevron-up");
	});

	$('.fmOptions').on('hide.bs.collapse', function () {
        $(this).siblings('.fmOptions').find('#glyphExpand').removeClass("glyphicon-chevron-up").addClass("glyphicon-chevron-down");
	});
  </script>
