     <div class="CalendarMain col-lg-12 col-md-12 col-sm-12 col-xs-12 collapse in" id="calendar">
      <div class="delayedLoad" data-url="/Calendar/GetCalendarWidgetItems">
      </div>
     </div>
     <script type="text/javascript">
      function CalendarChange(value) {
        $('#widgetButton').prop('disabled', true);
        $.post("Calendar/CalendarEvents", { calendarId: value })
		.done(function (data, status) {
		    $("#calendar").html(data);
		})
        .fail(function () {
            ShowJavascriptError('0')
        }).
		always(function () {
		    $('#widgetButton').prop('disabled', false);
		})
    }
     </script>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 TweetsOuter">
     <div class="NewsHeader col-lg-12 col-md-12 col-sm-12 col-xs-12">
