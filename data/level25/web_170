   jQuery(document).ready(function($) {
	// On main nav A focus
	$('nav.mk-main-navigation > ul#menu-main-nav > li.menu-item-has-children > a').on('focus',function(){
		// Hide all submenus
		$('nav.mk-main-navigation > ul#menu-main-nav > li.menu-item-has-children > a + ul').hide();
		// Show only the focused submenu
		$(this).next('ul').show();
	});
});
