  <div class="pane-content">
   <div class="fieldable-panels-pane">
    <div class="paragraphs-items paragraphs-items-field-paragraph-bundles paragraphs-items-field-paragraph-bundles-full paragraphs-items-full">
     <div class="field field-name-field-paragraph-bundles field-type-paragraphs field-label-hidden">
      <div class="field-items">
       <div class="field-item even">
        <div class="entity entity-paragraphs-item paragraphs-item-text paragraphs-item-66">
         <div class="content">
          <h2 class="pane-title">
           Reasons to Give
          </h2>
         </div>
        </div>
       </div>
       <div class="field-item odd">
        <div class="entity entity-paragraphs-item paragraphs-item-row paragraphs-item-51">
         <div class="content">
          <div class="paragraphs-items paragraphs-items-field-column paragraphs-items-field-column-full paragraphs-items-full">
           <div class="field field-name-field-column field-type-paragraphs field-label-hidden">
            <div class="field-items">
             <div class="field-item even">
              <div class="entity entity-paragraphs-item paragraphs-item-33-column paragraphs-item-26">
               <div class="content">
                <div class="paragraphs-items paragraphs-items-field-widget-single paragraphs-items-field-widget-single-full paragraphs-items-full">
                 <div class="field field-name-field-widget-single field-type-paragraphs field-label-hidden">
                  <div class="field-items">
                   <div class="field-item even">
                    <div class="entity entity-paragraphs-item paragraphs-item-statistic paragraphs-item-21 widget-coolgrey eb-color">
                     <div class="content">
                      <div class="field field-name-field-paragraph-view field-type-viewfield field-label-hidden">
                       <div class="field-items">
                        <div class="field-item even">
                         <div class="view view-statistics view-id-statistics view-display-id-pane_stastic_widget view-dom-id-f54c96efde4dfdd6df1de39266cd5587">
                          <div class="view-content">
                           <div class="item-list">
                            <ul>
                             <li class="views-row views-row-1 views-row-odd views-row-first views-row-last">
                              <div class="views-field views-field-views-conditional">
                               <span class="field-content">
                                <div class="both">
                                 <div class="img-wrap">
                                  <img alt="40 Percent" class="medium" height="117" src="https://give.morris.umn.edu/sites/give.morris.umn.edu/files/styles/medium/public/40-percent.png?itok=sm5mA8tp" width="220"/>
                                 </div>
                                 <div class="text">
                                  <div class="statistic-number">
                                   40%
                                  </div>
                                  <p>
                                   <span>
                                    of students are first generation college students
                                   </span>
                                  </p>
                                 </div>
                                </div>
                               </span>
