                config.searchMenuTheme = ('search-classic') ? 'search-classic' : null; // 'search-classic'; // 'default';
                config.searchLocalPath = ('') ? '' : null; // e.g. 'www.utmb.edu/president'

                // Breadcrumbs
                config.useBreadcrumbs = utmbWeb.setBooleanFromString('False');
                //config.breadcrumbOverride = null; // DEPRECATED

                // Site Ownership (Kudos) -- DEPRECATED -- MOVED TO PAGE META
                config.kudos1 = null; //('@site.Kudos1') ? '@site.Kudos1' : null;
                config.kudos1Url = null;
                config.kudos2 = null;
                config.kudos2Url = null;
                config.kudos3 = null;
                config.kudos3Url = null;

                // Root Domain
                config.showRootDomain = utmbWeb.setBooleanFromString('True'); // *show root domain in breadcrumbs - e.g.
                config.rootDomainTitle = ('UTMB Home') ? 'UTMB Home' : null;
                config.rootDomainUrl = ('http://www.utmb.edu/') ? 'http://www.utmb.edu/' : null;

                // Institutional Resources
                config.useUtmbAnalytics = utmbWeb.setBooleanFromString('False'); // App-Level?
                config.useUtmbAlertSystem = utmbWeb.setBooleanFromString('True');

                // Content Placeholder Regions (in Master)
                config.manageContentRegions = true; // Provide the option to hide (or whatever) content placeholder regions? Adds CSS class names to body
                config.useRegion_topbarContent = true; //utmbWeb.setBooleanFromString(siteSettings.UseRegion_TopbarContent),
                config.useRegion_atopContent = true; //true, //utmbWeb.setBooleanFromString(siteSettings.UseRegion_AtopContent),
                config.useRegion_featureContent = true; //utmbWeb.setBooleanFromString(siteSettings.UseRegion_FeatureContent),
                config.useRegion_articleContent = true; //utmbWeb.setBooleanFromString(siteSettings.UseRegion_ArticleContent),
                config.useRegion_baseContent = true; //utmbWeb.setBooleanFromString(siteSettings.UseRegion_BaseContent),
                config.useRegion_subContent = true; //utmbWeb.setBooleanFromString(siteSettings.UseRegion_SubContent)

            })(utmbWeb.config);
   </script>
