     <script>
      // [TEMPLATE] Douglas Crockford's supplant method (poor man's templating)
    if (!String.prototype.supplant) {
        String.prototype.supplant = function (o) {
            return this.replace(/{([^{}]*)}/g,
                function (a, b) {
                    var r = o[b];
                    return typeof r === 'string' || typeof r === 'number' || typeof r === 'boolean' ? r : a;
                }
            );
        };
    }

    //// [REQUIRES] jQuery.Color plugin
    //jQuery.fn.flash = function (color, duration) {
    //    var current = this.css('backgroundColor');
    //    this.animate({ backgroundColor: 'rgb(' + color + ')' }, duration / 2)
    //        .animate({ backgroundColor: current }, duration / 2);
    //};

    /*
        public int AlertCatId { get; set; }
        public string AlertCatTitle { get; set; }
        public string AlertCatThreatLevel { get; set; }
        public string AlertCatThreatLevelCss { get; set; }
        public int AlertId { get; set; }
        public string AlertUsername { get; set; }
        public string AlertTitle { get; set; }
        public string AlertMessage { get; set; }
        public string AlertPostDate { get; set; }
        public string AlertPostTime { get; set; }
        public int AlertCount { get; set; }

        public List<UtmbAlertsViewModel> UtmbAlertCateories { get; set; } // main loop
        public List<UtmbAlertsViewModel> UtmbAlertMessages { get; set; } // nested inside of main loop
    */

    // Row Template
    function alertCatTemp() {
        return '<li>' +
                // Alert Cat Modal Button
                '<a href="https://' + 'www.utmb.edu/alert/#alertCat_{AlertCatId}" ' +
                    'title="{AlertCatTitle} Alert" ' +
                    'class="btn btn-alert-bar btn-sm {AlertCatThreatLevelCss}" ' +
                    'data-toggle="modal" ' +
                    'data-target=".alert-cat-modal-{AlertCatId}"> ' +
                    '<em class="fi-alert"></em>' +
                    '{AlertCatTitle}' +
                    '{AlertMessageCount}' +
                '</a>' +
                // Alert Message(s) Modal
                '<div id="alertcat_{AlertCatId}" ' +
                    'class="modal fade alert-cat-modal-{AlertCatId}" ' +
                    'data-reveal="" ' +
                    'tabindex="-1" ' +
                    'role="dialog" ' +
                    'aria-labelledby="alert-msg-title"> ' +
                    '<div class="modal-dialog modal-lg">' +
                        '<div class="modal-content alert-modal {FormattedModalClassName}">' +
                            // Modal Alert Category Title
                            '<p class="alert-cat-title"><em class="fi-alert"></em> {AlertCatTitle}</p>' +
                            // Close Button
                            '<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>' +
                            // Formatted Alert Messages
                            '{FormattedAlertMessages}' +
                            // Footer
                            '<div class="alert-modal-footer"><a href="https://www.utmb.edu/alert/" title="UTMB Alert Site">UTMB Alert Site</a></div>' +
                        '</div>' +
                    '</div>' +
                '</div>' +
            '</li>';
    }

    $(function () {

        // SignalR Hub and Template Config
        var utmbAlertsHub = $.connection.utmbAlertsHub, // the generated client-side hub proxy
            $utmbAlertSystem = $('#utmbAlertSystem'),
            $utmbAlertsContainer = $('#alertsList'),
            rowBg = '240,240,240', // #f0f0f0
            rowTemplate = alertCatTemp();


        // Get Alert Bar Control
        function getAlertBarControl() {
            return '<div id="utmbAlertSystem" class="preloader">' +
                    '<div id="utmb-alert-bar">' +
                        '<div class="container">' +
                            '<div class="row">' +
                                '<div class="col-sm-12">' +
                                    // Alert Bar Container
                                    '<div id="alert-container" class="clearfix">' +
                                        // Alerts Title w/ Icon
                                        '<a id="lnkAlertSite" href="https://www.utmb.edu/alert/" class="alert-bar-title" title="UTMB Alert Site">' +
                                            '<i class="fa fa-volume-up " aria-hidden="true"></i> <span>Alerts</span></a>' +
                                        // Alert Bar Links/Buttons
                                        '<div class="alert-bar-links">' +
                                            '<ul id="alertsList"></ul>' +
                                        '</div>' +
                                    '</div>' +
                                '</div>' +
                            '</div>' +
                        '</div>' +
                    '</div>' +
                '</div>';
        }


        // Format Alerts
        function formatUtmbAlerts(utmbAlertsViewModel) {

            // Get Alert Modal Css ClassName
            function getAlertModalClassName(catThreatLevel) {
                var result = '';
                if (catThreatLevel) {
                    switch (catThreatLevel.toLowerCase()) {
                        case "red":
                            result = 'msg-red-alert';
                            break;
                        case "yellow":
                            result = 'msg-yellow-alert';
                            break;
                        case "orange":
                            result = 'msg-orange-alert';
                            break;
                        case "green":
                            result = 'msg-green-alert';
                            break;
                    }
                }
                return result;
            }

            // Get Alert Messages
            function getAlertMessages(alerts) {
                var result = '';
                if (alerts) {
                    for (var i = 0; i < alerts.length; i++) {
                        result += '<div class="alert-msg-container">';
                        result += '<p class="alert-msg-title">' + alerts[i].AlertTitle + '</p>';
                        result += '<p class="alert-msg-meta">' + alerts[i].AlertPostDate + ' &bull; ' + alerts[i].AlertPostTime + ' &bull; ' + alerts[i].AlertUsername + '</p>';
                        result += '<div class="alert-msg">' + alerts[i].AlertMessage + '</div>';
                        //result += '<hr />';
                        result += '</div>';
                    }
                }
                return result;
            }

            // Alert Message Count
            function getAlertMessageCount(alertCount) {
                var result = '';
                if (alertCount > 1) {
                    result = ' <span class="badge alert-count-badge"><small>' + alertCount + '</small></span>';
                }
                return result;
            }

            // Extend this Thing
            // Add nested alert messages from category's 'UtmbAlertMessages' array
            return $.extend(utmbAlertsViewModel, {
                FormattedModalClassName: getAlertModalClassName(utmbAlertsViewModel.AlertCatThreatLevel),
                FormattedAlertMessages: getAlertMessages(utmbAlertsViewModel.UtmbAlertMessages),
                AlertMessageCount: getAlertMessageCount(utmbAlertsViewModel.AlertCount)
            });
        };


        // Get Entries
        function getAlerts() {

            // Empty Container
            //$utmbAlertsContainer.empty();
            $('#utmbAlertSystem').remove();
            
            // Prepend Alert Bar Control
            $('#PublicWrapper').prepend(getAlertBarControl());

            //Fetching UTMB Alerts
            return utmbAlertsHub.server.getUtmbAlerts().done(function (utmbAlerts) {
                // Append Rows
                $.each(utmbAlerts, function () {
                    var utmbAlertsViewModel = formatUtmbAlerts(this);
                    $('#utmbAlertSystem').find('#alertsList').append(rowTemplate.supplant(utmbAlertsViewModel));
                });
            });
        };


        // Set Alert Bar is-active ClassName
        function setActiveClass() {
            var alertBar = document.getElementById('utmbAlertSystem');
            if (alertBar) alertBar.className = 'is-active';
        };


        // [CRUD MANAGER] Manage this Thing
        utmbAlertsHub.client.UtmbAlertsManager = function (action, utmbAlertsViewModel) {
            var thisUtmbAlert = formatUtmbAlerts(utmbAlertsViewModel);
            var $thisUtmbAlertRow = $(rowTemplate.supplant(thisUtmbAlert));
            var $thisUtmbAlertId = $('.alert-' + thisUtmbAlert.AlertCatId);

            switch (action) {

                case 'createRecord':
                    console.log("A new alert was created.");
                    $utmbAlertsList.prepend($thisUtmbAlertRow);
                    //$thisUtmbAlertRow.flash(rowBg, 1000);
                    break;

                case 'viewRecord':
                    console.log("Alert is currently currently locked as it's being viewed.");
                    $thisUtmbAlertId.closest("li").replaceWith($thisUtmbAlertRow);
                    //$thisUtmbAlertRow.flash(rowBg, 1000);
                    break;

                case 'updateRecord':
                    console.log("An alert record was updated.");
                    $thisUtmbAlertId.closest("li").replaceWith($thisUtmbAlertRow);
                    //$thisUtmbAlertRow.flash(rowBg, 1000);
                    break;

                case 'deleteRecord':
                    $thisUtmbAlertId.closest("li").remove();
                    break;
            }
        };

        // [LOG: OPTIONAL] Browser Console Log
        $.connection.hub.logging = true;

        // [LOG ERRORS] Only for Dev
        // -http://stackoverflow.com/questions/19688673/signalr-there-was-an-error-invoking-hub-method-xxx
        $.connection.hub.error(function (error) {
            console.log('SignalR error: ' + error)
        });

        // Start the connection
        $.connection.hub.start({ transport: ['serverSentEvents', 'longPolling'] })
            .then(getAlerts)
            .then(setActiveClass);

    });
     </script>
