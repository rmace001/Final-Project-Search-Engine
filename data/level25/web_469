										//         var formatDate = (monthNames[date.getMonth()]) + ' ' + date.getDate() + ', ' +  date.getFullYear();

										//       	newscard += "<div class='news-block masonry-panel'>";
										//       	if(response[index].featured_image)
										//       		{
										//       			newscard += "<img src='" + response[index].featured_image.source_url + "' alt='" + response[index].featured_image.alt + "' />";
										//       			newscard += "<div class='newscard-mobile-image' style='background-image:url(&#39;" + response[index].featured_image.source_url + "&#39;);'></div>"
										//       		}
										//       	newscard += "<div class='newscard-text'>";
										//         newscard += "<h3><a href='" + response[index].link.replace("https:","http:") + "' target='_blank'>" + response[index].title.rendered + "</a></h3>";
										//         newscard += "<div class='excerpt'>" + response[index].excerpt.rendered + "</div>";
										//         newscard += "<div class='newscard-bottom'>";
										//         newscard += "<p class='date'>" + formatDate + "</p>";
										//         newscard += "<a href='" + response[index].link.replace("https:","http:") + "' target='_blank'>Read More &raquo;</a>";
										//         newscard += "</div>";
										//         newscard += "</div>";
										//         newscard += "</div>";
										//       });

										//       // Take our created divs and put it into the above div
										//       $('.news-wrapper').html(newscard);
										//     }
										//   });
										// });


										// Multiple URLs
										function returnFeeds(feeds){
										  var arrayLength = feeds.length;
										  var feedLoaded = [];
										  var output = [];
										  for (var i = 0; i < arrayLength; i++) {
										      //alert(feeds[i]);
										      //Do something
										    feedLoaded[i] = false;
										    // console.log('feedloaded current: ' + i + ':' + feedLoaded);
										    getFeed(feeds[i], i);
										  }
										  function getFeed(url, feed){
										    $.getJSON(url, function(data){
										       // console.log(data)
										       for (var o=0; o<data.length; o++){
										          var object = {}
										          object = data[o]
										          output.push(object);
										        }
										        feedLoaded[feed] = true;
										        // console.log('feedloaded current: ' + i + ':' + feedLoaded);
										        sortData();
										    });
										  }
										  
										  function sortData(){
										    //console.log('status: ' + url1 +' | ' + url2);
										    // console.log(output);
										    if (feedLoaded.every(function(e){ return e === true })
										    ) {
										  // Here I'm building an article div with the title, excerpt and link to the live version.
										      output.sort(function (a, b) {
										        return a.date.localeCompare(b.date);
										      });
										      output.reverse();

										            var newscard = '';
										            $.each(output, function(index) {
									            		if (index > 8) {
										            		return false;
										            	}

														var monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "June", "July", "Aug", "Sept", "Oct", "Nov", "Dec"];
														var date = new Date(output[index].date);
														var formatDate = (monthNames[date.getMonth()]) + ' ' + date.getDate() + ', ' +  date.getFullYear();

														newscard += "<div class='news-block masonry-panel'>";
														if(output[index].featured_image)
															{
																newscard += "<img src='" + output[index].featured_image.source_url + "' alt='" + output[index].featured_image.alt + "' />";
																newscard += "<div class='newscard-mobile-image' style='background-image:url(&#39;" + output[index].featured_image.source_url + "&#39;);'></div>"
															}
														newscard += "<div class='newscard-text'>";
														newscard += "<h3><a href='" + output[index].link.replace("https:","http:") + "' target='_blank'>" + output[index].title.rendered + "</a></h3>";
														newscard += "<div class='excerpt'>" + output[index].excerpt.rendered + "</div>";
														newscard += "<div class='newscard-bottom'>";
														newscard += "<p class='date'>" + formatDate + "</p>";
														newscard += "<a href='" + output[index].link.replace("https:","http:") + "' target='_blank'>Read More &raquo;</a>";
														newscard += "</div>";
														newscard += "</div>";
														newscard += "</div>";

										            });

									            // Take our created divs and put it into the above div
									            $('.news-wrapper').html(newscard);
										    } else {
										      console.log('not loaded yet: ' + feedLoaded + ':');
										    }

										  }
										}

										//setting vars for the two feeds - this whole thing could likely be wrapped in a function that takes an array of feeds
										var mainSource = 'https://source.colostate.edu/wp-json/wp/v2/posts?filter[tag]=csu-energy-institute';
										var engrSource = 'https://engr.source.colostate.edu/wp-json/wp/v2/posts?filter[tag]=csu-energy-institute';

										returnFeeds([mainSource, engrSource]);
           </script>
