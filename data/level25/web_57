     <script>
      function updateSearch(){
		var term = jQuery('#search').val()
		
		jQuery('.search_type .search_link').each(function() {
            var href = jQuery(this).attr('href');
			jQuery(this).attr('href', href.replace(/(s=).*?(&)/,'$1' + term + '$2'));
        });

		jQuery('.collection_search').each(function() {
            var href = jQuery(this).attr('href');
			jQuery(this).attr('href', href.replace(/(q=).*?(&)/,'$1' + term + '$2'));
        });
		
		jQuery('.search_term.update').text(term);


	}
	jQuery( "#search" ).focus(function() {
		jQuery( "#search" ).animate({
			width: "250px",
		}, {
		complete: function() {
			jQuery('#search_dropdown').slideDown();
		}
		});

	});

	jQuery( "#search" ).blur(function() {
		jQuery('#search_dropdown').slideUp(function(){
			jQuery( "#search" ).animate({
				width: "190px",
			})
		});
	});

	jQuery( "#search" ).keyup(function() {
		updateSearch();
	});
     </script>
