     <script type="text/javascript">
      jQuery(document).ready(function() {
		jQuery(".middle").each(function() {
			contentHeight = jQuery(this).height();
			parentHeight = jQuery(this).parent().height();
			if(parentHeight > contentHeight){
				topMargin = (parentHeight - contentHeight)/2;
				jQuery(this).css("padding-top", topMargin);
			}
		});
    });
	//box 1 slide parameters
	jQuery("#box1 .box_slider").carouFredSel({
		items: 1,
		scroll: {
			items: 1,
			duration: 1500
		},
		auto:false,
		prev: {
			button: "#box1 .ctrl-left"
		},
		next: {
			button: "#box1 .ctrl-right"
		}
	});

	//box 2 slider parameters
	jQuery("#box2 .box_image").carouFredSel({
		circular: false,
		items: 1,
		auto:false,
		scroll: {
			fx:'crossfade'
		},
		pagination: {
			container: '#box2 .feature_dates',
			event: 'mouseenter',
			anchorBuilder: function( nr) {
				return '<li><a href="' + jQuery('img',this).data('link') + '" class="box_title">' + jQuery('img',this).data('title') + '</a></li>';
			}
		}
	});

		//box 3 slider parameters
	jQuery("#box3 .box_image").carouFredSel({
		circular: false,
		items: 1,
		auto:false,
		scroll: {
			fx:'crossfade'
		},
		pagination: {
