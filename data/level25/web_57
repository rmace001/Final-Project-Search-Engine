    <script>
     jQuery(function ($) {
                    function changeLink(items){ // set the link
                            var LinkURL = $(items).attr('href');
                            $('#livestream_link').attr('href',LinkURL);
                    }
                
                    //start the ticker
                    $("#stream_content").carouFredSel({
                         direction: 'down',
                        items: 1,
                        scroll: {
                            items: 1,
                            duration: 1500,
                            onAfter	: function(data) {
                                changeLink(data.items.visible);
                            }
                        },
                        auto:3000,
                    });
                    
                    //set the link on page load
                    changeLink($('#stream_content > *') );
                })
    </script>
