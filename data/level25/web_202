     <div class="form-group floatl width80p right2p">
      <label class="displayb">
       <span class="hidden_label">
        Search
       </span>
       <input class="searchBox searchBoxHor form-control" type="text"/>
      </label>
      <p class="red displayn errorHorizontal">
       Please enter search criteria
      </p>
     </div>
