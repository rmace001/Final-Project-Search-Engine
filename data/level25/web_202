             <script type="text/javascript">
              $(document).ready(function () {
                             var other = $('#degreesGoalSelect').val();
                             if (other.toLowerCase() == "other") {
                                 $('#custAddInfoOther').removeAttr('readonly');
                             }
                             $('#degreesGoalSelect').change(function () {
                                 var value = $(this).val();
                                 if (value.toLowerCase() == "other") {
                                     $('#custAddInfoOther').removeAttr('readonly');
                                     $('#custAddInfoOther').focus();
                                 } else {
                                     $('#custAddInfoOther').attr('readonly', 'true');
                                     $('#custAddInfoOther').val('');
                                 }
                             });
                         });
             </script>
