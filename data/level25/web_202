               $(document).ready(function () {
                            var required = 'True';
                            var alpha = 'False';
                            var pattern = '^[0-9]{9,10}$';
                            $('#custStudID').attr('maxlength', '10');
                            $('#custStudID').attr('minlength', '9');
                            $('#custStudID').attr('pattern', pattern);
                            if (required.toLowerCase() == "true") {
                                $('#custStudID').attr('required', true);
                                $('#custStudID').attr('placeholder', 'Student ID *');
                            } else {
                                $('#custStudID').attr('placeholder', 'Student ID');
