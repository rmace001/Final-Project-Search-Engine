             <input name="customerRegister.billAddress.ShippingAsBilling" type="hidden" value="false"/>
             <span>
              Use Billing Address as Shipping Address?
             </span>
            </label>
           </div>
           <script type="text/javascript">
            $(document).ready(function () {
                        $('#useSameShipping').change(function () {
                            var checkedValue = $(this).is(':checked');
                            var residential = $('[name="customerRegister.billAddress.Residential"]:checked').val();
                            var email = $('#custNewUserName').val();
                            var phoneRequired = 'True';
                            var fname = $('#custBillFirst').val();
                            var middle = $('#custBillMiddle').val();
                            var lname = $('#custBillLast').val();
                            var business = $('#custBillBusiness').val();
                            var add1 = $('#custBillAdd1').val();
                            var add2 = $('#custBillAdd2').val();
                            var city = $('#custBillCity').val();
                            var state = $('#custBillState').val();
                            var zip = $('#custBillZip').val();
                            var zip4 = $('#custBillZip4').val();
                            var country = $('#custBillCountry').val();
                            var phone = $('#custBillPhone').val();
                            var validate;
                            $('.useSameError').remove();
                            if (checkedValue == true) {
                                if (phoneRequired.toLowerCase() == "true") {
                                    if (phone == "") {
                                        $('<div class="red useSameError col-xs-12">Phone Number is required.</div>').appendTo('#custBillPhone');
                                        $('#custBillPhoneNumber').focus();
                                        validate = true
                                    }
                                }
                                if (country == "" || country == null) {
                                    $('<div class="red useSameError col-xs-12">Country is required.</div>').appendTo('#custBillCountryDropdown');
                                    $('#custBillCountry').focus();
                                    validate = true
                                }
                                if (zip == "") {
                                    $('<div class="red useSameError col-xs-12">Zip Code is required.</div>').appendTo('#custBillZipCode');
                                    $('#custBillZip').focus();
                                    validate = true
                                }
                                if (state == "" || state == null) {
                                    $('<div class="red useSameError col-xs-12">State is required.</div>').appendTo('#custBillStateDropdown');
                                    $('#custBillState').focus();
                                    validate = true
                                }
                                if (city == "") {
                                    $('<div class="red useSameError col-xs-12">City is required.</div>').appendTo('#custBillCName');
                                    $('#custBillCity').focus();
                                    validate = true
                                }
                                if (add1 == "") {
                                    $('<div class="red useSameError col-xs-12">Address is required.</div>').appendTo('#custBillAddress1');
                                    $('#custBillAdd1').focus();
                                    validate = true
                                }
                                if (residential == "false") {
                                    if (business == "") {
                                        $('<div class="red useSameError col-xs-12">Business name is required when residential address is set to "No" </div>').appendTo('#custBillBName');
                                        $('#custBillBusiness').focus();
                                        validate = true
                                    }
                                }
                                if (lname == "") {
                                    $('<div class="red useSameError col-xs-12">Last name is required.</div>').appendTo('#custBillLName');
                                    $('#custBillLast').focus();
                                    validate = true
                                }
                                if (fname == "") {
                                    $('<div class="red useSameError col-xs-12" role="alert">First name is required.</div>').appendTo('#custBillFName');
                                    $('#custBillFirst').focus();
                                    validate = true
                                }
                                if (email == "") {
                                    $('<div class="red useSameError col-xs-12" role="alert">Email Address is required.</div>').appendTo('#newUser');
                                    $('#custNewUserName').focus();
                                    validate = true
                                }
                                if (validate == true) {
                                    $('#useSameShipping').prop('checked', false);
                                    return false
                                }

                                $("input[name='customerRegister.shipAddress.Residential'][value=" + residential + "]").click();
                                var newResidential;
                                if (residential.toLowerCase() == "true") {
                                    newResidential = false;
                                } else {
                                    newResidential = true
                                }
                                $("input[name='customerRegister.shipAddress.Residential'][value=" + newResidential + "]").attr('disabled', 'true');
                                $('#custShipFirst').val(fname);
                                $('#custShipMiddle').val(middle);
                                $('#custShipLast').val(lname);
                                $('#custShipBusiness').val(business);
                                $('#custShipAdd1').val(add1);
                                $('#custShipAdd2').val(add2);
                                $('#custShipCity').val(city);
                                $('#custShipState').val(state);
                                $('#custShipZip').val(zip);
                                $('#custShipZip4').val(zip4);
                                $('#custShipCountry').val(country);
                                $('#custShipPhone').val(phone);
                                $('#custShipEmail').val(email);
                                $('#custShipFirst').attr('readonly', 'true');
                                $('#custShipMiddle').attr('readonly', 'true');
                                $('#custShipLast').attr('readonly', 'true');
                                $('#custShipBusiness').attr('readonly', 'true');
                                $('#custShipAdd1').attr('readonly', 'true');
                                $('#custShipAdd2').attr('readonly', 'true');
                                $('#custShipCity').attr('readonly', 'true');
                                $('#custShipState').attr('readonly', 'true');
                                $('#custShipZip').attr('readonly', 'true');
                                $('#custShipZip4').attr('readonly', 'true');
                                $('#custShipCountry').attr('readonly', 'true');
                                $('#custShipPhone').attr('readonly', 'true');
                                $('#custShipEmail').attr('readonly', 'true');
                                $('#custStudID').focus();

                            } else {
                                $("input[name='customerRegister.shipAddress.Residential']").removeAttr('disabled');

                                $('#custShipFirst').removeAttr('readonly');
                                $('#custShipMiddle').removeAttr('readonly');
                                $('#custShipLast').removeAttr('readonly');
                                $('#custShipBusiness').removeAttr('readonly');
                                $('#custShipAdd1').removeAttr('readonly');
                                $('#custShipAdd2').removeAttr('readonly');
                                $('#custShipCity').removeAttr('readonly');
                                $('#custShipState').removeAttr('readonly');
                                $('#custShipZip').removeAttr('readonly');
                                $('#custShipZip4').removeAttr('readonly');
                                $('#custShipCountry').removeAttr('readonly');
                                $('#custShipPhone').removeAttr('readonly');
                                $('#custShipEmail').removeAttr('readonly');

                            }
                        });
                    });
           </script>
          </div>
         </div>
         <script type="text/javascript">
          $(document).ready(function () {
        var sameShip = 'False';
            if (sameShip == 'True') {
                var residential = $('[name="customerRegister.billAddress.Residential"]:checked').val();
                var newResidential;
                if (residential.toLowerCase() == "true") {
                    newResidential = false;
                } else {
                    newResidential = true
                }
                $("input[name='customerRegister.shipAddress.Residential'][value=" + newResidential + "]").attr('disabled', 'true');
                $('#custShipFirst').attr('readonly', 'true');
                $('#custShipMiddle').attr('readonly', 'true');
                $('#custShipLast').attr('readonly', 'true');
                $('#custShipBusiness').attr('readonly', 'true');
                $('#custShipAdd1').attr('readonly', 'true');
                $('#custShipAdd2').attr('readonly', 'true');
                $('#custShipCity').attr('readonly', 'true');
                $('#custShipState').attr('readonly', 'true');
                $('#custShipZip').attr('readonly', 'true');
                $('#custShipZip4').attr('readonly', 'true');
                $('#custShipCountry').attr('readonly', 'true');
                $('#custShipPhone').attr('readonly', 'true');
                $('#custShipEmail').attr('readonly', 'true');
            }
        var state = '';
            var stateCheck = 'False';
            stateCheck = stateCheck.toLowerCase();
        if (state == "0" || state == "") {
            if (stateCheck == "true") {
                //$('#custBillState').prepend("<option value='' selected='selected'>Please Select</option>");
            } else {
                //$('#custBillState').prepend("<option value='' disabled selected='selected'>Please Select</option>");
            }
        } else {

            if (stateCheck == "true") {
                $('#custBillState').val(state);
                $('#custBillCity').val('');
                //$('#custBillState').prepend("<option value=''>Please Select</option>");
            } else {
                $('#custBillState').val(state);
                $('#custBillCity').val('');
                //$('#custBillState').prepend("<option value='' disabled >Please Select</option>");
                $('#custShipState').val(state);
                $('#custShipCity').val('');
                //$('#custShipState').prepend("<option value='' disabled >Please Select</option>");

            }
        }
        });
         </script>
