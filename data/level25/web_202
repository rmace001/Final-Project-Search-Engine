  <script>
   $(document).ready(function() {
//make navigation operate by hover also instead of click only
$("#myNavbar .dropdown").hover(function () {
        if ($('.navbar-toggle').css('display') === 'none') {
            $(this).addClass('open');
        }
    },
        function () {
            if ($('.navbar-toggle').css('display') === 'none') {
                $(this).removeClass('open');
            }
        });

$('.divCaptcha').wrap('<div class="col-lg-6 col-lg-offset-3 col-md-6 col-md-offset-3 col-sm-6 col-sm-offset-3"></div>');

//only execute on RWD pages...
   if (loc.indexOf('aspx') === -1) {
   $('.search-bar').append($('#H_Search'));
    }




//remove 4 meta tags per Phil's request
$('meta[content="text/html;charset=utf-8"]').remove();
$('meta[content="no-cache, no-store, must-revalidate"]').remove();
$('meta[http-equiv="Pragma"]').remove();
$('meta[http-equiv="Expires"]').remove();

//change home pg title - too long for inSite field
if (loc.indexOf('colostate.edu/home') >=0 ) {
  $('head>title').text('The Official Colorado State University Bookstore for CSU Rams Gear');
  //keyword attr and title are dynamically generated for other pages than home
  $('meta[name="KeyWord"]').attr('name', 'keyword').attr('content', 'CSU Rams Gear');
  $('meta[name="Description"]').attr('name', 'description').attr('content', 'The Official Colorado State University Bookstore. Every purchase of official CSU Rams Gear helps support student programs and services.');
}

});
  </script>
