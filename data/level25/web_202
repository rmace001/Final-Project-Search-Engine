             <script>
              $(document).ready(function () {
                    var countryValue = 'United States';
                    var countryName = 'custBill';
                    if (countryValue == "") {
                        $('#' + countryName + 'Country').prepend('<option value="" disabled>Please Select Country</option>');
                        $('#' + countryName + 'Country').val('United States');
                    }
                });
