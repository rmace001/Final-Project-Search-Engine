    var twitterFeed = $('#twitterfeed');
    var instaFeed = $('.instagram');

    function socialHeights() {instaFeed.css({'height':(twitterFeed.height()+'px')});
      instaFeed.height(twitterFeed.height());
      var highestCol = Math.max(instaFeed.height(),twitterFeed.height());
      instaFeed.height(highestCol);
    }
    socialHeights();

  }, 1000);
