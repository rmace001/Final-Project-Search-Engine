  <script>
   function startAnimations() {
    $("#news-rotate li").hide();
    function loop() {
      var $li = $("#news-rotate li:first-child").fadeIn(4000, function () {
        $li.fadeOut(8000, function () {
          $li.appendTo('#news-rotate');;
          loop()
        })
      });
    }
    loop()
  }
  startAnimations();

  var feed = new Instafeed({
    get: 'user',
    userId: 1281398317,
    accessToken: '1281398317.1677ed0.c5516c1696f240e888fc7ef10d5166e3',
    limit: 2,
    resolution: 'standard_resolution',
    template: '<div class="col-xs-6 col-sm-3 instagram"><a href="{{link}}"><img src="{{image}}" /></a></div>'
  });
  feed.run();


