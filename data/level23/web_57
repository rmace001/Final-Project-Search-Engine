  <script type="text/javascript">
   $(document).ready(function() {
	
        $("#recentSeminarTabBtn").click(function() {
            $("#homeSeminarUpcomingList").hide();
            $("#upcomingSeminarTabBtn").removeClass("active");
            $("#homeSeminarStage").show();
            $("#recentSeminarTabBtn").addClass("active");
            
        });
        
        $("#upcomingSeminarTabBtn").click(function() {
            $("#homeSeminarStage").hide();
            $("#recentSeminarTabBtn").removeClass("active");
            $("#homeSeminarUpcomingList").show();
            $("#upcomingSeminarTabBtn").addClass("active");
        });
        
        
       // Disable dropdwon-menu on small screens //
       function checkWidth() {
        var windowSize = $(window).width();
            if (windowSize > 480) {
                $('.no-toggle').removeClass('no-toggle').addClass('dropdown-toggle');
                $('.dropdown-toggle').attr('data-toggle','dropdown');
            } else if (windowSize <= 479) {
                $('.dropdown-toggle').removeClass('dropdown-toggle').addClass('no-toggle');
                $('.no-toggle').removeAttr('data-toggle');
            } 
        }
        // Execute on load
        checkWidth();
        // Bind event listener
        $(window).resize(checkWidth);
        
        
    });
  </script>
