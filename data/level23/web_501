<script>
 $(function () {
        var list = $(".tplNews .middle-snapshot");

        var items = [];
        var offsetTop = 0;

        for (var i = 0; i < list.length; i++) {
            var item = $(list[i]);

            if (offsetTop > 0 && offsetTop != item.offset().top && items.length > 0) {
                setDisplayClasses(items);
                items = [];

                offsetTop = item.offset().top;
                items.push(item);
            }
            else {
                offsetTop = item.offset().top;
                items.push(item);
            }
        }
        if (items.length > 0)
            setDisplayClasses(items);



        var defaultNewsHeightNoPhoto = 99999;
        var defaultNewsHeightWithPhoto=99999;
        var deviation = 99999  ;
        var news = $(".tplNews");

        for (var i = 0; i < news.length; i++) {
            var newsBox = $(news[i]).find(".rightInfo");
            var descBox =  $(news[i]).find(".description");
            var hasThumb = $(news[i]).find(".middle-snapshot.hidden");

            var hasFile = true;
            if (hasThumb.length >0 )
                hasFile = false;


            var newsBoxHeight = $(newsBox).height();
            var rowHeight = parseInt(window.getComputedStyle(descBox[0]).lineHeight.replace("px", ""));


            
            if (hasFile == true && defaultNewsHeightWithPhoto > newsBoxHeight)
                defaultNewsHeightWithPhoto = newsBoxHeight;

            if (hasFile == false && defaultNewsHeightNoPhoto > newsBoxHeight)
                defaultNewsHeightNoPhoto = newsBoxHeight;

            if (deviation > rowHeight)
                deviation = rowHeight;
        }


        $.each(news, function (index, elem) {

            var boxHeight, descHeight, descWidth;
            var newsBox = $(elem).find(".rightInfo");
            var descBox = $(elem).find(".description");
            var hasPhoto = true;
            if ($(elem).find(".middle-snapshot.hidden").length > 0)
                hasPhoto = false;

            
            var defaultNewsHeight;
            if (hasPhoto)
                defaultNewsHeight = defaultNewsHeightWithPhoto;
            else
                defaultNewsHeight = defaultNewsHeightNoPhoto;

            if (newsBox.length == 1 && descBox.length == 1) {
                boxHeight = $(newsBox).height();
                descHeight = $(descBox).height();
                descWidth =  $(descBox).width();
                // alert('boxHeight:' + boxHeight + ' - boxDescription:' + descHeight);

                if (boxHeight > defaultNewsHeight && boxHeight - defaultNewsHeight > deviation) {
                    var diff = boxHeight -defaultNewsHeight;
                    var rowHeight = parseInt(window.getComputedStyle(descBox[0]).lineHeight.replace("px", ""));
                    var removeNrRows = Math.ceil((boxHeight - defaultNewsHeight) / rowHeight);

                    var maxCharsOnRow = Math.ceil(descWidth / "w".visualLength()); //get nr of chars including spaces on a row

                    if (removeNrRows > 0 && maxCharsOnRow > 0) {
                        var text = descBox.html();
                        var trimText = getTextTrimByNrChars(text, removeNrRows * maxCharsOnRow);
                        descBox.html(trimText);
                    }
                }
            }
        });
    });

   

    String.prototype.visualLength = function () {
        var footer = document.getElementById("footer");
        var ruler = document.getElementById("ruler");
        if (ruler == null) {
            ruler = document.createElement("span");
            ruler.style = "visibility: hidden; white-space: nowrap;";
            footer.appendChild(ruler);
        }


        ruler.innerHTML = this;
        var retvalue = ruler.offsetWidth;
        footer.removeChild(ruler);

        return retvalue;
        
    }


    function hasThumbnail(items) {
        var hasThumbnail = false;
        for (var i = 0; i < items.length; i++) {
            if (items[i].attr("data-thumb") == "true")
                return true;
        }
        return false;
    }

    function setDisplayClasses(items) {
        var hasThumb = hasThumbnail(items);
        for (var i = 0; i < items.length; i++) {
            if (hasThumb && items[i].attr("data-thumb") == "false")
                items[i].addClass("whiteSpace");
            else if (hasThumb == false)
                items[i].addClass("hidden");
        }

    }

    function getTextTrimByNrChars(text, nrChars) {
        var length = text.length;
        if (length < nrChars)
            return text;

        //trim the string to the maximum length
        var maxLength = length - nrChars;
        if (text.endsWith("..."))
            text = text.replace("...", "");
        var trimmedString = text.substr(0, maxLength-1);

        //re-trim if we are in the middle of a word
        trimmedString = trimmedString.substr(0, Math.min(trimmedString.length, trimmedString.lastIndexOf(" ")))

        return trimmedString =trimmedString + " ...";
    }
</script>
