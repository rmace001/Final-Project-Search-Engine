   <script type="text/javascript">
    // append
	$(function () {				
		// logout SAML Url
		var logout = $('a.logout');
		if (logout) {
			$(logout).each(function (index) {
				$(this).attr("href", 'javascript:void(0)');
				$(this).on("click",
					function () {
						window.location.href = "/SamlHooks/PerformNaeSamlLogout.aspx";
					});
			});
		}
	});
   </script>
