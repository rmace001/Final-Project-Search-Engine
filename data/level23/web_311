      <div class="universal-region--main">
       <script type="text/javascript">
        var spudId = $Trumba.addSpud({
                                  webName: "tufts",
                                  spudType : "main" ,
                                  });
       </script>
       <noscript>
        <h2>
         Your browser must support JavaScript to view this content.
        </h2>
        Please enable JavaScript in your browser settings then try again.
        <br/>
        A static version of event content is also available at
        <a href="/static/">
         http://events.tufts.edu/static/
        </a>
        .
       </noscript>
       <script type="text/javascript">
        var mainSpud = $Trumba.Spuds.controller.getSpudById(spudId);
              mainSpud.container.addEventListener('onFetched', function(event) {
              if ( mainSpud.container.getIFrameDocument( mainSpud.container.iframe ) ) {
              	mainSpud.container.getIFrameDocument( mainSpud.container.iframe ).getElementById('ctl06_creditlink').style.color='#CCCCCC';
                var eventSummary = mainSpud.getEventSummary(); // get data of fetched trumba event
                	if (eventSummary) {  // only present on the event details view
                        	$("#linktext").show();
                                var labels = $(mainSpud.container.getIFrameDocument( mainSpud.container.iframe )).contents().find(".LabelAnchor");
                        	$.each( labels, function(i, item){
                                	if ( item.innerHTML == "Forward to Friends") {
                                		item.innerHTML = "Email to Friends";
                                        } else if ( item.innerHTML == "Add to My Calendar") {
                                        	item.innerHTML = "Add to Calendar";
                                        }
                                       });
                         } else {
                         	$("#linktext").hide();  
                         }
                                                                                  
               }  
               });
       </script>
       <div id="linktext" style="text-align:left;border-top: #CCCCCC 1px solid; padding-top:16px;display:none;">
        <!-- IMG DOES NOT EXIST <img src="/images/savelink.jpg" width="60" height="60" style="padding-right: 20px;" align="left" />-->
        <em>
         Hint
        </em>
        : To copy this event link to your clipboard and paste elsewhere, right-click the "
        <strong>
         Link Directly to this Event
        </strong>
        " link (CTRL-click on a Mac) and select the option to copy link address.
       </div>
       <div>
