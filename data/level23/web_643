  <script>
   $(document).ready(function(){
                formValidation.init();

                                
                // add ability to add more rows
                $(".addBtn").click(function(){
                    var dup = $(this).prev(".grid").clone();
                    var num = 1;
                    if ($(this).attr("data-num") !== undefined)
                        num = $(this).attr("data-num");
                    console.log("num: ", num);
                    
                    for(var i = 0; i < num; i++){
                        console.log("i: ", i);
                        // update id's and for's
                        dup.find("fieldset").each(function(){
                            var x = randNum();
                            var field = $(this).find("[id]").attr("id", x).prop("checked", false).val("").removeClass("invalid");
                            $(this).find("[for]").attr("for", x);
                            
                            console.log("name: ", name);
                            
                            // correct default value for select
                            if (field.is("select"))
                                field.val(formValidation.defaults.defaultSelectValue);
                            
                            var checkbox = $(this).find("input[type='checkbox']").prop("checked", false);
                            checkbox.siblings("[type='hidden'][name='"+checkbox.attr("name")+"']").prop("disabled", false);    // make sure hidden field isn't diabled for checkbox
                        });
                        $(this).before(dup);
                        dup = dup.clone();
                    }
                });

                function randNum(){
                    return Math.floor(Math.random() * 26) + Date.now();
                }
                
                // VALIDATE ROWS ADDED BY .ADDBTN
                function allFieldsHaveData(fields){
                    var r = true;
                    var f = null;
                    fields.each(function(){
                        f = $(this);
                        if ($(this).is('input:text')){
                            if ($(this).val() == ""){
                                r = false;
                                return false;
                            }
                        }
                        else if ($(this).is('select')){
                            if ($(this).val() == formValidation.defaults.defaultSelectValue){
                                r = false;
                                return false;
                            }
                        }
                        /*else if ($(this).is(':checkbox')){
                            if ($(this).is(":checked")){
                                r = false;
                                return false;
                            }
                        }
                        else if ($(this).is(':radio')){
                            if ($(this).is(":checked")){
                                r = false;
                                return false;
                            }
                        }*/
                    });
                    return {res: r, field: f};
                }

                function fieldsHaveData(fields){
                    var r = false;
                    fields.each(function(){
                        if ($(this).is('input:text')){
                            if ($(this).val() != ""){
                                r = true;
                                return false;
                            }
                        }
                        else if ($(this).is('select')){
                            if ($(this).val() != formValidation.defaults.defaultSelectValue){
                                r = true;
                                return false;
                            }
                        }
                       /* else if ($(this).is(':checkbox')){
                            if ($(this).is(":checked")){
                                r = false;
                                return false;
                            }
                        }
                        else if ($(this).is(':radio')){
                            if ($(this).is(":checked")){
                                r = false;
                                return false;
                            }
                        }*/
                    });
                    return r;
                }

                // form validation for rows of info
                formValidation.defaults.submitCallback = function(){
                    var r = true;
                    $(".addBtn").prevUntil("h3").each(function(){
                        console.log("row");
                        var inputs = $(this).find("select, input");
                        var rowHasData = fieldsHaveData(inputs);

                        if (rowHasData){
                            var rowGood = allFieldsHaveData(inputs);
                            if (rowGood.res == false){
                                formValidation.invalidScrollTo(rowGood.field);
                                r = false;
                                return false;   // break loop
                            }
                        }
                    });
                    return r;
                }
                
            });
  </script>
