  </script>
  <script>
   var video = '<video id="bgvid" preload="true" controls="false" autoplay loop muted poster="https://lib.wvu.edu/images/jerrywest/jerrywest.jpg">';
		video += '<source src="/videos/jerrywest.webm" type="video/webm">';
		video += '<source src="/videos/jerrywest.ogv" type="video/ogv">';
		video += '<source src="/videos/jerrywest.mp4" type="video/mp4"> ';
		video += '</video>';

	$(window).load(function() {
	  $('#main-container').append(video);
	});

	$(window).bind("load resize", function() {

		var wvu = $('.wvu-header:visible').outerHeight(true);
	    var wvrhc = $('.wvrhc-header:visible').outerHeight(true);
	    var con = $('#content').outerHeight(true);
		var windowih = window.innerHeight;
		var pieces2 = wvu + wvrhc;
		var feature2 = windowih - pieces2;
		var topm = feature2 - con;
		var footer = $('#footer').outerHeight(true);
		var adj = wvu / 2;
		var footeradj = footer - 20;


		// $('#main-container').css('max-height', 800);
		// $('#main-container').css('min-height', con);
		$('#main-container > div:nth-child(2)').css('height', feature2);
		$('#main-container > div:nth-child(2)').css('max-height', 800);
		$('#main-container > div:nth-child(2)').css('min-height', con);

		if( $(window).width() < 1024) {
	  		$('#bgvid').css('top', wvu);
	  		$('#main-container').css('height', feature2);
	  		$('#main-container').css('min-height', con);
		}
	  	else {
	  		$('#bgvid').css('top', '0px');
	  		$('#main-container').css('height', feature2 - 50);
		}
	});
  </script>
