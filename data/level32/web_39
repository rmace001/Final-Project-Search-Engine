<script type="text/javascript">
 $(function(){
  if (!String.prototype.startsWith) {
    String.prototype.startsWith = function(searchString, position){
      return this.substr(position || 0, searchString.length) === searchString;
    };
  }
  $.fn.select2.defaults.set("theme", "bootstrap");
  $.fn.select2.defaults.set("containerCssClass", ":all:");
  $.fn.select2.defaults.set("width", "");
  $.fn.select2.amd.require('select2/results').prototype.sort = function (data) {
    var sorter = this.options.get("sorter");
    if (sorter !== $.fn.select2.defaults.defaults.sorter) {
      return sorter(data)
    }
    var sel_class = ".select2-search__field"
    var $searchField = this.data.container.$dropdown.find(sel_class);
    if ($searchField.length === 0) {
      $searchField = this.data.container.$container.find(sel_class)
    }
    var query = $searchField.val() || '';
    query = query.toLowerCase()
    data.sort(function(a, b) {
      var astarts = a.text.toLowerCase().startsWith(query)
      var bstarts = b.text.toLowerCase().startsWith(query)
      if (astarts && bstarts) return 0;
      if (astarts && !bstarts) return -1;
      if (!astarts && bstarts) return 1;
      return 0;
    })
    return data;
};
  $(".tip").tooltip()
  $(".pop").popover()

  $(document).ajaxSend(function(event, xhr, settings) {
    safeMethod = /^(GET|HEAD|OPTIONS|TRACE)$/.test(settings.type)
    if (!safeMethod && !settings.crossDomain) {
      xhr.setRequestHeader("X-CSRFToken", $.cookie('csrftoken'))
    }
  })

  $('.dropdown-menu form,input[type=text],textarea,select').bind('click', function(e) { e.stopPropagation() })

  $('.dropdown-toggle').on('click', function(e, data) {
    var menu = $(e.currentTarget).parent().find('.dropdown-menu')
    _.defer(function() {
      if (menu.is(':visible')) {
        menu.find('input[type=text],textarea,select').first().focus().select()
      }
    })
  })
})
</script>
