     <div class="wdn-band">
      <div class="node node-page" id="node-21">
       <div class="content">
        <div class="field field-name-body field-type-text-with-summary field-label-hidden clearfix">
         <div class="field-items">
          <div class="field-item even">
           <head>
            <script>
             // Data Grabber 
        /**
         * Maps the spreadsheet cells into a dataframe, consisting of an array of rows (i.e. a 2d array)
         * In many cases we have empty rows or incomplete rows, so you can skip those by including 
         * the realrowlength parameter - it will skip any rows that don't have this length.
         * Alternatively, you can just choose to skip the first 'n' rows by setting the skip parameter.
         */
        var dataframe;
        var htmlMonth;
        var eventArray;

        function mapEntries(json, realrowlength, skip) {
            if (!skip) skip = 0;
            dataframe = new Array();
            var row = new Array();
            for (var i = 0; i < json.feed.entry.length; i++) {
                var entry = json.feed.entry[i];
                if (entry.gs$cell.col == '1') {
                    if (row != null) {
                        if ((!realrowlength || row.length === realrowlength) && (skip === 0)) {
                            dataframe.push(row);
                        } else {
                            if (skip > 0) skip--;
                        }
                    }
                    var row = new Array();
                }
                row.push(entry.content.$t);
            }
            dataframe.push(row);
            return dataframe;

        };

        function sheetLoaded(data) {
            // do something with spreadsheet data here
            dataframe = mapEntries(data, null, 1);
            buildUseableData(dataframe);
            /*
            for (var i = 0; i < dataframe.length; i++) {
                
                console.log(eventArray[i][1]);
            }
            */
            buildHTML(eventArray);


        };

        function buildUseableData(rawdata) {
            eventArray = rawdata;
        };

        /*
        function dateParse(rawDate, lineNumber) {
            var monthNumber;
            var dayNumber;
            var yearNumber;
            var monthString;
            var rawDateString = rawDate.toString();
            if (rawDateString[1] == "-") {
