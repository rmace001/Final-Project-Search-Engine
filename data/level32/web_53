        function buildHTML(data) {
            var splitHolder;
            var weekHolder;
            var monthHolder;
            var weekClass;
            var completeHTML;
            var i;
            var x;
            var urlMaker;


            /*
            <div class="wdn-band brwEvent">
                <div class="wdn-inner-wrapper">
                    <section class="wdn-grid-set">
                        <div class="wdn-pull-left bp1-wdn-col-one-sixth">
                            <div class="date-badge">
                                <!-- Date Here -->
                            </div>
                        </div>
                        <div class="snippet wdn-pull-left bp1-wdn-col-five-sixths">
                            <div class="container">
                                <h1 class="r-bottom"><!----NAME----><span class="label"><!----WHEN----></span></h1>
                                <h4 class="r-both Tdarkred"><!----WHERE----></h4>
                                <h6 class="r-both"><!----WHOM----></h6>
                                <p><!----WHAT----></p>
                            </div>
                        </div>
                        


                    </section>
                </div>
            </div>
            */
            for (i = 0; i < data.length; i++) {
                splitHolder = data[i][2].split("/");
                weekHolder = splitHolder[1];
                monthHolder = splitHolder[0];

                /*###############################################
                BRW Week Applier
                ###############################################*/
                /*
                BRW August 17 - August 19
                Week 1 August 20 - August 26
                Week 2 August 27- September 2
                Week 3 September 3 - September 9
                Week 4 September 10 - September 16
                Week 5 September 17 - September 23
                Week 6 September 24 - September 30
                */
                if (monthHolder == 8) {
                    if (weekHolder >= 17 && weekHolder <= 19) {
                        weekClass = "BRWweekend";
                    } else if (weekHolder <= 16) {
                        weekClass = "BRWweekend";
                    } else if (weekHolder >= 20 && weekHolder <= 26) {
                        weekClass = "week-1";
                    } else if (weekHolder >= 27) {
                        weekClass = "week-2";
                    } else {
                        console.log("How? " + monthHolder + " " + weekHolder);
                    }
                } else if (monthHolder == 9) {
                    if (weekHolder <= 2) {
                        weekClass = "week-2";
                    } else if (weekHolder >= 3 && weekHolder <= 9) {
                        weekClass = "week-3";
                    } else if (weekHolder >= 10 && weekHolder <= 16) {
                        weekClass = "week-4";
                    } else if (weekHolder >= 17 && weekHolder <= 23) {
                        weekClass = "week-5";
                    } else if (weekHolder >= 24) {
                        weekClass = "week-6";
                    } else {
                        console.log("How? " + monthHolder + " " + weekHolder);
                    }
                } else if (monthHolder >= 10) {
                    weekClass = "week-6";
                } else {
                    console.log("How? " + monthHolder + " " + weekHolder);
                };
                if (data[i][5])
                /*###############################################
                BRW Decription Crawler
                ###############################################*/
                if (data[i][5].includes("http")) {
                    urlMaker = data[i][5].split(" ");
                    for (x = 0; x < urlMaker.length; x++) {
                        if (urlMaker[x].includes("http")) {
                            if (urlMaker[x].charAt(urlMaker[x].length - 1) == ".") {
                                urlMaker[x]=urlMaker[x].slice(0,-1);
                            }
                            urlMaker[x] = "<a href='" + urlMaker[x] + "' target='_blank'>" + urlMaker[x] + "</a>";
                        };
                    };
                    data[i][5] = urlMaker.join(" ");
                };
                /*
                0 - Group Name
                1 - Event Title
                2 - Event Date
                3 - Location
                4 - Event Time
                5 - End Time Check
                6 - End Time
                7 - Description
                */
                
                if (data[i][5] == "Yes" || data[i][5] == "yes") {
                    data[i][4] = data[i][4] + " - " + data[i][6];
                } else {
                    data[i][4] = data[i][4];
                    data[i][7] = data[i][6];
                }
console.log(data[i][1]+" "+data[i][4]+" "+data[i][5]+" "+data[i][6]+" "+data[i][7]);
                completeHTML =
                    '<div class="wdn-band BRWevent ' + weekClass + '" id="' + 'event' + (i + 1) + '"><div class="wdn-inner-wrapper"><section class="wdn-grid-set"><div class="wdn-pull-left bp1-wdn-col-one-sixth"><div class="date-badge">' + data[i][2] + '<!-- Date Here --></div></div><div class="snippet wdn-pull-left bp1-wdn-col-five-sixths"><div class="container"><h1 class="r-bottom">' + data[i][1] + '<!----NAME----><span class="label">' + data[i][4] + '<!----WHEN----></span></h1><h4 class="r-both Tdarkred">' + data[i][3] + '<!----WHERE----></h4><h6 class="r-both">' + data[i][0] + '<!----WHOM----></h6><p>' + data[i][7] + '<!----WHAT----></p></div></div></section></div></div>'
                $("#event" + i).after(completeHTML);
            }
            $('.BRWevent').hide();
            $('.BRWweekend').show;
            $('.BRWweekend').fadeTo(100, 1);

        };
            </script>
           </head>
           <body>
            <div class="wdn-band">
             <div class="flexslider no-dots maxheight" id="wdn_Carousel">
              <ul class="slides">
               <span class="slideContent">
