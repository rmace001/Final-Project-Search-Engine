            <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js">
            </script>
            <script src="https://spreadsheets.google.com/feeds/cells/1SFxN7ci-LjDRXVBF3VOXFS6ix1N5PYNw32KIV7aT8GI/od6/public/values?alt=json-in-script&amp;callback=sheetLoaded">
            </script>
            <script>
             $(document).ready(function() {

            var currentDate = new Date();
            var m_names = new Array("January", "February", "March",
                "April", "May", "June", "July", "August", "September",
                "October", "November", "December");
            var d_names = new Array("Sunday", "Monday", "Tuesday",
                "Wednesday", "Thursday", "Friday", "Saturday");


            /*###############################################
              Dynamically change margin/padding in HTML
            ###############################################*/
            $("[class*='setcss']").each(setCSS);



            /*###############################################
              BRW Current Week Tab
            ###############################################*/

            var startTime = new Date(2018, 07, 19); // Starting Day of BRW Zero Indexing: 00=January
            console.log("The start time is " + startTime + " and the Current Date is " + currentDate);
            console.log(currentDate - startTime);
            if ((currentDate - startTime) > 0) {
                var weekNumber = Math.floor((currentDate - startTime) / 604800000) + 1; // 604800000 milliseconds in weeks
                $(".wdn_tabs #week-" + weekNumber).parent().addClass('selected');
            }


            /*###############################################
              Current Month Tab
            ###############################################*/
            var currentmonth = m_names[currentDate.getMonth()];
            $(".event").hide();
            $(".wdn_tabs ." + currentmonth).parent().addClass('selected');
            $(".si-events-band ." + currentmonth).show();

            $('.wdn_tabs li a').click(function() {
                $('.event').fadeTo(100, 0);
                $('.event').hide();
                if ($(this).attr('class') === "All") {
                    $('.event').fadeTo(100, 1);
                } else {
                    currentmonth = $(this).attr('class');
                    $('.si-events-band .' + currentmonth).fadeTo(100, 1);
                }
            });

            /*###############################################
              BRW Current Week Tab
            ###############################################*/
            var currentweek = "BRWweekend";
            $(".wdn_tabs #" + currentweek).parent().addClass('selected');
            $('.wdn_tabs li a').click(function() {
                $('.BRWevent').fadeTo(100, 0);
                $('.BRWevent').hide();
                if ($(this).attr('id') === "All") {
                    $('.BRWevent').fadeTo(100, 1);
                } else {
                    currentweek = $(this).attr('id');
                    console.log(currentweek);
                    $('.' + currentweek).fadeTo(100, 1);
                }
            });

            /*##########################################################
                Date Tag (Liven up the provided date (i.e. X/XX/XX))
            ##########################################################*/
            $('.date-badge').each(function() {
                var tagArray = $(this).contents().text().trim().split("/");
                $(this).contents().remove();
                var tagDate = new Date(tagArray[2], tagArray[0] - 1, tagArray[1]);

                if (isNaN(tagDate)) {
                    $(this).append("<span id='top'>" + d_names[currentDate.getDay()].substring(0, 3) + "</span>");
                    $(this).append("<span id='middle'>" + currentDate.getDate() + "</span>");
                    if (m_names[currentDate.getMonth()] === "June" || m_names[currentDate.getMonth()] === "July") {
                        $(this).append("<span id='bottom'>" + m_names[currentDate.getMonth()] + "</span>");
                    } else {
                        $(this).append("<span id='bottom'>" + m_names[currentDate.getMonth()].substring(0, 3) + "</span>");
                    };
                } else {
                    $(this).append("<span id='top'>" + d_names[tagDate.getDay()].substring(0, 3) + "</span>");
                    $(this).append("<span id='middle'>" + tagDate.getDate() + "</span>");
                    if (m_names[tagDate.getMonth()] === "June" || m_names[tagDate.getMonth()] === "July") {
                        $(this).append("<span id='bottom'>" + m_names[tagDate.getMonth()] + "</span>");
                    } else {
                        $(this).append("<span id='bottom'>" + m_names[tagDate.getMonth()].substring(0, 3) + "</span>");
                    };
                }
            });
            /*
            if($(window).width() <= 480){
              $(".date-badge").find("#top").removeAttr('id').attr('id' : 'middle');
              $(".date-badge").find("#middle").removeAttr('id').attr('id' : 'top');
              $(".date-badge").parent().toggleClass("bp1-wdn-col-one-sixth bp1-wdn-col-full margin-auto")
            };
            */

            /*###############################################
                Alternating Band Colors
            ###############################################*/
            $(".BGalternate ").each(function() {
                var count = 0;
                $(this).children().each(function() {
                    if (count % 2 === 1 && count != 0) {
                        $(this).addClass("BGdarkcream");
                    };
                    count += 1;
                });
            });


            /*###############################################
            	  Information Shortener
            ###############################################*/
            $(".hide").hide();

            $(".read-more").click(function() {
                $(this).find("[class*='wdn-icon-']").toggleClass("wdn-icon-plus wdn-icon-minus");

                //if($(this))
                $(this).next($(".hide")).slideToggle("slow");
            });


            /*##########################################################
                Carousel (Add the right HTML to the Carousel)
            ##########################################################*/
            $("ul.slides").each(function() {
                var slideContent = $(this).children($(".slideContent")).html();

                $(this).children("img.wdn-stretch").each(function() {
                    if (!($(this).parent().is("figure"))) {
                        $(this).wrap("<figure></figure>");
                    };
                    $(this).parent().wrap("<li></li>");
                });
                $(this).children("li").each(function() {
                    if (!($.contains(".gradient", $(this)))) {
                        $(this).append("<div class='gradient'></div>");
                    };
                    if (!($.contains(".smart-width-text", $(this)))) {
                        $(this).append("<div class='smart-width-text'></div>");
                        $(this).children(".smart-width-text").append(slideContent);
                    };
                });
                $(this).find($(".slideContent")).remove();

            });

            /*###############################################
              Resources page
            ###############################################*/
            $(".demo").click(function(e) {
                var target = $(e.target);
                if (target.parent().is(".demo") && (e.target.nodeName).toLowerCase() != "span") {
                    $(".demo span#target").text((e.target.nodeName).toLowerCase());
                    $(".demo").find("[class*='setcss']").removeClass("[class*='setcss']");
                    target.addClass($(".demo input").val());
                    setCSS();
                };
            });
        });

        var setCSS = function() {
            var classList = $(this).attr('class').split(" ");
            /*.match(/(?:setspacing)(.|\w)[^\s]+[^\s\-\s]/)*/
            for (var i = classList.length - 1; i >= 0; i--) {
                if (classList[i].indexOf("setcss") >= 0) {
                    var splitName = classList[i].split("-");

                    var cssName = null,
                        cssValue = null;
                    var cssObject = {};

                    for (var j = 1; j < splitName.length - 1; j++) { // start from 2nd item

                        if (splitName.length <= 3) {
                            cssName = splitName[1];
                        } else {
                            if (j == 1) continue; // if it is the 2nd skip..
                            part = splitName[j].substring(0, 1).toUpperCase() + splitName[j].substring(1);
                            cssName = splitName[1] + part;
                        };

                        cssValue = splitName[splitName.length - 1]; // remove !imprtant since it is not supported +" !important";
                        cssObject[cssName] = cssValue;
                    };
                    console.log(JSON.stringify(cssObject));
                    $(this).css(cssObject);
                };
            };
        };
            </script>
           </body>
          </div>
         </div>
        </div>
       </div>
      </div>
