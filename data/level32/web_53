            }
            if (rawDateString[4] == "-") {
                rawDateString = rawDateString.slice(0, 3) + "0" + rawDateString.slice(3, rawDateString.length);
            }
            monthNumber = rawDateString[0] + rawDateString[1];
            dayNumber = rawDateString[3] + rawDateString[4];
            yearNumber = rawDateString[6] + rawDateString[7];

            console.log(monthNumber);
            console.log(dayNumber);
            console.log(yearNumber);
            if (monthNumber == "01") {
                monthString = "January";
                console.log(monthString);
            } else if (monthNumber == "02") {
                monthString = "Febuary";
                console.log(monthString);
            } else if (monthNumber == "03") {
