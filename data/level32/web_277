         <script>
          $(function() {
    $( "#tabs" ).tabs();
  });

  $(document).ready( function() {
    $('#main-nav').on('hide.bs.collapse', function(){
      $('#main-nav .panel-heading[role=tab]').addClass('active');
      $('#main-nav .panel-heading').removeClass('active');
    });
    $('#main-nav').on('shown.bs.collapse', function(){
      $('#main-nav .panel-heading[role=tab]').addClass('active');
      $('#main-nav .panel-heading:has(".collapsed")').removeClass('active');
    });
  });
         </script>
