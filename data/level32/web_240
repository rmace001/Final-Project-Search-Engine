       <script type="text/javascript">
        function SearchKey(e) {
        if (e.keyCode == 13) {
            SearchRedirect();
            return false;
        }
    }

    function SearchRedirect() {
        var txtValue = document.getElementById("ctl00_uxHeader_uxSearch_txtKeyword");
        window.location.replace("/Search/?q=" + txtValue.value);
    }
       </script>
       <img class="search-opener" src="/images/search-button.png"/>
