!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+"://platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");

// There really is no defined load callback for Twitter once it's loaded. So
// this is a close second. If you do come across a load callback, fix this shit.
if ($('.twitter-timeline').length) {
  var interval = window.setInterval(function () {
    console.log('Interval-ing...');

    if ($('.twitter-timeline').hasClass('twitter-timeline-rendered') && $('.twitter-timeline').width() > 10) {
      clearInterval(interval);

      console.log('Clearing interval.');

      // Wait a tenth of a second for rendering to finish.
      setTimeout(function () {
        console.log('Overriding CSS.');

        $('#twitter-widget-0').contents().find('body > div').css({
          'border':             '1px solid #cbc29d',
          'background-color': '#D2E0DC'
        });
      }, 100);
    } // if ($('.twitter-timeline').hasClass('twitter-timeline-rendered') && $('.twitter-timeline').width() > 10)
  }, 100);
} // if ($('.twitter-timeline').length)
  </script>
