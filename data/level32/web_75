<script>
 $(document).ready(function() {
                    prettyPrint();
                    
                    $("#no-stretch-hero").click(function() {
                        updateSlideshow($(this), $("#revert-stretch-hero"), true, "#hero", "no-stretch");
                    });
                    $("#revert-stretch-hero").click(function() {
                        updateSlideshow($(this), $("#no-stretch-hero"), false, "#hero", "no-stretch");
                    });
                    $("#no-squeeze-hero").click(function() {
                        updateSlideshow($(this), $("#revert-squeeze-hero"), true, "#hero", "no-squeeze");
                    });
                    $("#revert-squeeze-hero").click(function() {
                        updateSlideshow($(this), $("#no-squeeze-hero"), false, "#hero", "no-squeeze");
                    });
                    $("#no-captions-hero").click(function() {
                        updateSlideshow($(this), $("#revert-captions-hero"), true, ".carousel-caption", "hidden");
                    });
                    $("#revert-captions-hero").click(function() {
                        updateSlideshow($(this), $("#no-captions-hero"), false, ".carousel-caption", "hidden");
                    });
                    $("#no-indicators-hero").click(function() {
                        updateSlideshow($(this), $("#revert-indicators-hero"), true, ".carousel-indicators", "hidden");
                    });
                    $("#revert-indicators-hero").click(function() {
                        updateSlideshow($(this), $("#no-indicators-hero"), false, ".carousel-indicators", "hidden");
                    });
                    $("#no-controls-hero").click(function() {
                        updateSlideshow($(this), $("#revert-controls-hero"), true, ".carousel-control", "hidden");
                    });
                    $("#revert-controls-hero").click(function() {
                        updateSlideshow($(this), $("#no-controls-hero"), false, ".carousel-control", "hidden");
                    });
                    
                    function updateSlideshow(control, compliment, is_add, target, class_name) {
                        control.addClass("hidden");
                        compliment.removeClass("hidden");
                        if (is_add) {
                            $(target).addClass(class_name);
                        } else {
                            $(target).removeClass(class_name);
                            
                        }
                    }
                });
</script>
<!--Script for Circular Bootstrap tabs Bootstrap 3.1.0 Snippet by riliwanrabo, http://bootsnipp.com/snippets/featured/content-in-bootstrap-tabs  -->
<script>
 $(document).off('click.tab.data-api');
        $(document).on('click.tab.data-api', '[data-toggle="tab"]', function (e) {
            e.preventDefault();
            var tab = $($(this).attr('href'));
            var activate = !tab.hasClass('active');
            $('div.board.tab-content>div.tab-pane.active').removeClass('active');
            $('ul.board.nav.nav-tabs>li.active').removeClass('active');
            if (activate) {
                $(this).tab('show')
            }
        });
</script>
<!--Script for mobile swipe capabilities on the carousel slides -->
<script>
 $("#hero-slider").swiperight(function() {
        $(this).carousel('prev');
    });
    $("#hero-slider").swipeleft(function() {
        $(this).carousel('next');
    });
</script>
<!--Script for tooltip capability -->
<script>
 $(function(){
    $('a[title]').tooltip();
    });
</script>
<!-- Primo scripts that convert the query string into valid parameter -->
<script type="text/javascript">
 function searchPrimo1() {
document.getElementById("primoQuery1").value = "any,contains," + 
document.getElementById("primoQueryTemp1").value.replace(/[,]/g, " ");
document.forms["searchForm1"].submit();
}
</script>
