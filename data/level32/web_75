     <div class="tab-pane fade in active" id="searchall" role="tabpanel">
      <form action="https://nmsu.primo.exlibrisgroup.com/discovery/search?vid=01NEWMEX_INST:NMSU_LC&amp;tab=Everything" enctype="application/x-www-form-urlencoded; charset=utf-8" id="simple1" method="get" name="searchForm1" onsubmit="searchPrimo1()" target="_self">
