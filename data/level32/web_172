        var marker = new google.maps.Marker({position: northCampus, map: map, icon: blueMarker, title: 'NEIU North Campus'});
        var marker2 = new google.maps.Marker({position: elCentro, map: map, icon: blueMarker, title: 'NEIU El Centro'});
        var marker3 = new google.maps.Marker({position: carruthers, map: map, icon: blueMarker, title: 'NEIU Carruthers'});
        var marker4 = new google.maps.Marker({position: universityCenter, map: map, icon: blueMarker, title: 'NEIU University Center'});

        var infowindow = new google.maps.InfoWindow({
