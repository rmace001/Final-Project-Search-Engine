        var clickedStatusOne = "not clicked";
        var clickedStatusTwo = "not clicked";
        var clickedStatusThree = "not clicked";
        var clickedStatusFour = "not clicked";

        google.maps.event.addListener(marker,'mouseover',function() {

                infowindow.open(map,marker);

        });

        google.maps.event.addListener(marker,'mouseout',function() {

                if(clickedStatusOne == "not clicked"){

                    infowindow.close(map,marker);

                }

        });

        google.maps.event.addListener(marker,'click',function() {

                if(clickedStatusOne == "not clicked"){


                    infowindow.open(map,marker);
                    clickedStatusOne = "clicked";

                }

        });

        google.maps.event.addListener(infowindow,'closeclick',function(){

           clickedStatusOne = "not clicked";

        });


        google.maps.event.addListener(marker2,'mouseover',function() {

                infowindow2.open(map,marker2);

        });

        google.maps.event.addListener(marker2,'mouseout',function() {

                if(clickedStatusTwo == "not clicked"){

                    infowindow2.close(map,marker2);

                }

        });

        google.maps.event.addListener(marker2,'click',function() {

                if(clickedStatusTwo == "not clicked"){

                    infowindow2.open(map,marker2);
                    clickedStatusTwo = "clicked";

                }

        });

        google.maps.event.addListener(infowindow2,'closeclick',function(){

           clickedStatusTwo = "not clicked";

        });


        google.maps.event.addListener(marker3,'mouseover',function() {

                infowindow3.open(map,marker3);

        });

        google.maps.event.addListener(marker3,'mouseout',function() {

                if(clickedStatusThree == "not clicked"){

                    infowindow3.close(map,marker3);

                }

        });

        google.maps.event.addListener(marker3,'click',function() {

                if(clickedStatusThree == "not clicked"){

                    infowindow3.open(map,marker3);
                    clickedStatusThree = "clicked";

                }

        });

        google.maps.event.addListener(infowindow3,'closeclick',function(){

           clickedStatusThree = "not clicked";

        });


        google.maps.event.addListener(marker4,'mouseover',function() {

                infowindow4.open(map,marker4);

        });

        google.maps.event.addListener(marker4,'mouseout',function() {

                if(clickedStatusFour == "not clicked"){

                    infowindow4.close(map,marker4);

                }

        });

        google.maps.event.addListener(marker4,'click',function() {

                if(clickedStatusFour == "not clicked"){

                    infowindow4.open(map,marker4);
                    clickedStatusFour = "clicked";

                }

        });

        google.maps.event.addListener(infowindow4,'closeclick',function(){

           clickedStatusFour = "not clicked";

        });


      }
