  <script type="text/javascript">
   //Checks for browser support. If not supported, loads polyfill (for older browsers)
	Modernizr.load({
		test: Modernizr.input.placeholder,
		nope: ['/assets/site/v2/js/placeholders.min.js']
	});
	Modernizr.load({
		test: Modernizr.mq('only all'),
		nope: '//cdnjs.cloudflare.com/ajax/libs/respond.js/1.1.0/respond.min.js'
	});
	
	$(document).ready(function(){
       $('.subnav').navgoco({
            accordion: true,
            openClass: 'active'
        });

  	    $('.slideshow').bxSlider({
        	mode: 'horizontal',
        	speed: 150,
        	touchEnabled: true,
    			captions: true,
    			useCSS: false
		});

        $('.sort-by-count').each(function() {
            var $list = $(this);
            var items = $(this).find('li').get();

            items.sort(function(a,b){
                var keyA = parseInt( $(a).find('.sort-count').first().text() );
                var keyB = parseInt( $(b).find('.sort-count').first().text() );
                
                if (keyA < keyB) return 1;
                if (keyA > keyB) return -1;
                return 0;
            });
    
            $.each(items, function(i, li){
                $list.append(li);
            });
        });
        
        var headings = document.getElementsByClassName('panel-heading jump');
        for(i=0; i<headings.length; i++) {
        	document.getElementById('alpha-key').innerHTML += "<li><span class='sr-only' id='key-" + headings[i].innerHTML + "'>Go to " + headings[i].innerHTML + "</span><a aria-labelledby='key-" + headings[i].innerHTML + "' href='#jump-to-" + headings[i].innerHTML + "'>" + headings[i].innerHTML + "</a></li>";
        }

	});

    buckeyeAlert({
        messageClass: "container"
    });
  </script>
