<script>
 (function($) {
  var container = document.getElementById('mm_live_pane__container');
  var template = document.getElementById('mm_live_pane__template').innerHTML;

  $.getJSON('https://mediamagnet.osu.edu/api/v1/items.json?channel_id[]=377&channel_id[]=376&channel_id[]=379&per_page=7', function(data) {
    $.each(data.items, function(idx, item) {
      item['formatted_published_at'] = moment(item['published_at'], 'YYYY-MM-DD HH:mm:ss Z').fromNow();
      var html = Mustache.to_html(template, item);
      $(html).hide().appendTo(container).fadeIn();
    });
  }).fail(function() {
    $(container).append('<p>Cannot load content.</p>');
  });
})(jQuery);
</script>
