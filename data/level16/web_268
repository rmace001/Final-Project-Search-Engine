             <div class="panel-pane pane-node">
              <h2 class="pane-title">
              </h2>
              <div class="pane-content">
               <div about="/main-page" class="node node-general-content node-promoted node-teaser clearfix" id="node-231" typeof="sioc:Item foaf:Document">
                <span class="rdf-meta element-hidden" content="" property="dc:title">
                </span>
                <div class="node-content clearfix ">
                 <div class="image-wrapper" style="width:176px;">
                  <img alt="Logo" height="200" src="https://writingcenter.gwu.edu/sites/g/files/zaxdzs2016/f/downloads/logo.png" typeof="foaf:Image" width="176"/>
                  <span class="play">
                   <span>
                   </span>
                  </span>
                  <div class="clearfix">
                  </div>
