 <script charset="utf-8" src="assets/site/js/jquery.iosslider.v1.3.20.min.js" type="text/javascript">
 </script>
 <script charset="utf-8" src="assets/site/js/jquery.easing.1.3.js" type="text/javascript">
 </script>
 <script type="text/javascript">
  $(document).ready(function() {
			
				$('.iosSlider').iosSlider({
					snapToChildren: true,
					desktopClickDrag: true,
					responsiveSlideWidth: true,
					navSlideSelector: $('.iosSliderButtons .navbutton'),
					infiniteSlider: true,
					autoSlide: true,
					autoSlideTimer: 8000,
					onSlideChange: slideContentChange,
					onSlideComplete: slideContentComplete,
					onSliderLoaded: slideContentLoaded
				});
				// New style controls added 2-14-18
				$('.iosSliderControlLeft').click(function(){
				    $('.iosSlider').iosSlider('prevSlide');
				});
				$('.iosSliderControlRight').click(function(){
				    $('.iosSlider').iosSlider('nextSlide');
				});
				$('.iosSliderControlShadowLeft').click(function(){
				    $('.iosSlider').iosSlider('prevSlide');
				});
				$('.iosSliderControlShadowRight').click(function(){
				    $('.iosSlider').iosSlider('nextSlide');
				});
			
			    // Bugfix with links 
			    
			    $('.iosSlider .slider .item').click(function(){
			        window.location.href = $(this).children('.text1').children('span').children('a').attr('href');
			    }
			        );
			        //Mobile image replacement
			       // $('.iosSlider .slider .item')
			       
				
				function slideContentChange(args) {
					
					/* indicator */
					$(args.sliderObject).parent().find('.iosSliderButtons .navbutton').removeClass('selected');
					$(args.sliderObject).parent().find('.iosSliderButtons .navbutton:eq(' + (args.currentSlideNumber - 1) + ')').addClass('selected');
					
				}
				
				function slideContentComplete(args) {
					
					if(!args.slideChanged) return false;
					
					/* animation */
					$(args.sliderObject).find('.text1, .text2').attr('style', '');
					
					$(args.currentSlideObject).children('.text1').animate({
						right: '0px',
						opacity: '1'
					}, 400, 'easeOutQuint');
					
					$(args.currentSlideObject).children('.text2').delay(200).animate({
						right: '0px',
						opacity: '1'
					}, 400, 'easeOutQuint');
					
				}
				
				function slideContentLoaded(args) {
					
					/* animation */
					$(args.sliderObject).find('.text1, .text2').attr('style', '');
					
					$(args.currentSlideObject).children('.text1').animate({
						right: '0px',
						opacity: '1'
					}, 400, 'easeOutQuint');
					
					$(args.currentSlideObject).children('.text2').delay(200).animate({
						right: '0px',
						opacity: '1'
					}, 400, 'easeOutQuint');
					
					/* indicator */
					$(args.sliderObject).parent().find('.iosSliderButtons .navbutton').removeClass('selected');
					$(args.sliderObject).parent().find('.iosSliderButtons .navbutton:eq(' + (args.currentSlideNumber - 1) + ')').addClass('selected');
					
				}
				
			});
 </script>
 <link href="/assets/site/magnific/magnific-popup.css" media="screen, projection" rel="stylesheet"/>
