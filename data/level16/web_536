  <script>
   /**
	 *  TAMHSC mod -
	 *  Ugly hack to apply css to the twitter widget, Twitter doesn't provide a way to change the font size...
	 *  based on code from http://stackoverflow.com/questions/17436881/making-script-wait-until-iframe-has-loaded-before-running
	 */
	jQuery( document ).ready(function() {
		setTimeout(function() {
			var head = jQuery("#twitter-widget-0").contents().find("head");
			var css = '<style type="text/css">' +
			'.env-narrow .tweet .header, html, body, .SandboxRoot, h1, h2, h3, blockquote, p, ol, ul, li, iframe, button' +
			'.env-narrow .header .profile .p-name, .env-narrow .timeline-header .byline, .env-narrow .timeline-header .list-description, .env-narrow .e-entry-title, .env-narrow .cards-base h3' +
			'{font-size: 16px; font-family: "Open Sans"; line-height: 22.4px;} ' +
			'.env-narrow.var-chromeless li.tweet { padding: 0 0 18px 0; }'
			'</style>';
			jQuery(head).append(css);
		}, 2000);
	});
  </script>
