  <script>
   function eventTypeGo() {
        var selectedType = document.getElementById('event-categories');
        if(selectedType.selectedIndex != 0) {
            //As long as the first/default option is not selected, redirect to the chosen page
            self.location=selectedType.options[selectedType.selectedIndex].value;
        }
    }
  </script>
