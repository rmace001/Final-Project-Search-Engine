       <style>
        section .quick-links {
							 margin-top: 0 !important;
							 }
							.padded-image {
								padding: 0px 10px 0px 0px !important;				
							}
							.dispDT 
							{
								color: #620010;
								font-weight: 400;
							}
       </style>
       <script src="//code.jquery.com/ui/1.9.1/jquery-ui.min.js" type="text/javascript">
       </script>
       <script type="text/javascript">
        // 
					  
    $(function () {
            $.ajax({
                url: '//campusdata.uark.edu/apiv2/articles/SearchArticle?$orderby=publishDate+desc&$top=5&$filter=((Schools/any(s14:+s14/id+eq+14)))&$top=3',
                data: {},
                contentType: "application/json; charset=utf-8",
			   	dataType: 'jsonp',
//jsonpCallback: 'Articles',
			   	async: "true",
			   	cache: "false",
                success: function (articles) {
                    dataDisplay = "";
                    $.each(articles, function (key, a) {
                        var briefText = "", image = "";
                        if (a.brief != null && a.brief != "") { briefText = a.brief.substr(0, 100) + '...';  briefText = briefText.replace(/(<p>|<\/p>)/g, "");}
                        if (a.image != null && a.image != "") {
                            image = a.image;
                        } else { image = "PMUA_Logo-web.jpg"; }
                        dataDisplay = "<div class='media'>" +
                                        "<img class='img-responsive pull-left padded-image' src='//campusdata.uark.edu/resources/images/articles/" + image + "?w=100' alt='' /></a>" +
"<h4 style='margin-top: 0;'><a href='http://news.uark.edu/articles/" + a.id + "/'>" + a.headline + "</a></h4>" +
                                            "<p>" + briefText + "</p>"+ 
										"</div>";
                        $("#newsArticles").append(dataDisplay);
                    });
                }
            });

    });
//
       </script>
