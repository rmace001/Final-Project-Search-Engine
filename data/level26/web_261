  </div>
  <div class="bc">
   <div class="content pure-g">
    <div class="content pure-u-1">
     <!-- c -->
     <!-- begin page content area-->
     <div class="page-content">
      <!-- feature -->
      <div class="feature">
       <script src="http://www.lsop.colostate.edu/wp-content/themes/webcom-lsop/_support/modules/feature-list/jquery.featureList-1.0.1.js">
       </script>
       <script type="text/javascript">
