         <!--front panel regions beg-->
         <div class="at-panel gpanel panel-display content clearfix" id="content-panels">
          <div class="region region-content-top">
           <div class="region-inner clearfix">
            <section class="block block-boxes block-boxes-os_slideshow_box" delta="1535411019" id="block-boxes-1535411019" module="boxes">
             <div class="block-inner clearfix">
              <h2 class="block-title" ng-non-bindable="">
               Undergraduate Science Education at Harvard University
              </h2>
              <div class="block-content content" ng-non-bindable="">
               <div class="boxes-box" id="boxes-box-1535411019">
                <div class="boxes-box-content">
                 <div id="layout_16_9_side">
                  <div class="slide">
                   <ul class="rslides">
                    <li>
                     <a href="http://lifesciences.fas.harvard.edu/huros">
                      <img alt="HUROS 2018 Photo" class="image-style-os-slideshow-169-980" height="551" src="//static.projects.iq.harvard.edu/files/styles/os_slideshow_16%3A9_980/public/lifesci/files/img_3747.jpg?m=1549050359&amp;itok=zKKvUtp5" title="HUROS 2018 Photo" width="980"/>
                     </a>
                     <div class="caption slide-copy">
                      <h2>
                       <a href="http://lifesciences.fas.harvard.edu/huros">
                        Look and Learn at HUROS: Poster Sessions Give Insight to Research.
                       </a>
                      </h2>
                      <p>
                       <a href="http://lifesciences.fas.harvard.edu/huros">
                        Find Your Perfect Balance in the Sciences
                       </a>
                      </p>
                     </div>
                    </li>
                    <li>
                     <img alt="Science image" class="image-style-os-slideshow-169-980" height="551" src="//static.projects.iq.harvard.edu/files/styles/os_slideshow_16%3A9_980/public/lifesci/files/losick_1_0.jpg?m=1535411096&amp;itok=IQH_07PQ" title="Science image" width="980"/>
                     <div class="caption slide-copy">
                      <h2>
                       The Beauty of Science
                      </h2>
                      <p>
                       Science Education is Form and Function: Where Undergraduate Education in the Physical Sciences &amp; the Life Sciences Clusters Meet
                      </p>
                     </div>
                    </li>
                    <li>
                     <img alt="Science Art" class="image-style-os-slideshow-169-980" height="551" src="//static.projects.iq.harvard.edu/files/styles/os_slideshow_16%3A9_980/public/lifesci/files/cd1_tree.4_01-05_0.jpg?m=1535411522&amp;itok=cvdIxTZX" title="Science Art" width="980"/>
                     <div class="caption slide-copy">
                      <h2>
