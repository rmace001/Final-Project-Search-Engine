	function CheckForm() {
		
		// Do simple checks for the presence of necessary input values first.
		
		//alert("Checking form inputs...");
		
		if ($("#user_type").val() == "") {
			alert("Please select an account type.");
			$("#user_type").focus();
			return false;
		}
		if ($("#schoolid").length) {
			if ($("#schoolid").prop("selectedIndex") == 0) {
				alert("Please select your institution.");
				$("#schoolid").focus();
				return false;
			}
		}
		if ($("#fname").length) {
			if ($("#fname").val() == "") {
				alert("Please enter your first name.");
				$("#fname").focus();
				return false;
			}
		}
		if ($("#lname").length) {
			if ($("#lname").val() == "") {
				alert("Please enter your last name.");
				$("#lname").focus();
				return false;
			}
		}
		var user_email = $("#user_email").val()
		if (user_email == "") {
			alert("Please enter an e-mail address for this account.");
			$("#user_email").focus();
			return false;
		}
		if ($("#user_username_div").length) {
			if ($("#user_username").val() == "") {
				alert("Please enter a username.");
				$("#user_username").focus();
				return false;
			}
		}
		if ($("#schoolid").length) {
			if ($("#schoolid").value == 175) {
				if ($("#user_department").val() == "") {
					alert("Please enter your School.");
					$("#user_department").focus();
					return false;
				}
				if ($("#reason").val() == "") {
					alert("Please enter the reason for your account request.");
					$("#reason").focus();
					return false;
				}
			} else if (requiresDepartment) {
				if ($("#user_department").val().trim() == "") {
					alert("Please enter your Department.");
					$("#user_department").focus();
					return false;
				}
			}
		}
		
		// Validity checks.
		
		// Check fname, lname for validity.
		if ($("#fname").length && $("#lname").length) {
			for (i=0; i < $("#fname").val().length; i++) {
				if ( GoodName.indexOf( $("#fname").val().charAt(i) ) == -1) {
					alert("Please enter a valid first name.\n" + "Special characters such as `~!@#$%^&*_+={}[]\\|;:\"><?/ are not allowed.");
					$("#fname").focus();
					$("#fname").select();
					return false;
				}
			}
			for (i=0; i < $("#lname").val().length; i++) {
				if ( GoodName.indexOf( $("#lname").val().charAt(i) ) == -1) {
					alert("Please enter a valid last name.\n" + "Special characters such as `~!@#$%^&*_+={}[]\\|;:\"><?/ are not allowed.");
					$("#lname").focus();
					$("#lname").select();
					return false;
				}
			}
		}
		
		
		// Check the email for validity.
		if (user_email.indexOf("@") < 1 || 
				user_email.indexOf(".") == -1 || 
				"-_+'./".indexOf(user_email[user_email.length-1]) >= 0) {
			alert("Please enter a valid institutional email address. \nExample: yourname@yourschool.edu");
			$("#user_email").focus();
			$("#user_email").select();
			return false;
		}
		for (i=0; i < user_email.length; i++) {
			if ( GoodEmail.indexOf( user_email.charAt(i) ) == -1) {
				alert("The email address supplied is invalid.\nSpecial characters such as `~!#$%^&*()=[]{}\\|;:\",>< are not allowed.");
				$("#user_email").focus();
				$("#user_email").select();
				return false;
			}
		}
		
		
		// Check the user_name for validity.
		if ($("#user_username_div").length) {
			if ($("#user_username").val().length < 3) {
				alert("Please enter a valid username at least 3 characters long.");
				$("#user_username").focus();
				$("#user_username").select();
				return false;
			} else if ( !(isNaN($("#user_username").val().charAt(0))) ) {
				alert("Please enter a username that starts with a lowercase letter.\n" +
					"Numbers may be used only after the first character.");
				$("#user_username").focus();
				$("#user_username").select();
				return false;
			}
			for (i=0; i < $("#user_username").val().length; i++) {
				if ( GoodUsername.indexOf( $("#user_username").val().charAt(i) ) == -1) {
					alert("Please enter a valid username at least 3 characters long containing only lowercase letters and numbers.\n" +
						"Uppercase letters and special characters such as ~`!@#$%^&*()-_+=[]{}\\|/;:\",.><? or space are not allowed.");
					$("#user_username").focus();
					$("#user_username").select();
					return false;
				}
			}
		}
		
		
		// Check the value of the user_expdate, if needed.
		if ($("#user_expdate").val() == "") {
			alert("Please enter the expiration date for this account.");
			$("#user_expdate").focus();
			return false;
		}
		
		// Check the fac_fname, fac_lname, and fac_email, or user_sponsor and user_sponsor_email, if needed.
		if ($("#fac_email_div").length) {
			if ($("#user_type").val() == "ra") {
				if ($("#fac_fname").val() == "") {
					alert("Please enter the faculty sponsor's first name.");
					$("#fac_fname").focus();
					return false;
				}
				for (i=0; i < $("#fac_fname").val().length; i++) {
					if ( GoodName.indexOf( $("#fac_fname").val().charAt(i) ) == -1) {
						alert("Please enter a valid first name for the faculty sponsor.\n" + "Special characters such as `~!@#$%^&*_+={}[]\\|;:\"><?/ are not allowed.");
						$("#fac_fname").focus();
						$("#fac_fname").select();
						return false;
					}
				}
				if ($("#fac_lname").val() == "") {
					alert("Please enter the faculty sponsor's last name.");
					$("#fac_lname").focus();
					return false;
				}
				for (i=0; i < $("#fac_lname").val().length; i++) {
					if ( GoodName.indexOf( $("#fac_lname").val().charAt(i) ) == -1) {
						alert("Please enter a valid last name for the faculty sponsor.\n" + "Special characters such as `~!@#$%^&*_+={}[]\\|;:\"><?/ are not allowed.");
						$("#fac_lname").focus();
						$("#fac_lname").select();
						return false;
					}
				}
				if ($("#fac_email").val().indexOf("@") < 1 || 
				$("#fac_email").val().indexOf(".") == -1 || 
				$("#fac_email").val().indexOf(".com") > $("#fac_email").val().indexOf("@") || 
				$("#fac_email").val().indexOf(".net") > $("#fac_email").val().indexOf("@") ) {
					alert("Please enter the faculty sponsor's valid institutional email address.\n" + "Example: facultyname@yourschool.edu");
					$("#fac_email").focus();
					$("#fac_email").select();
					return false;
				}
				for (i=0; i < $("#fac_email").val().length; i++) {
					if ( GoodEmail.indexOf( $("#fac_email").val().charAt(i) ) == -1) {
						alert("Please enter the faculty sponsor's valid institutional email address.\n" + "Special characters such as `~!#$%^&*()=[]{}\\|;:\",>< are not allowed.");
						$("#fac_email").focus();
						$("#fac_email").select();
						return false;
					}
				}
			}
		} else if ($("#user_sponsor").length) {
			if ($("#user_type").val() == "ra") {
				if ($("#user_sponsor").val() == "") {
					alert("Please enter the faculty sponsor's name.");
					$("#user_sponsor").focus();
					return false;
				}
				for (i=0; i < $("#user_sponsor").val().length; i++) {
					if ( GoodName.indexOf( $("#user_sponsor").val().charAt(i) ) == -1) {
						alert("Please enter a valid name for the faculty sponsor.\n" + "Special characters such as `~!@#$%^&*_+={}[]\\|;:\"><?/ are not allowed.");
						$("#user_sponsor").focus();
						$("#user_sponsor").select();
						return false;
					}
				}
				if ($("#user_sponsor_email").val().indexOf("@") < 1 || 
				$("#user_sponsor_email").val().indexOf(".") == -1 || 
				$("#user_sponsor_email").val().indexOf(".com") > $("#user_sponsor_email").val().indexOf("@") || 
				$("#user_sponsor_email").val().indexOf(".net") > $("#user_sponsor_email").val().indexOf("@") ) {
					alert("Please enter the faculty sponsor's valid institutional email address.\n" + "Example: facultyname@yourschool.edu");
					$("#user_sponsor_email").focus();
					$("#user_sponsor_email").select();
					return false;
				}
				for (i=0; i < $("#user_sponsor_email").val().length; i++) {
					if ( GoodEmail.indexOf( $("#user_sponsor_email").val().charAt(i) ) == -1) {
						alert("Please enter the faculty sponsor's valid institutional email address.\n" + "Special characters such as `~!#$%^&*()=[]{}\\|;:\",>< are not allowed.");
						$("#user_sponsor_email").focus();
						$("#user_sponsor_email").select();
						return false;
					}
				}
			}
		}		
		
		// Check the value of the class_no, if needed.
		if ($("#user_type").val() == "class" && $("#class_no").length) {
			if ($("#class_no").val() == "") {
				alert("Please enter the number of the class.");
				$("#class_no").focus();
				return false;
			}
			for (i=0; i < $("#class_no").val().length; i++) {
				if ( GoodClassSemester.indexOf( $("#class_no").val().charAt(i) ) == -1) {
					alert("Special characters such as `~!@$%^*={}|\\:;\"'><?/ are not allowed for Course Code.");
					$("#class_no").focus();
					$("#class_no").select();
					return false;
				}
			}
		}
		
		// Check the value of the semester, if needed.
		if ($("#user_type").val() == "class" && $("#semester").length) {
			if ($("#semester").val() == "") {
				alert("Please enter the semester or quarter of the class.");
				$("#semester").focus();
				return false;
			}
			for (i=0; i < $("#semester").val().length; i++) {
				if ( GoodClassSemester.indexOf( $("#semester").val().charAt(i) ) == -1) {
					alert("Special characters such as `~!@$%^*={}|\\:;\"'><?/ are not allowed for Semester/Quarter.");
					$("#semester").focus();
					$("#semester").select();
					return false;
				}
			}
		}
		
		return true;
		
	}
        </script>
        <script type="text/javascript">
         <!--
