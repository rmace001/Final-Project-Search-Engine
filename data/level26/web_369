	var GoodClassSemester = "0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ#&()-_+[],. ";
	var GoodName = "0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ()-',. ";
	var GoodEmail = "0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ@-_+'./";
	var requiresDepartment = false;
	var schoolsRequireDepartment = new Array();
	
				schoolsRequireDepartment[0] = "70";
				
				schoolsRequireDepartment[1] = "731";
				
				schoolsRequireDepartment[2] = "264";
				
				schoolsRequireDepartment[3] = "785";
				
				schoolsRequireDepartment[4] = "454";
				
				schoolsRequireDepartment[5] = "67";
				
				schoolsRequireDepartment[6] = "112";
				
				schoolsRequireDepartment[7] = "41";
				
				schoolsRequireDepartment[8] = "127";
				
				schoolsRequireDepartment[9] = "16";
				
	var userAuthLevel = 0;
	var startingExpDate = '';
	
	$(document).ready(function(){
		SwitchInstitution();
	});
	
	
	// Updated 2013-10-01 to include a check for UPenn, so we can change Department to School field and 
	// a required Reason for Account Request field to be emailed to wrds-support, per Beverley's request.
	function SwitchInstitution() {
		// Start with the assumption that the chosen institution does not require Department to be filled in.
		requiresDepartment = false;
		
		// See if the chosen schoolid is in the array schoolsRequireDepartment[] or not.
		for (var i=0; i < schoolsRequireDepartment.length; i++) {
				if (schoolsRequireDepartment[i] == $("#schoolid").val()) {
				requiresDepartment = true;
				break;
			}
		}
		// Display the correct label based on whether Department is required or optional.
		if (requiresDepartment) {
			$("#user_department_label").html('Department');
			$("#user_department").attr('placeholder', 'Department');
		} else {
			$("#user_department_label").html('Department <small>(optional)</small>');
			$("#user_department").attr('placeholder', 'Department (optional)');
		}

		// UPenn is a special case, needing the School and Reason fields.
		if ($("#schoolid").val() == 175) {
			// Update the label to say: School
			$("#user_department_label").html('School or Center');
			$("#user_department").attr('placeholder', 'School or Center');
			// Also need to show the reason element.
			$("#reason_div").show();
		} else if ($("#schoolid").val() == 56) {
			// Also need to show the reason element.
			$("#reason_div").show();
		} else {
			// Also need to hide the reason element, in case it had been showing.
			$("#reason_div").hide();
		}
	}
	
	
	function SwitchType() {
		// Empty & hide all the fields that are hidden by default in the registration page.
		$("#user_sponsor").val("");
		$("#user_sponsor_email").val("");
		$("#user_expdate_div").hide();
		$("#user_expdate").value = '';
		$("#fac_fname").val("");
		$("#fac_fname_div").hide();
		$("#fac_lname").val("");
		$("#fac_lname_div").hide();
		$("#fac_email").val("");
		$("#fac_email_div").hide();
		$("#class_no").val("");
		$("#class_no_div").hide();
		$("#semester").val("");
		$("#semester_div").hide();
		

		// User selected Visiting Faculty, Ph.D., Research Assistant, Class, or Masters.
		if ($("#user_expdate_div")) {
			$("#user_expdate_div").show();
		}
		// If user selected Visiting Faculty, set the default expiration date.
		if ($("#user_type").val() == "visitor") {
			$("#user_expdate_label").html("When Does Your Visit End? (mm/dd/yyyy)");				
		} else if ($("#user_type").val() == "phd") {
			$("#user_expdate_label").html("When Do You Expect Your Degree? (mm/dd/yyyy)");
		} else if ($("#user_type").val() == "masters" || $("#user_type").val() == "masters_noblackout") {
			$("#user_expdate_label").html("When Do You Expect Your Degree? (mm/dd/yyyy)");
		} else if ($("#user_type").val() == "ra") {
			$("#user_expdate_label").html("When Do You Finish Your Research? (mm/dd/yyyy)");
			// For RA, also need to show the Faculty Member fields
			$("#fac_fname_div").show();
			$("#fac_lname_div").show();
			$("#fac_email_div").show();
		} else if ($("#user_type").val() == "class") {
			$("#user_expdate_label").html("When Does The Current Class Finish? (mm/dd/yyyy)");
			// For class, also need to show the class-related fields
			$("#class_no_div").show();
			$("#class_no_dd").show();
			$("#semester_div").show();
			$("#semester_dd").show();
		} else if ($("#user_type").val() == "faculty" || $("#user_type").val() == "staff" || $("#user_type").val() == "partner") {
			$("#user_expdate_label").html("Your account's renewal date (mm/dd/yyyy)");
		}

		
		//Update Exp Date field according to Types returned from DB
		updateExpDates();
		
	} 
	
	
	// just a function for setting the starting exp date in the case this is a user edit 
	// need this if a user is being edited for other reasons than exp date updating - otherwise updateExpDates() will overwrite existing exp date
	function setStartingExpdate()
	{
		startingExpDate = $("#user_expdate").val();
	}
	
	
	function updateExpDates()
	{
		selectedType = $("#user_type").val();

		
		switch (selectedType) {
			
			case 'class': 
				defaultDate = '03/09/2020';
				newMaxDate = '03/09/2020';
				newMinDate = '03/11/2019';
				
				if(startingExpDate == '')
				{
					$("#user_expdate").val(defaultDate);
				}
	
				//reset the min and max of dates depending on the user type settings
				$('#user_expdate').datepicker('option', 'minDate', newMinDate);
				$('#user_expdate').datepicker('option', 'maxDate', newMaxDate);
				
				$('#user_expdate').prop('title','Maximum Date: ' + newMaxDate + '');
				

				$("#user_expdate").change(function() {
					$(this).datepicker('setDate', $(this).val());
				});

				//alert("minDate: " + newMinDate + ", maxDate: " + newMaxDate + ", curr: " + $("#user_expdate").val());

				break;
			
			case 'demo': 
				defaultDate = '06/09/2019';
				newMaxDate = '09/09/2019';
				newMinDate = '03/11/2019';
				
