				{
					$("#user_expdate").val(defaultDate);
				}
	
				//reset the min and max of dates depending on the user type settings
				$('#user_expdate').datepicker('option', 'minDate', newMinDate);
				$('#user_expdate').datepicker('option', 'maxDate', newMaxDate);
				
				$('#user_expdate').prop('title','Maximum Date: ' + newMaxDate + '');
				

