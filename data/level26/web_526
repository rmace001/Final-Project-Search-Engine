<script type="text/javascript">
 var activeStateID = '';
    $(document).ready(function () {
        setTimeout(function(){
            
            $(window).resize(function() {
                simplemaps_usmap.load();
            });
        
            // STATE HOOK CLICK  - if locations exist
            simplemaps_usmap.hooks.zoomable_click_state = function (id) {
                stateZoom(id);
            }

            // LOCATION HOOK CLICK
            simplemaps_usmap.hooks.click_location = function (id) {
                var location = simplemaps_usmap_mapdata.locations[id]
                if (location.zoomable) {
                    $('#corpUSAMap_inner').toggle();
                    google.maps.event.trigger(map, 'resize');
                    map.setCenter(new google.maps.LatLng(location.lat, location.lng));
                    map.setZoom(8);
                    $('#backBtn').addClass('hidden');
                    $('#backBtnFromMicro').removeClass('hidden');
                }

            }
            simplemaps_usmap.hooks.back = function () {
                simplemaps_usmap_mapdata.state_specific[activeStateID] = activeStateSettings; // reset settings for the previously active state
                simplemaps_usmap_mapdata.main_settings.all_locations_hidden = 'yes'; // hide locations
                simplemaps_usmap.refresh();
                $('#' + activeStateID + '_panel').addClass('hidden');
                $('#US_panel').show();
                $('#backBtn').addClass('hidden');
                activeStateID = '';
            }
            simplemaps_usmap.hooks.complete = function () {
                $('#corpUSAMap_inner svg').css('overflow', 'visible');
            }
        }, 1000);
      

        $('#backBtn').click(function (e) {
            simplemaps_usmap.back();
        });
        $('#backBtnFromMicro').click(function (e) {
            $('#backBtn').removeClass('hidden');
            $('#backBtnFromMicro').addClass('hidden');
            $('#corpUSAMap_inner').toggle();
        });

        var hoverColor = $("#corpUSAMap_hoverColor").css("color");
        var hasPropertiesColor = $("#corpUSAMap_hasPropertiesColor").css("color");
        var emptyColor = $("#corpUSAMap_emptyColor").css("color");
        var strokeColor = $("#corpUSAMap_strokeColor").css("color");
        var labelColor = $("#corpUSAMap_stateLabelColor").css("color");
        
        if(hoverColor == hasPropertiesColor && hasPropertiesColor == emptyColor && emptyColor == strokeColor){
            hoverColor = "#336699";
            hasPropertiesColor = "#666";
            emptyColor = "#ccc";
            strokeColor = "#333";
        }

        simplemaps_usmap_mapdata.main_settings.state_color = emptyColor;
        simplemaps_usmap_mapdata.main_settings.state_hover_color = emptyColor;
        simplemaps_usmap_mapdata.main_settings.label_color = labelColor;
        
        var activeStates = ["IL"];
        for(i=0; i < activeStates.length; i++) {
            simplemaps_usmap_mapdata.state_specific[activeStates[i]].color = hasPropertiesColor;
            simplemaps_usmap_mapdata.state_specific[activeStates[i]].hover_color = hasPropertiesColor;
        }
    });
    
    function stateZoom(id) {
        if(activeStateID != '') {
            simplemaps_usmap_mapdata.state_specific[activeStateID] = activeStateSettings; // reset settings for the previously active state
        }
        var zoomColor = $("#corpUSAMap_stateZoomColor").css("color");
        var locationColor = $("#corpUSAMap_locationColor").css("color");
        activeStateID = id; // gte the active state ID
        activeStateSettings = jQuery.extend({}, simplemaps_usmap_mapdata.state_specific[id]); // copy the default settings
        simplemaps_usmap_mapdata.state_specific[id].color = zoomColor;
        simplemaps_usmap_mapdata.state_specific[id].hover_color = zoomColor;
        simplemaps_usmap_mapdata.state_specific[id].inactive = 'yes';
        simplemaps_usmap_mapdata.main_settings.all_locations_hidden = 'no';
        for(i=0; i < simplemaps_usmap_mapdata.locations.length; i++) {
            simplemaps_usmap_mapdata.locations[i].color = locationColor;
        }
        simplemaps_usmap.refresh();
        $('#' + id + '_panel').removeClass('hidden');
        $('#US_panel').hide();
        $('#backBtn').removeClass('hidden');
    }
</script>
