<script type="text/javascript">
 $(window).load(function () {
       function addLazyScript(src, tmout) {
           var script = document.createElement("script");
           document.getElementsByTagName("body")[0].appendChild(script);
           script.src = src;
           script.async = true;
           clearTimeout(tmout);
       }
         var timeout0 = setTimeout(function() { addLazyScript('https://cdngeneral.rentcafe.com/js/ysiPortalValidation.min.1551777688.js', timeout0) }, 1000);
   });
</script>
<script type="text/javascript">
 if (location.pathname.indexOf("searchlisting.aspx") > -1) //Search Results
					$("body").attr("id", "corp_searchresults");
	else if (location.pathname.indexOf("default.aspx") > -1) //Property Detail Page
					$("body").attr("id", "propertydetail");
	else if (location.pathname.indexOf("ourproperties.aspx") > -1) // Corporate Home Page
					$("body").attr("id", "corp_homepage");
	else if (location.pathname.indexOf("index.aspx") > -1 || location.pathname.length == 1) //Home Page
					$("body").attr("id", "homepage");
</script>
<!-- temporary solution to remove the dark grey background being added to the photo gallery images to make them to the same size, will need to be taken care of at global level in the next release -->
<script type="text/javascript">
 $( document ).ajaxComplete(function() {
		if($('#propertydetail #gallery .carousel-inner').length > 0) {
			newImages= $('#propertydetail #gallery .carousel-inner').html().replace(/bgcolor=333333/g,'bgcolor=F2F2F2');
			$('#propertydetail #gallery .carousel-inner').html(newImages);
		}
	});
</script>
