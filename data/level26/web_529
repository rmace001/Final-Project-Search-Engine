      <div class=" m_image-text__img l_self_t l_col_s-full l_m_b--20 l_m_md_b--40 l_m_lg_b--0 ">
       <picture class="u_blobject-fit">
        <source sizes="(min-width: 1280px) 500px, 100vw" srcset="https://polsky.uchicago.edu/wp-content/uploads/2017/11/Katlin-02-150x150.webp 150w, https://polsky.uchicago.edu/wp-content/uploads/2017/11/Katlin-02-500x500.webp 500w, https://polsky.uchicago.edu/wp-content/uploads/2017/11/Katlin-02-1000x1000.webp 1000w, https://polsky.uchicago.edu/wp-content/uploads/2017/11/Katlin-02-1500x1500.webp 1500w" type="image/webp"/>
        <source sizes="(min-width: 1280px) 500px, 100vw" srcset="https://polsky.uchicago.edu/wp-content/uploads/2017/11/Katlin-02-150x150.jpg 150w, https://polsky.uchicago.edu/wp-content/uploads/2017/11/Katlin-02-500x500.jpg 500w, https://polsky.uchicago.edu/wp-content/uploads/2017/11/Katlin-02-1000x1000.jpg 1000w, https://polsky.uchicago.edu/wp-content/uploads/2017/11/Katlin-02-1500x1500.jpg 1500w" type="image/jpeg"/>
