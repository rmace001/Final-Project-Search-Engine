    var map = AmCharts.makeChart("chartdiv", {
        "type": "map",
        "theme": "light",
        "projection": "eckert3",
        "allLabels": [{
            "text": "Click on the continents to \n explore Rice's impact",
            "bold": true,
            "color": "#f1f1f1",
            "size": 20,
            "x": 50,
            "y": 400
        }],
        "areasSettings": {
            "descriptionWindowWidth": "600",
            "autoZoom": true,
            "outlineColor": "#004080",
            "rollOverOutlineColor": "#ffffff",
            "selectedColor": "#0076ec",
            "color": "#ffffff",
            "rollOverColor": "#cccccc",
            "descriptionWindowY": 0,
            "descriptionWindowX": 0
        },


        "dataProvider": continentsDataProvider,


        "listeners": [{
                "event": "homeButtonClicked",
                "method": handleGoHome
            }, {
                "event": "clickMapObject",
                "method": handleMapObjectClick
            },
            {
                "event": "descriptionClosed",
                "method": function (ev) {
                    ev.chart.selectObject();
                }
            }
        ]

    });

    var processorsArray = [
        require('autoprefixer')({
            grid: true,
            browsers: ['>1%']
        })
    ];

    function handleGoHome() {
        map.dataProvider = continentsDataProvider;
        map.validateNow();
    }

    function handleMapObjectClick(event) {
        if (event.mapObject.id == "backButton") {
            handleGoHome();
        }
    }
               </script>
               <div class="chartwrapper-wrapper">
                <div class="chartwrapper">
                 <div class="chartdiv" id="chartdiv">
                 </div>
                </div>
               </div>
              </link>
             </link>
            </div>
           </div>
