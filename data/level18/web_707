        <script type="text/javascript">
         $(document).ready(function() {
            var $flexslider = $('.flexslider');
            $flexslider.flexslider({
                animation: "fade",
                controlNav: true,
                slideshowSpeed: 8000,
                directionNav: false,
                before: function() {
                       $flexslider.find('.slider-youtube').each(function(){$(this).tubeplayer('pause');});
                   },
                after:function(){
                    $flexslider.css({height: $flexslider.find('.flex-active-slide').height() + $flexslider.find('flex-active-slide').height() });
                    $flexslider.find('.flex-active-slide .slider-youtube[data-autoPlay=1]').each(function() {$(this).tubeplayer('play');});
                },
                start: function(){
                      $flexslider.find('.slider-youtube').each(function(){
                         var _videoDiv = $(this);
                         _videoDiv.tubeplayer({
                             width: $flexslider.width(),
                             height: $flexslider.height(),
                             initialVideo: _videoDiv.data('video'),
                             allowFullScreen: 'true',
                             showControls: 0,
                             protocol: 'https',
                             theme: 'light',
                             color: 'white',
                             onPlayerPlaying: function() {
                                $flexslider.flexslider('pause');
                                $flexslider.data('flexslider').manualPause = true;
                                $flexslider.data('flexslider').manualPlay = false;
                             },
                             onPlayerEnded: function() {
                                $flexslider.data('flexslider').manualPause = false;
                                $flexslider.flexslider('next');
                             },
                             onPlayerPaused: function() {
                                $flexslider.data('flexslider').manualPause = false;
                             }
                         });
                      });
                   }
            });
        });
        </script>
        <img alt="" class="under-slider-img" src="_includes/images/sections/homepage/under-slider-960x25.png"/>
