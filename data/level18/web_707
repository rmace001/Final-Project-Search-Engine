       //
    $subNavs = $(' ul.sub-nav');
    $('.nav-item').hover(
        function(){
            var $this = $(this);
            
            var topOffset = $this.height();
            var topWidth = $this.width();
            $subNavs.css('display', 'none');
           // $(this).find('ul.sub-nav').css({ 'top' : topOffset+'px', 'z-index':'999', 'width' : topWidth+'px'}).slideDown();
           // $(this).find('ul.sub-nav > li').css('width', topWidth+'px');
           $(this).find('ul.sub-nav').css({ 'top' : topOffset+'px', 'z-index':'999'}).slideDown();
           
        },
        function(){
            $(this).find('ul.sub-nav').css('display', 'none');
        }
    );
    $('#subject-navigation').css('overflow','visible');
    $('.subject-navigation').css('overflow','visible');
//
