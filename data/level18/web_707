        // 
    if($.getInstagramJSON){
            $.getInstagramJSON({
                accessToken : "476546681.e77fa16.4c4ffbd2a5ee44debc219517b04420c7",
                get : "user",
                user: 476546681,
                limit: 6,
                callback: function(dataSet){
                    if(dataSet.data){
                        $.each(dataSet.data, function(i, imageData){
                            if(i < 6){
                                var altText = "Candler School of Theology - Instagram Feed Image";
                                if(imageData.caption && imageData.caption.text){
                                    altText = imageData.caption.text; 
                                }
                                $('.instagram-stream').append('<a target="_blank" href="'+imageData.link+'" title="'+altText.replace('"',"''")+'"><img src="'+imageData.images.thumbnail.url+'" alt="'+altText.replace('"',"''")+'" /></a>');
                            }
                        });
                    }
                }
            });
     }
     //
       </script>
