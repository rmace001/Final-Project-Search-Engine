       <script type="text/javascript">
        // 
    $(document).ready(function(){
        var totalCallouts = 2;
        var $calloutContainer = $('#homepage-callout-set');
        var $staticCallouts = $calloutContainer.find('.static');
        var $randomCallouts = $calloutContainer.find('.random');
        if(totalCallouts < $staticCallouts.length){
            $staticCallouts.find(':gt('+(totalCallouts-1)+')').hide();
            $randomCallouts.hide();
        } else {
            var neededCallouts = totalCallouts - $staticCallouts.length;
            
            $randomCallouts.hide();
            for(var i=0; i < neededCallouts ; i++){
                if($randomCallouts[0]){
                     var randomCallout = $randomCallouts.splice(Math.floor(Math.random() * $randomCallouts.length),1);
                     $(randomCallout).show();
                }
            }
        }
    });
//
       </script>
