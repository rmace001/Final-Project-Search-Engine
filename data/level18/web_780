  <script type="text/javascript">
   // mobile menu setup
			jQuery(document).ready(function(){ 
  				
  				/*--SITE MENU MOBILE--*/
				jQuery('#navToggle').click(function(){
					jQuery('#menuContainer').slideToggle(function() {
					    jQuery('#menu').show();
						if (jQuery('#menu').is(':hidden')) {
				            jQuery('body').css('overflow','auto');	           
				        } else {
				           jQuery('body').css('overflow','hidden');
				        }
					});
				});


				/*--move filter menu on screen in mobile--*/
				jQuery('#programSearchLaunch').click(function(){
					jQuery('body').css('overflow','hidden');
					jQuery('#programSearch').animate({ left: 0 });
				});

				/*--hide filter menu in mobile--*/
				jQuery('#mobileFilterDone').click(function() {
					jQuery('body').css('overflow','auto');
					jQuery('#programSearch').animate({ left: '100%' }, 200);
				});
			
				jQuery('#allNavRight > a.social').click(function()
				{
					//console.log(this.title);
					ga('send','event','socialMedia','click',this.title);
					//return false;
				});

			
				
			});
  </script>
