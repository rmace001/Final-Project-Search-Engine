</script>
<!-- CLOSE all html-elements (+wrapper) before this line (body and html tags excluded)-->
<!-- Custom JS (if available)-->
<!-- slideshow -->
<script src="js/jquery.bxSlider.min.js" type="text/javascript">
</script>
<script src="js/jquery.easing.1.3.js" type="text/javascript">
</script>
<!-- initialize slideshow -->
<script type="text/javascript">
 $(document).ready(function(){
	
	if ( navigator.userAgent.match(/Android/i) ||  navigator.userAgent.match(/webOS/i) ||  navigator.userAgent.match(/iPad/i) || navigator.userAgent.match(/iPhone/i) ||  navigator.userAgent.match(/iPod/i)){
		sliderMode = 'horizontal';
		sliderSpeed = 500;
		sliderControls = false;
		sliderPause = 6000;
		sliderPager = true;
		
		var tempImageSlider = document.getElementById('imageSlider');
 
		if (tempImageSlider.hasChildNodes()) {
			
			// Get all children of node
			var children = tempImageSlider.childNodes;               
	
			// Loop through the children
			for(var c=0; c < children.length; c++) {
				if(children[c].style) {
					children[c].style.display = 'list-item';
				}
			}
		}
	} 
	else {
		sliderMode = 'fade';
		sliderSpeed = 1000;
		sliderControls = true;
		sliderPause = 12000;
		sliderPager = false;
	}
	
	var slider = $('#imageSlider').bxSlider({
		mode: sliderMode, 		//type of transition
		speed: sliderSpeed,		//amount of time for transition
		controls: sliderControls,		//show controls
		randomStart: false,	//display in random order
		pause: sliderPause,		//time bewteen slides
		auto: true,		//start automatically
		pager: sliderPager,		//shows page numbers
		prevText: 'Previous item', //Text for the Previous link
		nextText: 'Next item', //Text for the Next link 
		useCSS: false,
		easing: 'easeOutCubic',	//type of transition easing
		slideMargin: 20		//margin between the slides
	});
 
});
</script>
<script>
 // This script is used to fix a bug with the current Twitter Widget (9/13/2013). In IE8 and below, the progress bar in the status bar never completes.
// If the browser is IE8 or below, this script will add and then remove an iframe to make the status bar complete.

(function ($) {
    "use strict";

    // Detecting IE
    var oldIE;
    if ($('html').is('.ie6, .ie7, .ie8')) {
        oldIE = true;
    }

    if (oldIE) {
		window.setTimeout(function() {
		  var fakeFrame = document.createElement("IFRAME");
		  fakeFrame.id = "fakeFrame";
		  fakeFrame.name = 'hehe';
		  fakeFrame.src = "/favicon.ico";
		  fakeFrame.style.border='0';
		  fakeFrame.style.width='0';
		  fakeFrame.style.height='0';
		  fakeFrame.style.frameBorder = 0;
		  document.body.appendChild(fakeFrame);
		  document.body.removeChild(fakeFrame);
		}, 1500);
    } 

}(jQuery));
</script>
<script>
 jQuery(function($) {
		$('body').css('display','block');
	});
</script>
<script>
 // This script will override the default link hover color in the Twitter widget so that it matches the rest of the page
// It also removes the gray "on touch" highlight that iOS adds. 
	  
setInterval(function() {
	$('#twitter-widget-0').contents().find('head').append('<style>.timeline a:focus, .timeline a:hover {color:#BB0000;} body {-webkit-tap-highlight-color: rgba(0,0,0,0);} .e-entry-content img {display: none;}</style>'); 
}, 500);
</script>
