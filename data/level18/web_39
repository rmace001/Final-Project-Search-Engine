      <script>
       (function($){ 
  $('.breadcrumbs .breadcrumb>span').each(function( index ) { 
    console.log( index + ": " + $( this ).text() );
    if($(this).text() === "Group") { 
      $(this).prev().remove(); /* Remove previous sibling (delimiter) */
      $(this).remove(); }
  }); 
})(jQuery);
