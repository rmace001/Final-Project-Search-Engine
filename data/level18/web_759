(function($){
  jQuery(document).ready(function($) {

    //Nav Search
    jQuery("#header-inside .search-btn").on("click", function() {
    var sfield = jQuery('.nav-search .search-field');

      if ( sfield.hasClass('open')) {
        sfield.removeClass('open');
      } else {
        sfield.addClass('open');
      }

  });
});
})(jQuery);

(function($){

	/* Start - Nuke Un-necessary P tags */

	/* Empty P tags */
	// var pTags = jQuery("p");

	// $(pTags).each(function(index) {
	// 	var pTag = $(this)[0]
	// 	if ( pTag.innerHTML == "" ) {
	// 		pTag.remove();
	// 	}
	// });

	/* End - Nuke Un-necessary P tags */


	var course_p_length = $('.blog_short_content p').length;
		pElementsHeight = 0;

		$(".blog_short_content p").each(function(){
			pElementsHeight += $(this).height();
		});

	console.log(course_p_length , " << p_length")
	console.log(pElementsHeight , " << pElements combined height")

	if(course_p_length > 3 && pElementsHeight > 200) {
		console.log("Sufficient P Tags, Showing Expander")

		$(".blog_short_content p").slice(0, 2).show();
		$("#loadMore").toggleClass("change_arrow");

		$("#loadMore").on('click', function (e) {
			e.preventDefault();

			$(".blog_short_content").toggleClass('expanded');
			$("#loadMore").toggleClass("change_arrow");
			var visible_block = $(".blog_short_content p:visible").length;
			if (visible_block >= 3) {
				$(".blog_short_content p").slice(0, 2).show();
				$(".blog_short_content p").slice(2, 20).slideUp();
			}  else {
				$(".blog_short_content p:hidden").slice(0, 20).slideDown();
			}
		});
	}else{
		console.log("Not Enough P Tags, Hiding Expander")
		$("#loadMore").hide();
	}
})(jQuery);

(function($){

	$(document).ready(function() {
		console.log("Checking URL..")
		//FAQ Filters by URL
		if( window.location.href.indexOf("e=0") != -1 ) {
			$("select#edit-field-department-reference-faq-tid").val(0).change();
		}
		if( window.location.href.indexOf("e=1") != -1 ) { //Center for College Readiness
			$("select#edit-field-department-reference-faq-tid").val(1).change();
		}
		if( window.location.href.indexOf("e=2") != -1 ) { //Center for Philanthropy & Nonprofit Leadership
			$("select#edit-field-department-reference-faq-tid").val(2).change();
		}
		if( window.location.href.indexOf("e=3") != -1 ) { //Education Department
			$("select#edit-field-department-reference-faq-tid").val(3).change();
		}
		if( window.location.href.indexOf("e=4") != -1 ) { //English as a Second Language ( Not Used, outside link )
			$("select#edit-field-department-reference-faq-tid").val(4).change();
		}
		if( window.location.href.indexOf("e=5") != -1 ) { //Foreign Languages ( Not Used, outside link )
			$("select#edit-field-department-reference-faq-tid").val(5).change();
		}
		if( window.location.href.indexOf("e=6") != -1 ) { //Graduate Liberal Studies
			$("select#edit-field-department-reference-faq-tid").val(6).change();
		}
		if( window.location.href.indexOf("e=7") != -1 ) { //Personal Development
			$("select#edit-field-department-reference-faq-tid").val(7).change();
		}
		if( window.location.href.indexOf("e=8") != -1 ) { //Professional Development
			$("select#edit-field-department-reference-faq-tid").val(8).change();
		}
		if( window.location.href.indexOf("e=9") != -1 ) { //School Literacy & Culture
			$("select#edit-field-department-reference-faq-tid").val(9).change();
		}
	})

})(jQuery);

(function($){
	$("a.down-arrow-banner").click(function(){
		$('html,body').animate({
            scrollTop: $($(this).attr('href')).offset().top
        }, 1500);
	});
})(jQuery);

(function($){

	var windowWidth = $(window).width();

	if(windowWidth >= 992){
		//var inst_height = $("#coll_inner_pane_left" ).height();
		//	alert(inst_height);
		setTimeout(function(){
			var inst_height = $("#coll_inner_pane_left" ).height();
			if( inst_height < 200 ) {
				$(".certif_right").css('height', 'auto');
			}else if( inst_height < 750 ) {
				$(".certif_right").css('height', 'auto');
			}else{
				$(".certif_right").css('height', $("#coll_inner_pane_left" ).height());
			}
		}, 2000);

		$(window).resize(function(){
			setTimeout(function(){
			var inst_height = $("#coll_inner_pane_left" ).height();
			if( inst_height < 200 ) {
				$(".certif_right").css('height', 'auto');
			}else if( inst_height < 750 ) {
				$(".certif_right").css('height', 'auto');
			}else{
				$(".certif_right").css('height', $("#coll_inner_pane_left" ).height());
			}
		}, 2000);
		});
	}
})(jQuery);

