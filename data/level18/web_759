jQuery(document).ready(function($) {

	$('.form-item.form-type-bef-checkbox').bind('click',function() {
		setTimeout(function(){
			jQuery('#edit-submit-test3').click();
		},1000)
	});

	$('.form-item.form-type-textfield.form-item-shs-term-node-tid-depth.shs-wrapper-processed').bind('click',function() {
		setTimeout(function(){
			jQuery('#edit-submit-test3').click();
		},1000)
	});

	$('.form-item.form-type-select').bind('click',function() {
		setTimeout(function(){
			jQuery('#edit-submit-test3').click();
		},1000)
	});
});





$('#search-header-input').keypress(function(event){
	var keycode = (event.keyCode ? event.keyCode : event.which);
	var search_id = $('#search_id').html();
	var name = $("#search-header-input").val();
	var location_url = window.location.protocol + "//" + window.location.host + "/";
	//alert(location_url);
	if(keycode == '13'){
		if(search_id != null && search_id != undefined && name != undefined && name != null) {
			window.location = "/search-catalog?keys=" + name + "&&field_department_reference_cours_tid[]=" + search_id;
		}
		else {


			var vals = $("#search-header-input").val();


			window.location = "/search-catalog?keys=" + vals;
			// location.href = "http://google.com";
		}
	}
	event.stopPropagation();
});


	var windowWidth4 = $(window).width();
	if(windowWidth4 >= 1200){
		var result_height = $('.page-search-catalog #promoted-inside #block-system-main .view-header').height();

		$('.page-search-catalog #promoted-inside #block-system-main .view-filters').css('top',-result_height);

		//console.log(result_height);
	}
	$(window).resize(function(){
		var windowWidth4 = $(window).width();
		if(windowWidth4 >= 1200){
			var result_height = $('.page-search-catalog #promoted-inside #block-system-main .view-header').height();

			$('.page-search-catalog #promoted-inside #block-system-main .view-filters').css('top',-result_height);

			//console.log(result_height);
		}
	});

	$('img').bind('contextmenu', function(e){
		return false;
	});

	var windowWidth5 = $(window).width();
	var container_width = $('.container').width();
	var total_width = (windowWidth5  - container_width) / 2;
	var final_total = total_width - 15;
	$('.header_strip_color').css('width',final_total);

	$(window).resize(function(){
		var windowWidth5 = $(window).width();
		var container_width = $('.container').width();
		var total_width = (windowWidth5  - container_width) / 2;
		var final_total = total_width - 15;
		$('.header_strip_color').css('width',final_total);
	});


	var btn_count = $('.certif_right .btns a').length;
	// alert(btn_count);
	if( btn_count == 4 || btn_count == 2 ) {
		$('.certif_right .btns a').attr('style','width:40% !important');
	}




	/* Video Autoplay script */

	setTimeout(function(){

		$(".front .banner_img-Dept video").get(0).play();
		$(".front .banner_img-Dept video").attr('loop','loop');
		$(".front .banner_img-Dept video").removeAttr('controls','controls');
		//$(".front .banner_img-Dept video").attr('poster','/sites/default/files/glasscock-school-video-block.gif');

	},1000);

	var IS_ANDROID = navigator.userAgent.match(/Android/i);
	if(IS_ANDROID){
		$(".front .banner_img-Dept video").attr('poster','/sites/default/files/glasscock-school-video-block.gif');
		setTimeout(function(){
			$(".front .banner_img-Dept video").attr('poster','/sites/default/files/glasscock-school-video-block.gif');
		},1000);
	}else {
		//alert("ios");
		/* $(".front .banner_img-Dept video").css('background','url("/sites/default/files/glasscock-school-video-block.gif") no-repeat top center') */
	}



})(jQuery);
</script>
