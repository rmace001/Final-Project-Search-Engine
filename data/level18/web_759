(function($){

	$(".page-search-catalog #promoted-inside .views-exposed-widget > label").click(function() {
		$(this).next().slideToggle( "slow");
		$(this).toggleClass( "expand_link_search" );
	});
	$(".page-search-catalog #promoted-inside #edit-field-avilability-value-wrapper.views-exposed-widget  > label").click(function() {
		event.stopPropagation();
	});

	var topLocation = window.location.href;
	jQuery(document).on('click','.view-header p',function(){
	var urlValue = jQuery(this).data('qstr');

	var checkid = $('#' + urlValue);
	if(urlValue=='edit-keys'){
		$(checkid).val('');
	}
	else if(urlValue.match("edit-shs")){

			if(urlValue.match("depth-select-1"))
		{

			$('#edit-shs-term-node-tid-depth').val('All');

		}else{
			var allval = $('#edit-shs-term-node-tid-depth').val();
			var outg = $(checkid).val();

			//outg = outg.concat("+");

			var finval = allval.replace(outg,'');

			var lastchar = finval[finval.length-1];
			if(lastchar=='+'){
				var newStr = finval.substring(0, finval.length-1);
				$('#edit-shs-term-node-tid-depth').val(newStr);
		}
		else{
				$('#edit-shs-term-node-tid-depth').val(finval);
		}
				 $( "#views-exposed-form-test3-page" ).submit();
				}
			}
			else{
				 checkid.prop("checked", false);
			}
			$( "#views-exposed-form-test3-page" ).submit();
	});


	jQuery(document).on('click','#edit-field-avilability-value-2',function(){
		var topLocation = window.location.href;




           if($("#edit-field-avilability-value-2").prop('checked') == true &&  topLocation.search( 'field_avilability_value=2')!=-1){
    //do something

	  $('#edit-field-avilability-value-1').prop("checked", true);

	  $( "#views-exposed-form-test3-page" ).submit();

}//
else{
	  $( "#views-exposed-form-test3-page" ).submit();

}
	});

})(jQuery);

(function($){
	$(".front .search_banner input[type='text']").attr("placeholder", "Search Courses...");
	$(".front .search_banner input[type='text']").attr('autocomplete', 'off');
	$('.page-search-catalog #promoted-inside #block-system-main .views-exposed-widget input[type="text"]').attr('placeholder','Search');
	$('.node-type-department-sub-page #quicktabs-media_center  .views-exposed-widget input[type="text"]').attr('placeholder','Search');
})(jQuery);

