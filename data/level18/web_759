<script>
 (function($){


		$(".not-front #search_btn").click(function() {
				var search_id = $('#search_id').html();
				var name = $("#search_key").val();
				if(name==undefined){
					name = $('#search-footer-input').val();
				}

				if(search_id != null && search_id != undefined && name != undefined && name != null) {
					window.location = "/search-catalog?keys=" + name + "&&field_department_reference_cours_tid[]=" + search_id;
				}
				else {
					window.location = "/search-catalog?keys=" + name;
				}
		});

		$(".front #search_btn").click(function() {

				var search_id = $('#search_id').html();
				var name = $(".search_banner input[type='text']").val();
				if(name==undefined){
					name = $('#search-footer-input').val();
				}

				if(search_id != null && search_id != undefined && name != undefined && name != null) {
					window.location = "/search-catalog?keys=" + name + "&&field_department_reference_cours_tid[]=" + search_id;
				}
				else {
					window.location = "/search-catalog?keys=" + name;
				}
		});

		$(".search-btn1").click(function() {
				var search_id = $('#search_id').html();
				var name = $("#search-footer-input").val();
				if(name==undefined){
					name = $('#search-footer-input').val();
				}

				if(search_id != null && search_id != undefined && name != undefined && name != null) {
					window.location = "/search-catalog?keys=" + name + "&&field_department_reference_cours_tid[]=" + search_id;
				}
				else {
					window.location = "/search-catalog?keys=" + name;
				}
		});


		$('.search_banner  input[type="text"]').keypress(function(event){
			var keycode = (event.keyCode ? event.keyCode : event.which);
			var search_id = $('#search_id').html();
			var name = $(".search_banner  input[type='text']").val();

			if(keycode == '13'){
				if(search_id != null && search_id != undefined && name != undefined && name != null) {
					window.location = "/search-catalog?keys=" + name + "&&field_department_reference_cours_tid[]=" + search_id;
				}
				else {
					window.location = "/search-catalog?keys=" + name;
				}
			}
			event.stopPropagation();
		});

		$('#search-header-input').keypress(function(event){
			var keycode = (event.keyCode ? event.keyCode : event.which);
			var search_id = $('#search_id').html();
			var name = $("#search-header-input").val();

			if(keycode == '13'){
				if(search_id != null && search_id != undefined && name != undefined && name != null) {
					window.location = "/search-catalog?keys=" + name + "&&field_department_reference_cours_tid[]=" + search_id;
				}
				else {


					var vals = $("#search-header-input").val();


					 //window.location = "/search-catalog?keys=" + vals;
					// location.href = "http://google.com";
				}
			}
			event.stopPropagation();
		});


	})(jQuery);
</script>
