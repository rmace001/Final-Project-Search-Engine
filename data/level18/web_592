		if(window.location.search.substring(1)!=""){
		var lrfi=$("a:contains('Request Info')");
		if(lrfi.attr("href").indexOf('?')==-1)
		{lrfi.attr("href",lrfi.attr("href")+window.location.search);}
		else{lrfi.attr("href",lrfi.attr("href")+"&"+window.location.search.substring(1));}
		}
		var loc=window.location.search.substring(1);
		loc=loc.replace('utm_source','sys%3Afield%3Autm_source');
		loc=loc.replace('utm_medium','sys%3Afield%3Autm_medium');
		loc=loc.replace('utm_term','sys%3Afield%3Autm_term');
		loc=loc.replace('utm_campaign','sys%3Afield%3Autm_campaign');
		document.cookie="ricerfi="+loc+";domain=.business.rice.edu;path=/";
	})
