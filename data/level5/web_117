    <div id="TEST_Sidebar" style="background:transparent; position: relative;">
     <script src="https://volunteer.ucla.edu/wp-content/themes/UCLA_Volunteer/static/js/feature/calendar/moment.min.js">
     </script>
     <script src="https://volunteer.ucla.edu/wp-content/themes/UCLA_Volunteer/static/js/feature/calendar/fullcalendar.js">
     </script>
     <script src="https://volunteer.ucla.edu/wp-content/themes/UCLA_Volunteer/static/js/feature/calendar/gcal.js">
     </script>
     <script>
      var mobdetect = false;

            if( mobdetect && window.location.href.indexOf("form=show") >= 0) {
                window.location.replace("/newsletter/");
            }

             jQuery(document).ready(
            
                function($) {
                    var numEvts = 0;
                    var evtArray; 
                    var date = new Date();
                    var m = date.getMonth();

                $('#calendarNow').fullCalendar({
                    googleCalendarApiKey: 'AIzaSyApeQzEl3Im38CAzOfhkm1KfyV_K8NoDAA',
                    eventSources: [

                            {  
                                googleCalendarId: '7d95lvgqhsgda0oimrhqq1033k@group.calendar.google.com',
                                className: 'gcal-event'
                            }
                    ],
                    eventRender: function(event, element) {
                       var now = new Date();

                       if(event.end > now){
                            var start=moment(event.start).format('dddd, MMMM Do'); 
                            var theTitle = event.title;
                            var end = (moment(event.end).diff(event.start, "hours") < 25) ? "" : " - " + moment(event.end - 1).format('dddd, MMMM Do');
                            if(typeof event.description !== "undefined"){ 
                                var desc = event.description;
                                if(desc.indexOf('THIS IS THE URL: ') >= 0)
                                    var url = 'http://www.' + event.description.substring(desc.indexOf('THIS IS THE URL: ') + 17);
                                else
                                    var url = "#";
                            }
                             else
                                    var url = "#";

                             $('#upcoming').append('<li data-date="'+ event.start + '"><a href="'+ url +'"><div class="ename">' + event.title + '</div></a><div class="edate" style=" margin-top: 5px; ">' + start + end + '</div>');
                            numEvts++;
                            $('#upcoming li').hide();   
                        }
                    }
                     
                });
                
                $('#calendarNextMonth').fullCalendar({
                    googleCalendarApiKey: 'AIzaSyApeQzEl3Im38CAzOfhkm1KfyV_K8NoDAA',
                    eventSources: [

                            {  
                                googleCalendarId: '7d95lvgqhsgda0oimrhqq1033k@group.calendar.google.com',
                                className: 'gcal-event'
                            }
                    ],
                    eventRender: function(event, element) {
                       var now = new Date();
                       if(event.end > now){
                            var start=moment(event.start).format('dddd, MMMM Do'); 
                            var theTitle = event.title;
                            var end = (moment(event.end).diff(event.start, "hours") < 25) ? "" : " - " + moment(event.end - 1).format('dddd, MMMM Do');
                            if(typeof event.description !== "undefined"){ 
                                var desc = event.description;
                                if(desc.indexOf('THIS IS THE URL: ') >= 0)
                                    var url = 'http://www.' + event.description.substring(desc.indexOf('THIS IS THE URL: ') + 17);
                                else
                                    var url = "#";
                            }
                             else
                                    var url = "#";

                             $('#upcoming').append('<li data-date="'+ event.start + '"><a href="'+ url +'"><div class="ename">' + event.title + '</div></a><div class="edate" style=" margin-top: 5px; ">' + start + end + '</div>');
                            numEvts++;
                            $('#upcoming li').hide();   
                        }
                    }
                     
                });
                

                 $('#calendarNextMonth').fullCalendar('next');

                $('#calendarMonthAfterNext').fullCalendar({
                    googleCalendarApiKey: 'AIzaSyApeQzEl3Im38CAzOfhkm1KfyV_K8NoDAA',
                    eventSources: [

                            {  
                                googleCalendarId: '7d95lvgqhsgda0oimrhqq1033k@group.calendar.google.com',
                                className: 'gcal-event'
                            }
                    ],
                    eventRender: function(event, element) {
                       var now = new Date();

                       if(event.end > now){
                            var start=moment(event.start).format('dddd, MMMM Do'); 
                            var theTitle = event.title;
                            var end = (moment(event.end).diff(event.start, "hours") < 25) ? "" : " - " + moment(event.end - 1).format('dddd, MMMM Do');
                            if(typeof event.description !== "undefined"){ 
                                var desc = event.description;
                                if(desc.indexOf('THIS IS THE URL: ') >= 0)
                                    var url = 'http://www.' + event.description.substring(desc.indexOf('THIS IS THE URL: ') + 17);
                                else
                                    var url = "#";
                            }
                             else
                                    var url = "#";

                             $('#upcoming').append('<li data-date="'+ event.start + '"><a href="'+ url +'"><div class="ename">' + event.title + '</div></a><div class="edate" style=" margin-top: 5px; ">' + start + end + '</div>');
                            numEvts++;
                            $('#upcoming li').hide();   
                        }
                    }
                     
                });

                 $('#calendarMonthAfterNext').fullCalendar('next');
                 $('#calendarMonthAfterNext').fullCalendar('next');

                $(window).load(function(){

                    // sort function callback
                    $("#upcoming li").sort(function(a, b){
                        return $(a).data('date') - $(b).data('date') ;    
                    }).appendTo('#upcoming');

                    var seen = {};
                    $('#upcoming li').each(function(i) {
                        var txt = $(this).children('a').children('.ename').text();
                        var before = $(this).prev().children('a').children('.ename').text();
                        var after = $(this).next().children('a').children('.ename').text();

                        if(txt === before || txt === after){
                            $(this).remove();
                        }
                    });

                    $("#upcoming li:nth-child(-n+4)").show();
                    if($("#upcoming li").length === 0){
                        $("#upcoming").html("<li class='moreEvtSoon'>More events coming soon!</li><span></span>");
                    }
                })
            })
     </script>
