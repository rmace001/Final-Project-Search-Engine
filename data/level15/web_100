  </script>
  <script type="text/javascript">
   jQuery(function($) {
    jQuery(document).bind('gform_post_render', function(){

        $('.ginput_container_address > span, .ginput_container_name > span').each(function (index, value) {
            $(this).find('input,textarea').attr('placeholder',$(this).find('label').html());
            if (   $(this).parent().hasClass('ginput_container_address')   ) {
                $(this).find('select').val('United States');
                if (     $(this).hasClass('address_state')     ) {
                    $(this).find('input').val('North Carolina');
                }
            }

        });

        $('.ginput_container_time').each(function (index, value) {
            $(this).find('input').attr('placeholder',$(this).find('label').html());
        });

        $('form.secondary-form').each(function () {
            $(this).find('.gfield').each(function (index, value) {
                if ($(this).find('> .ginput_container').is('.ginput_container_multiselect, .ginput_container_textarea, .ginput_container_checkbox, .ginput_container_list, .ginput_container_fileupload')) {
                    $(this).find('> label').show();
                }

                if ($(this).find('> .ginput_container').is('.ginput_container_name, .ginput_container_address')) {
                    if ($(this).find('.gfield_label span').length > 0 ) {
                        $(this).find('> .ginput_container span input').each(function () {
                            var placeholderText = $(this).attr('placeholder') + ' *';
                            $(this).attr('placeholder', placeholderText);
                        });
                    }
                }
                $(this).find('.clear-multi .ginput_container_time ').each(function () {
                    if ($(this).is('.gfield_time_hour, .gfield_time_minute')) {
                        var placeholderText = $(this).find('input').attr('placeholder') + ' *';
                        $(this).find('input').attr('placeholder', placeholderText);
                    }
                });


                if ($(this).find('> .ginput_container').is('.ginput_container_select')) {
                    if ($(this).find('> label > span').length > 0 ) {
                        $(this).find('> label > span').remove();
                        var labelText = $(this).find('> label').html() + ' *';
                    } else {
                        var labelText = $(this).find('> label').html();
                    }
                    $(this).find('> .ginput_container > select').prepend($('<option>', {value:'', text:labelText, selected:'selected'}));
                }

                if (!$(this).find('> .ginput_container').is('.ginput_container_name, .ginput_container_address, .ginput_container_multiselect, .ginput_container_textarea, .ginput_container_checkbox, .ginput_container_list, .ginput_container_fileupload')) {
                    if ($(this).find('> label > span').length > 0 ) {
                        $(this).find('> label > span').remove();
                        var labelText = $(this).find('> label').html() + ' *';
                    } else {
                        var labelText = $(this).find('> label').html();
                    }
                    $(this).find('> .ginput_container > input, > .ginput_container > textarea').attr('placeholder',labelText );
                }



            });
        });

      });
});
  </script>
 </body>
</html>

