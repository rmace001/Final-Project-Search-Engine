  <div class="secondarymaroonicons-academics" id="secondarymaroonicons">
   <div class="opaqueback">
    <div class="container">
     <div class="section-header text-center center">
      <h2 id="degree">
       Degree Programs
      </h2>
     </div>
     <div class="row">
      <div>
       <ul class="nav nav-tabs nav-justified" role="tablist">
        <li class="active" role="presentation">
         <a aria-controls="undergraduate" data-toggle="tab" href="#undergraduate" role="tab">
