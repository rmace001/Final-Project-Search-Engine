            <section class="block block-boxes block-boxes-os_sv_list_box events" delta="1502307879" id="block-boxes-1502307879" module="boxes">
             <div class="block-inner clearfix">
              <h2 class="block-title" ng-non-bindable="">
               Upcoming Public Events at Harvard
              </h2>
              <div class="block-content content" ng-non-bindable="">
               <div class="boxes-box" id="boxes-box-1502307879">
                <div class="boxes-box-content">
                 <div class="os-sv-list os_sv_list_box sv-list-event sv-list-view-sidebar_teaser sv-list-layout-list sv-grid-3" data-delta="1502307879" data-page="0" id="box-1502307879-page">
                  <article class="node node-event article event-start clearfix" id="node-1157774" role="article" target="_top">
                   <div class="event-start">
                    <span class="event-year">
                     2019
                    </span>
                    <span class="event-start-month">
                     Mar
                    </span>
                    <span class="event-start-day">
                     10
                    </span>
                   </div>
                   <div class="event-content">
                    <header class="node-header">
                     <h1 class="node-title" ng-non-bindable="" rel="nofollow">
