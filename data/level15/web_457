  <script src="./assets/js/vendor/owl.carousel.min.js">
  </script>
  <script>
   $(function() {

    		var isInt = function(n) {
				return n % 1 === 0;
			}

    		var countUpOptions = {
				useEasing: true,
				useGrouping: true,
				separator: ',',
				decimal: '.',
			};

			$('.hero-video-controls').on('click', function() {
				var heroVideo = $('.hero-video')[0];
				$(this).toggleClass('is-paused');
				if(heroVideo.paused) {
					heroVideo.play();
				} else {
					heroVideo.pause();
				}
			});

			$('.js-counter-waypoint').waypoint({
				handler: function() {

					var countUpDecimalPlaces = 0;
					var countUpSpeed = parseFloat($(this.element).data('speed')) || 3;
					var countUpMax = this.element.innerText.replace(/,/g, '');

					if ( isInt(countUpMax) ) {
						var countUpMax = parseInt(countUpMax.replace(/,/g, ''), 10);
					} else {
						var countUpMax = parseFloat(countUpMax);
						countUpDecimalPlaces = 1;
					}

					var countUpUtility = new CountUp(this.element.id, 0, countUpMax, countUpDecimalPlaces, countUpSpeed, countUpOptions);

					if (!countUpUtility.error) {
						countUpUtility.start();
					} else {
						console.error(countUpUtility.error);
					}

					this.destroy();
				},
				offset: '95%'
			});

			$(".owl-carousel").owlCarousel({
				margin: 15,
				nav: true,
				navText: [
					"<i class='fa fa-caret-left'></i>",
					"<i class='fa fa-caret-right'></i>"
				],
				responsive: {
					0: {
						items: 1
					},
					768: {
						items: 2
					},
					960: {
						items: 3
					},
					1200: {
						items: 4
					}
				}
			});

        });
  </script>
