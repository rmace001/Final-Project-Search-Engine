 $(document).ready(function () {
            $(".lazy").lazy({
                visibleOnly: 0,
                enableThrottle: 0,
                throttle: 250
            });

            window.sr = ScrollReveal();
            var srOptions = {
                origin: "bottom",
                distance: "100px",
                duration: 750,
                easing: "cubic-bezier(0.5, 0, .5, 1)",
                viewFactor: .2,
                opacity: 0
            };
            srNewsOptions = srOptions;
            srNewsOptions.delay = 0, sr.reveal(".news-blurb-featured-image", srNewsOptions), srNewsOptions.delay =
                150, sr.reveal(".news-blurb-right", srNewsOptions), srNewsOptions.viewFactor = .8,
                srNewsOptions.delay = 400, srNewsOptions.distance = "200px", sr.reveal(
                    ".news-card:nth-child(1)", srNewsOptions), srNewsOptions.delay = 500, sr.reveal(
                    ".news-card:nth-child(2)", srNewsOptions), srNewsOptions.delay = 600, sr.reveal(
                    ".news-card:nth-child(3)", srNewsOptions);
            var srEventsOptions = srOptions;
            srEventsOptions.viewFactor = .9, srEventsOptions.distance = "100px", sr.reveal(
                    ".highlight-triangles-container", srEventsOptions), srEventsOptions.delay = 300, sr.reveal(
                    ".toggles", srEventsOptions), srEventsOptions.viewFactor = .4, srEventsOptions.delay = 175,
                sr.reveal(".event-display", srEventsOptions);
            var srGridOptions = srOptions,
                delayMax = 300;
            srGridOptions.delay = 150, srGridOptions.distance = "15px", sr.reveal(".grid-item", srGridOptions);
            var srTwitterOptions = srOptions;
            srTwitterOptions.viewFactor = .7;
            srTwitterOptions.viewOffset = {
                top: 85
            };
            srTwitterOptions.distance = "15px", sr.reveal(".section-title-social-container",
                srTwitterOptions), srTwitterOptions.viewFactor = 1, srTwitterOptions.viewOffset = {
                top: 200
            };
            sr.reveal(".twitter-feed-wrapper", srTwitterOptions), $("#sidebar").mCustomScrollbar({
                theme: "minimal",
                contentTouchScroll: !1,
                documentTouchScroll: !1
            })
        });
